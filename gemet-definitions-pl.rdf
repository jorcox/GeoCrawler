<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
            xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
            xmlns:skos="http://www.w3.org/2004/02/skos/core#"
            xmlns:gemet="http://www.eionet.europa.eu/gemet/2004/06/gemet-schema.rdf#"
            xml:lang="pl">
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7">
<skos:prefLabel>opuszczony obiekt przemysłowy</skos:prefLabel>
<skos:definition>teren, który nie może być użytkowany z uwagi na zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8">
<skos:prefLabel>porzucony pojazd</skos:prefLabel>
<skos:definition>pojazd porzucony w środowisku miejskim lub poza miejskim, często uszkodzony lub zdekompletowany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11">
<skos:prefLabel>abiotyczny czynnik</skos:prefLabel>
<skos:definition>czynniki fizyczny, chemiczny lub inny nieożywiony, stanowiący konieczny element ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13">
<skos:prefLabel>absorpcja (ekspozycja)</skos:prefLabel>
<skos:definition>przyjmowanie płynów lub innych substancji przez komórki lub tkanki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/20">
<skos:prefLabel>dopuszczalny poziom ryzyka</skos:prefLabel>
<skos:definition>poziom ryzyka, uznany za akceptowany z uwagi na korzyści wynikające z działania związanego z ryzykiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/21">
<skos:prefLabel>porozumienie (administracyjne)</skos:prefLabel>
<skos:definition>porozumienie dwóch lub więcej stron dotyczące ich praw i obowiązków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/22">
<skos:prefLabel>droga dojazdowa</skos:prefLabel>
<skos:definition>wydzielony pas ziemi łączący poszczególne miejscowości lub punkty terenu, przystosowany do komunikacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/23">
<skos:prefLabel>dostęp do dóbr kultury</skos:prefLabel>
<skos:definition>zdolność i prawo korzystania z zasobów związanych z przekazywaniem wierzeń, zwyczajów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/24">
<skos:prefLabel>dostęp do morza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/25">
<skos:prefLabel>awaria</skos:prefLabel>
<skos:definition>niespodziewane zdarzenie, potencjalnie stwarzające zagrożenie dla środowiska i zdrowia ludzi lub własności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/28">
<skos:prefLabel>przypadkowe uwalnianie organizmów</skos:prefLabel>
<skos:definition>genetycznie zmodyfikowane organizmy przypadkowo uwalniane do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/30">
<skos:prefLabel>źródła awarii</skos:prefLabel>
<skos:definition>źródło lub powód niespodziewanego wydarzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/36">
<skos:prefLabel>gromadzenie substancji w tkankach ciała</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/38">
<skos:prefLabel>akumulator</skos:prefLabel>
<skos:definition>urządzenie do magazynowania energii, zwłaszcza urządzenie do magazynowania energii elektrycznej w postaci energii chemicznej, stanowiące źródło prądu stałego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/42">
<skos:prefLabel>opad kwaśnego deszczu</skos:prefLabel>
<skos:definition>kwaśny opad powstały na skutek wymywania z powietrza związków siarki i azotu, emitowanych z zanieczyszczeniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/44">
<skos:prefLabel>zakwaszenie</skos:prefLabel>
<skos:definition>dodanie kwasu do roztworu tak, aby pH spadło poniżej 7.0</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/47">
<skos:prefLabel>kwasowość</skos:prefLabel>
<skos:definition>cecha roztworów elektrolitów, w których jest przewaga stężenia jonów wodorowych nad stężeniem jonów wodorotlenowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/48">
<skos:prefLabel>poziom zakwaszenia</skos:prefLabel>
<skos:definition>zawartość kwasu wyrażana poziomem pH</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/51">
<skos:prefLabel>kwaśny deszcz</skos:prefLabel>
<skos:definition>deszcz o pH poniżej 5.6</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/52">
<skos:prefLabel>kwasy</skos:prefLabel>
<skos:definition>związki chemiczne odznaczające się kwaśnym smakiem, zdolnością do barwnych reakcji ze wskaźnikami (barwią lakmus na czerwono) i zdolnością reagowania z niektórymi metalami (z wydzielaniem wodoru) oraz rozpuszczaniem wielu substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/54">
<skos:prefLabel>filtr akustyczny</skos:prefLabel>
<skos:definition>układ akustyczny ograniczający przenoszenie fal akustycznych o odpowiednim zakresie częstotliwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/55">
<skos:prefLabel>izolacja dźwiękowa</skos:prefLabel>
<skos:definition>proces zapobiegania rozprzestrzenianiu się dźwięku poprzez izolowanie materiałem nieprzewodzącym dźwięku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/57">
<skos:prefLabel>poziom akustyczny</skos:prefLabel>
<skos:definition>pomiar fizyczny dźwięku, wyrażony najczęściej w decybelach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/59">
<skos:prefLabel>cechy akustyczne</skos:prefLabel>
<skos:definition>charakterystyka konstrukcji określająca jakość dźwięku i sposób jego odbioru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/60">
<skos:prefLabel>akustyka</skos:prefLabel>
<skos:definition>dział fizyki i techniki obejmujący naukę o wytwarzaniu, odbiorze i rozchodzeniu się fal sprężystych w ośrodku gazowym, ciekłym i stałym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/66">
<skos:prefLabel>aktynowce</skos:prefLabel>
<skos:definition>promieniotwórcze, ciężkie pierwiastki chemiczne, metale, maksymalnie sześciowartościowe, należące do siódmego okresu okresowego układu pierwiastków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/68">
<skos:prefLabel>aktyn</skos:prefLabel>
<skos:definition>(Ac) promieniotwórczy pierwiastek chemiczny należący do aktynowców, liczba atomowa 89, metal srebrzysty; występuje w minimalnych ilościach w rudach uranu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/71">
<skos:prefLabel>grupa działania</skos:prefLabel>
<skos:definition>zbiorowość, której członkowie połączeni są jakąś więzią, społeczną, ideologiczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/72">
<skos:prefLabel>węgiel aktywny</skos:prefLabel>
<skos:definition>czarna, porowata substancja stała, otrzymywana z węgla kamiennego, brunatnego, torfu, drewna, kości itp.; stosowana jako adsorbent w przemyśle chemicznym i w lecznictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/73">
<skos:prefLabel>czynny osad</skos:prefLabel>
<skos:definition>zespół mikroorganizmów, które rozkładają organiczne składniki ścieków na proste, nietoksyczne substancje nieorganiczne, najczęściej w ramach drugiego stopnia oczyszczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/77">
<skos:prefLabel>aktywny udział</skos:prefLabel>
<skos:definition>uczestnictwo zarówno indywidualne jak grupowe w rządzeniu lub innej działalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/78">
<skos:prefLabel>interwencja na lądzie stałym</skos:prefLabel>
<skos:definition>wkraczanie lub udział w procesie rozstrzygania sporów, w przypadku gdy działania mają miejsce na lądzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/79">
<skos:prefLabel>akt</skos:prefLabel>
<skos:definition>1) czyn, działanie, wykonanie, spełnienie czegoś jako objaw zewnętrzny, fizyczny lub jako proces psychiczny; 2) dokument urzędowy, prawny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/81">
<skos:prefLabel>gatunki łatwo przystosowujące się do otoczenia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/83">
<skos:prefLabel>okres adaptacji</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/84">
<skos:prefLabel>reakcja chemiczna</skos:prefLabel>
<skos:definition>przemiana jednych substancji w inne, której towarzyszy wymiana z otoczeniem pewnej ilości energii, najczęściej ciepła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/86">
<skos:prefLabel>opakowanie dodatkowe</skos:prefLabel>
<skos:definition>dodatkowe opakowanie stanowiące zabezpieczenie przed kradzieżą lub pełniące funkcję reklamy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/87">
<skos:prefLabel>polimer powstały w wyniku reakcji przyłączenia</skos:prefLabel>
<skos:definition>polimer powstały w wyniku reakcji przyłączenia cząsteczek monomeru, gdy nie powstają produkty uboczne reakcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/88">
<skos:prefLabel>dodatki</skos:prefLabel>
<skos:definition>substancje dodawane do produktu w małych ilościach, mające zmienić jego własności chemiczne lub stan fizyczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/90">
<skos:prefLabel>spoiwo</skos:prefLabel>
<skos:definition>materiał spajający części</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/91">
<skos:prefLabel>dopuszczalna dawka dobowa</skos:prefLabel>
<skos:definition>miara ilości substancji, którą organizm człowieka może bezpiecznie przyjąć w ciągu doby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/95">
<skos:prefLabel>administracja</skos:prefLabel>
<skos:definition>ogół czynności wykonywanych przez organy państwowe lub samorządowe w zakresie władzy wykonawczej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/100">
<skos:prefLabel>organ administracji</skos:prefLabel>
<skos:definition>wyodrębniona jednostka organizacyjna aparatu państwowego pełniąca określone funkcje w dziedzinie życia społecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/103">
<skos:prefLabel>kompetencje personelu administracyjnego</skos:prefLabel>
<skos:definition>określona wiedza, kwalifikacje, które stanowią podstawę działania, wydawania decyzji, wypowiadania autorytatywnych i miarodajnych sądów, opinii, ocen</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/105">
<skos:prefLabel>zarządzenie administracyjne</skos:prefLabel>
<skos:definition>akt normatywny wydany przez organ administracji na podstawie ustawy i w celu jej wykonania, regulujący sprawy nie wymagające rozporządzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/107">
<skos:prefLabel>administracyjna jurysdykcja</skos:prefLabel>
<skos:definition>uprawnienie organu państwa do rozstrzygania spornych spraw; obszar, na który rozciągają się te uprawnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/108">
<skos:prefLabel>prawo administracyjne</skos:prefLabel>
<skos:definition>ogół norm regulujących działalność organów zarządu państwowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/111">
<skos:prefLabel>procedura administracyjna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/112">
<skos:prefLabel>sankcje administracyjne</skos:prefLabel>
<skos:definition>konsekwencje prawne (kara pieniężna, konfiskata, zajęcie mienia itd.) jakie powinny być zastosowane wobec podmiotów (osób), których zachowanie nie było zgodne z normą prawną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/120">
<skos:prefLabel>adsorbcja</skos:prefLabel>
<skos:definition>proces polegający na powierzchniowym wiązaniu cząsteczek płynu (cieczy lub gazu) przez cząsteczki ciała stałego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/121">
<skos:prefLabel>dorosły osobnik</skos:prefLabel>
<skos:definition>człowiek w pełni dorosły, gotowy do określonych zadań, odpowiednio ukształtowany, osiągający wysoki stopień swoich możliwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/122">
<skos:prefLabel>edukacja dorosłych</skos:prefLabel>
<skos:definition>nieformalne lub formalne szkolenie osób dorosłych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/126">
<skos:prefLabel>reklama</skos:prefLabel>
<skos:definition>rozpowszechnianie informacji o towarach, ich zaletach, wartości, miejscach i możliwościach nabycia, chwalenie kogoś, zalecanie czegoś przez prasę, radio, telewizję; środki (np. plakaty, napisy, ogłoszenia itp.) służące temu celowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/129">
<skos:prefLabel>reklamowanie produktów</skos:prefLabel>
<skos:definition>tworzenie i rozpowszechnianie płatnych ogłoszeń, mających na celu zwrócenie uwagi na konkretny produkt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/130">
<skos:prefLabel>rada</skos:prefLabel>
<skos:definition>ocena, zaopiniowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/132">
<skos:prefLabel>napowietrzanie</skos:prefLabel>
<skos:definition>nasycanie powietrzem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/135">
<skos:prefLabel>zdjęcie lotnicze</skos:prefLabel>
<skos:definition>wizerunek powierzchni ziemi utrwalony na materiale światłoczułym, wykonany ze znacznej wysokości; np. z samolotu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/143">
<skos:prefLabel>warunki tlenowe</skos:prefLabel>
<skos:definition>warunki życia typowe dla wielu gatunków, wymagające obecności tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/146">
<skos:prefLabel>reakcje tlenowe</skos:prefLabel>
<skos:definition>reakcje wymagające obecności tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/148">
<skos:prefLabel>biologia atmosfery</skos:prefLabel>
<skos:definition>badania rozprzestrzeniania się w powietrzu aerozoli bakteryjnych, drobnych owadów i nasion oraz zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/151">
<skos:prefLabel>hałas aerodynamiczny</skos:prefLabel>
<skos:definition>hałas powodowany przez turbulentny przepływ powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/157">
<skos:prefLabel>aerosol</skos:prefLabel>
<skos:definition>układ rozproszony, w którym ośrodkiem rozpraszającym jest gaz, a fazą rozproszoną ciecz lub ciało stałe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/163">
<skos:prefLabel>estetyka</skos:prefLabel>
<skos:definition>nauka o pięknie zajmująca się badaniem wartości artystycznych oraz ocen estetycznych, dociekająca przyczyn ich kształtowania się oraz ustalająca kryteria tych wartości i ocen</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/167">
<skos:prefLabel>zalesianie</skos:prefLabel>
<skos:definition>zakładanie młodego lasu na terenach leśnych lub dotychczas niezalesionych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/170">
<skos:prefLabel>Afryka</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/171">
<skos:prefLabel>dopalanie</skos:prefLabel>
<skos:definition>urządzenie umożliwiające utlenianie (dopalanie palnych składników) gazów wylotowych, wykorzystywane także do zwiększania ciągu silników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/173">
<skos:prefLabel>wiek</skos:prefLabel>
<skos:definition>przeciąg czasu od urodzenia człowieka lub zwierzęcia do danej chwili lub do śmierci; liczba lat od wykiełkowania rośliny (zwłaszcza drzewa) do chwili obecnej lub do jej obumarcia; liczba lat przeżytych; czas istnienia czegoś</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/182">
<skos:prefLabel>porozumienie (prawne)</skos:prefLabel>
<skos:definition>uzgodnienie stanowiska w określonej kwestii przez dwie lub więcej stron</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/183">
<skos:prefLabel>porozumienie (kontrakt)</skos:prefLabel>
<skos:definition>umowa, konwencja lub zgodne wyrażenie woli dwóch lub więcej stron (osób) w formie pisemnej, stwarzające wzajemne prawa i obowiązki, np. umowa &quot;kupno-sprzedaż&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/185">
<skos:prefLabel>agrobiotechnologia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/186">
<skos:prefLabel>budynki wiejskie</skos:prefLabel>
<skos:definition>budynki wchodzące w skład gospodarstwa rolnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/191">
<skos:prefLabel>ekologia w rolnictwie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/193">
<skos:prefLabel>ekonomika rolnictwa</skos:prefLabel>
<skos:definition>dyscyplina ekonomiczna badająca zjawiska i prawidłowości występujące w dziedzinie rolnictwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/194">
<skos:prefLabel>odpady rolnicze</skos:prefLabel>
<skos:definition>emisje stałe, ciekłe lub gazowe, trafiające do środowiska jako uboczny skutek produkcji rolnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/195">
<skos:prefLabel>inżynieria rolna</skos:prefLabel>
<skos:definition>dyscyplina wiedzy zajmująca się tworzeniem i rozwijaniem możliwości zapewnienia pożywienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/197">
<skos:prefLabel>sprzęt rolniczy</skos:prefLabel>
<skos:definition>sprzęt stosowany do prac rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/199">
<skos:prefLabel>użytkowanie rolnicze</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/202">
<skos:prefLabel>grunty rolne</skos:prefLabel>
<skos:definition>tereny wykorzystywane przede wszystkim do produkcji rolnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/204">
<skos:prefLabel>krajobraz rolniczy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/206">
<skos:prefLabel>prawodawstwo dotyczące rolnictwa</skos:prefLabel>
<skos:definition>ogół ustaw, przepisów prawnych obowiązujących w dziedzinie rolnictwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/207">
<skos:prefLabel>sprzęt rolniczy</skos:prefLabel>
<skos:definition>maszyny stosowane do prac rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/208">
<skos:prefLabel>zarządzanie rolnictwem</skos:prefLabel>
<skos:definition>administracja i obrót ziemią, zbożami lub zwierzętami gospodarskimi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/210">
<skos:prefLabel>metody agrotechniczne</skos:prefLabel>
<skos:definition>ogół zabiegów, stosowanych przy uprawie roli i roślin w celu uzyskania jak najlepszych plonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/212">
<skos:prefLabel>szkodniki rolne</skos:prefLabel>
<skos:definition>owady i gryzonie, których działanie powoduje spadek plonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/213">
<skos:prefLabel>planowanie w rolnictwie</skos:prefLabel>
<skos:definition>planowanie rozwoju i środków w celu zwiększenia produkcji artykułów rolnych, planowanie ochrony środowiska i zarządzanie zasobami rolnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/215">
<skos:prefLabel>polityka rolna</skos:prefLabel>
<skos:definition>działalność władzy państwowej, rządu w dziedzinie rolnictwa; cele i zadania takiej działalności, metody realizacji tych zadań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/216">
<skos:prefLabel>zanieczyszczenie powodowane przez rolnictwo</skos:prefLabel>
<skos:definition>zanieczyszczenie powstające na skutek prowadzenia produkcji rolnej, w tym spływ z nawożonych pól, padłe zwierzęta, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/219">
<skos:prefLabel>produkcja rolna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/220">
<skos:prefLabel>produkty rolne</skos:prefLabel>
<skos:definition>efekt uprawy ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/224">
<skos:prefLabel>magazynowanie  produktów rolnych</skos:prefLabel>
<skos:definition>magazynowanie lub gromadzenie produktów rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/229">
<skos:prefLabel>odpady rolnicze</skos:prefLabel>
<skos:definition>materiały ciekłe lub stałe powstające na skutek produkcji rolnej, nie nadające się do wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/232">
<skos:prefLabel>rolnictwo</skos:prefLabel>
<skos:definition>uprawa i hodowla roślin oraz chów i hodowla zwierząt gospodarskich mająca na celu uzyskanie produktów roślinnych i zwierzęcych; jedna z podstawowych i najstarszych dziedzin gospodarki człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/233">
<skos:prefLabel>intensywne uprawa i hodowla zwierząt</skos:prefLabel>
<skos:definition>wielkoskalowa produkcja roślinna i zwierzęca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/235">
<skos:prefLabel>plan rozwoju rolnictwa</skos:prefLabel>
<skos:definition>systematyczna metoda zarządzania w rolnictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/237">
<skos:prefLabel>agrochemia</skos:prefLabel>
<skos:definition>nauka zajmująca się badaniem żyzności gleby i zagadnieniami nawożenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/239">
<skos:prefLabel>gospodarka rolno-leśna</skos:prefLabel>
<skos:definition>łączenie w przestrzeni rolniczego i leśnego użytkowania terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/240">
<skos:prefLabel>przemysł rolno-spożywczy</skos:prefLabel>
<skos:definition>przemysł zajmujący się gromadzeniem, przetwarzaniem i dystrybucją produktów rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/241">
<skos:prefLabel>meteorologia rolnicza</skos:prefLabel>
<skos:definition>badanie zależności pomiędzy czynnikami meteorologicznymi i hydrologicznymi a szeroko pojętym rolnictwem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/243">
<skos:prefLabel>AIDS</skos:prefLabel>
<skos:definition>choroba wirusowa polegająca na załamaniu się odporności organizmu prowadzącym do trudnych do opanowania zakażeń oraz rozwoju nowotworów złośliwych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/245">
<skos:prefLabel>powietrze</skos:prefLabel>
<skos:definition>mieszanina gazów (głównie azotu i tlenu) otaczająca grubą warstwą kulę ziemską, tworząca jej atmosferę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/250">
<skos:prefLabel>klimatyzacja</skos:prefLabel>
<skos:definition>system kontrolowania temperatury, czasem wilgotności i czystości powietrza w pomieszczeniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/253">
<skos:prefLabel>statek powietrzny</skos:prefLabel>
<skos:definition>urządzenie latające, które może się poruszać lub utrzymywać w atmosferze wskutek statycznego lub dynamicznego oddziaływania powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/255">
<skos:prefLabel>spaliny silników lotniczych</skos:prefLabel>
<skos:definition>emisje gazowe z silników samolotów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/256">
<skos:prefLabel>hałas lotniczy</skos:prefLabel>
<skos:definition>hałas powodowany przez działania związane z użytkowaniem pojazdów powietrznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/262">
<skos:prefLabel>ruch powietrza</skos:prefLabel>
<skos:definition>ruch powietrza w obrębie atmosfery ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/263">
<skos:prefLabel>zanieczyszczenia powietrza</skos:prefLabel>
<skos:definition>zanieczyszczenia fizyczne, chemiczne, biologiczne lub promieniotwórcze lub zespół zanieczyszczeń wprowadzanych do powietrza i potencjalnie szkodliwych dla środowiska lub zdrowia ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/265">
<skos:prefLabel>zanieczyszczenie powietrza</skos:prefLabel>
<skos:definition>obecność w atmosferze nadmiernej ilości gazów, pyłów lub promieniowania powstałych na skutek działalności człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/267">
<skos:prefLabel>lotnisko</skos:prefLabel>
<skos:definition>wydzielona powierzchnia przeznaczona i przystosowana do przylotów, postojów i odlotów statków powietrznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/270">
<skos:prefLabel>jakość powietrza</skos:prefLabel>
<skos:definition>stopień zanieczyszczenia powietrza; dopuszczalne zanieczyszczenie powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/272">
<skos:prefLabel>kontrola jakości powietrza</skos:prefLabel>
<skos:definition>pomiar stężenia zanieczyszczeń w powietrzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/274">
<skos:prefLabel>sterowanie jakością powietrza</skos:prefLabel>
<skos:definition>tworzenie przepisów i planów działań zmierzających do osiągnięcia wyznaczonego celu, dotyczącego jakości powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/277">
<skos:prefLabel>bezpieczeństwo powietrzne</skos:prefLabel>
<skos:definition>działanie, którego celem jest ograniczenie ryzyka powodowanego przez ruch statków powietrznych, pocisków lub zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/279">
<skos:prefLabel>planowanie w przestrzeni powietrznej</skos:prefLabel>
<skos:definition>działanie związane z organizowaniem transportu w powietrzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/281">
<skos:prefLabel>temperatura powietrza</skos:prefLabel>
<skos:definition>temperatura atmosfery, odzwierciedlająca średnią energie kinetyczną ruchów cząsteczkowych w mikroregionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/282">
<skos:prefLabel>ruch lotniczy</skos:prefLabel>
<skos:definition>statki powietrzne poruszające się w powietrzu lub na lotniskach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/285">
<skos:prefLabel>prawo dotyczące ruchu powietrznego</skos:prefLabel>
<skos:definition>ogół przepisów, norm prawnych regulujących transport powietrzny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/286">
<skos:prefLabel>przepisy ruchu powietrznego</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/289">
<skos:prefLabel>transport lotniczy</skos:prefLabel>
<skos:definition>ogół środków i działań związanych z transportem powietrznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/290">
<skos:prefLabel>wzajemne oddziaływanie wód i powietrza</skos:prefLabel>
<skos:definition>procesy fizyczne zachodzące na styku powietrza i wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/293">
<skos:prefLabel>alarm</skos:prefLabel>
<skos:definition>sygnał, najczęściej akustyczny, ostrzegający przed niebezpieczeństwem;stan gotowości w wypadku powszechnego zagrożenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/298">
<skos:prefLabel>alkohole</skos:prefLabel>
<skos:definition>związki organiczne, pochodne węglowodorów, w których zamiast atomu wodoru występuje grupa wodorotlenowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/301">
<skos:prefLabel>glony</skos:prefLabel>
<skos:definition>Algae, najprostsze samożywne rośliny plechowe, występujące w ponad dwudziestu tysiącach gatunków, żyją głównie w wodach morskich i słodkich, stanowią pożywienie zwierząt, niektóre gatunki dostarczają agaru, są surowcem w przemyśle chemicznym, farmaceutycznym i spożywcz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/303">
<skos:prefLabel>zakwit glonów</skos:prefLabel>
<skos:definition>szybki i nadmierny rozwój glonów spowodowany zanieczyszczeniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/305">
<skos:prefLabel>środek glonobójczy</skos:prefLabel>
<skos:definition>substancja lub środek chemiczny służący do ograniczenia populacji glonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/308">
<skos:prefLabel>związki alicykliczne</skos:prefLabel>
<skos:definition>cykliczne związki organiczne o pierścieniu zbudowanym z atomów węgla, podobne pod względem właściwości do związków acyklicznych; używane między innymi jako rozpuszczalniki i środki zapachowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/309">
<skos:prefLabel>węglowodory alicykliczne</skos:prefLabel>
<skos:definition>organiczne związki cykliczne o pierścieniach zbudowanych tylko z atomów węgla i nie mające charakteru aromatycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/311">
<skos:prefLabel>związki alifatyczne</skos:prefLabel>
<skos:definition>związki organiczne o nie zamkniętym łańcuchu węglowym; jedna z podstawowych grup związków organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/312">
<skos:prefLabel>węglowodory alifatyczne</skos:prefLabel>
<skos:definition>związki organiczne, o cząsteczkach, w których atomy węgla są ze sobą powiązane, tworząc nie zamknięty łańcuch węglowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/313">
<skos:prefLabel>grunty alkaliczne</skos:prefLabel>
<skos:definition>obszar, najczęściej płaski, gdzie gleba charakteryzuje się wysoką zawartością soli mineralnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/319">
<skos:prefLabel>ziemia alkaliczna</skos:prefLabel>
<skos:definition>ziemia zawierająca ilość związków sodu stanowiąca przeszkodę dla rozwoju roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/322">
<skos:prefLabel>alkany</skos:prefLabel>
<skos:definition>nasycone węglowodory acykliczne, parafiny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/326">
<skos:prefLabel>związki alkilowe</skos:prefLabel>
<skos:definition>jednowartościowe grupy organiczne, pochodne węglowodorów alifatycznych, uboższe od nich o jeden atom wodoru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/329">
<skos:prefLabel>alergen</skos:prefLabel>
<skos:definition>ciało chemiczne, które wprowadzone do ustroju wywołuje alergię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/332">
<skos:prefLabel>alergia</skos:prefLabel>
<skos:definition>swoista reakcja ustroju na pewne związki chemiczne znajdujące się m.in. w powietrzu, w pokarmach, w bakteriach, będąca przyczyną wielu chorób, np. astmy, pokrzywki i i</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/335">
<skos:prefLabel>rozmieszczenie</skos:prefLabel>
<skos:definition>przypisanie zasobów poszczególnym rodzajom użytkowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/336">
<skos:prefLabel>plan rozmieszczenia</skos:prefLabel>
<skos:definition>opracowanie i wdrożenie działań zapewniających równowagę pomiędzy potrzebami społeczeństwa a zasobami środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/340">
<skos:prefLabel>stop</skos:prefLabel>
<skos:definition>substancja metaliczna otrzymywana najczęściej przez stopienie dwóch lub więcej metali (niekiedy z domieszką niemetali), wytwarzana w celu uzyskania lepszych właściwości wytrzymałościowych i technologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/341">
<skos:prefLabel>zalewy powodziowe</skos:prefLabel>
<skos:definition>wystąpienie wezbranych wód z brzegów i zalanie obszarów położonych wzdłuż brzegów; zalanie wodą, w której znajdują się znaczne ilości zawiesiny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/342">
<skos:prefLabel>promieniowanie alfa</skos:prefLabel>
<skos:definition>rodzaj promieniowania wysyłanego przez promieniotwórcze jądra atomowe;składa się cząstek α (jąder helu);skuteczną osłonę stanowi papier</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/349">
<skos:prefLabel>materiał alternatywny</skos:prefLabel>
<skos:definition>materiały stosowane w celu zastąpienia innych materiałów, bardziej groźnych dla środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/351">
<skos:prefLabel>technologia alternatywna</skos:prefLabel>
<skos:definition>technologia dążąca do minimalizowania zużycia zasobów i szkód w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/352">
<skos:prefLabel>tlenek glinu</skos:prefLabel>
<skos:definition>związek glinu z tlenem, bezbarwne kryształy o dużej twardości i wysokiej temperaturze topnienia;w przyrodzie występuje jako minerał korund (jego zabarwionymi odmianami są między innymi szafir i rubin); otrzymywany na skalę przemysłową z boksytu, kaolinu lub z gliny, używany do produkcji metalicznego glinu (aluminium)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/353">
<skos:prefLabel>aluminium</skos:prefLabel>
<skos:definition>(Al) pierwiastek chemiczny z grupy glinowców, liczba atomowa 13, srebrzystobiały metal, lekki, kowalny, ciągliwy, dobry przewodnik ciepła i elektryczności, odporny na działanie czynników atmosferycznych, rozpowszechniony w przyrodzie; stosowany między innymi jako składnik lekkich stopów, do wyrobu aparatury chemicznej, przewodów elektrycznych, naczyń, folii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/354">
<skos:prefLabel>pojemnik aluminiowy</skos:prefLabel>
<skos:definition>puszka lub pudełko z aluminium</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/355">
<skos:prefLabel>zawartość aluminium</skos:prefLabel>
<skos:definition>ilość aluminium w roztworze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/356">
<skos:prefLabel>przemysł aluminiowy</skos:prefLabel>
<skos:definition>dział przemysłu zajmujący się procesami wydobycia i przetwarzania aluminium</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/360">
<skos:prefLabel>pęcherzyki płucne</skos:prefLabel>
<skos:definition>pęcherzykowate zakończenia najdrobniejszych odgałęzień oskrzeli w płucach, gdzie następuje wymiana gazowa tlenu i dwutlenku węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/363">
<skos:prefLabel>amalgamat</skos:prefLabel>
<skos:definition>stop metalu, np. złota lub srebra z rtęcią</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/367">
<skos:prefLabel>Ameryka</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/370">
<skos:prefLabel>test Ames&apos;a</skos:prefLabel>
<skos:definition>test wykonywany na bakteriach, dotyczący mutacji wywołanych działaniem substancji chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/372">
<skos:prefLabel>aminy</skos:prefLabel>
<skos:definition>organiczne pochodne amoniaku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/373">
<skos:prefLabel>aminokwasy</skos:prefLabel>
<skos:definition>związki organiczne rozpuszczalne w wodzie, zawierające w cząsteczce co najmniej jedną grupę karboksylową (–COOH) i jedną aminową (–NH2), odznaczające się właściwościami elektrolitów amfoterycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/375">
<skos:prefLabel>amoniak</skos:prefLabel>
<skos:definition>związek azotu z wodorem, bezbarwny gaz o ostrej woni, rozpuszczalny w wodzie, występujący w przyrodzie jako produkt gnicia ciał białkowych, otrzymywany także sztucznie, używany do produkcji nawozów sztucznych, mocznika, włókien syntetycznych it</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/378">
<skos:prefLabel>amonifikacja</skos:prefLabel>
<skos:definition>proces rozkładu azotowych substancji organicznych powodujący wydzielanie się amoniaku, zachodzący pod wpływem drobnoustrojów, głównie bakterii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/379">
<skos:prefLabel>amon</skos:prefLabel>
<skos:definition>jednododatni rodnik, w którego skład wchodzi wodór i azot, zachowujący się jak metal alkaliczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/387">
<skos:prefLabel>płazy</skos:prefLabel>
<skos:definition>gromada wodno-lądowych, zmiennocieplnych kręgowców z grupy bezowodniowców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/388">
<skos:prefLabel>park rozrywki</skos:prefLabel>
<skos:definition>urządzenia służące rozrywce, umieszczona na wolnym powietrzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/391">
<skos:prefLabel>warunki beztlenowe</skos:prefLabel>
<skos:definition>rozwój życia bez dostępu do tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/394">
<skos:prefLabel>proces beztlenowy</skos:prefLabel>
<skos:definition>proces zachodzący bez dostępu tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/397">
<skos:prefLabel>analiza</skos:prefLabel>
<skos:definition>metoda badawcza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/398">
<skos:prefLabel>program analityczny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/399">
<skos:prefLabel>chemia analityczna</skos:prefLabel>
<skos:definition>dział chemii zajmujący się określaniem chemicznego składu substancji i stosunków ilościowych wykrytych składników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/401">
<skos:prefLabel>sprzęt analityczny</skos:prefLabel>
<skos:definition>sprzęt wykorzystywany w technikach analitycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/402">
<skos:prefLabel>metoda analityczna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/404">
<skos:prefLabel>łabędzie, gęsi i kaczki</skos:prefLabel>
<skos:definition>ptactwo wodne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/406">
<skos:prefLabel>anatomia</skos:prefLabel>
<skos:definition>nauka o budowie organizmu człowieka, zwierząt i roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/411">
<skos:prefLabel>okrytozalążkowe</skos:prefLabel>
<skos:definition>Angiospermae, podgromada roślin nasiennych, charakteryzujących się obecnością zalążka ukrytego w owocolistkach zrośniętych w słupek; dzieli się na rośliny jednoliścienne i dwuliścienne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/412">
<skos:prefLabel>wędkarstwo</skos:prefLabel>
<skos:definition>amatorskie łowienie ryb na wędkę, wędkowanie; także dyscyplina sportowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/415">
<skos:prefLabel>zachowania zwierząt</skos:prefLabel>
<skos:definition>zachowanie zwierząt w ich naturalnym środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/417">
<skos:prefLabel>choroby zwierząt</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/419">
<skos:prefLabel>nawóz zwierzęcy jako opał</skos:prefLabel>
<skos:definition>odchody zwierzęce suszone i wykorzystywane jako nawóz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/420">
<skos:prefLabel>ekologia zwierząt</skos:prefLabel>
<skos:definition>nauka zajmująca się relacjami pomiędzy zwierzętami, a środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/421">
<skos:prefLabel>doświadczenia na zwierzętach</skos:prefLabel>
<skos:definition>badania prowadzone na zwierzętach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/423">
<skos:prefLabel>pasza zwierzęca</skos:prefLabel>
<skos:definition>substancja pochodzenia roślinnego, zwierzęcego lub mineralnego, stanowiąca pożywienie zwierząt gospodarskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/425">
<skos:prefLabel>genetyka zwierząt</skos:prefLabel>
<skos:definition>dział biologii, nauka zajmująca się badaniem dziedziczności i zmienności organizmów zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/428">
<skos:prefLabel>zabudowania dla zwierząt</skos:prefLabel>
<skos:definition>budynek lub schronienie przeznaczona na przebywanie zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/429">
<skos:prefLabel>hodowla zwierząt</skos:prefLabel>
<skos:definition>dział rolnictwa związany z chowem i hodowla zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/432">
<skos:prefLabel>nawóz zwierzęcy</skos:prefLabel>
<skos:definition>substancja pochodzenia naturalnego, np. obornik, odchody zwierzęce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/434">
<skos:prefLabel>hałas wywołany przez zwierzęta</skos:prefLabel>
<skos:definition>hałas wywołany przez zwierzęta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/435">
<skos:prefLabel>żywienie zwierząt</skos:prefLabel>
<skos:definition>przyjmowanie pokarmu i przyswajanie pożywienia przez zwierzęta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/437">
<skos:prefLabel>fizjologia zwierząt</skos:prefLabel>
<skos:definition>nauka o procesach zachodzących w organizmach zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/441">
<skos:prefLabel>produkcja zwierzęca</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/442">
<skos:prefLabel>produkty zwierzęce</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/443">
<skos:prefLabel>ochrona zwierząt</skos:prefLabel>
<skos:definition>działania podejmowane zapobiegawczo, aby ograniczyć zagrożenie dla zwierząt powodowane bezpośrednio lub pośrednio przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/445">
<skos:prefLabel>zasoby przyrody ożywionej</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/448">
<skos:prefLabel>zwierzęta rzeźne</skos:prefLabel>
<skos:definition>zwierzęta hodowane w celu zabicia ich i przetworzenia na produkty mięsne; przeznaczone do uboju i konsumpcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/449">
<skos:prefLabel>schronisko dla zwierząt</skos:prefLabel>
<skos:definition>budowle zapewniające zwierzętom schronienie na czas złej pogody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/450">
<skos:prefLabel>zwierzę</skos:prefLabel>
<skos:definition>organizm jednokomórkowy lub wielokomórkowy, cudzożywny, zwykle mający zdolność ruchu (z wyjątkiem zwierząt osiadłych, jak gąbki, koralowce)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/451">
<skos:prefLabel>włókno naturalne</skos:prefLabel>
<skos:definition>surowiec naturalny (konopie, len, wełna), z którego wyrabia się przędzę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/452">
<skos:prefLabel>handel zwierzętami</skos:prefLabel>
<skos:definition>proces wymieniania, sprzedawania lub kupowania zwierząt, głównie domowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/453">
<skos:prefLabel>odpady pochodzenia zwierzęcego</skos:prefLabel>
<skos:definition>odpady z produkcji związanej z hodowla zwierząt lub przetwórstwem surowców pochodzenia zwierzęcego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/454">
<skos:prefLabel>anion</skos:prefLabel>
<skos:definition>atom lub grupa atomów, które na skutek przyłączenia jednego lub większej liczby elektronów wykazują ujemny ładunek elektryczny; jon o ładunku ujemnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/456">
<skos:prefLabel>pierścienice</skos:prefLabel>
<skos:definition>typ zwierząt bezkręgowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/458">
<skos:prefLabel>antagonizm</skos:prefLabel>
<skos:definition>przeciwstawność oddziaływania dwóch substancji, działających w substancji, działających w taki sposób, że działanie jednej z nich niweluje działanie tej drugiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/460">
<skos:prefLabel>efekt synergiczny działania substancji toksycznych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/461">
<skos:prefLabel>Antarktyda</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/463">
<skos:prefLabel>ekosystem Antarktydy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/464">
<skos:prefLabel>Ocen Antarktyczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/465">
<skos:prefLabel>Region Antarktyki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/468">
<skos:prefLabel>działalność człowieka</skos:prefLabel>
<skos:definition>działania wynikające bezpośrednio lub pośrednio z działań człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/470">
<skos:prefLabel>czynnik antropogeniczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/471">
<skos:prefLabel>rezerwat antropologiczny</skos:prefLabel>
<skos:definition>obszar chroniony dla zachowania stylu życia, w tym produkcji i upraw, typowego dla danego społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/473">
<skos:prefLabel>antybiotyk</skos:prefLabel>
<skos:definition>substancja wytwarzana przez niektóre drobnoustroje, zwłaszcza pewne bakterie i pleśnie, wstrzymująca wzrost lub zabijająca inne drobnoustroje; stosowana w lecznictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/474">
<skos:prefLabel>przeciwciała</skos:prefLabel>
<skos:definition>swoiste ciała, białka surowicy krwi z grupy globulin, wytwarzane w organizmie człowieka i wyższych zwierząt pod wpływem antygenów, z którymi się łączą i unieszkodliwiają je, broniąc ustrój przed zakażeniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/475">
<skos:prefLabel>przewidywanie zagrożeń</skos:prefLabel>
<skos:definition>przewidywanie przyszłych zagrożeń i podejmowanie działań zapobiegawczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/478">
<skos:prefLabel>środek przeciwporostowy</skos:prefLabel>
<skos:definition>środek zapobiegający obrastaniu dna statku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/483">
<skos:prefLabel>bodziec przeciwdziałający zanieczyszczaniu</skos:prefLabel>
<skos:definition>nagroda lub kara finansowa, mająca nakłonić do przestrzegania wymogów ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/486">
<skos:prefLabel>zawartość organicznych halogenów AOX</skos:prefLabel>
<skos:definition>miara ilości chlorowców i innych halogenów w substancjach organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/488">
<skos:prefLabel>apartamentowiec</skos:prefLabel>
<skos:definition>blok mieszkalny, w którym każde mieszkanie stanowi własność odrębną, a grunt własność wspólną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/490">
<skos:prefLabel>pszczelarstwo</skos:prefLabel>
<skos:definition>gałąź rolnictwa zajmująca się całokształtem spraw związanych z chowem pszczół i ich użytkowaniem w celu uzyskania miodu, jadu i mleczka pszczelego oraz wosku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/492">
<skos:prefLabel>apelacja</skos:prefLabel>
<skos:definition>odwołanie się od jakiejś decyzji, wyroku do wyższej instancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/493">
<skos:prefLabel>procedura apelacyjna</skos:prefLabel>
<skos:definition>procedura odwołania się od jakiejś decyzji, wyroku do wyższej instancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/494">
<skos:prefLabel>ekologia stosowana</skos:prefLabel>
<skos:definition>stosowanie zasad ekologii do rozwiązywania problemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/495">
<skos:prefLabel>dietetyka stosowana</skos:prefLabel>
<skos:definition>stosowanie zasad dietetyki do rozwiązywania problemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/496">
<skos:prefLabel>nauki stosowane</skos:prefLabel>
<skos:definition>gałąź wiedzy, nauki mająca służyć potrzebom praktycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/498">
<skos:prefLabel>technologia optymalna</skos:prefLabel>
<skos:definition>technologia optymalnie dobrana do warunków lokalizacji, wymogów obsługi i kosztów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/500">
<skos:prefLabel>uzgadnianie instalacji</skos:prefLabel>
<skos:definition>uzyskanie pozwolenia na zbudowanie lub zmodyfikowanie instalacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/501">
<skos:prefLabel>akwakultura</skos:prefLabel>
<skos:definition>uprawa roślin w basenach z roztworami soli mineralnych, stosowana głównie w szklarniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/503">
<skos:prefLabel>zwierzę wodne</skos:prefLabel>
<skos:definition>zwierzę bytujące w środowisku wodnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/504">
<skos:prefLabel>ekologia wód</skos:prefLabel>
<skos:definition>nauka zajmująca się relacjami między organizmami wodnymi a ich środowiskiem życia z uwzględnieniem wzajemnych stosunków między tymi organizmami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/505">
<skos:prefLabel>ekosystem wodny</skos:prefLabel>
<skos:definition>środowisko wodne, w którym zwierzęta i rośliny oddziaływuja z fizycznymi i chemicznymi składnikami środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/508">
<skos:prefLabel>ssak wodny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/510">
<skos:prefLabel>mikroorganizmy wodne</skos:prefLabel>
<skos:definition>mikroorganizmy bytujące w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/511">
<skos:prefLabel>organizmy wodne</skos:prefLabel>
<skos:definition>organizmy bytujące w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/514">
<skos:prefLabel>roślina wodna</skos:prefLabel>
<skos:definition>rośliny dostosowane do życia w całkowitym lub częściowym zanurzeniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/516">
<skos:prefLabel>środki rekreacji i sportów wodnych</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/517">
<skos:prefLabel>akwedukt</skos:prefLabel>
<skos:definition>wodociąg wraz z konstrukcją nośną (arkadami, mostem) doprowadzający wodę najczęściej z odległych źródeł na zasadzie spadku terenu, stosowany w starożytności i średniowieczu, obecnie głównie w melioracjach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/518">
<skos:prefLabel>warstwa wodonośna</skos:prefLabel>
<skos:definition>przepuszczalna warstwa skał, w której gromadzi się i przemieszcza woda wypełniająca pory i szczeliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/521">
<skos:prefLabel>gospodarstwo rolne</skos:prefLabel>
<skos:definition>gospodarstwo nastawione na uprawę ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/527">
<skos:prefLabel>uprawa drzew</skos:prefLabel>
<skos:definition>sadzenie i pielęgnowanie roślin drzewiastych, szczególnie drzew</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/529">
<skos:prefLabel>stanowisko archeologiczne</skos:prefLabel>
<skos:definition>miejsce występowania pozostałości materialnych dawnych społeczeństw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/530">
<skos:prefLabel>archeologia</skos:prefLabel>
<skos:definition>nauka badająca pozostałości materialne dawnych społeczeństw, głównie na podstawie prac wykopaliskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/531">
<skos:prefLabel>archipelag</skos:prefLabel>
<skos:definition>grupa wysp leżących blisko siebie, zwykle tego samego pochodzenia (np. wulkanicznego) i mających podobną budowę geologiczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/538">
<skos:prefLabel>architektura</skos:prefLabel>
<skos:definition>sztuka kształtowania przestrzeni, wyrażająca się w projektowaniu, wznoszeniu i artystycznym kształtowaniu wszelkiego rodzaju budowli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/541">
<skos:prefLabel>ekosystem Arktyki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/542">
<skos:prefLabel>Ocean Arktyczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/543">
<skos:prefLabel>region arktyczny</skos:prefLabel>
<skos:definition>obszar polarny na półkuli północnej wokół bieguna północnego, obejmujący Ocean Arktyczny z wszystkimi wyspami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/544">
<skos:prefLabel>obszar rozmieszczenia</skos:prefLabel>
<skos:definition>przestrzeń zajmowana przez grupę lub wspólnotę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/547">
<skos:prefLabel>obszar potencjalnego rozprzestrzenienia zanieczyszczeń</skos:prefLabel>
<skos:definition>obszar, w którym mogą wystąpić zagrożenia dla zdrowia ludzi i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/551">
<skos:prefLabel>ekosystem obszarów suchych</skos:prefLabel>
<skos:definition>ekosystem w regionach suchych, gdzie średni roczny opad deszczu jest mniejszy niż 10 cali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/552">
<skos:prefLabel>obszary suche</skos:prefLabel>
<skos:definition>regiony, gdzie średni roczny opad deszczu jest mniejszy niż 250 mm</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/556">
<skos:prefLabel>uzbrojenie</skos:prefLabel>
<skos:definition>broń, sprzęt bojowy, w który wyposażone jest wojsko, jednostka wojskowa lub pojedynczy żołnierz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/557">
<skos:prefLabel>konwersja uzbrojenia</skos:prefLabel>
<skos:definition>zmiana rodzaju lub sposobu użycia broni lub uzbrojenia danej jednostki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/558">
<skos:prefLabel>siły zbrojne</skos:prefLabel>
<skos:definition>organizacja militarna oparta na powszechnym obowiązku służby wojskowej lub zaciągu ochotniczym, obejmująca wojska lądowe, lotnicze i marynarkę wojenną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/560">
<skos:prefLabel>związki aromatyczne</skos:prefLabel>
<skos:definition>cykliczne związki organiczne o pierścieniach płaskich, zawierających sprzężony układ wiązań podwójnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/562">
<skos:prefLabel>węglowodory aromatyczne</skos:prefLabel>
<skos:definition>cykliczne związki organiczne (węglowodory i ich pochodne) mające skład chemiczny właściwy związkom nienasyconym; otrzymywane głównie ze smoły węglowej i ropy naftowej, stosowane w syntezie barwników, leków, tworzyw sztucznych it</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/564">
<skos:prefLabel>substancje aromatyczne</skos:prefLabel>
<skos:definition>substancje wyróżniające się aromatycznym zapachem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/566">
<skos:prefLabel>zastaw</skos:prefLabel>
<skos:definition>czasowe posiadanie czegoś przez wierzyciela; danie czegoś wierzycielowi na zabezpieczenie spłaty długu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/567">
<skos:prefLabel>arsen</skos:prefLabel>
<skos:definition>(As) pierwiastek chemiczny o liczbie atomowej 33, należący do grupy azotowców; półmetal, częsty w związkach, pod każdą postacią bardzo trujący; stosowany jako dodatek stopowy oraz w medycynie i do produkcji środków przeciwko gryzoniom, owadom i chwastom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/568">
<skos:prefLabel>sztuka</skos:prefLabel>
<skos:definition>dziedzina ludzkiej działalności artystycznej, wyróżniana ze względu na związane z nią wartości estetyczne (zwłaszcza piękno); jej wytwory stanowią trwały dorobek kultury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/571">
<skos:prefLabel>stawonogi</skos:prefLabel>
<skos:definition>Arthropoda, najliczniejszy w świecie zwierząt typ bezkręgowców, obejmujący ponad milion gatunków, których charakterystyczną cechą jest ciało zbudowane z segmentów tworzących głowotułów i odwłok lub głowę, tułów i odwłok; należą do nich m.in.:skorupiaki, pajęczaki, wije i owady; występują we wszystkich strefach klimatycznych w różnych środowiskach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/572">
<skos:prefLabel>członowce</skos:prefLabel>
<skos:definition>Entomostraca, podgromada drobnych skorupiaków, z których liczne są wolno żyjące, inne osiadłe lub pasożytnicze; skorupiaki niższe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/579">
<skos:prefLabel>sztuczne jezioro</skos:prefLabel>
<skos:definition>zbiornik wodny powstały w wyniku budowy betonowej lub żelbetowej konstrukcji, przegradzającej dolinę rzeki; utworzony dla potrzeb energetycznych i ochrony przeciwpowodziowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/587">
<skos:prefLabel>sztuczny satelita</skos:prefLabel>
<skos:definition>urządzenie o specjalnej konstrukcji wystrzelone w przestrzeń międzyplanetarną i wprowadzone na orbitę wokół danej planety lub jej naturalnego satelity</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/591">
<skos:prefLabel>azbest</skos:prefLabel>
<skos:definition>minerał o budowie włóknistej, z grupy serpentynu i amfiboli, odporny na działanie wysokiej temperatury; używany głównie do wyrobu materiałów ogniotrwałych i izolacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/594">
<skos:prefLabel>azbestocement</skos:prefLabel>
<skos:definition>materiał budowlany wyrabiany z azbestu i cementu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/595">
<skos:prefLabel>ASEAN</skos:prefLabel>
<skos:definition>Stowarzyszenie Narodów Azji Południowo-Wschodniej; organizacja założona w 1967 w Bangkoku, z siedzibą w Dżakarcie (Indonezja)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/596">
<skos:prefLabel>popiół</skos:prefLabel>
<skos:definition>pozostałość (w postaci szarego proszku) po spaleniu substancji pochodzenia organicznego, zwykle drzewa, węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/598">
<skos:prefLabel>Azja</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/605">
<skos:prefLabel>asymilacja</skos:prefLabel>
<skos:definition>przemiana substancji pobranych z zewnątrz na składniki własnego organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/607">
<skos:prefLabel>stowarzyszenie</skos:prefLabel>
<skos:definition>dobrowolny i trwały związek grupy osób, zorganizowany dla realizacji wspólnych zamierzeń, dla zaspokojenia zainteresowań lub dla niezarobkowego prowadzenia jakiejś działalności, np. społecznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/611">
<skos:prefLabel>kosmonautyka</skos:prefLabel>
<skos:definition>zespół dziedzin nauki i techniki zajmujących się lotami poza atmosferę Ziemi i poznawaniem przestrzeni kosmicznej oraz znajdujących się w niej obiektów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/612">
<skos:prefLabel>astronomia</skos:prefLabel>
<skos:definition>nauka o ciałach niebieskich, ich budowie, ruchach, pochodzeniu i ewolucji oraz o materii rozproszonej przestrzeni kosmicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/616">
<skos:prefLabel>atlas</skos:prefLabel>
<skos:definition>zbiór map lub rycin wydanych w formie książki lub albumu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/617">
<skos:prefLabel>atmosfera</skos:prefLabel>
<skos:definition>powłoka gazowa otaczająca Ziemię, a także niektóre inne planety i gwiazdy; w użyciu potocznym: powłoka powietrza otaczająca kulę ziemską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/621">
<skos:prefLabel>chemia atmosfery</skos:prefLabel>
<skos:definition>badanie przemiam i rozprzestrzeniania się składników powietrza atmosferycznego w troposferze i stratosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/622">
<skos:prefLabel>krążenie mas powietrza</skos:prefLabel>
<skos:definition>ruchy mas powietrza i związany z tym przepływ energii pomiędzy warstwami powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/623">
<skos:prefLabel>składniki powietrza</skos:prefLabel>
<skos:definition>składniki powietrza: azot (79,1%), tlen (20,9%) i dwutlenek wegla (około 0,03%) oraz śladowe ilości gazów szlachetnych, pary wodnej, substancji organicznej i pyłu zawieszonego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/624">
<skos:prefLabel>skład atmosfery</skos:prefLabel>
<skos:definition>skład chemiczny atmosfery ziemskiej: tlen, azot, argon, dwutlenek węgla, para wodna, ozon, neon, hel krypton, metan, wodór i tlenki azotu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/625">
<skos:prefLabel>emisja do atmosfry</skos:prefLabel>
<skos:definition>zanieczyszczenia gazowe lub w formie drobnych cząstek stałych, wprowadzane do atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/626">
<skos:prefLabel>wilgotność atmosfery</skos:prefLabel>
<skos:definition>wielkość określająca zawartość wody w powietrzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/627">
<skos:prefLabel>warstwy atmosfery</skos:prefLabel>
<skos:definition>warstwy wyodrębniane w atmosferze ziemskiej, najczęściej na podstawie profilu temperatury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/629">
<skos:prefLabel>modele atmosferyczne</skos:prefLabel>
<skos:definition>symulacja mająca na celu zobrazowanie wpływu atmosfery na inne elementy, w tym powierzchnię ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/631">
<skos:prefLabel>monitoring atmosfery</skos:prefLabel>
<skos:definition>regularne pobieranie próbek powietrza przez administrację rządową lub zakłady przemysłowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/632">
<skos:prefLabel>warstwa ozonowa</skos:prefLabel>
<skos:definition>odmiana alotropowa tlenu o cząsteczkach trójatomowych, występująca w znacznym stężeniu w warstwie stratosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/633">
<skos:prefLabel>pył zawieszony</skos:prefLabel>
<skos:definition>stężenie pyłu w atmosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/634">
<skos:prefLabel>fizyka atmosfery</skos:prefLabel>
<skos:definition>badanie zjawisk fizycznych w atmosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/636">
<skos:prefLabel>zanieczyszczenie atmosfery</skos:prefLabel>
<skos:definition>obecność substancji w atmosferze, w ilościach powodujących zagrożenie dla zdrowia ludzi lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/637">
<skos:prefLabel>opady atmosferyczne</skos:prefLabel>
<skos:definition>woda w postaci ciekłej lub stałej spadająca z chmur na powierzchnię Ziemi albo osadzająca się bezpośrednio z atmosfery jako rezultat kondensacji pary wodnej na powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/639">
<skos:prefLabel>procesy atmosferyczne</skos:prefLabel>
<skos:definition>fizyczne i chemiczne procesy zachodzące w atmosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/640">
<skos:prefLabel>nauka o atmosferze</skos:prefLabel>
<skos:definition>nauka zajmująca się badaniem dynamiki, fizyki i chemii zjawisk atmosferycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/641">
<skos:prefLabel>struktura atmosfery</skos:prefLabel>
<skos:definition>budowa i charakterystyka warstw atmosfery ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/654">
<skos:prefLabel>przynależne prawo</skos:prefLabel>
<skos:definition>prawo podatkowe przypisujące jednemu podatnikowi odsetki z podatku płaconego przez innego podatnika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/658">
<skos:prefLabel>media audio-wizualne</skos:prefLabel>
<skos:definition>urządzenia i metody operujące dźwiękiem i obrazem (w postaci grafiki, tekstu, ruchomych obrazów) w celu przekazania informacj</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/661">
<skos:prefLabel>organ władzy</skos:prefLabel>
<skos:definition>wyodrębniona jednostka organizacyjna aparatu państwowego pełniąca określone funkcje w dziedzinie życia społecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/662">
<skos:prefLabel>zatwierdzanie</skos:prefLabel>
<skos:definition>procedura administracyjna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/664">
<skos:prefLabel>autoekologia</skos:prefLabel>
<skos:definition>ekologia dotycząca konkretnego gatunku i jego relacji ze środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/665">
<skos:prefLabel>automatyczne wykrywanie</skos:prefLabel>
<skos:definition>automatyczne przetwarzanie i identyfikacja danych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/668">
<skos:prefLabel>przemysł samochodowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/678">
<skos:prefLabel>lawina</skos:prefLabel>
<skos:definition>wielka masa śniegu, kamieni lub lodu staczająca się gwałtownie ze zboczy górskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/680">
<skos:prefLabel>ochrona przed lawinami</skos:prefLabel>
<skos:definition>działania i środki podejmowane, aby chronić ludzi, nieruchomości i środowisko przed lawinami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/685">
<skos:prefLabel>prawo lotnicze</skos:prefLabel>
<skos:definition>przepisy międzynarodowe regulujące transport powietrzny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/686">
<skos:prefLabel>awifauna</skos:prefLabel>
<skos:definition>ogół ptaków zamieszkujących określony obszar, środowisko lub żyjących w określonej epoce geologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/693">
<skos:prefLabel>poziom tła</skos:prefLabel>
<skos:definition>stały lub naturalny poziom danej substancji lub zjawiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/695">
<skos:prefLabel>poziom promieniowania</skos:prefLabel>
<skos:definition>promieniowanie pochodzące ze źródeł naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/698">
<skos:prefLabel>bakteria</skos:prefLabel>
<skos:definition>drobnoustroje, mikroskopijne, zwykle jednokomórkowe, w większości cudzożywne organizmy roślinne, mające ogromne znaczenie w przyrodzie, wykorzystywane w przemyśle spożywczym, fermentacyjnym i farmaceutycznym; liczne z nich wywołują choroby u ludzi, roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/699">
<skos:prefLabel>złoże biologiczne</skos:prefLabel>
<skos:definition>urządzenie do oczyszczania ścieków filtrujących przez złoże w procesie tlenowego rozkładu przez mikroorganizmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/700">
<skos:prefLabel>bakteriocyd</skos:prefLabel>
<skos:definition>środki niszczące lub hamujące rozwój bakterii, głównie antybiotyki, sulfamidy, preparaty rtęciowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/702">
<skos:prefLabel>bakteriologia</skos:prefLabel>
<skos:definition>nauka o budowie, życiu i właściwościach bakterii oraz ich roli w świecie organicznym i nieorganicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/709">
<skos:prefLabel>równowaga (ekonomiczna)</skos:prefLabel>
<skos:definition>jednakowy, równy układ sił, wartości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/710">
<skos:prefLabel>równoważenie interesów</skos:prefLabel>
<skos:definition>uwzględnianie i równoważenie często przeciwstawnych interesów politycznych i finansowych różnych grup społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/716">
<skos:prefLabel>operacja bankowa</skos:prefLabel>
<skos:definition>transakcje pomiędzy bankiem i jego klientami dotyczące zabezpieczenia, wymiany, otrzymania lub pożyczenia pieniędzy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/718">
<skos:prefLabel>zakaz wprowadzania ścieków</skos:prefLabel>
<skos:definition>zakaz wprowadzania ścieków do ziemi w celu zapobieżenia koncentracji metali szkodliwych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/719">
<skos:prefLabel>bar</skos:prefLabel>
<skos:definition>(Ba) pierwiastek chemiczny z grupy wapniowców, liczba atomowa 56; miękki, srebrzystobiały metal, aktywny chemicznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/726">
<skos:prefLabel>monitoring tła</skos:prefLabel>
<skos:definition>badanie długofalowych zmian w składzie atmosfery, wpływających na pogodę i klimat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/728">
<skos:prefLabel>zasada (chemia)</skos:prefLabel>
<skos:definition>związek chemiczny, którego cząsteczki składają się z atomu pierwiastka metalicznego i z grupy wodorotlenowej; odznacza się między innymi zdolnością do barwnych reakcji ze wskaźnikami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/730">
<skos:prefLabel>podstawowe potrzeby żywieniowe</skos:prefLabel>
<skos:definition>minimalne wymagania żywieniowe, odpowiadającej danej płci, wiekowi i aktywności fizycznej, konieczne dla zapewnienia życia, zdrowia i rozwoju.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/731">
<skos:prefLabel>zasadowość</skos:prefLabel>
<skos:definition>mający właściwości zasady; alkaliczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/733">
<skos:prefLabel>postawczaki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/736">
<skos:prefLabel>proces periodyczny</skos:prefLabel>
<skos:definition>proces, który nie jest ciągły lub masowy, gdzie obróbka odbywa się partiami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/740">
<skos:prefLabel>zbiorniki wodne do kąpieli</skos:prefLabel>
<skos:definition>wody śródlądowe lub morskie, przeznaczone do kąpieli lub w których kąpiel nie jest zabroniona</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/745">
<skos:prefLabel>bateria</skos:prefLabel>
<skos:definition>zespół urządzeń będących źródłem energii, połączonych w jeden układ w celu zwiększenia mocy, wydajności itp..</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/746">
<skos:prefLabel>unieszkodliwianie baterii</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/749">
<skos:prefLabel>zatoka</skos:prefLabel>
<skos:definition>część morza lub jeziora wcinająca się w ląd</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/751">
<skos:prefLabel>obszar nadmorski</skos:prefLabel>
<skos:definition>piaszczysty lub żwirowy teren, pozbawiony roślinności, nisko położony nad brzegiem morza, rzeki lub jeziora</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/755">
<skos:prefLabel>wyrzucenie na brzeg</skos:prefLabel>
<skos:definition>wyrzucenie na brzeg wielorybów lub innych waleni, zmarłych z przyczyn naturalnych, na skutek zanieczyszczenia lub uduszenia w sieciach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/760">
<skos:prefLabel>ochrona pszczół</skos:prefLabel>
<skos:definition>opieka nad i ochrona owadów cenionych za zapylanie roślin i produkcję miodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/761">
<skos:prefLabel>bydło opasowe</skos:prefLabel>
<skos:definition>bydło hodowane dla mięsa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/762">
<skos:prefLabel>pszczoły</skos:prefLabel>
<skos:definition>Apis mellifica, owad z rodziny o tej samej nazwie, żyjący w rodzinach (rojach), dawno udomowiony i hodowany niemal na całej kuli ziemskiej dla miodu i wosku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/764">
<skos:prefLabel>chrząszcze</skos:prefLabel>
<skos:definition>Coleoptera, rząd owadów obejmujący około 300 tysięcy gatunków odznaczających się dwiema parami skrzydeł, z których przednie, przekształcone w twarde pokrywy, osłaniają błoniaste skrzydła tylne i odwłok; żyją we wszelkich środowiskach na kuli ziemskiej (oprócz mórz)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/765">
<skos:prefLabel>zachowanie (się)</skos:prefLabel>
<skos:definition>działanie lub reakcja organizmu, grupy lub gatunku na czynniki środowiskowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/766">
<skos:prefLabel>zachowanie się substancji</skos:prefLabel>
<skos:definition>skłonność substancji do wchodzenia w reakcje zależna od struktury cząsteczkowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/767">
<skos:prefLabel>wzorzec postępowania</skos:prefLabel>
<skos:definition>wzór, schemat reprezentujący określony typ zjawisk, mogący być normą dla innych zjawisk danego typu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/768">
<skos:prefLabel>pożyteczny organizm</skos:prefLabel>
<skos:definition>organizmy wykorzystywane do zapylania roślin lub kontrolowania populacji szkodników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/772">
<skos:prefLabel>osad bentosowy</skos:prefLabel>
<skos:definition>cześć przydenna zbiornika wodnego, gdzie często gromadzi się osad</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/773">
<skos:prefLabel>ekosystem denny</skos:prefLabel>
<skos:definition>system biologiczny dna zbiornika wodnego oraz jego otoczenie abiotyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/775">
<skos:prefLabel>bentos</skos:prefLabel>
<skos:definition>organizmy zwierzęce i roślinne zamieszkujące dna zbiorników wodnych, przyczepione do podłoża, czołgające się po nim, tkwiące w nim, nie unoszące się w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/776">
<skos:prefLabel>benzen</skos:prefLabel>
<skos:definition>najprostszy ciekły węglowodór aromatyczny, ważny surowiec w przemyśle chemicznym i farmaceutycznym, stosowany również jako składnik mieszanek napędowych do silników spalinowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/777">
<skos:prefLabel>benzopiren</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/781">
<skos:prefLabel>beryl</skos:prefLabel>
<skos:definition>minerał, krzemian berylu i glinu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/782">
<skos:prefLabel>promieniowanie beta</skos:prefLabel>
<skos:definition>elektrony wysyłane przez jądra atomowe w wyniku rozpadu promieniotwórczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/783">
<skos:prefLabel>przemysł napojów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/784">
<skos:prefLabel>napój</skos:prefLabel>
<skos:definition>każdy rodzaj płynu przeznaczonego do picia z wyjątkiem wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/786">
<skos:prefLabel>bibliografia</skos:prefLabel>
<skos:definition>uporządkowany według określonych kryteriów spis książek, druków, czasopism, artykułów oraz dokumentów z najważniejszymi danymi o każdej pozycji (autor, tytuł, miejsce i rok wydania itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/788">
<skos:prefLabel>rower</skos:prefLabel>
<skos:definition>pojazd drogowy napędzany siłą mięśni ludzkich, za pomocą pedałów, zwykle o dwóch kołach jednakowej średnicy, umieszczonych jedno za drugim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/792">
<skos:prefLabel>porozumienie dwustronne</skos:prefLabel>
<skos:definition>dwustronne porozumienie międzynarodowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/794">
<skos:prefLabel>ropa zęzowa</skos:prefLabel>
<skos:definition>odpady olejowe, najczęściej zmieszane z wodą, gromadzące się w najniższej części kadłuba statku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/795">
<skos:prefLabel>woda zęzowa</skos:prefLabel>
<skos:definition>woda gromadząca się w dolnej, przylegającej do stępki, wewnętrznej części kadłuba statku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/798">
<skos:prefLabel>bioakumulacja</skos:prefLabel>
<skos:definition>gromadzenie się w organizmach zanieczyszczeń lub innych substancji zbędnych z punktu widzenia organizmu, przyjmowanych z pożywieniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/800">
<skos:prefLabel>bioakumulacja zanieczyszczeń</skos:prefLabel>
<skos:definition>zanieczyszczenia gromadzące się w organizmach na skutek przyjmowania ich drogą pokarmową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/802">
<skos:prefLabel>przyswajalność biologiczna</skos:prefLabel>
<skos:definition>zdolność, łatwość podlegania przyswojeniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/807">
<skos:prefLabel>metoda biochemiczna</skos:prefLabel>
<skos:definition>metoda oparta na wykorzystaniu mechanizmów biochemicznych, np. reakcji katalizowanych przez enzymy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/812">
<skos:prefLabel>reakcja biochemicza</skos:prefLabel>
<skos:definition>reakcje chemiczne zachodzące w żywych organizmach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/813">
<skos:prefLabel>substancja biochemiczna</skos:prefLabel>
<skos:definition>substancje chemiczne występujące w roślinach, zwierzętach i mikroorganizmach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/815">
<skos:prefLabel>biochemia</skos:prefLabel>
<skos:definition>nauka mająca na celu poznanie składu chemicznego i właściwości substancji w żywych organizmach oraz całokształtu zachodzących w nich procesów chemicznych; chemia fizjologiczna, biologiczna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/816">
<skos:prefLabel>biocydy</skos:prefLabel>
<skos:definition>różnorodna grupa substancji trujących obejmująca m.in. pestycydy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/818">
<skos:prefLabel>bioklimatologia</skos:prefLabel>
<skos:definition>badanie związków klimatu z fauną i florą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/819">
<skos:prefLabel>biocenoza</skos:prefLabel>
<skos:definition>zespół populacji różnych gatunków zwierząt i roślin, żyjący w określonym środowisku — biotopie, podlegający jego czynnikom i powiązany ze sobą zależnościami pokarmowymi oraz konkurencją biologiczną wewnątrzgatunkową i międzygatunkową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/820">
<skos:prefLabel>czynnik biokoncentracji</skos:prefLabel>
<skos:definition>współczynnik obrazujący stężenie substancji chemicznej w organizmach wodnych w stosunku do jej stężenia w wodzie w tym samym przedziale czasowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/821">
<skos:prefLabel>rozkład biologiczny</skos:prefLabel>
<skos:definition>podatność substancji na rozkład pod wpływem działania bakterii i grzybów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/823">
<skos:prefLabel>zanieczyszczenia podlegające rozkładowi biologicznemu</skos:prefLabel>
<skos:definition>zanieczyszczenie, które może ulec przekształceniu w procesie biologicznego rozkładu w proste związki nieorganiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/826">
<skos:prefLabel>rozkład biologiczny</skos:prefLabel>
<skos:definition>rozkład substancji pod wpływem mikroorganizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/827">
<skos:prefLabel>różnorodność biologiczna</skos:prefLabel>
<skos:definition>1) różnorodność genetyczna; 2) rozmaitość i różnorodność występująca w naturze na poziomie genetycznym, gatunkowym i ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/830">
<skos:prefLabel>aspekty etyczne biotechnologii</skos:prefLabel>
<skos:definition>nauka o problemach etycznych związanych z badaniami biologicznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/832">
<skos:prefLabel>biogaz</skos:prefLabel>
<skos:definition>łatwo palny gaz powstający podczas fermentacji beztlenowej świeżego obornika lub innych substancji organicznych, zawierający około 60% metanu, stosowany do celów energetycznych, głównie w gospodarstwach rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/833">
<skos:prefLabel>cykl biogeochemiczny</skos:prefLabel>
<skos:definition>obieg pierwiastków chemicznych w przyrodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/834">
<skos:prefLabel>biogeochemia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/835">
<skos:prefLabel>region biogeograficzny</skos:prefLabel>
<skos:definition>obszar powierzchni Ziemi, którego granice określane są na podstawie występujących gatunków flory i fauny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/836">
<skos:prefLabel>biogeografia</skos:prefLabel>
<skos:definition>nauka o obszarach występowania roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/839">
<skos:prefLabel>aktywność biologiczna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/841">
<skos:prefLabel>analiza biologiczna</skos:prefLabel>
<skos:definition>analiza substancji mająca na celu określenie jej oddziaływania na organizmy żywe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/842">
<skos:prefLabel>cecha biologiczna</skos:prefLabel>
<skos:definition>właściwości lub charakterystyki żywych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/846">
<skos:prefLabel>rozwój biologiczny</skos:prefLabel>
<skos:definition>rozwój żywych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/849">
<skos:prefLabel>skutek biologiczny</skos:prefLabel>
<skos:definition>oddziaływanie zanieczyszczeń  na żywe organizmy obejmujące reakcje alergiczne, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/851">
<skos:prefLabel>bioinżynieria</skos:prefLabel>
<skos:definition>stosowanie zasad i technik inżynierii w stosunku do żywych organizmów, w tym konstruowanie i stosowanie sztucznych organów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/856">
<skos:prefLabel>wskaźnik biologiczny</skos:prefLabel>
<skos:definition>organizm służący do określania jakości środowiska lub zachodzących w nim zmian</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/861">
<skos:prefLabel>monitoring środowiskowy</skos:prefLabel>
<skos:definition>bezpośredni pomiar zmian warunków środowiskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/862">
<skos:prefLabel>biologiczne wiązanie azotu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/864">
<skos:prefLabel>zwalczanie szkodników metodami biologicznymi</skos:prefLabel>
<skos:definition>celowe wprowadzanie do środowiska organizmów, których działanie ograniczyć ma populacje innych organizmów uznanych za szkodliwe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/865">
<skos:prefLabel>zanieczyszczenia pochodzenia biologicznego</skos:prefLabel>
<skos:definition>wirusy, bakterie, grzyby i alergeny obecne w środowisku, które mogą powodować ujemne oddziaływanie zdrowotne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/866">
<skos:prefLabel>zanieczyszczenie pochodzenia biologicznego</skos:prefLabel>
<skos:definition>zachwianie równowagi biologicznej poprzez przypadkowe lub celowe uwolnienie obcego dla danego ekosystemu organizmu do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/867">
<skos:prefLabel>proces biologiczny</skos:prefLabel>
<skos:definition>proces związany z działaniem żywych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/868">
<skos:prefLabel>produkcja biologiczna</skos:prefLabel>
<skos:definition>1)wielkośc i tempo produkcji ekosystemu; 2) ilość substancji organicznej powstała w określonym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/873">
<skos:prefLabel>zasoby biologiczne</skos:prefLabel>
<skos:definition>zwierzęta lub rośliny, dzikie lub hodowlane, pozyskiwane w celu zapewnienie pożywienia, dla handlu lub rozrywki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/878">
<skos:prefLabel>biologiczne oczyszczanie gazu</skos:prefLabel>
<skos:definition>proces związany z wykorzystaniem mikroorganizmów do usuwania zanieczyszczeń z gazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/879">
<skos:prefLabel>biologiczne przetwarzanie odpadów</skos:prefLabel>
<skos:definition>proces wykorzystywania mikroorganizmów do rozkładu zanieczyszczeń organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/880">
<skos:prefLabel>broń biologiczna</skos:prefLabel>
<skos:definition>broń masowego rażenia wyzyskująca działanie zarazków chorobotwórczych i substancji chemicznych na organizmy żywe; stosowana w celu wywołania epidemii chorób zakaźnych oraz niszczenia roślin uprawnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/881">
<skos:prefLabel>biologia</skos:prefLabel>
<skos:definition>nauka o organizmach żywych, o życiu, jego przejawach i właściwościach, o jego powstaniu i rozwoju na Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/882">
<skos:prefLabel>bioluminescencja</skos:prefLabel>
<skos:definition>generowanie różnych kolorów światła przez organizmy żywe; świecenie żywych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/883">
<skos:prefLabel>biomasa</skos:prefLabel>
<skos:definition>ciężar materii organicznej zawartej w danym momencie w organizmie zwierzęcym lub roślinnym; stanowi podstawę obliczeń produkcyjności biologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/885">
<skos:prefLabel>energia z biomasy</skos:prefLabel>
<skos:definition>energia ze źródeł odnawialnych jak np. biogaz (metan)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/888">
<skos:prefLabel>biofizyka</skos:prefLabel>
<skos:definition>dziedzina nauk biologicznych badająca fizyczne i fizykochemiczne procesy życiowe oraz wpływ zewnętrznych czynników fizycznych na te procesy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/889">
<skos:prefLabel>bioreaktor</skos:prefLabel>
<skos:definition>zbiornik służący do utrzymywania organizmów żywych wykorzystywanych do produkcji na skalę przemysłową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/890">
<skos:prefLabel>biorytm</skos:prefLabel>
<skos:definition>rytmiczne wahania intensywności procesów biologicznych, umożliwiające organizmom przystosowanie się do cyklicznych zmian środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/891">
<skos:prefLabel>bezpieczeństwo biologiczne</skos:prefLabel>
<skos:definition>wiedza, technika i sprzęt stosowany w celu zmniejszenia ryzyka ludzi i środowiska związanego z wykorzystywaniem lub badaniem materiału biologicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/892">
<skos:prefLabel>biosfera</skos:prefLabel>
<skos:definition>strefa życia, przestrzeń zamieszkana przez organizmy żywe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/894">
<skos:prefLabel>rezerwat biosfery</skos:prefLabel>
<skos:definition>teren objęty ochroną zgodnie z wymogami programu MAB w ramach konwencji CITES</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/895">
<skos:prefLabel>biosynteza</skos:prefLabel>
<skos:definition>synteza związków organicznych zachodząca w żywym organizmie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/898">
<skos:prefLabel>biotechnologia</skos:prefLabel>
<skos:definition>technologia polegająca na wykorzystaniu drobnoustrojów, enzymów itp. w procesach produkcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/899">
<skos:prefLabel>czynniki biotyczne</skos:prefLabel>
<skos:definition>żywe organizmy oddziałujące bezpośrednio lub pośrednio na inne organizmy (np. pasożyty w stosunku do swych żywicieli)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/901">
<skos:prefLabel>biotop</skos:prefLabel>
<skos:definition>środowisko życia organizmów zwierzęcych i roślinnych odznaczające się swoistym zespołem czynników ekologicznych na danym terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/902">
<skos:prefLabel>system biotopów</skos:prefLabel>
<skos:definition>sieć korytarzy ekologicznych umożliwiających łączność pomiędzy rozdrobnionymi ekosystemami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/903">
<skos:prefLabel>ochrona biotopów</skos:prefLabel>
<skos:definition>działania podejmowane dla zapewnienia równowagi pomiędzy biologicznymi i fizycznymi elementami biotopu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/912">
<skos:prefLabel>ptaki</skos:prefLabel>
<skos:definition>Aves, gromada kręgowców stałocieplnych z grupy owodniowców, obejmuje około 8500 gatunków (w Polsce około 220 gatunków gniazdowych), o ciele pokrytym piórami, kończynach przednich przekształconych w skrzydła, szczękach bezzębnych, okrytych rogowym dziobem; rozmnażają się przez składanie jaj</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/913">
<skos:prefLabel>ostoja ptaków</skos:prefLabel>
<skos:definition>teren objęty ochroną ze względu na występujące tam, ciągle lub okresowo, gatunki ptaków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/914">
<skos:prefLabel>ptak drapieżny</skos:prefLabel>
<skos:definition>Accipitres, Falconiformes, rząd ptaków różnej wielkości, o mocnym tułowiu i silnych kończynach zakończonych ostrymi pazurami (szponami); żywią się przeważnie mięsem zwierząt lub padliną; należą do nich np. ścierwniki, sekretarze, sokoły, jastrzębie, orły, sępy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/915">
<skos:prefLabel>gatunki ptaków</skos:prefLabel>
<skos:definition>stałocieplne kręgowce, z rzędu Aves</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/917">
<skos:prefLabel>kontrola urodzin</skos:prefLabel>
<skos:definition>ograniczanie liczby urodzonych dzieci poprzez zapobieganie ciąży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/921">
<skos:prefLabel>bituminy</skos:prefLabel>
<skos:definition>naturalne produkty stanowiące mieszaninę węglowodorów łańcuchowych i cyklicznych, a często również ich pochodnych zawierających tlen, azot, siarkę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/926">
<skos:prefLabel>czarny węgiel</skos:prefLabel>
<skos:definition>skała osadowa, organiczna, o barwie od brunatnej do czarnej, powstała (głównie w górnym karbonie, permie i trzeciorzędzie) z resztek substancji roślinnych, z domieszek produktów ich rozkładu oraz z substancji mineralnych; kopalina palna stanowiąca ważny surowiec energetyczny i chemiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/930">
<skos:prefLabel>wieki piec hutniczy</skos:prefLabel>
<skos:definition>urządzenie przemysłowe do przeprowadzania procesów technologicznych, które zachodzą w wysokich temperaturach, uzyskiwanych przez spalanie paliw lub elektrycznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/932">
<skos:prefLabel>środek wybielający</skos:prefLabel>
<skos:definition>środek stosowany do wybielenia, utleniania lub redukcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/933">
<skos:prefLabel>ziemia bieląca</skos:prefLabel>
<skos:definition>glina posiadająca zdolność adsorpcji innych substancji, m.in. olejów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/934">
<skos:prefLabel>proces wybielania</skos:prefLabel>
<skos:definition>usuwanie elementów kolorowych z tkaniny lub utlenianie masy służącej do wyrobu papieru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/935">
<skos:prefLabel>krew (tkanka)</skos:prefLabel>
<skos:definition>płynna tkanka krążąca w układzie żył i tętnic kręgowców, składająca się z płynnego osocza i krwinek; dostarcza tkankom tlen z narządów oddechowych i zabiera dwutlenek węgla, rozprowadza substancje odżywcze i hormony, odprowadza do narządów wydalniczych szkodliwe produkty przemiany materii, pełni funkcje obronne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/939">
<skos:prefLabel>sinice</skos:prefLabel>
<skos:definition>Cyanophyceae, klasa prokariotycznych organizmów jednokomórkowych, kolonijnych lub tworzących prymitywne, nitkowate plechy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/945">
<skos:prefLabel>żeglowanie</skos:prefLabel>
<skos:definition>podróżowanie lub poruszanie się na łodzi jako forma odpoczynku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/947">
<skos:prefLabel>biochemiczne zapotrzebowanie tlenu</skos:prefLabel>
<skos:definition>ilość tlenu potrzebna do biologicznego utlenienia jednostkowej objętości wody, w określonej temperaturze i w określonym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/953">
<skos:prefLabel>kocioł</skos:prefLabel>
<skos:definition>zamknięty zbiornik, urządzenie, w którym w wyniku ogrzewania, woda zmienia stan skupienia i w postaci pary nasyconej lub przegrzanej o ciśnieniu wyższym od atmosferycznego jest użytkowana na zewnątrz kotłów w celu zapewnienia dostaw ciepła lub wytwarzania energii elektrycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/954">
<skos:prefLabel>punkt wrzenia</skos:prefLabel>
<skos:definition>temperatura wrzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/959">
<skos:prefLabel>książka</skos:prefLabel>
<skos:definition>pewna liczba złożonych, zbroszurowanych i oprawionych arkuszy papieru, zadrukowanych tekstem literackim, naukowym lub użytkowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/961">
<skos:prefLabel>księgowość</skos:prefLabel>
<skos:definition>prowadzenie ksiąg, w które wpisuje się wpływy, wydatki, transakcje i inne zmiany stanu majątkowego przedsiębiorstwa lub instytucji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/963">
<skos:prefLabel>brzeg</skos:prefLabel>
<skos:definition>linia dzieląca regiony polityczne lub geograficzne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/965">
<skos:prefLabel>bor</skos:prefLabel>
<skos:definition>(B) pierwiastek chemiczny z grupy glinowców, liczba atomowa 5, niemetal, trudno topliwy czarny proszek; w przyrodzie występuje w małych ilościach (kwas borowy i jego sole, borany); stosowany w przemyśle chemicznym, w hutnictwie szkła i w obróbce cieplnej stali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/969">
<skos:prefLabel>ogród botaniczny</skos:prefLabel>
<skos:definition>ogród, w którym hoduje się dla celów naukowych rośliny różnych stref klimatycznych i środowisk; dostępny dla publiczności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/971">
<skos:prefLabel>botanika</skos:prefLabel>
<skos:definition>nauka o budowie, właściwościach i funkcjach roślin, o ich klasyfikacji i rozmieszczeniu na kuli ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/975">
<skos:prefLabel>przejście graniczne</skos:prefLabel>
<skos:definition>przekraczanie granicy państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/976">
<skos:prefLabel>warstwa graniczna</skos:prefLabel>
<skos:definition>warstwa płynu w bezpośrednim sąsiedztwie ograniczenia fizycznego, wpływającego na jej swobodny ruch</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/977">
<skos:prefLabel>krętorogie</skos:prefLabel>
<skos:definition>zwierzęta z rodziny Bovidae</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/984">
<skos:prefLabel>woda słonawa (u ujścia rzek)</skos:prefLabel>
<skos:definition>słonawa woda, w wyniku zmieszanie wody słodkiej i słonej wody morskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/986">
<skos:prefLabel>ruchy skorupy ziemskiej</skos:prefLabel>
<skos:definition>długotrwała, wyjątkowo wolna, pionowa niestabilność skorupy ziemskiej występująca na zachód od Neapolu we Włoszech, osiągająca amplitudę od 6 m poniżej poziomu morza do 6 m powyżej poziomu morza w okresie ponad 2000 lat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/992">
<skos:prefLabel>mleko matki</skos:prefLabel>
<skos:definition>biała, nieprzezroczysta wydzielina gruczołów mlecznych samic ssaków stanowiąca pokarm dla nowo urodzonego potomstwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/994">
<skos:prefLabel>hodowla</skos:prefLabel>
<skos:definition>planowa opieka nad wzrostem i rozwojem zwierząt lub roślin, zabiegi mające na celu doskonalenie cech dziedzicznych hodowanych osobników; okazy będące pod opieką hodowcy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/995">
<skos:prefLabel>ptaki w okresie lęgowym</skos:prefLabel>
<skos:definition>ptaki - osobniki rozmnażające się (w danym miejscu i czasie)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/997">
<skos:prefLabel>przemysł piwowarski</skos:prefLabel>
<skos:definition>dział gospodarki związany z produkcją i dystrybucją piwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/998">
<skos:prefLabel>cegła</skos:prefLabel>
<skos:definition>materiał budowlany z gliny i piasku lub z innych surowców mineralnych, uformowany w kształcie prostopadłościanów; kostka takiego materiału</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1001">
<skos:prefLabel>most</skos:prefLabel>
<skos:definition>budowla służąca do przeprowadzania drogi komunikacyjnej nad przeszkodą wodną, składająca się z przęseł opartych za pośrednictwem łożysk na podporach (filarach, przyczółkach)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1007">
<skos:prefLabel>brom</skos:prefLabel>
<skos:definition>(Br) pierwiastek chemiczny z grupy chlorowców, liczba atomowa 35, ciecz lotna, brunatnoczerwona o przykrej woni, działa drażniąco na błony śluzowe; w przyrodzie występuje w postaci bromków, stosowany w przemyśle chemicznym, technice i medycynie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1014">
<skos:prefLabel>wysiadywanie jaj</skos:prefLabel>
<skos:definition>wysiadywanie jaj lub ogrzewanie młodych piskląt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1015">
<skos:prefLabel>strumyk</skos:prefLabel>
<skos:definition>niewielki ciek wodny; mały strumień; rzeczka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1020">
<skos:prefLabel>zarośla krzewiaste</skos:prefLabel>
<skos:definition>gęste krzaki, bez wartości handlowej z punktu widzenia pozyskiwania drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1023">
<skos:prefLabel>mszaki</skos:prefLabel>
<skos:definition>Bryophyta, drobne rośliny zarodnikowe tworzące gromadę rodniowców, plechowate albo o pędach liściastych, mające zamiast korzeni chwytniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1024">
<skos:prefLabel>polityka ustalania kwot emisji</skos:prefLabel>
<skos:definition>traktowanie emisji z zakładu lub regionu łącznie jako wielkości nieprzekraczalnej, dopuszczając jednakże zmiany w wielkości emisji poszczególnych źródeł, pod warunkiem, że wzrostowi emisji z jednego źródła towarzyszy co najmniej równoważny spadek emisji z innego (handel emisją pomiędzy źródłami)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1025">
<skos:prefLabel>budżet</skos:prefLabel>
<skos:definition>zestawienie, plan przewidzianych na przyszły okres dochodów i wydatków; ogół dochodów i wydatków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1028">
<skos:prefLabel>pluskwa</skos:prefLabel>
<skos:definition>Cimex lectularius, pluskwiak z rzędu różnoskrzydłych, czerwonobrunatny, o silnie spłaszczonym, owalnym ciele, żyjący w meblach, boazeriach itp.;pasożyt człowieka żywiący się jego krwią</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1029">
<skos:prefLabel>budynek</skos:prefLabel>
<skos:definition>budowla naziemna jednokondygnacyjna lub wielokondygnacyjna, ograniczona ścianami i dachem, mająca pomieszczenia mieszkalne lub o innym przeznaczeniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1031">
<skos:prefLabel>plac budowy</skos:prefLabel>
<skos:definition>teren na którym wykonywana ma być budowa lub gdzie ma być zlokalizowane zaplecze budowlane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1033">
<skos:prefLabel>element budowlany</skos:prefLabel>
<skos:definition>produkt przemysłowy wykorzystywany w budownictwie, przeznaczony do połączenia z innymi elementami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1039">
<skos:prefLabel>przemysł budowlany</skos:prefLabel>
<skos:definition>sztuka i technika budowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1041">
<skos:prefLabel>tereny budowlane</skos:prefLabel>
<skos:definition>teren, na którym możliwa jest zabudowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1042">
<skos:prefLabel>materiał budowlany</skos:prefLabel>
<skos:definition>materiał wykorzystywany w budownictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1044">
<skos:prefLabel>przemysł materiałów budowlanych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1046">
<skos:prefLabel>planowanie przyszłej zabudowy</skos:prefLabel>
<skos:definition>działanie związane z planowaniem, organizacją i przygotowaniem budowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1049">
<skos:prefLabel>przepisy budowlane</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1052">
<skos:prefLabel>plac budowy</skos:prefLabel>
<skos:definition>teren, na którym odbywa się budowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1054">
<skos:prefLabel>przygotowanie placu budowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1058">
<skos:prefLabel>technologia budowlana</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1059">
<skos:prefLabel>odpady budowlane</skos:prefLabel>
<skos:definition>odpady powstające przy rozbiórce lub budowie konstrukcji lub budynków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1061">
<skos:prefLabel>systemy odwodnienia</skos:prefLabel>
<skos:definition>urządzenia i sposób odprowadzania nadmiaru wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1063">
<skos:prefLabel>środowisko zurbanizowane</skos:prefLabel>
<skos:definition>otoczenie fizyczne tworzone przez człowieka (budynki, drogi, itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1064">
<skos:prefLabel>budowla</skos:prefLabel>
<skos:definition>konstrukcja stworzona przez człowieka, np.dom, zakład przemysłowy itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1065">
<skos:prefLabel>teren zabudowany</skos:prefLabel>
<skos:definition>teren, na którym znajdują się budynki i zabudowania, o znacznie ograniczonej przestrzeni otwartej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1067">
<skos:prefLabel>uprawa kwiatów cebulkowych</skos:prefLabel>
<skos:definition>uprawa kwiatów cebulkowych w celu uzyskania kwiatów lub rozmnożenia cebulek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1075">
<skos:prefLabel>biurokratyzacja</skos:prefLabel>
<skos:definition>czynienie biurokratycznym, podporządkowanie czegoś biurokratycznemu systemowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1082">
<skos:prefLabel>autobus</skos:prefLabel>
<skos:definition>duży, kryty samochód przystosowany do przewożenia większej liczby osób, używany w komunikacji miejskiej lub międzymiastowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1084">
<skos:prefLabel>działalność komercyjna i zarobkowa</skos:prefLabel>
<skos:definition>działalność komercyjna i zarobkowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1091">
<skos:prefLabel>motyl</skos:prefLabel>
<skos:definition>Lepidoptera, rząd owadów obejmujący około 150 tysięcy gatunków, mających ciało wydłużone, dwie pary błoniastych skrzydeł, pokrytych barwnymi łuskami, głowę opatrzoną czułkami oraz narządami gębowymi ssącymi; żywią się nektarem kwiatów za pomocą ssawki, biorąc przy tym udział w zapylaniu kwiatów, przechodzą przeobrażenie zupełne, roślinożerne ich larwy (zwane gąsienicami) przeważnie są szkodnikami; zasiedlają lądy całego świata</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1092">
<skos:prefLabel>bateryjka</skos:prefLabel>
<skos:definition>mała, cienka bateria do zegarków itp. urządzeń elektronicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1093">
<skos:prefLabel>przyłowy</skos:prefLabel>
<skos:definition>przypadkowe zagarnięcie w sieci gatunków innych niż celowo poławiane w danym miejscu i czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1094">
<skos:prefLabel>produkt uboczny</skos:prefLabel>
<skos:definition>produkt, którego powstanie nie jest zasadniczym celem produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1098">
<skos:prefLabel>przewód</skos:prefLabel>
<skos:definition>zaizolowane przewody elektryczne, często umieszczone we wspólnej izolacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1100">
<skos:prefLabel>kadm</skos:prefLabel>
<skos:definition>(Cd) pierwiastek chemiczny o liczbie atomowej 48, należący do cynkowców, metal rzadki towarzyszący złożom rud cynkowych; używany jako składnik stopów niskotopliwych i do pokrywania innych metali dla zabezpieczenia ich przed korozją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1101">
<skos:prefLabel>skażenie kadmem</skos:prefLabel>
<skos:definition>uwolnienie i obecność w środowisku toksycznych dawek kadmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1103">
<skos:prefLabel>cez</skos:prefLabel>
<skos:definition>(Cs) pierwiastek chemiczny, liczba atomowa 55, miękki, srebrzysty, lekki metal należący do grupy potasowców; stosowany jest w komórkach fotoelektrycznych, lampach elektronowych i jarzeniowych, w medycynie, jako odczynnik chemiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1111">
<skos:prefLabel>wapń</skos:prefLabel>
<skos:definition>(Ca) pierwiastek chemiczny z grupy wapniowców, liczba atomowa 20, miękki, lekki, srebrzystobiały metal, aktywny chemicznie, bardzo rozpowszechniony w przyrodzie w postaci związków, wchodzący w skład licznych minerałów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1112">
<skos:prefLabel>zawartość wapnia</skos:prefLabel>
<skos:definition>ilość wapnia zawarta w roztworze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1113">
<skos:prefLabel>metoda kalkulacji</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1114">
<skos:prefLabel>wzorcowanie</skos:prefLabel>
<skos:definition>określanie skali urządzenia pomiarowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1115">
<skos:prefLabel>wzorcowanie sprzętu pomiarowego</skos:prefLabel>
<skos:definition>weryfikacja skali instrumentu pomiarowego z zgodnie z ustalonymi zasadami, dla uzyskania porównywalności wyników wykonywanych na różnych urządzeniach tego samego typu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1121">
<skos:prefLabel>kemping</skos:prefLabel>
<skos:definition>teren ogrodzony, strzeżony, przystosowany do obozowania w namiotach, domkach kempingowych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1123">
<skos:prefLabel>infrastruktura turystyczna</skos:prefLabel>
<skos:definition>teren przystosowany do obozowania w namiotach, domkach kempingowych, najczęsciej ogrodzony, strzeżony, wyposażony w urządzenia sanitarne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1126">
<skos:prefLabel>kanał</skos:prefLabel>
<skos:definition>sztuczna droga wodna; wykorzystywany do transportu, jako kanał energetyczny lub kanał irygacyjny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1130">
<skos:prefLabel>rak</skos:prefLabel>
<skos:definition>nowotwór złośliwy pochodzenia nabłonkowego atakujący tkanki organizmu ludzkiego lub zwierzęcego, mający skłonność do przerzutów drogami limfatycznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1133">
<skos:prefLabel>ryzyko zachorowania na raka</skos:prefLabel>
<skos:definition>prawdopodobieństwo powstania nowotworu na skutek narażenia na działania określonej substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1134">
<skos:prefLabel>psowate</skos:prefLabel>
<skos:definition>drapieżne ssaki z rodziny Caniea, w tym psy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1148">
<skos:prefLabel>samochód</skos:prefLabel>
<skos:definition>dwuśladowy pojazd mechaniczny mający najmniej trzy koła, napędzany silnikiem, zwykle spalinowym tłokowym, składający się z nadwozia i podwozia, służący do przewozu po drogach osób lub ładunków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1159">
<skos:prefLabel>węglowodany</skos:prefLabel>
<skos:definition>organiczne związki chemiczne zbudowane z atomów węgla, wodoru i tlenu, występujące szczególnie obficie w świecie roślinnym (głównie w postaci skrobi i celulozy), stanowiące podstawowe źródło energii, koniecznej do podtrzymania procesów życiowych wszystkich żywych organizmów, niezbędny składnik pożywienia ludzi i zwierząt; cukry</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1160">
<skos:prefLabel>węgiel</skos:prefLabel>
<skos:definition>(C) pierwiastek chemiczny z grupy węglowców, liczba atomowa 6, niemetal, odporny chemicznie w niskiej temperaturze; niezbędny do życia, wchodzący w skład wszystkich organizmów w postaci związków; w stanie czystym rzadko spotykany w przyrodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1162">
<skos:prefLabel>węglan</skos:prefLabel>
<skos:definition>związki chemiczne metali, sole kwasu węglowego, substancje krystaliczne, obojętne lub kwaśne, zwykle białe lub bezbarwne; rozpowszechnione w przyrodzie jako minerały skałotwórcze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1164">
<skos:prefLabel>obieg węgla</skos:prefLabel>
<skos:definition>obieg węgla w przyrodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1168">
<skos:prefLabel>dwutlenek węgla</skos:prefLabel>
<skos:definition>związek chemiczny jednego atomu węgla z dwoma atomami tlenu, gaz bezbarwny, niepalny, bez zapachu, cięższy od powietrza; występuje w powietrzu i w wodzie, powstaje podczas spalania węgla, drewna, torfu, paliw płynnych oraz różnych związków organicznych, a także podczas procesów oddychania, butwienia, fermentacji itp.; używany m.in. do wyrobu sody amoniakalnej, do produkcji napojów musujących, do wytwarzania suchego lodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1170">
<skos:prefLabel>podatek od dwutlenku węgla</skos:prefLabel>
<skos:definition>obowiązkowe opłaty za stosowanie paliwa, pobierane proporcjonalnie do ilości emitowanego dwutlenku węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1173">
<skos:prefLabel>tlenek węgla</skos:prefLabel>
<skos:definition>związek chemiczny jednego atomu węgla z jednym atomem tlenu, gaz trujący, bezbarwny, bezwonny, powstający np. w czasie spalania węgla przy niedostatecznym dopływie powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1177">
<skos:prefLabel>utylizacja odpadów rzeźnych</skos:prefLabel>
<skos:definition>unieszkodliwianie odpadów rzeźnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1180">
<skos:prefLabel>rakotwórczość</skos:prefLabel>
<skos:definition>zdolność lub tendencja sprzyjająca tworzeniu się raka — nowotworu powodującego raka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1181">
<skos:prefLabel>test na kancerogenność</skos:prefLabel>
<skos:definition>badania mające na celu określenia ryzyka wystąpienia raka na skutek narażenia na kontakt z daną substancją lub zjawiskiem fizycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1182">
<skos:prefLabel>substancja rakotwórcza</skos:prefLabel>
<skos:definition>substancja powodująca zmiany rakotwórcze w żywych organizmach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1184">
<skos:prefLabel>kardiologia</skos:prefLabel>
<skos:definition>dział medycyny wewnętrznej zajmujący się chorobami serca i naczyń krwionośnych, a także fizjologią i patofizjologią układu krążenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1185">
<skos:prefLabel>choroby sercowo-naczyniowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1190">
<skos:prefLabel>Region Karaibów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1191">
<skos:prefLabel>zwierzę mięsożerne</skos:prefLabel>
<skos:definition>Carnivora, rząd ssaków obejmujący około 270 gatunków zwierząt mających duże, ostre zęby i mocne pazury, żywiących się głównie mięsem innych zwierząt; należą do nich wiwery, hieny, koty, łasicowate, niedźwiedzie, psy i szopy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1198">
<skos:prefLabel>pojemność ekologiczna</skos:prefLabel>
<skos:definition>maksymalna liczba gatunków, które mogą rozwijać się na danym terenie; maksymalna ilość biomasy, która może powstać na danym terenie; maksymalna liczba zwierząt hodowlanych, które mogą być utrzymywane na danym terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1199">
<skos:prefLabel>efekt przenoszenia</skos:prefLabel>
<skos:definition>oddziaływania powodowane przez zanieczyszczenia na kolejnych etapach łańcucha pokarmowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1200">
<skos:prefLabel>kartografia</skos:prefLabel>
<skos:definition>nauka o metodach sporządzania map oraz o sposobach ich wykorzystywania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1201">
<skos:prefLabel>opona samochodowa</skos:prefLabel>
<skos:definition>sprężysta powłoka z pokrytych gumą i złączonych ze sobą warstw tkaniny, stanowiąca zewnętrzną, stykającą się z nawierzchnią drogi część ogumienia pojazdu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1203">
<skos:prefLabel>uprawy na sprzedaż</skos:prefLabel>
<skos:definition>rośliny uprawiane w celu sprzedaży lokalnej lub eksportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1209">
<skos:prefLabel>kataliza</skos:prefLabel>
<skos:definition>zjawisko zmiany szybkości reakcji chemicznych zachodzące pod wpływem katalizatorów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1210">
<skos:prefLabel>katalizator</skos:prefLabel>
<skos:definition>substancja, której obecność zmienia szybkość reakcji chemicznych bez naruszenia stanu równowagi i własności termodynamicznych układu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1212">
<skos:prefLabel>katalityczny konwentor</skos:prefLabel>
<skos:definition>urządzenia służące do oczyszczania spalin odlotowych z silników spalinowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1213">
<skos:prefLabel>katastrofa</skos:prefLabel>
<skos:definition>wydarzenie nagłe, tragiczne w skutkach, w którym ktoś ucierpiał lub poniósł śmierć, które spowodowało straty materialne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1215">
<skos:prefLabel>zlewnia</skos:prefLabel>
<skos:definition>teren odwadniany przez jeden ciek; obszar, z którego wody (podziemne i powierzchniowe) spływają do jednego zbiornika (rzeki, jeziora)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1220">
<skos:prefLabel>połów</skos:prefLabel>
<skos:definition>ilość ryb pozyskanych z łowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1221">
<skos:prefLabel>gatunek chroniony</skos:prefLabel>
<skos:definition>gatunek zagrożony wyginięciem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1224">
<skos:prefLabel>kation</skos:prefLabel>
<skos:definition>jon o ładunku dodatnim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1225">
<skos:prefLabel>bydło</skos:prefLabel>
<skos:definition>udomowione zwierzęta, hodowane dla produkcji mleka, mięsa, skór oraz jako siła pociągowa, pochodzące od dzikich gatunków, z podrodziny Bovinae, np. krowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1229">
<skos:prefLabel>zasada istnienia przyczyn zaniepokojenia</skos:prefLabel>
<skos:definition>zasada związana z zasadą przezorności: gdy istnieje podejrzenie ryzyka dla środowiska brak pewności jego wystąpienia nie jest wystarczającym argumentem do zaprzestania działań jemu przeciwdziałających</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1231">
<skos:prefLabel>jaskinia</skos:prefLabel>
<skos:definition>naturalne, głębokie wydrążenie w skale z wylotem na zewnątrz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1239">
<skos:prefLabel>komórka (biologia)</skos:prefLabel>
<skos:definition>podstawowa jednostka strukturalna i funkcjonalna organizmów roślinnych i zwierzęcych, mikroskopijnej wielkości i różnego kształtu, złożona z jądra komórkowego i cytoplazmy otoczonej błoną komórkową; może stanowić samodzielny organizm lub wchodzić w skład organizmów wielokomórkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1242">
<skos:prefLabel>ogniwo (elektryczność)</skos:prefLabel>
<skos:definition>źródło stałego prądu elektrycznego zamieniające energię reakcji chemicznej (ogniwo galwaniczne) lub promieniowania elektromagnetycznego (ogniwo fotoelektryczne) na energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1244">
<skos:prefLabel>celuloza</skos:prefLabel>
<skos:definition>związek organiczny z grupy wielocukrów, trudno rozpuszczalny, główny składnik roślinnych błon komórkowych; stanowi podstawowy surowiec w produkcji papieru, włókien, tworzyw sztucznych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1245">
<skos:prefLabel>przemysł celulozowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1247">
<skos:prefLabel>cement</skos:prefLabel>
<skos:definition>spoiwo do zapraw budowlanych i betonów, mające postać szarego proszku twardniejącego po rozrobieniu wodą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1248">
<skos:prefLabel>przemysł cementowy</skos:prefLabel>
<skos:definition>przemysł produkujący cement charakteryzujący się znacznymi emisjami pyłu, dwutlenu węgla i tlenków azotu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1249">
<skos:prefLabel>cementownia</skos:prefLabel>
<skos:definition>wytwórnia cementu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1255">
<skos:prefLabel>Afryka Środkowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1256">
<skos:prefLabel>Ameryka Środkowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1257">
<skos:prefLabel>Azja Środkowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1259">
<skos:prefLabel>władze federalne</skos:prefLabel>
<skos:definition>w państwie związkowym: władze centralne w przeciwieństwie do władz poszczególnych krajów wchodzących w skład związku (stanowych, kantonalnych itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1261">
<skos:prefLabel>centralizacja</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1262">
<skos:prefLabel>odwirowywanie</skos:prefLabel>
<skos:definition>oddzielanie czegoś od czegoś przez wirowanie, zwłaszcza rozdzielić w ten sposób składniki ciekłej mieszaniny niejednorodnej, różniące się gęstością</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1265">
<skos:prefLabel>głowonogi</skos:prefLabel>
<skos:definition>Cephalopoda, gromada mięczaków morskich odznaczających się charakterystycznymi ramionami otaczającymi otwór gębowy, uzbrojonymi w przyssawki lub haczyki, służącymi do chwytania zdobyczy; należą do nich m.in. ośmiornice, kalmary, sepie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1266">
<skos:prefLabel>ceramika</skos:prefLabel>
<skos:definition>sztuka wyrabiania i wypalania przedmiotów z różnych glinek; wyroby użytkowe i dekoracyjne formowane z naturalnych glin oraz ich mieszanin z dodatkami mineralnymi i organicznymi, a następnie suszone i wypalane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1267">
<skos:prefLabel>przemysł ceramiczny</skos:prefLabel>
<skos:definition>dział technologii, przemysł zajmujący się wytwarzaniem wyrobów użytkowych i dekoracyjnych formowanych z naturalnych glin oraz ich mieszanin z dodatkami mineralnymi i organicznymi, a następnie suszonych i wypalanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1275">
<skos:prefLabel>walenie</skos:prefLabel>
<skos:definition>Cetacea, rząd wielkich ssaków morskich przystosowanych do życia wodnego, niezdolnych do przebywania na lądzie; obejmuje kilkadziesiąt gatunków zwierząt o krępym, wrzecionowatym, bezwłosym ciele, w którym kończyny przednie w kształcie płetw tworzą narząd sterowy, tylne są w zaniku, a głównym narządem ruchu jest pozioma płetwa ogonowa; żyją w morzach całego świata, należą do nich m.in. delfiny, kaszaloty, wale</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1279">
<skos:prefLabel>choroba Chagasa</skos:prefLabel>
<skos:definition>trypanosomatoza amerykańska, choroba zakaźna wywoływana przez świdrowca Trypanosoma cruzi, występująca w Afryce Południowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1280">
<skos:prefLabel>administracja przedsiębiorstwa</skos:prefLabel>
<skos:definition>zarządzanie i planowanie przepływu towarów, w tym sieć dystrybutorów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1286">
<skos:prefLabel>zmiany cen</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1288">
<skos:prefLabel>system dystrybucji</skos:prefLabel>
<skos:definition>system podziału dóbr materialnych (m.in. woda, gaz, energia elektryczna) między członków społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1290">
<skos:prefLabel>węgiel drzewny</skos:prefLabel>
<skos:definition>węgiel otrzymywany w procesie suchej destylacji drewna, mający dużą zdolność adsorpcyjną; stosowany w metalurgii i w lecznictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1291">
<skos:prefLabel>opłata umowna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1292">
<skos:prefLabel>mapa graficzna</skos:prefLabel>
<skos:definition>mapa, na której naniesiono dodatkowe informacje , np. przydatne w nawigacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1296">
<skos:prefLabel>szczękoczułkowce</skos:prefLabel>
<skos:definition>podtyp stawonogów mających szczękoczułki, obejmujący staroraki i pajęczaki (około 40 tysięcy gatunków)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1298">
<skos:prefLabel>analiza chemiczna</skos:prefLabel>
<skos:definition>badanie jakościowego i ilościowego składu chemicznego substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1299">
<skos:prefLabel>skład chemiczny</skos:prefLabel>
<skos:definition>stały, charakterystyczny dla danego ciała, prawidłowy jakościowy i ilościowy zestaw części składowych (np. pierwiastków, związków chemicznych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1303">
<skos:prefLabel>usuwanie skażeń chemicznych</skos:prefLabel>
<skos:definition>usuwanie szkodliwych substancji chemicznych z budynków, ubrań, środowiska, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1305">
<skos:prefLabel>inżynieria chemiczna</skos:prefLabel>
<skos:definition>nauka o operacjach jednostkowych stosowanych w przemyśle chemicznym i przemysłach pokrewnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1306">
<skos:prefLabel>opad chemiczny</skos:prefLabel>
<skos:definition>wypadanie z atmosfery substancji chemicznych, powstałych na skutek działalności przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1307">
<skos:prefLabel>nawóz chemiczny</skos:prefLabel>
<skos:definition>sole mineralne lub inne związki chemiczne dodawane do gleby w celu wyrównania ubytku pewnych pierwiastków pobieranych przez uprawiane na niej rośliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1310">
<skos:prefLabel>przemysł chemiczny</skos:prefLabel>
<skos:definition>przemysł związany z produkcją substancji chemicznych potencjalnie powodujący wiele rodzajów zanieczyszczeń, z uwagi na różnorodność powstających produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1311">
<skos:prefLabel>budynek przemysłowy</skos:prefLabel>
<skos:definition>budynek, w którym wytwarzane są produkty chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1312">
<skos:prefLabel>oceanografia chemiczna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1314">
<skos:prefLabel>zwalczanie szkodników metodami chemicznymi</skos:prefLabel>
<skos:definition>ograniczanie populacji roślin i zwierząt uznanych za szkodliwe metodami chemicznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1316">
<skos:prefLabel>fabryka chemiczna</skos:prefLabel>
<skos:definition>zakład przemysłowy wytwarzający maszynowo na wielką skalę środki chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1317">
<skos:prefLabel>polityka w zakresie chemii</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1318">
<skos:prefLabel>odpady chemiczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1319">
<skos:prefLabel>zanieczyszczenie chemiczne</skos:prefLabel>
<skos:definition>zanieczyszczenie powodowane przez substancje chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1320">
<skos:prefLabel>proces chemiczny</skos:prefLabel>
<skos:definition>metoda produkcji wykorzystująca zjawiska chemiczne i skutkująca powstaniem produktu chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1322">
<skos:prefLabel>właściwość chemiczna</skos:prefLabel>
<skos:definition>właściwość substancji zależna od ułożenia atomów w cząsteczce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1324">
<skos:prefLabel>reakcja chemiczna</skos:prefLabel>
<skos:definition>przemiana jednych substancji (substratów) w inne (produkty), której towarzyszy wymiana z otoczeniem pewnej ilości energii, najczęściej ciepła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1326">
<skos:prefLabel>zagrożenie chemiczne</skos:prefLabel>
<skos:definition>prawdopodobieństwo wystąpienia szkód na skutek kontaktu z daną substancją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1327">
<skos:prefLabel>chemikalia</skos:prefLabel>
<skos:definition>preparaty, odczynniki i inne substancje chemiczne, stosowane w laboratoriach i w przemyśle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1328">
<skos:prefLabel>akt prawny dotyczący substancji i preparatów chemicznych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1330">
<skos:prefLabel>substancje chemiczne w środowisku</skos:prefLabel>
<skos:definition>obecność w środowisku substancji chemicznych, na skutek procesów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1332">
<skos:prefLabel>badanie chemikaliów</skos:prefLabel>
<skos:definition>określenie toksyczności produktów chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1333">
<skos:prefLabel>struktura chemiczna</skos:prefLabel>
<skos:definition>układ atomów w cząsteczce związku chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1336">
<skos:prefLabel>chemiczne przetwarzanie</skos:prefLabel>
<skos:definition>proces zmiany struktury chemicznej odpadów, przeprowadzany w celu ograniczenia ich uciążliwości dla środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1337">
<skos:prefLabel>chemiczne przetwarzanie odpadów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1341">
<skos:prefLabel>odpady chemiczne</skos:prefLabel>
<skos:definition>produkt uboczny procesu chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1343">
<skos:prefLabel>broń chemiczna</skos:prefLabel>
<skos:definition>broń masowego rażenia wyzyskująca substancje chemiczne o działaniu toksycznym w celu porażenia ludzi, zwierząt, a także skażenia terenu, wody, żywności itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1347">
<skos:prefLabel>adsorpcja chemiczna</skos:prefLabel>
<skos:definition>adsorpcja polegająca na zagęszczaniu się substancji na powierzchni adsorbentu, któremu towarzyszy tworzenie się wiązań chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1348">
<skos:prefLabel>chemia</skos:prefLabel>
<skos:definition>nauka zajmująca się badaniem właściwości i budowy różnych substancji oraz badaniem przemian jednych substancji w inne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1350">
<skos:prefLabel>dziecko</skos:prefLabel>
<skos:definition>człowiek od urodzenia do wieku młodzieńczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1355">
<skos:prefLabel>komin</skos:prefLabel>
<skos:definition>konstrukcja z pionowym kanałem do odprowadzania spalin z paleniska do atmosfery oraz do wytwarzania naturalnego ciągu powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1356">
<skos:prefLabel>wysokość komina</skos:prefLabel>
<skos:definition>wysokość komina dobrana, tak by ograniczyć powodowane stężenia zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1362">
<skos:prefLabel>nietoperz</skos:prefLabel>
<skos:definition>Chiroptera, rząd drobnych ssaków mających zdolność latania za pomocą błon lotnych rozpiętych pomiędzy kończynami przednimi, bokami ciała a kończynami tylnymi i ogonem, prowadzących nocny i zmierzchowy tryb życia, odznaczających się doskonale rozwiniętym zmysłem słuchu; większość gatunków jest owadożerna, niektóre z nich (wampiry) żywią się krwią dużych ssaków; w Polsce występuje 20 gatunków objętych ochroną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1366">
<skos:prefLabel>chlorek</skos:prefLabel>
<skos:definition>każdy dwuskładnikowy związek nieorganiczny chloru ujemnie jednowartościowego z innym pierwiastkiem; w języku ogólnym także każdy związek organiczny zawierający chlor</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1368">
<skos:prefLabel>węglowodory chlorowane</skos:prefLabel>
<skos:definition>trwałe substancje chemiczne, kumulujące się w środowisku, wykorzystywane jako insektycydy, np. DDT</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1371">
<skos:prefLabel>chlorowanie wody</skos:prefLabel>
<skos:definition>stosowanie związków chloru w procesie uzdatniania wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1373">
<skos:prefLabel>chlor</skos:prefLabel>
<skos:definition>(Cl) pierwiastek chemiczny, liczba atomowa 17, trujący gaz z grupy chlorowców, o drażniącym zapachu i właściwościach utleniających i bakteriobójczych; stosowany m.in. do dezynfekcji wody, jako środek bielący, w syntezach związków organicznych i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1378">
<skos:prefLabel>chloroetylen</skos:prefLabel>
<skos:definition>palny, wybuchowy gaz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1381">
<skos:prefLabel>chlorofluorowęglowodory</skos:prefLabel>
<skos:definition>gaz stosowany często jako propelent w sprejach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1384">
<skos:prefLabel>zakaz używania chlorofluorowęglowodorów i halonów</skos:prefLabel>
<skos:definition>zakaz produkcji i stosowania produktów powodujących emisję chlorofluorowęglowodorów i halonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1385">
<skos:prefLabel>częściowo podstawione chlorowcem chlorofluorowęglowodory</skos:prefLabel>
<skos:definition>węglowodory, w których niektóre atomy wodoru zostały zastąpione chlorem lub fluorem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1389">
<skos:prefLabel>chlorofenol</skos:prefLabel>
<skos:definition>grupa węglowodorów chlorowanych, obejmująca pestycydy i biodydy o szerokim zastosowaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1391">
<skos:prefLabel>chlorofil</skos:prefLabel>
<skos:definition>związek organiczny, zielony barwnik, główny składnik ciałek zieleni roślin, umożliwiający przemianę energii światła słonecznego w energię chemiczną, wykorzystywaną w procesie fotosyntezy; stosowany jako barwnik do środków kosmetycznych oraz jako składnik preparatów odwaniających</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1393">
<skos:prefLabel>chloroza</skos:prefLabel>
<skos:definition>choroba roślin na skutek zahamowania tworzenia się chlorofilu, objawiająca się blednięciem lub żółknięciem liści i ogólnym niedorozwojem roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1399">
<skos:prefLabel>analiza chromatograficzna</skos:prefLabel>
<skos:definition>analiza substancji chemicznych przy pomocy specjalnego urządzenia, wykorzystującego metodę rozdzielania ciekłych lub gazowych mieszanin związków chemicznych na składniki, oparta na wyzyskaniu różnej zdolności adsorbowania i osadzania się danych składników w oparciu o własności adsorpcyjne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1400">
<skos:prefLabel>chromatografia</skos:prefLabel>
<skos:definition>metoda rozdzielania ciekłych lub gazowych mieszanin związków chemicznych na składniki, oparta na wyzyskaniu różnej zdolności adsorbowania i osadzania się danych składników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1401">
<skos:prefLabel>chrom</skos:prefLabel>
<skos:definition>(Cr) pierwiastek chemiczny należący do chromowców, liczba atomowa 24, srebrzystoniebieski twardy metal, używany jako dodatek do stopów ze względu na właściwości antykorozyjne, do chromowania, stosowany też w przemyśle chemicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1406">
<skos:prefLabel>Chrysophyta</skos:prefLabel>
<skos:definition>gromada obejmująca glony jednokomórkowe o barwie złocistobrunatnej, wywołanej obecnością dużej ilości karotenoidów, w większości organizmy samożywne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1410">
<skos:prefLabel>budynek sakralny</skos:prefLabel>
<skos:definition>budynek kultu religijnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1419">
<skos:prefLabel>świadomość społeczna</skos:prefLabel>
<skos:definition>ogół idei, poglądów, przekonań wspólnych dla całych grup, klas, społeczeństw, wyznaczony przez ich byt społeczny; obejmuje zarówno postawy i opinie jednostek, jak i utrwalone formy życia duchowego: ideologię, prawo, moralność, naukę itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1421">
<skos:prefLabel>miasto</skos:prefLabel>
<skos:definition>duży obszar intensywnie i planowo zabudowany, podlegający odrębnej administracji, będący skupiskiem ludności wykonującej zawody nierolnicze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1422">
<skos:prefLabel>centrum miasta</skos:prefLabel>
<skos:definition>obszar centrum miejskiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1426">
<skos:prefLabel>wysypisko komunalne</skos:prefLabel>
<skos:definition>miejsce deponowania odpadów komunalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1431">
<skos:prefLabel>cywilny ruch powietrzny</skos:prefLabel>
<skos:definition>ruch powietrzny wykorzystywany w komunikacji publicznej, nie wykorzystywany na cele wojskowe, militarne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1434">
<skos:prefLabel>inżynieria lądowa i wodna</skos:prefLabel>
<skos:definition>nauka wykonywania robót inżynierskich, umiejętność projektowania i wznoszenia różnych rodzajów budowli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1436">
<skos:prefLabel>ochrona cywilna</skos:prefLabel>
<skos:definition>organizacja i działania podejmowane w celu zapobiegania i ograniczania skutków nadzwyczajnych zagrożeń i awarii przemysłowych oraz ochrony ludności cywilnej i mienia społecznego, także w czasie wojny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1437">
<skos:prefLabel>prawo cywilne</skos:prefLabel>
<skos:definition>ogół przepisów normujących stosunki osobowe i majątkowe jednostki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1445">
<skos:prefLabel>żądanie zwrotu przywłaszconego mienia</skos:prefLabel>
<skos:definition>prawo strony do żądania przewrócenia własności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1446">
<skos:prefLabel>proces prawny w interesie grupy</skos:prefLabel>
<skos:definition>proces wytaczany przez osobę lub grupę osób w imieniu określonej grupy interesów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1447">
<skos:prefLabel>klasyfikacja</skos:prefLabel>
<skos:definition>systematyczny podział czegoś, np. przedmiotów lub zjawisk na klasy, działy, poddziały itp. według określonej zasady; zaklasyfikowanie danego zjawiska lub przedmiotu do odpowiedniego działu, grupy itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1449">
<skos:prefLabel>glina</skos:prefLabel>
<skos:definition>skała osadowa składająca się głównie z iłu z domieszką mułu, piasku, niekiedy żwiru, zwykle żółta, ceglasta lub brunatna, składnik ciężkich gleb, zwilżona wodą staje się plastyczna; stanowi podstawowy surowiec w przemyśle ceramicznym (np. do produkcji cegły, dachówki, materiałów ogniotrwałych), stosowana w budownictwie (do zapraw budowlanych), w garncarstwie, rzeźbiarstwie itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1456">
<skos:prefLabel>czyszczenie</skos:prefLabel>
<skos:definition>proces oczyszczania terenów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1457">
<skos:prefLabel>wydział oczyszczania</skos:prefLabel>
<skos:definition>dział administracji terenowej odpowiedzialny za oczyszczanie terenów publicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1458">
<skos:prefLabel>produkt czyszczący</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1460">
<skos:prefLabel>czystsza technologia</skos:prefLabel>
<skos:definition>proces przemysłowy, w którym ograniczono ilość powstających zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1462">
<skos:prefLabel>klimat</skos:prefLabel>
<skos:definition>charakterystyczny dla danego obszaru zespół zjawisk i procesów atmosferycznych, kształtujących się pod wpływem właściwości fizycznych i geograficznych tego obszaru, określony na podstawie wyników wieloletnich obserwacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1464">
<skos:prefLabel>wpływ klimatu</skos:prefLabel>
<skos:definition>klimat o podstawowym znaczeniu dla potrzeb i działań człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1466">
<skos:prefLabel>ochrona klimatu</skos:prefLabel>
<skos:definition>działania, procedury i instalacje służące do zapobiegania lub ograniczania negatywnego wpływu zanieczyszczeń na pogodę, temperaturę, skład atmosfery itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1467">
<skos:prefLabel>zasoby klimatyczne</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1469">
<skos:prefLabel>typ klimatu</skos:prefLabel>
<skos:definition>warunki pogodowe typowe dla danego obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1470">
<skos:prefLabel>zmienność klimatu</skos:prefLabel>
<skos:definition>stopniowa zmiana charakterystyk klimatu w czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1471">
<skos:prefLabel>zmiana klimatu</skos:prefLabel>
<skos:definition>długofalowe zmiany temperatury, opadów, siły i kierunku wiatru itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1473">
<skos:prefLabel>eksperymenty klimatyczne</skos:prefLabel>
<skos:definition>działania prowadzone, aby na podstawie modelowania oszacować przyszłe warunki klimatyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1474">
<skos:prefLabel>czynniki klimatyczne</skos:prefLabel>
<skos:definition>warunki fizyczne określające klimat w danym regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1476">
<skos:prefLabel>strefa klimatyczna</skos:prefLabel>
<skos:definition>obszar jednorodnego pod wieloma względami klimatu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1479">
<skos:prefLabel>klimatologia</skos:prefLabel>
<skos:definition>nauka o klimacie zajmująca się fizycznymi procesami kształtowania się klimatów, wpływem czynników geograficznych na przebieg tych procesów, opisywaniem i klasyfikowaniem klimatów różnych obszarów kuli ziemskiej, także klimatów minionych epok geologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1480">
<skos:prefLabel>zbiorowisko klimaksowe</skos:prefLabel>
<skos:definition>dynamiczna równowaga gatunków roślin; końcowe, trwałe stadium rozwojowe ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1481">
<skos:prefLabel>pnącze</skos:prefLabel>
<skos:definition>światłolubne rośliny o długich, wiotkich pędach wymagających podpór, oplatające podporę (np. powój) lub przyczepiające się do niej za pomocą wąsów, korzeni czepnych albo kolców (np. groch, bluszcz, róże pnące)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1482">
<skos:prefLabel>objawy kliniczne</skos:prefLabel>
<skos:definition>obiektywny dowód choroby; zjawisko, poddające się ocenie lekarskiej i stanowiące podstawę do wnioskowania o czynnościach ustroju, jego narządów i tkanek tak w stanie zdrowia, jak i choroby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1484">
<skos:prefLabel>klonowanie</skos:prefLabel>
<skos:definition>produkowanie identycznych genetycznie osobników z jednego organizmu rodzicielskiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1486">
<skos:prefLabel>zamknięcie</skos:prefLabel>
<skos:definition>zakończenie, zaprzestanie lub przerwanie transakcji lub umowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1487">
<skos:prefLabel>zamknięcie firmy</skos:prefLabel>
<skos:definition>tymczasowe lub trwałe zakończenie lub zawieszenie działalności firmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1489">
<skos:prefLabel>ubranie</skos:prefLabel>
<skos:definition>odzież jako grupa artykułów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1490">
<skos:prefLabel>przemysł odzieżowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1491">
<skos:prefLabel>chmura</skos:prefLabel>
<skos:definition>zawieszone w atmosferze widzialne skupienie drobniutkich kropelek wody, kryształków lodu albo ich mieszaniny, powstające w wyniku kondensacji pary wodnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1499">
<skos:prefLabel>koagulacja</skos:prefLabel>
<skos:definition>łączenie się cząstek koloidalnych w większe zespoły, co powoduje wytrącenie się osadu w postaci zwartego koagulatu lub przejście zolu w żel</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1500">
<skos:prefLabel>węgiel</skos:prefLabel>
<skos:definition>surowiec naturalny pochodzenia roślinnego; kopalina palna stanowiąca ważny surowiec energetyczny i chemiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1502">
<skos:prefLabel>energia z węgla</skos:prefLabel>
<skos:definition>energia wytwarzana pod wpływem pary generowanej w procesie spalania węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1503">
<skos:prefLabel>ciepłownia</skos:prefLabel>
<skos:definition>zakład energetyczny wytwarzający energię cieplną w postaci gorącej wody lub pary wodnej, stosowaną w przemyśle oraz do centralnego ogrzewania budynków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1505">
<skos:prefLabel>gazyfikacja węgla</skos:prefLabel>
<skos:definition>proces zgazowania węgla mający na celu przemianę węgla pierwiastkowego zawartego w surowcu stałym na gaz, który wykorzystywany jest jako paliwo w elektrowniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1506">
<skos:prefLabel>upłynnianie węgla</skos:prefLabel>
<skos:definition>proces destylacji węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1507">
<skos:prefLabel>wydobycie węgla</skos:prefLabel>
<skos:definition>działania techniczne i mechaniczne związane z wydobyciem węgla spod powierzchni ziemi i jego dystrybucją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1509">
<skos:prefLabel>rafinacja węgla</skos:prefLabel>
<skos:definition>obróbka węgla; usuwanie zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1511">
<skos:prefLabel>technologia przetwórstwa węglowego</skos:prefLabel>
<skos:definition>przetwarzanie węgla na paliwo płynne lub gazowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1512">
<skos:prefLabel>wybrzeże</skos:prefLabel>
<skos:definition>obszar lądu przylegający do brzegu wód (zwykle oceanu lub morza)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1513">
<skos:prefLabel>obszar nadmorski</skos:prefLabel>
<skos:definition>teren obejmujący brzeg morza oraz graniczące z nim tereny wybrzeża i wody przybrzeżne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1515">
<skos:prefLabel>zagospodarowanie obszarów nadbrzeżnych</skos:prefLabel>
<skos:definition>intensywne zagospodarowanie terenów nadmorskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1516">
<skos:prefLabel>ekosystem wybrzeży</skos:prefLabel>
<skos:definition>ekosystem wybrzeża morskiego oraz wód szelfowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1517">
<skos:prefLabel>środowisko obszarów nadbrzeżnych</skos:prefLabel>
<skos:definition>środowisko obszarów na styku lądu i morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1519">
<skos:prefLabel>erozja morska</skos:prefLabel>
<skos:definition>niszczenie brzegów morza przez falowanie i przypływ oraz prądy przybrzeżne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1520">
<skos:prefLabel>rybołówstwo przybrzeżne</skos:prefLabel>
<skos:definition>łowienie ryb, homarów, langust, ostryg itp. wzdłuż linii brzegowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1523">
<skos:prefLabel>zanieczyszczenie obszaru nadmorskiego</skos:prefLabel>
<skos:definition>emisja zanieczyszczeń w obszarach nadmorskich lub bezpośrednio do morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1526">
<skos:prefLabel>wody nadbrzeżne</skos:prefLabel>
<skos:definition>wody szelfu kontynentalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1527">
<skos:prefLabel>planowanie obszaru nadmorskiego</skos:prefLabel>
<skos:definition>obiektywne zarządzanie i planowanie wykorzystania zasobów obszarów nadmorskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1528">
<skos:prefLabel>przybrzeżna służba ratownicza</skos:prefLabel>
<skos:definition>służba ratownicza i porządkowa na wodach przybrzeżnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1530">
<skos:prefLabel>powłoka</skos:prefLabel>
<skos:definition>materiał stosowany powierzchniowo w celach ochronnych, technologicznych lub dekoracyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1532">
<skos:prefLabel>kobalt</skos:prefLabel>
<skos:definition>(Co) pierwiastek chemiczny z grupy żelazowców, liczba atomowa 27, srebrzystoszary metal, trudno topliwy, dość odporny chemicznie, używany m.in. do wyrobu stali szybkotnących, stopów węglików spiekanych, trwałych magnesów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1533">
<skos:prefLabel>karaluch</skos:prefLabel>
<skos:definition>Blatta orientalis, owad z rzędu karaczanów, o brunatnoczarnym ubarwieniu, synantropijny, żyje w ciemnych, wilgotnych miejscach (kuchniach, magazynach) niszcząc produkty żywnościowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1536">
<skos:prefLabel>chemiczne zapotrzebowanie na tlen</skos:prefLabel>
<skos:definition>ilość tlenu potrzebna do biologicznego i niebiologicznego utlenienia zanieczyszczeń zawartych w wodzie; miara jakości wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1541">
<skos:prefLabel>kodeks postępowania</skos:prefLabel>
<skos:definition>zbiór zasad proceduralnych ustanawiających metody stosowania przepisów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1542">
<skos:prefLabel>kodeks</skos:prefLabel>
<skos:definition>zbiór usystematyzowanych i opartych na wspólnych zasadach przepisów, dotyczących jednej gałęzi prawa, regulujący określoną dziedzinę stosunków społecznych, wydawany zwykle w formie ustawy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1548">
<skos:prefLabel>jamochłon</skos:prefLabel>
<skos:definition>Coelenterata, wodne zwierzęta bezkręgowe zbudowane z dwu warstw komórek: zewnętrznej ektodermy i wewnętrznej entodermy, charakteryzujące się najprostszym typem układu pokarmowego — jamą chłonno-trawiącą; zamieszkują przeważnie wody morskie i występują na ogół w dwu postaciach: osiadłego dzbanuszkowatego polipa i swobodnie pływającej parasolowatej meduzy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1550">
<skos:prefLabel>wytwarzanie łączne</skos:prefLabel>
<skos:definition>łączne wytwarzanie ciepła i energii elektrycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1553">
<skos:prefLabel>łączne spalanie odpadów</skos:prefLabel>
<skos:definition>spalanie odpadów niebezpiecznych wraz z innymi odpadami lub osadami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1554">
<skos:prefLabel>koks</skos:prefLabel>
<skos:definition>produkt suchej destylacji węgla kamiennego; także: pozostałość po destylacji ropy naftowej, paku lub innych substancji organicznych; substancja stała, porowata, lżejsza od węgla, o dużej wartości opałowej, stosowana w hutnictwie, używana do produkcji karbidu, gazu generatorowego oraz jako opał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1556">
<skos:prefLabel>zimno</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1559">
<skos:prefLabel>ekosystem strefy chłodnej</skos:prefLabel>
<skos:definition>ekosystem obszarów o temperaturze powietrza poniżej 10C przez 8 do 11 miesięcy w roku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1561">
<skos:prefLabel>bakterie Coli</skos:prefLabel>
<skos:definition>bakterie typu fekalnego; Gram-ujemne z rodziny Enterobacteriaceae (pałeczki jelitowe), z rodzaju Escherichia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1567">
<skos:prefLabel>koloidy</skos:prefLabel>
<skos:definition>rozpowszechnione w przyrodzie układy o dużym stopniu rozdrobnienia, stanowiące ogniwo pośrednie między roztworami rzeczywistymi a zawiesinami; należą do nich aerozole, piany, zole</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1568">
<skos:prefLabel>stan koloidowy</skos:prefLabel>
<skos:definition>cząsteczki zawieszone w ośrodku rozpraszającym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1569">
<skos:prefLabel>kolonizacja</skos:prefLabel>
<skos:definition>skuteczne zajęcie nowego terenu przez jakiś gatunek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1570">
<skos:prefLabel>kolorymetria</skos:prefLabel>
<skos:definition>technika pomiarów służąca do ilościowego opisu barw, stosowana w farbiarstwie, poligrafii, fotografii barwnej, przemyśle papierniczym i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1571">
<skos:prefLabel>kolor</skos:prefLabel>
<skos:definition>postrzegana wzrokowo właściwość przedmiotu zależna od stopnia pochłaniania, rozpraszania lub przepuszczania promieni świetlnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1574">
<skos:prefLabel>oddziaływania wzmacniające się</skos:prefLabel>
<skos:definition>łączne oddziaływanie dwóch lub więcej substancji lub czynników, większe niż suma poszczególnych oddziaływań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1575">
<skos:prefLabel>elektrociepłownia wykorzystująca różne rodzaje paliw</skos:prefLabel>
<skos:definition>elektrociepłownia, w której gazy wylotowe z turbiny gazowej wykorzystywane są do wytworzenia pary napędzającej turbinę parową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1576">
<skos:prefLabel>ścieki mieszane</skos:prefLabel>
<skos:definition>mieszanina ścieków bytowych lub przemysłowych i deszczowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1578">
<skos:prefLabel>palność</skos:prefLabel>
<skos:definition>zdolność ciała do zapalania się i podtrzymywania palenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1582">
<skos:prefLabel>silnik spalinowy</skos:prefLabel>
<skos:definition>silnik cieplny, w którym czynnikiem roboczym są rozprężające się gazy spalinowe powstałe ze spalania się mieszanki paliwowo-powietrznej wewnątrz silnika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1583">
<skos:prefLabel>gazy powstające na skutek spalania</skos:prefLabel>
<skos:definition>gazy wylotowe z procesu spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1584">
<skos:prefLabel>pozostałości po spalaniu</skos:prefLabel>
<skos:definition>osad zestalonego popiołu na ścianach komory spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1588">
<skos:prefLabel>komercjalizacja</skos:prefLabel>
<skos:definition>oparcie jakiejś działalności na zasadach handlowych, zwykle kosztem jakości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1590">
<skos:prefLabel>prawo handlowe</skos:prefLabel>
<skos:definition>ogół przepisów normujących stosunki prawne kupca oraz stosunki prawne wynikające z czynności handlowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1591">
<skos:prefLabel>hałas pochodzący od obiektów handlowych i usługowych</skos:prefLabel>
<skos:definition>hałas generowany na skutek działalności handlowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1594">
<skos:prefLabel>ruch handlowy</skos:prefLabel>
<skos:definition>działania związane z przemieszczaniem towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1595">
<skos:prefLabel>środek transportu towarowego</skos:prefLabel>
<skos:definition>środek transportu zaprojektowany i wyposażony dla przewozu towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1596">
<skos:prefLabel>odpady handlowe</skos:prefLabel>
<skos:definition>odpady powstające na skutek działalności biur, restauracji, sklepów i innego rodzaju działalności innej niż produkcyjna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1600">
<skos:prefLabel>Wspólna Polityka Rolna UE</skos:prefLabel>
<skos:definition>przepisy i zasady praktyczne przyjęte przez kraje członkowskie Wspólnoty Europejskiej w celu zapewnienia godnego standardu życia na wsi, właściwego poziomu podaży żywności i sposobu zagospodarowania nadwyżek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1601">
<skos:prefLabel>porozumienie społeczne</skos:prefLabel>
<skos:definition>prawo oparte na precedensach i zwyczajach społecznych, bardziej niż na literze przepisów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1605">
<skos:prefLabel>komunikacja</skos:prefLabel>
<skos:definition>proces informacyjny mający na celu przekazanie informacji, składający się z nadawania komunikatu połączonego najczęściej z kodowaniem informacji, przekazywania oraz odbierania komunikatu połączonego z odkodowywaniem informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1613">
<skos:prefLabel>biocenoza</skos:prefLabel>
<skos:definition>zespół populacji różnych gatunków zwierząt i roślin, żyjący w określonym środowisku — biotypie, podlegający jego czynnikom i powiązany ze sobą zależnościami pokarmowymi oraz konkurencją biologiczną wewnątrzgatunkową i międzygatunkową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1616">
<skos:prefLabel>prawo wspólnotowe</skos:prefLabel>
<skos:definition>prawo Wspólnoty Europejskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1618">
<skos:prefLabel>udział w samorządzeniu</skos:prefLabel>
<skos:definition>udział społeczeństwa w działaniach lub wywieranie presji społecznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1619">
<skos:prefLabel>zasada &quot;społeczeństwo płaci&quot;</skos:prefLabel>
<skos:definition>zasada polityki środowiskowej zakładająca, że koszty poprawy stanu środowiska oraz usuwania zagrożeń pokrywane są przez społeczeństwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1622">
<skos:prefLabel>ruch dojazdowy</skos:prefLabel>
<skos:definition>ruch polegający na utrzymywaniu łączności między dzielnicami podmiejskimi a centrum, między miejcem zamieszkania a miejscem pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1623">
<skos:prefLabel>dojazdy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1625">
<skos:prefLabel>zgniatanie</skos:prefLabel>
<skos:definition>mechaniczne zmniejszanie objętości odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1628">
<skos:prefLabel>polityka przedsiębiorstwa</skos:prefLabel>
<skos:definition>zasady przyjęte przez firmę i stosowane w praktyce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1630">
<skos:prefLabel>prawo porównawcze</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1631">
<skos:prefLabel>badania porównawcze</skos:prefLabel>
<skos:definition>badania prowadzone by określić różnice pomiędzy dwoma procedurami badawczymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1632">
<skos:prefLabel>porównanie</skos:prefLabel>
<skos:definition>zestawienie dwóch lub więcej przedmiotów aby  wykazać ich podobieństwa i różnice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1633">
<skos:prefLabel>odszkodowanie</skos:prefLabel>
<skos:definition>wynagrodzenie, zapłata za wyrządzone szkody, za poniesione przez kogoś straty, doznane krzywdy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1636">
<skos:prefLabel>kompensacja strat ekologicznych</skos:prefLabel>
<skos:definition>działania prawne lub administracyjne podejmowane w celu zrównoważenia strat w ekosystemie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1637">
<skos:prefLabel>podatek wyrównawczy</skos:prefLabel>
<skos:definition>obowiązkowa opłata, nakładana dla wyrównania różnic gospodarczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1638">
<skos:prefLabel>konkurencja (biologiczna)</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1639">
<skos:prefLabel>konkurencja gospodarcza</skos:prefLabel>
<skos:definition>rywalizacja między osobami prywatnymi lub jednostkami gospodarczymi, mająca na celu zdobycie rynków zbytu, źródeł zakupu surowców i osiągnięcie jak największych korzyści przy sprzedaży towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1641">
<skos:prefLabel>konkurencyjność</skos:prefLabel>
<skos:definition>zdolność firny do skutecznego konkurowania z innymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1645">
<skos:prefLabel>kompleksacja</skos:prefLabel>
<skos:definition>tworzenie cząstki złożonej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1646">
<skos:prefLabel>czynnik kompleksujący</skos:prefLabel>
<skos:definition>substancja zdolna do kompleksacji z inną substancją w roztworze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1647">
<skos:prefLabel>zanieczyszczenia złożone</skos:prefLabel>
<skos:definition>emisja substancji zubażających warstwę ozonową, gazów cieplarnianych, metanu, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1649">
<skos:prefLabel>kompost</skos:prefLabel>
<skos:definition>nawóz organiczny otrzymywany z mieszaniny różnych odpadków, głównie roślinnych, przekładanych ziemią lub torfem w stosach, po ich częściowym rozkładzie tlenowym przez drobnoustroje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1651">
<skos:prefLabel>odpady nadające się do kompostowania</skos:prefLabel>
<skos:definition>odpady złożone głównie z substancji organicznej podatnej na rozkład biologiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1652">
<skos:prefLabel>kompostowanie</skos:prefLabel>
<skos:definition>naturalny tlenowy rozkład biologiczny substancji organicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1653">
<skos:prefLabel>kompostowanie przez producenta</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1659">
<skos:prefLabel>ściskanie</skos:prefLabel>
<skos:definition>ograniczanie objętości na skutek działania ciśnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1660">
<skos:prefLabel>sprężarka</skos:prefLabel>
<skos:definition>maszyna do zwiększania ciśnienia gazów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1663">
<skos:prefLabel>użytkowanie wymuszone</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1669">
<skos:prefLabel>Wspólnoty Europejskie</skos:prefLabel>
<skos:definition>ciało kolektywne, które powstało w 1967 r. w wyniku fuzji EWWS Europejskiej Wspólnoty Węgla i Stali, EWG Europejskiej Wspólnoty Gospodarczej oraz EURATOM Europejskiej Wspólnoty Energii Atomowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1673">
<skos:prefLabel>stężenie (wartość)</skos:prefLabel>
<skos:definition>zawartość danego składnika w jednostce objętości lub masy substancji, wyrażona w określony sposób (np. procent wagowy, ułamek molowy)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1674">
<skos:prefLabel>zatężanie (proces)</skos:prefLabel>
<skos:definition>proces zwiększania ilości jakiegoś składnika roztworu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1677">
<skos:prefLabel>beton</skos:prefLabel>
<skos:definition>tworzywo uzyskane z materiału wiążącego, najczęściej cementu, kruszywa i wody, oraz odpowiednich domieszek, w zależności od przeznaczenia, w wyniku ich wiązania i twardnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1679">
<skos:prefLabel>produkcja elementów z betonu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1681">
<skos:prefLabel>kondensacja (proces)</skos:prefLabel>
<skos:definition>skraplanie, przejście substancji ze stanu pary w stan ciekły, wskutek oziębienia lub zwiększenia ciśnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1686">
<skos:prefLabel>przewodnictwo</skos:prefLabel>
<skos:definition>zjawisko przewodzenia ciepła lub prądu elektrycznego właściwe niektórym ciałom, substancjom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1688">
<skos:prefLabel>kolizja interesów w użytkowaniu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1689">
<skos:prefLabel>konflikt celów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1690">
<skos:prefLabel>konflikt interesów</skos:prefLabel>
<skos:definition>sprzeczność pomiędzy interesem publicznym, a interesem prywatnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1691">
<skos:prefLabel>kongres</skos:prefLabel>
<skos:definition>zjazd krajowy lub międzynarodowy przedstawicieli nauki, sztuki, polityki, dyplomacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1692">
<skos:prefLabel>rośliny iglaste</skos:prefLabel>
<skos:definition>Coniferae, klasa roślin nagozalążkowych, do której należy sześć rodzin obejmujących około pięciuset gatunków drzew i krzewów, o kilkuletnich najczęściej liściach w postaci igieł lub łusek; rosną głównie w umiarkowanej i zimnej strefie półkuli północnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1694">
<skos:prefLabel>las iglasty</skos:prefLabel>
<skos:definition>las złożony z drzew iglastych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1700">
<skos:prefLabel>ochrona</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1704">
<skos:prefLabel>ochrona wód słodkich</skos:prefLabel>
<skos:definition>kontrolowane wykorzystanie i ochrona zasobów wód słodkich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1706">
<skos:prefLabel>ochrona zasobów genowych</skos:prefLabel>
<skos:definition>kontrolowane wykorzystanie i ochrona zasobów puli genowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1708">
<skos:prefLabel>ochrona zabytków</skos:prefLabel>
<skos:definition>działania podejmowane dla ochrony zabytków historycznych i pomników sztuki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1712">
<skos:prefLabel>prawo konstytucyjne</skos:prefLabel>
<skos:definition>prawo dotyczące konstytucji jako ustawy określającej ustrój społeczny i polityczny państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1718">
<skos:prefLabel>sprzęt budowlany</skos:prefLabel>
<skos:definition>przedmioty używane w budownictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1724">
<skos:prefLabel>hałas pochodzący od obiektów budowlano-konstrukcyjnych</skos:prefLabel>
<skos:definition>hałas związany z przygotowaniem i przeprowadzeniem budowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1725">
<skos:prefLabel>budowa obiektów</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1728">
<skos:prefLabel>technologia budowlana</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1730">
<skos:prefLabel>konstrukcja z materiałów pochodzących z recyklingu</skos:prefLabel>
<skos:definition>konstrukcja wykorzystująca materiał odpadowy jako budulec</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1731">
<skos:prefLabel>prace budowlano-konstrukcyjne</skos:prefLabel>
<skos:definition>budowa, modyfikacja, konserwacja lub wyburzanie budynków i budowli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1732">
<skos:prefLabel>konsultacja</skos:prefLabel>
<skos:definition>zasięganie opinii u fachowców i specjalistów; udzielanie rad, wskazówek i wyjaśnień przez rzeczoznawców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1736">
<skos:prefLabel>zachowania konsumenckie</skos:prefLabel>
<skos:definition>przewidywalne zachowania związane z nabywaniem dóbr i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1737">
<skos:prefLabel>towary konsumpcyjne</skos:prefLabel>
<skos:definition>wytwór pracy ludzkiej zaspokajający jakąś potrzebę człowieka, produkowany na sprzedaż, mający wartość rzeczową i użytkową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1738">
<skos:prefLabel>grupa konsumencka</skos:prefLabel>
<skos:definition>grupa osób o wspólnym interesie dotyczącym praw nabywcy i uzytkowanika określonych towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1739">
<skos:prefLabel>informacje dla konsumenta</skos:prefLabel>
<skos:definition>informacja o towarach i usługach i ich jakości oraz ich producentach i dostawcach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1740">
<skos:prefLabel>ochrona konsumenta</skos:prefLabel>
<skos:definition>działania podejmowane by zapobiec lub ograniczyć szkody powstałe na skutek korzystania z towarów lub usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1742">
<skos:prefLabel>odpady bytowe</skos:prefLabel>
<skos:definition>odpady powstała na skutek konsumpcji dóbr lub usług, a nie ich produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1743">
<skos:prefLabel>konsumpcja</skos:prefLabel>
<skos:definition>wydatki na przetrwanie lub rozrywkę, a nie produkcję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1745">
<skos:prefLabel>wzorce konsumpcji</skos:prefLabel>
<skos:definition>typowy dla danego społeczeństwa sposób wykorzystania zasobów dla przetrwania, wygody i rozrywki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1746">
<skos:prefLabel>kontener statkowy</skos:prefLabel>
<skos:definition>duży pojemnik do przenoszenia materiałów lub przedmiotów, który może być transportowany przez samochód ciężarowy i z łatwością przepakowany na statek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1748">
<skos:prefLabel>hermetyzacja (przemysł atomowy)</skos:prefLabel>
<skos:definition>uszczelnienie reaktora jądrowego zabezpieczające przed przenikaniem pierwiastków radioaktywnych do środowiska oraz przed atakiem terrorystycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1751">
<skos:prefLabel>gleba zanieczyszczona</skos:prefLabel>
<skos:definition>gleba, w której na skutek jej wcześniejszego lub obecnego użytkowania znajdują się substancje szkodliwe dla ludzi lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1752">
<skos:prefLabel>zanieczyszczenie</skos:prefLabel>
<skos:definition>zanieczyszczenie środowiska (wody, powietrza, gleby i in.) mikroorganizmami, chemikaliami, substancjami toksycznymi, odpadami, ściekami i in. zanieczyszczeniami w stopniu uniemożliwiającym korzystanie ze środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1757">
<skos:prefLabel>szelf kontynentalny</skos:prefLabel>
<skos:definition>płytka, przybrzeżna strefa dna morskiego lub oceanicznego będąca najniższą, płaską częścią zalanego wodą kontynentu, przechodząca wyraźnym załamaniem w stok kontynentalny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1759">
<skos:prefLabel>stałe zanieczyszczenia</skos:prefLabel>
<skos:definition>ilość zanieczyszczenia dopływająca w sposób ciągły</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1761">
<skos:prefLabel>warstwicowa uprawa stoków</skos:prefLabel>
<skos:definition>uprawa stoków wzdłuż linii łączących punkty o tej samej wysokości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1762">
<skos:prefLabel>umowa</skos:prefLabel>
<skos:definition>pisemne lub ustne porozumienie stron, mające na celu ustalenie czegoś, zwłaszcza wzajemnych praw i obowiązków, zgodzenie się na coś</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1763">
<skos:prefLabel>serwis sprzątający</skos:prefLabel>
<skos:definition>firma świadcząca usługi usuwania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1769">
<skos:prefLabel>wypalanie kontrolowane</skos:prefLabel>
<skos:definition>planowe i kontrolowanie wypalanie powierzchni terenu w celu osiągnięcia założonych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1772">
<skos:prefLabel>obwód łowiecki</skos:prefLabel>
<skos:definition>obszar wydzielony umownie przeznaczony na polowania na dziką zwierzynę, ograniczone przepisami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1773">
<skos:prefLabel>pełnomocnictwo do kontrolowania</skos:prefLabel>
<skos:definition>prawo osoby lub organizacji do zarządzania, nadzoru i kontroli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1774">
<skos:prefLabel>środki kontroli</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1775">
<skos:prefLabel>zespół aglomeracji</skos:prefLabel>
<skos:definition>znaczny, gęsto zabudowany obszar, często złożony z kilku ośrodków miejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1776">
<skos:prefLabel>konwencja</skos:prefLabel>
<skos:definition>umowa międzynarodowa dotycząca spraw politycznych, gospodarczych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1777">
<skos:prefLabel>energia konwencjonalna</skos:prefLabel>
<skos:definition>energia pozyskiwana z takich źródeł jak węgiel, drewno, gaz, itp. w przeciwieństwie do energii alternatywnej takiej jak energia słoneczna, energia wiatru itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1781">
<skos:prefLabel>chłodzenie</skos:prefLabel>
<skos:definition>izolowanie materiału promieniotwórczego do czasu zmniejszenia jego promieniotwórczości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1782">
<skos:prefLabel>olej chłodniczy</skos:prefLabel>
<skos:definition>olej używany jako czynnik chłodzący</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1784">
<skos:prefLabel>chłodnia kominowa</skos:prefLabel>
<skos:definition>konstrukcja wspomagająca odprowadzanie ciepła z wód obiegu chłodzącego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1786">
<skos:prefLabel>woda chłodnicza</skos:prefLabel>
<skos:definition>woda stosowana do obniżenia temperatury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1787">
<skos:prefLabel>współpraca</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1788">
<skos:prefLabel>zasady współpracy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1791">
<skos:prefLabel>koordynacja</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1792">
<skos:prefLabel>miedź</skos:prefLabel>
<skos:definition>(Cu) pierwiastek chemiczny, liczba atomowa 29, należący do podgrupy miedziowców; ciężki, ciągliwy i kowalny metal o czerwonawym połysku, dobry przewodnik ciepła i prądu elektrycznego; występuje w przyrodzie najczęściej w związkach, stosowany (w stopach) w elektrotechnice, budownictwie, używany do budowy różnych maszyn i aparatur</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1793">
<skos:prefLabel>las odroślowy</skos:prefLabel>
<skos:definition>niewielki obszar zadrzewiony, gdzie drzewa są wycinane i odrastają od korzeni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1794">
<skos:prefLabel>korale</skos:prefLabel>
<skos:definition>Corallium rubrum, koralowiec występujący głównie w Morzu Śródziemnym; wytwarza czerwony szkielet wapienny, ceniony w zdobnictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1796">
<skos:prefLabel>rafa koralowa</skos:prefLabel>
<skos:definition>rafy utworzone przez szkielety koralowca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1798">
<skos:prefLabel>strunowce</skos:prefLabel>
<skos:definition>Chordata, typ zwierząt tkankowych, u których przez całe życie lub tylko w okresie zarodkowym występuje struna grzbietowa; należą tu bezczaszkowce, osłonice i kręgowce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1799">
<skos:prefLabel>stopienie się rdzenia</skos:prefLabel>
<skos:definition>awaryjne przegrzanie części reaktora jądrowego, w której następuje rozpad atomowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1800">
<skos:prefLabel>korek</skos:prefLabel>
<skos:definition>tkanka wtórna okrywająca starsze łodygi (pnie), korzenie i bulwy, wytwarzana przez miazgę korkotwórczą (fellogen)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1807">
<skos:prefLabel>korytarz</skos:prefLabel>
<skos:definition>fizyczne połączenie pomiędzy dwoma różnymi siedliskami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1808">
<skos:prefLabel>korozja</skos:prefLabel>
<skos:definition>proces zachodzący na powierzchni metali i ich stopów, a także tworzyw niemetalowych polegający na niszczącym oddziaływaniu chemicznego lub elektrochemicznego środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1809">
<skos:prefLabel>inhibitor korozji</skos:prefLabel>
<skos:definition>czynnik chemiczny, który spowalnia lub zapobiega reakcji korozji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1813">
<skos:prefLabel>przemysł kosmetyczny</skos:prefLabel>
<skos:definition>przemysł produkujący środki do pielęgnacji ciała</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1814">
<skos:prefLabel>promieniowanie kosmiczne</skos:prefLabel>
<skos:definition>promieniowanie dochodzące do ziemi z kosmosu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1817">
<skos:prefLabel>koszt-zysk</skos:prefLabel>
<skos:definition>zależność pomiędzy kosztem działania a wynikłymi z niego korzyściami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1818">
<skos:prefLabel>analiza kosztów i zysków</skos:prefLabel>
<skos:definition>działania podejmowane dla określenia, porównania i uzasadnienie kosztów działalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1820">
<skos:prefLabel>wzrost kosztów</skos:prefLabel>
<skos:definition>wzrost ilości pieniędzy potrzebnych do wygenerowania dóbr i usług lub żądanych za dobra i usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1822">
<skos:prefLabel>opłata na zasadzie zwrotu kosztów</skos:prefLabel>
<skos:definition>zwrot poniesionych kosztów lub nakładów na świadczenie usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1823">
<skos:prefLabel>obniżenie kosztów</skos:prefLabel>
<skos:definition>spadek ilości pieniędzy potrzebnych do wygenerowania dóbr i usług lub żądanych za dobra i usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1824">
<skos:prefLabel>koszty</skos:prefLabel>
<skos:definition>nakłady poniesione na produkcję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1825">
<skos:prefLabel>koszty zanieczyszczenia</skos:prefLabel>
<skos:definition>koszty powstałe na skutek zanieczyszczenia środowiska przez działania człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1826">
<skos:prefLabel>narodowe koszty gospodarki</skos:prefLabel>
<skos:definition>koszty zarządzania narodowymi zasobami finansowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1827">
<skos:prefLabel>bawełna</skos:prefLabel>
<skos:definition>Gossypium, krzew lub drzewo z rodziny ślazowatych, o torebkach nasiennych wysłanych włóknami, które stanowią jeden z głównych surowców włókienniczych, uprawiane w krajach podzwrotnikowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1837">
<skos:prefLabel>powiat</skos:prefLabel>
<skos:definition>teren większy niż miasto, którego granice wyznaczono administracyjnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1842">
<skos:prefLabel>sąd</skos:prefLabel>
<skos:definition>państwowy organ wymiaru sprawiedliwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1846">
<skos:prefLabel>przykrycie</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1850">
<skos:prefLabel>rzemiosło</skos:prefLabel>
<skos:definition>drobna wytwórczość o charakterze przemysłowym, obejmująca wykonywanie i naprawianie przedmiotów codziennego użytku prostymi narzędziami i maszynami; dziedzina sztuk plastycznych obejmująca ręczną wytwórczość artystycznych przedmiotów użytkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1854">
<skos:prefLabel>pomoc kredytowa</skos:prefLabel>
<skos:definition>pomoc w formie korzystnego kredytu udzielana przez banki i inne instytucje finansowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1860">
<skos:prefLabel>przestępczość</skos:prefLabel>
<skos:definition>naruszenie prawa, podlegające sankcjom karnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1862">
<skos:prefLabel>prawo karne</skos:prefLabel>
<skos:definition>przepisy zawierające zakazy i nakazy, których naruszenie lub niespełnienie zagrożone jest karą; ogół norm określających postępowanie karne organów państwowych wobec obywateli naruszających te przepisy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1864">
<skos:prefLabel>poziom krytyczny</skos:prefLabel>
<skos:definition>stężenie powyżej którego występuje negatywne oddziaływanie na organizmy żywe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1865">
<skos:prefLabel>obciążenie krytyczne</skos:prefLabel>
<skos:definition>maksymalny ładunek, który dany system zdolny jest przyjąć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1870">
<skos:prefLabel>krokodyl</skos:prefLabel>
<skos:definition>Crocodylus, gad drapieżny z rzędu o tej samej nazwie występujący w około dwudziestu gatunkach; odznacza się długim ciałem pokrytym tarczkami rogowymi i długim pyskiem; żyje głównie w słodkich wodach krajów tropikalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1874">
<skos:prefLabel>ochrona zasiewów</skos:prefLabel>
<skos:definition>działania podejmowane dla ochrony roślin uprawnych przed szkodnikami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1875">
<skos:prefLabel>płodozmian</skos:prefLabel>
<skos:definition>system gospodarki rolnej racjonalny dla danego gospodarstwa, ustalający na szereg lat stałą rotację upraw kolejno następujących po sobie na określonym obszarze, podzielonym na pola</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1877">
<skos:prefLabel>odpady powstające w czasie żniw</skos:prefLabel>
<skos:definition>odpady pozostałe po zbiorach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1880">
<skos:prefLabel>przejście drogowe</skos:prefLabel>
<skos:definition>przejście na skrzyżowaniu dróg często oznakowane, ze światłami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1881">
<skos:prefLabel>przejście dla zwierząt</skos:prefLabel>
<skos:definition>kładki i tunele ułatwiające zwierzętom przekroczenie obiektów liniowych (dróg, kolei)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1885">
<skos:prefLabel>ropa naftowa</skos:prefLabel>
<skos:definition>ciekły surowiec kopalny barwy brunatnej, będący mieszaniną węglowodorów; przez destylację otrzymuje się z niego benzynę, wazelinę, parafinę, wosk, naftę itp.; w stanie naturalnym ma zastosowanie jako paliwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1888">
<skos:prefLabel>żegluga turystyczna</skos:prefLabel>
<skos:definition>morska żegluga turystyczna uprawiana dla przyjemności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1889">
<skos:prefLabel>skorupiaki</skos:prefLabel>
<skos:definition>Crustacea, gromada stawonogów skrzelodysznych, przeważnie wodnych, w większości rozdzielnopłciowych, obejmująca około 20 tys. gatunków, o ciele różnej długości (od ułamka milimetra do 80 cm), zbudowanym z segmentów, okrytym twardym oskórkiem, podzielonym na głowotułów i odwłok; skorupiaki kopalne należą do najdawniejszych znanych skamielin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1890">
<skos:prefLabel>rośliny zarodnikowe</skos:prefLabel>
<skos:definition>Cryptogamae, rośliny nie wytwarzające kwiatów, rozmnażające się głównie przez zarodniki, np. bakterie, glony, grzyby, porosty, mszaki, paprotniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1891">
<skos:prefLabel>krystalizacja</skos:prefLabel>
<skos:definition>proces wydzielania się substancji w postaci kryształów z przesyconego roztworu, spowodowany np. obniżeniem temperatury, odparowaniem rozpuszczalnika lub przemianami chemicznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1892">
<skos:prefLabel>krystalografia</skos:prefLabel>
<skos:definition>nauka o wewnętrznej i zewnętrznej budowie oraz o fizycznych i fizykochemicznych właściwościach ciał krystalicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1898">
<skos:prefLabel>rośliny uprawne</skos:prefLabel>
<skos:definition>rośliny uprawiane w celach użytkowych np. pszenica, koniczyna itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1899">
<skos:prefLabel>uprawa</skos:prefLabel>
<skos:definition>cykl zabiegów od zasiewu lub zasadzenia rośliny do momentu zbioru; planowa opieka nad wzrostem i rozwojem roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1902">
<skos:prefLabel>metody uprawy</skos:prefLabel>
<skos:definition>sposób przygotowania ziemi do plonowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1909">
<skos:prefLabel>rozwój kulturalny</skos:prefLabel>
<skos:definition>proces wspomagania rozwoju wierzeń, zwyczajów i działalności artystycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1911">
<skos:prefLabel>instytucja kulturalna</skos:prefLabel>
<skos:definition>zakład o charakterze publicznym zajmujący się określonym zakresem spraw w dziedzinie kultury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1912">
<skos:prefLabel>dziedzictwo kulturowe</skos:prefLabel>
<skos:definition>całokształt materialnego i duchowego dorobku ludzkości przekazywany z pokolenia na pokolenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1915">
<skos:prefLabel>wskaźniki kulturowe</skos:prefLabel>
<skos:definition>wskaźniki dostarczające informacji o społeczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1917">
<skos:prefLabel>polityka kulturalna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1921">
<skos:prefLabel>kultura</skos:prefLabel>
<skos:definition>całokształt materialnego i duchowego dorobku ludzkości gromadzony, utrwalany i wzbogacany w ciągu jej dziejów, przekazywany z pokolenia na pokolenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1926">
<skos:prefLabel>program nauczania</skos:prefLabel>
<skos:definition>zakres wiedzy wykładanej w różnego rodzaju szkołach, uczelniach, w ramach edukacji dorosłych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1928">
<skos:prefLabel>obyczaj i praktyka</skos:prefLabel>
<skos:definition>zwyczajowe działania, przekazywane w formie tradycji lub prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1929">
<skos:prefLabel>cło</skos:prefLabel>
<skos:definition>opłata pobierana przez państwo od przywozu i wywozu towarów przez jego granice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1930">
<skos:prefLabel>wyrąb (lasu)</skos:prefLabel>
<skos:definition>wycinanie drzew na danym terenie celem pozyskania drewna jako surowca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1931">
<skos:prefLabel>cyjanian</skos:prefLabel>
<skos:definition>sól kwasu cyjanowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1933">
<skos:prefLabel>cyjanek</skos:prefLabel>
<skos:definition>sól kwasu cyjanowodorowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1941">
<skos:prefLabel>cyklon</skos:prefLabel>
<skos:definition>potężny, szybko się poruszający wir powietrzny, w którym ciśnienie maleje ku środkowi, powstający zwykle na głównych frontach atmosferycznych oddzielających masy powietrza ciepłego od mas powietrza chłodnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1945">
<skos:prefLabel>cytologia</skos:prefLabel>
<skos:definition>nauka o budowie i czynnościach komórek roślinnych i zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1946">
<skos:prefLabel>cytotoksyczność</skos:prefLabel>
<skos:definition>zdolność substancji do niszczenia określonego rodzaju komórek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1950">
<skos:prefLabel>ferma mleczna</skos:prefLabel>
<skos:definition>gospodarstwo o charakterze specjalistycznym, zajmujące się zwierząt mlecznych i produkcją wyrobów mlecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1951">
<skos:prefLabel>przemysł mleczarski</skos:prefLabel>
<skos:definition>przemysł przetwórstwa mleka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1952">
<skos:prefLabel>produkt mleczny</skos:prefLabel>
<skos:definition>produkty wytworzone z mleka takie jak masło, ser, laktoza itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1955">
<skos:prefLabel>zapora</skos:prefLabel>
<skos:definition>budowla, zwykle betonowa lub żelbetowa, przegradzająca dolinę rzeki w celu spiętrzenia wody i utworzenia zbiornika wodnego dla potrzeb energetycznych, ochrony przeciwpowodziowej itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1956">
<skos:prefLabel>szkoda</skos:prefLabel>
<skos:definition>strata materialna lub moralna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1958">
<skos:prefLabel>szkoda spowodowana przez działania militarne</skos:prefLabel>
<skos:definition>szkoda spowodowana przez celowe działania militarne mające charakter ćwiczeń wojskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1959">
<skos:prefLabel>zapobieganie szkodom</skos:prefLabel>
<skos:definition>sposoby i działania zapobiegające powstawaniu szkód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1968">
<skos:prefLabel>eksport niebezpiecznych produktów chemicznych</skos:prefLabel>
<skos:definition>międzynarodowy przewóz towarów niebezpiecznych w handlu zagranicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1969">
<skos:prefLabel>wyroby niebezpieczne</skos:prefLabel>
<skos:definition>wyroby i produkty, które są niebezpieczne w trakcie ich użytkowania, transportu itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1970">
<skos:prefLabel>przepisy regulujące wykorzystanie produktów niebezpiecznych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1971">
<skos:prefLabel>przepisy regulujące obrót towarami niebezpiecznymi</skos:prefLabel>
<skos:definition>przepisy dotyczące obrotu towarami, które stanowią zagrożenie dla zdrowia, bezpieczeństwa i mienia i których transport wymaga szczególnych zabezpieczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1974">
<skos:prefLabel>zbieranie danych</skos:prefLabel>
<skos:definition>czynność gromadzenia dowolnych informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1975">
<skos:prefLabel>analiza danych</skos:prefLabel>
<skos:definition>analiza danych cyfrowych np. kolejności znaków kodowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1979">
<skos:prefLabel>baza danych</skos:prefLabel>
<skos:definition>zasób danych zorganizowanych elektronicznie w celu dogodnego dostępu do nich, zarządzania i aktualizowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1981">
<skos:prefLabel>nośnik danych</skos:prefLabel>
<skos:definition>nośnik, na którym można zarejestrować dane i łatwo przenosić np. dyskietka, CD, taśma, papier</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1984">
<skos:prefLabel>wymiana danych</skos:prefLabel>
<skos:definition>przekazywanie dowolnych danych pomiędzy dwoma lub większą ilością uczestników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1986">
<skos:prefLabel>przetwarzanie danych</skos:prefLabel>
<skos:definition>obróbka danych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1988">
<skos:prefLabel>zabezpieczenie danych</skos:prefLabel>
<skos:definition>sposoby, procedury i urządzenia służące do zapewnienia bezpieczeństwa informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1989">
<skos:prefLabel>technika zapisu danych</skos:prefLabel>
<skos:definition>specjalistyczne techniki, procedury i metody służące do rejestracji informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1992">
<skos:prefLabel>datowanie</skos:prefLabel>
<skos:definition>technika określania wieku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1993">
<skos:prefLabel>produkt rozpadu promieniotwórczego</skos:prefLabel>
<skos:definition>izotop powstały na skutek rozpadu innego izotopu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/1996">
<skos:prefLabel>DDT</skos:prefLabel>
<skos:definition>trwały pestycyd; bezbarwna substancja krystaliczna, rozpuszczalna w rozpuszczalnikach organicznych; za odkrycie owadobójczej roli DDT, P.H. Müller otrzymał Nagrodę Nobla w dziedzinie fizjologii i medycyny w 1948 r.;
DDT zwalcza przenosicieli zimnicy(malarii) i duru plamistego; wycofany z użycia w krajach uprzemysłowionych; jego masowe stosowanie w latach powojennych spowodowało katastrofalne wysycenie środowiska przyrodniczego i zatrucie całych łańcuchów pokarmowych;
od początku lat siedemdziesiątych notuje się spadek zawartosci DDT w organizmie człowieka, np. w USA, Kanadzie i Norwegii; w krajach Ameryki Łacińskiej, Afryce i tropikalnej Azji w dalszym ciągu używany przeciw komarom roznoszącym malarię; podejrzewany o ujemny wpływ na równowagę hormonalną i wywoływanie chorób nowotworowych u człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2000">
<skos:prefLabel>dług</skos:prefLabel>
<skos:definition>zaciągnięta pożyczka zwykle pieniężna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2001">
<skos:prefLabel>obsługa długów</skos:prefLabel>
<skos:definition>kwoty potrzebne na pokrycie spłat zaciągniętych wcześniej zobowiązań finansowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2002">
<skos:prefLabel>dekantacja</skos:prefLabel>
<skos:definition>oddzielanie cieczy od osadu polegające na ostrożnym usunięciu jej (np. przez zlanie) znad osadu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2003">
<skos:prefLabel>decentralizacja</skos:prefLabel>
<skos:definition>przekazywanie prawa podejmowania decyzji podległym organom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2004">
<skos:prefLabel>las liściasty</skos:prefLabel>
<skos:definition>las złożony z drzew liściastych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2005">
<skos:prefLabel>drzewo liściaste</skos:prefLabel>
<skos:definition>drzewo zrzucające liście jesienią</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2006">
<skos:prefLabel>decyzja</skos:prefLabel>
<skos:definition>sposób sprawowania władzy przez administrację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2009">
<skos:prefLabel>proces decyzyjny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2014">
<skos:prefLabel>rozkładanie (się)</skos:prefLabel>
<skos:definition>podział, rozpad cząsteczki na mniejsze fragmenty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2015">
<skos:prefLabel>odkażanie</skos:prefLabel>
<skos:definition>usuwanie skażeń chemicznych, biologicznych i radiologicznych lub neutralizacja skażeń dotycząca osób, obiektów i powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2020">
<skos:prefLabel>rozporządzenie</skos:prefLabel>
<skos:definition>akt prawodawczy wydany przez naczelny organ administracji państwowej (Radę Ministrów lub ministra) na podstawie upoważnienia zawartego w ustawie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2022">
<skos:prefLabel>otwarte morze</skos:prefLabel>
<skos:definition>przestrzeń mórz i oceanów poza obrębem wód terytorialnych, otwarta dla żeglugi wszystkich narodów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2023">
<skos:prefLabel>złoże głębinowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2024">
<skos:prefLabel>zrzut odpadów poza strefą brzegową</skos:prefLabel>
<skos:definition>wyrzucanie odpadów na otwartym morzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2025">
<skos:prefLabel>rybołówstwo dalekomorskie</skos:prefLabel>
<skos:definition>łowienie ryb na otwartym morzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2029">
<skos:prefLabel>jeleń</skos:prefLabel>
<skos:definition>Cervus elaphus, ssak łowny z rodziny jeleniowatych, o ubarwieniu przeważnie brązowopłowym i o rozgałęzionym porożu u samca; występuje w wielu podgatunkach w lasach Eurazji i północnej Afryki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2031">
<skos:prefLabel>obrona</skos:prefLabel>
<skos:definition>rodzaj działań bojowych mających na celu zatrzymanie lub odparcie natarcia sił nieprzyjaciela</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2035">
<skos:prefLabel>defoliacja</skos:prefLabel>
<skos:definition>wywołanie opadania liści roślin przez ich opylanie lub opryskiwanie, stosowane w rolnictwie w celu przyspieszenia dojrzewania roślin i ułatwienia mechanicznego sprzętu plonów (np. bawełny)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2036">
<skos:prefLabel>wylesianie</skos:prefLabel>
<skos:definition>zmiana terenów leśnych w uprawne lub zabudowane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2039">
<skos:prefLabel>podatność na degradację</skos:prefLabel>
<skos:definition>podatność na rozkład chemiczny lub biologiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2040">
<skos:prefLabel>degradacja</skos:prefLabel>
<skos:definition>proces rozpadu związku chemicznego w wyniku reakcji hydrolizy, utleniania, redukcji i innych na substancje prostsze, łatwo dające się zidentyfikować</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2041">
<skos:prefLabel>degradacja zasobów naturalnych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2042">
<skos:prefLabel>produkty degradacji</skos:prefLabel>
<skos:definition>produkty chemiczne powstałe na skutek rozpadu substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2044">
<skos:prefLabel>odtłuszczanie</skos:prefLabel>
<skos:definition>usuwanie tłuszczu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2048">
<skos:prefLabel>odbarwianie makulatury</skos:prefLabel>
<skos:definition>proces usuwania farby drukarskiej przy przetwarzaniu makulatury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2050">
<skos:prefLabel>przestępstwo</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2052">
<skos:prefLabel>delta</skos:prefLabel>
<skos:definition>obszar u ujścia rzeki do morza lub jeziora utworzony z osadów przez nią naniesionych, pocięty jej rozgałęzieniami, mający zwykle kształt trójkąta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2053">
<skos:prefLabel>zapotrzebowanie</skos:prefLabel>
<skos:definition>potrzeba, zdolność i chęć nabycia dobra lub usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2054">
<skos:prefLabel>demokracja</skos:prefLabel>
<skos:definition>forma ustroju państwa, w którym uznaje się wolę większości obywateli jako źródło władzy i przyznaje się wszystkim obywatelom swobody i prawa polityczne zapewniające im udział w sprawowaniu władzy, bezpośredni lub pośredni za pośrednictwem przedstawicieli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2055">
<skos:prefLabel>wzrost demograficzny</skos:prefLabel>
<skos:definition>wzrost liczby urodzeń w obrębie danej populacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2056">
<skos:prefLabel>ewolucja demograficzna</skos:prefLabel>
<skos:definition>stopniowa zmiana tempa przyrostu populacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2060">
<skos:prefLabel>demografia</skos:prefLabel>
<skos:definition>nauka o ludności zajmująca się jakościowym i ilościowym badaniem (na podstawie danych statystycznych) stanów, struktur, ruchów i rozwoju ludności w określonych warunkach historycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2062">
<skos:prefLabel>wyburzanie</skos:prefLabel>
<skos:definition>wyburzanie ruder, starych dzielnic w celu wybudowania nowych domów, poszerzenia ulicy itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2064">
<skos:prefLabel>odpady z rozbiórki</skos:prefLabel>
<skos:definition>gruz i inne odpady powstałe przy wyburzaniu budowli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2066">
<skos:prefLabel>prawo do demonstracji</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2069">
<skos:prefLabel>dendrochronologia</skos:prefLabel>
<skos:definition>sztuka określania wieku drzewa na podstawie pierścieni rocznych przyrostów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2070">
<skos:prefLabel>dendrometria</skos:prefLabel>
<skos:definition>nauka zajmująca się oznaczaniem miąższości, przyrostu i wieku drzew i drzewostanów oraz metodami pomiaru drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2071">
<skos:prefLabel>denitryfikacja</skos:prefLabel>
<skos:definition>proces rozkładu związków azotu połączony z wydzielaniem się wolnego azotu, wywołany przez bakterie glebowe i wodne przy braku dostępu tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2072">
<skos:prefLabel>ograniczanie emisji azotu</skos:prefLabel>
<skos:definition>różnorakie metody ograniczania emisji azotu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2083">
<skos:prefLabel>opad cząstek pyłu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2084">
<skos:prefLabel>odkładanie (się)</skos:prefLabel>
<skos:definition>proces opadania zanieczyszczeń z atmosfery i kumulowania się w ekosystemach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2088">
<skos:prefLabel>deregulacja</skos:prefLabel>
<skos:definition>ograniczenie ingerencji rządowej w działalność gospodarczą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2093">
<skos:prefLabel>skórki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2098">
<skos:prefLabel>odsalanie</skos:prefLabel>
<skos:definition>usuwanie soli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2099">
<skos:prefLabel>instalacje do odsalania</skos:prefLabel>
<skos:definition>zakłady odsalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2103">
<skos:prefLabel>pustynia</skos:prefLabel>
<skos:definition>obszar skrajnie suchy, stale lub okresowo gorący, o ujemnym bilansie wodnym przy parowaniu potencjalnym przewyższającym wielokrotnie opady</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2105">
<skos:prefLabel>pustynnienie</skos:prefLabel>
<skos:definition>powstawanie warunków pustynnych na skutek działania człowieka lub zmian klimatycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2109">
<skos:prefLabel>walka z pustynnieniem</skos:prefLabel>
<skos:definition>działania zapobiegające pustynnieniu spowodowanemu działaniami człowieka lub zmianami klimatycznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2111">
<skos:prefLabel>szarańcza pustynna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2115">
<skos:prefLabel>projekt</skos:prefLabel>
<skos:definition>plan, szkic czegoś, np. budowli, konstrukcji, przedsięwzięcia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2117">
<skos:prefLabel>pracownia</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2119">
<skos:prefLabel>desorpcja</skos:prefLabel>
<skos:definition>uwalnianie substancji zaadsorbowanej z powierzchni adsorbentu lub wydzielanie gazu rozpuszczonego w cieczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2124">
<skos:prefLabel>odsiarczanie</skos:prefLabel>
<skos:definition>usuwanie domieszek siarki z gazów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2125">
<skos:prefLabel>odsiarczanie paliw</skos:prefLabel>
<skos:definition>usuwanie siarki z paliwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2127">
<skos:prefLabel>wykrywanie</skos:prefLabel>
<skos:definition>działania lub proces zmierzający do dostarczenia dowodów łamania prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2129">
<skos:prefLabel>wykrywacze</skos:prefLabel>
<skos:definition>urządzenia identyfikujące i rejestrujące wystąpienie zjawiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2131">
<skos:prefLabel>środek piorący</skos:prefLabel>
<skos:definition>substancja powierzchniowo czynna o właściwościach piorących, myjących i czyszczących, działająca w postaci rozpuszczonej, stosowana do usuwania brudu z powierzchni ciał stałych, np. tkanin, szkła, ceramiki, metali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2139">
<skos:prefLabel>metoda oceny</skos:prefLabel>
<skos:definition>metoda do stwierdzenia wystąpienia zjawiska lub oszacowania jego ilości lub jakości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2140">
<skos:prefLabel>środek odstraszający</skos:prefLabel>
<skos:definition>sposób zniechęcenia od podejmowania jakiegoś działania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2143">
<skos:prefLabel>odtruwanie</skos:prefLabel>
<skos:definition>proces usuwania trucizny lub własności toksycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2147">
<skos:prefLabel>kraj rozwinięty</skos:prefLabel>
<skos:definition>kraj o stosunkowo wysokim poziomie życia mieszkańców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2151">
<skos:prefLabel>zadłużenie krajów rozwijających się</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2152">
<skos:prefLabel>kraj rozwijający się</skos:prefLabel>
<skos:definition>kraj o stosunkowo niskim poziomie życia mieszkańców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2155">
<skos:prefLabel>pomoc na rzecz rozwoju</skos:prefLabel>
<skos:definition>pomoc gospodarcza dla krajów rozwijających się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2156">
<skos:prefLabel>obszar aktywizacji gospodarczej</skos:prefLabel>
<skos:definition>obszar, w ktorym za pomocą specjalnych środków rząd promuje inwestycje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2157">
<skos:prefLabel>współpraca na rzecz rozwoju</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2158">
<skos:prefLabel>model rozwoju</skos:prefLabel>
<skos:definition>charakterystyka procesu rozwoju w regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2159">
<skos:prefLabel>wzorce rozwoju</skos:prefLabel>
<skos:definition>charakterystyka przemian gospodarczych i społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2160">
<skos:prefLabel>planowanie rozwoju</skos:prefLabel>
<skos:definition>określenie kierunku działań promujących rozwój gospodarczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2161">
<skos:prefLabel>plan rozwoju</skos:prefLabel>
<skos:definition>formalne określenie lokalnej polityki planistycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2167">
<skos:prefLabel>dializa</skos:prefLabel>
<skos:definition>rozdzielanie substancji polegające na różnej zdolności przenikania przez odpowiednie przegrody półprzepuszczalne; metoda oczyszczania roztworów koloidowych z elektrolitów (kwasów, zasad, soli)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2168">
<skos:prefLabel>okrzemki</skos:prefLabel>
<skos:definition>Diatomeae, gromada jednokomórkowych glonów mikroskopijnej wielkości okrytych dwuczęściową krzemionkową skorupką o charakterystycznej ornamentacji; obejmuje około 10 tysięcy gatunków żyjących w wodach słodkich, słonych jak również na wilgotnych glebach i skałach, na korze drzew itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2173">
<skos:prefLabel>słownik</skos:prefLabel>
<skos:definition>zbiór wyrazów ułożonych i opracowanych według pewnej zasady, zwykle alfabetycznie, najczęściej objaśnianych pod względem znaczeniowym i ilustrowanych przykładami użycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2175">
<skos:prefLabel>dydaktyka</skos:prefLabel>
<skos:definition>dział pedagogiki: nauka o metodach nauczania i uczenia się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2177">
<skos:prefLabel>silnik dieslowski</skos:prefLabel>
<skos:definition>silnik spalinowy tłokowy o zapłonie samoczynnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2179">
<skos:prefLabel>paliwo do silników dielsowskich</skos:prefLabel>
<skos:definition>cięższa frakcja ropy naftowej stosowana jako paliwo do niektórych silników diesla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2182">
<skos:prefLabel>zróżnicowanie</skos:prefLabel>
<skos:definition>rozwój komórek pozwalający na pełnienie przez nie różnych funkcji w organizmie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2185">
<skos:prefLabel>zródło powierzchniowe</skos:prefLabel>
<skos:definition>zanieczyszczenie nie pochodzące ze źródła punktowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2186">
<skos:prefLabel>rozprzestrzenianie (się)</skos:prefLabel>
<skos:definition>przemieszczanie się atomów i cząsteczek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2187">
<skos:prefLabel>osady ściekowe przefermentowane</skos:prefLabel>
<skos:definition>osady poddane obróbce beztlenowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2188">
<skos:prefLabel>komora fermentacyjna</skos:prefLabel>
<skos:definition>urządzenie służące do produkcji metanu z odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2190">
<skos:prefLabel>fermentacja ścieków</skos:prefLabel>
<skos:definition>ograniczenie objętości substancji organicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2195">
<skos:prefLabel>numeryczny model terenu</skos:prefLabel>
<skos:definition>informacja topograficzna w formie numerycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2196">
<skos:prefLabel>digitalizacja</skos:prefLabel>
<skos:definition>konwersja danych z postaci analogowej na numeryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2200">
<skos:prefLabel>kwas rozcieńczony</skos:prefLabel>
<skos:definition>kwas o mniejszym stężeniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2207">
<skos:prefLabel>dioksyna</skos:prefLabel>
<skos:definition>produkt uboczny, m.in. spalania organicznych związków chloru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2211">
<skos:prefLabel>bezpośrednie źródło ścieków</skos:prefLabel>
<skos:definition>zakłady przemysłowe zrzucające ścieki bezpośrednio do odbiornika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2212">
<skos:prefLabel>dyrektywa</skos:prefLabel>
<skos:definition>rozporządzenie, akt administracyjny wyższej rangi wydany przez radę lub komisję Unii Europejskiej rządom krajów członkowskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2213">
<skos:prefLabel>osoba niepełnosprawna</skos:prefLabel>
<skos:definition>osoba pozbawiona części władz fizycznych, np.zdolności chodzenia lub koordynacji ruchów, w wyniku choroby, wypadku lub upośledzenia umysłowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2215">
<skos:prefLabel>katastrofa</skos:prefLabel>
<skos:definition>wydarzenie nagłe, tragiczne w skutkach, w którym ktoś ucierpiał lub poniósł śmierć, które spowodowało straty materialne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2216">
<skos:prefLabel>działania porządkowe po klęskach żywiołowych</skos:prefLabel>
<skos:definition>postępowanie ukierunkowane na uporządkowanie lub usunięcie skutków katastrofy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2218">
<skos:prefLabel>plan działania na wypadek klęski żywiołowej</skos:prefLabel>
<skos:definition>awaryjny plan działania w przewidywaniu wystąpienia klęski żywiołowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2219">
<skos:prefLabel>służba ochrony przed skutkami klęsk żywiołowych</skos:prefLabel>
<skos:definition>prace lub organizacja ustanowiona w celu analizowania, planowania, rozdziału zadań i koordynacji środków służących ochronie przed klęskami żywiołowymi oraz zwalczaniu ich skutków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2220">
<skos:prefLabel>gotowość do zapobiegania skutkom klęsk żywiołowych</skos:prefLabel>
<skos:definition>zespół środków, które należy przedsięwziąć w obliczu klęsk żywiołowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2221">
<skos:prefLabel>zapobieganie katastrofom</skos:prefLabel>
<skos:definition>zespół środków, które należy przedsięwziąć w celu zapobiegania skutkom klęsk żywiołowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2222">
<skos:prefLabel>pomoc dla ofiar katastrof</skos:prefLabel>
<skos:definition>pomoc w postaci pieniędzy, żywności itp. dostarczona poszkodowanym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2226">
<skos:prefLabel>ustawodawstwo dotyczące odprowadzania ścieków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2227">
<skos:prefLabel>wydajność przepływu</skos:prefLabel>
<skos:definition>tempo przepływu rzeki w określonym przedziale czasowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2232">
<skos:prefLabel>choroba</skos:prefLabel>
<skos:definition>proces patologiczny objawiający się zaburzeniem czynności organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2233">
<skos:prefLabel>przyczyna choroby</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2237">
<skos:prefLabel>środek odkażający</skos:prefLabel>
<skos:definition>czynniki takie jak ciepło, promieniowanie lub środki chemiczne służące do niszczenia lub zahamowania rozwoju mikroorganizmów chorobotwórczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2238">
<skos:prefLabel>odkażanie</skos:prefLabel>
<skos:definition>zespół działań o charakterze fizycznym, chemicznym lub mechanicznym podejmowanych w celu zniszczenia zarazków chorobotwórczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2242">
<skos:prefLabel>nota wysyłkowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2245">
<skos:prefLabel>rozpraszanie</skos:prefLabel>
<skos:definition>stan rozdrobnienia substancji rozproszonej w roztworach koloidowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2246">
<skos:prefLabel>obliczanie dyspersji zanieczyszczeń</skos:prefLabel>
<skos:definition>obliczanie dyspersji zanieczyszczeń oblicza się wykorzystując modele matematyczne, które symulują warunki atmosferyczne i zachowanie się zanieczyszczeń. Na ich podstawie można oszacować stężenie i depozycję zanieczyszczeń co pozwala na ocenę istniejących i hipotetycznych scenariuszy emisyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2247">
<skos:prefLabel>przesiedleńcy</skos:prefLabel>
<skos:definition>osoby, które z różnych powodów lub warunków zostały zmuszone do opuszczenia swoich domów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2253">
<skos:prefLabel>chowanie zmarłych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2254">
<skos:prefLabel>zniszczenie zapasów broni</skos:prefLabel>
<skos:definition>usuwanie materialnych pozostałości po wojnie, które mogą poważnie hamować rozwój i powodować szkody i straty w ludziach oraz straty materialne; usuwanie odpadów wojennych jest problematyczne z powodu niebezpieczeństwa, toksyczności oraz długiego okresu życia, ich utylizacja wymaga specyficznych i skomplikowanych technologii, szczególnie w przypadku min oraz niewypałów pozostawionych na polu działań wojennych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2257">
<skos:prefLabel>rozpuszczanie (się)</skos:prefLabel>
<skos:definition>o ciałach stałych: ulec rozpuszczeniu, rozprowadzeniu w cieczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2259">
<skos:prefLabel>rozpuszczony węgiel organiczny</skos:prefLabel>
<skos:definition>ułamek całkowitego węgla organicznego (wszystkich atomów węgla kowalencyjnie przyłączonych do cząsteczek organicznych) w wodzie, który przechodzi przez filtr o średnicy porów równej 0,45 mikronów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2260">
<skos:prefLabel>destylacja</skos:prefLabel>
<skos:definition>wprowadzanie cieczy w stan pary i ponowne skraplanie jednego lub kilku składników ciekłej mieszaniny w celu ich rozdzielenia lub oczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2262">
<skos:prefLabel>przemysł napojów alkoholowych</skos:prefLabel>
<skos:definition>sektor gospodarki, w którym zespół przedsiębiorstw zajmuje się wytwarzaniem i wprowadzaniem na rynek napojów alkoholowych wytworzonych metodą destylacji, takich jak wódka, rum, whiskey i innych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2263">
<skos:prefLabel>naruszenie zasad konkurencji</skos:prefLabel>
<skos:definition>Art. 85 Umowy Komisji Europejskiej zobowiązuje do zapobiegania i przeciwdziałania ograniczeniom uniemożliwiającym firmom działanie na rynku i sprzedaż ich produktów lub usług; są to zazwyczaj ograniczenia poza celne (taryfa celna jest wspólna dla całego obszaru Unii), de facto odnoszą się wyłącznie do produktów importowanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2264">
<skos:prefLabel>dystrybucja</skos:prefLabel>
<skos:definition>kontekst środowiskowy dotyczy stanu rozproszenia zanieczyszczeń powietrza i zależy od rodzaju źródła zanieczyszczeń, siły i kierunku wiatru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2269">
<skos:prefLabel>rejonowa dostawa ciepła</skos:prefLabel>
<skos:definition>dostarczanie energii cieplnej w postaci gorącej wody lub pary wodnej stosowanej do centralnego ogrzewania budynków w danej dzielnicy lub okręgu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2270">
<skos:prefLabel>ciepłownia rejonowa</skos:prefLabel>
<skos:definition>zakład energetyczny wytwarzający energię cieplną w postaci gorącej wody lub pary wodnej, przeznaczonej do centralnego ogrzewania budynków w danej dzielnicy lub rejonie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2275">
<skos:prefLabel>opuszczona baza wojskowa</skos:prefLabel>
<skos:definition>tereny wojskowe, na których zakończono działalność; takie obszary dokładnie strzeżone w wielu przypadkach są mniej zdewastowane przez ludzi i mogą zawierać cenne naturalne siedliska i rzadkie lub zagrożone gatunki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2276">
<skos:prefLabel>rów</skos:prefLabel>
<skos:definition>długi, wąski wykop wykonany w sposób sztuczny; otwarty i zwykle nieutwardzony ciek wodny, kanał lub rów przeznaczony do transportu wody w celu melioracji lub irygacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2279">
<skos:prefLabel>DNA</skos:prefLabel>
<skos:definition>podstawowy materiał dziedziczności; występuje w chromosomach, składa się z cząsteczek w postaci długich nierozgałęzionych łańcuchów, zbudowanych z wielu nukleotydów; każdy nukleotyd składa się z kwasu fosforowego, monosacharydu - dezoksyrybozy oraz jednej z czterech zasad: tyminy, cytozyny, adeniny i guaniny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2282">
<skos:prefLabel>dokument</skos:prefLabel>
<skos:definition>każdego rodzaju nośnik informacji zawierający zapis wydarzeń, myśli, poglądów, faktów z życia publicznego itp. przy wykorzystaniu dowolnej formy zapisu np. słownej, audio-wizualnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2284">
<skos:prefLabel>dokumentacja</skos:prefLabel>
<skos:definition>proces gromadzenia, opracowania, przetwarzania i rozpowszechniania informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2285">
<skos:prefLabel>ośrodek dokumentacji</skos:prefLabel>
<skos:definition>instytucja obejmująca zasięgiem działania całokształt spraw związanych z gromadzeniem, opracowaniem i rozpowszechnianiem informacji z danej dziedziny wiedzy lub działalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2286">
<skos:prefLabel>rodzaj dokumentu</skos:prefLabel>
<skos:definition>każdy rodzaj nośnika informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2287">
<skos:prefLabel>pies</skos:prefLabel>
<skos:definition>Canis familiaris, zwierzę domowe z rodziny o tej samej nazwie, powszechnie hodowane na świecie w różnych rasach i odmianach (około 300), często tresowane dla specjalnych celów, np. dla tropienia zwierzyny, szukania przestępców, ratowania tonących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2290">
<skos:prefLabel>urządzenia domowe</skos:prefLabel>
<skos:definition>maszyny i urządzenia zwłaszcza elektryczne wykorzystywane w gospodarstwie domowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2291">
<skos:prefLabel>zwierzę udomowione</skos:prefLabel>
<skos:definition>1) dzikie zwierzę przyuczone do życia w pobliżu domu, nie obawiające się ludzi 2) gatunki będące niegdyś dzikie obecnie selektywnie hodowane dla potrzeb ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2293">
<skos:prefLabel>paliwa stosowane w gospodarstwach domowych</skos:prefLabel>
<skos:definition>paliwa otrzymywane z różnych źródeł stosowane do ogrzewania mieszkań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2294">
<skos:prefLabel>paliwa otrzymywane z ropy naftowej stosowane w gospodarstwach domowych</skos:prefLabel>
<skos:definition>paliwa otrzymywane z ropy naftowej wykorzystywane do ogrzewania mieszkań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2298">
<skos:prefLabel>hałas pochodzący z mieszkań</skos:prefLabel>
<skos:definition>hałas pochodzący z urządzeń domowych i aktywności mieszkańców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2300">
<skos:prefLabel>handel wewnętrzny</skos:prefLabel>
<skos:definition>handel prowadzony na obszarze celnym danego państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2301">
<skos:prefLabel>odpady z gospodarstw domowych</skos:prefLabel>
<skos:definition>odpady powstające na zamieszkałych terenach, składające się z materiałów niechcianych i niepotrzebnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2304">
<skos:prefLabel>odpady komunalne</skos:prefLabel>
<skos:definition>miejsce składowania odpadów z gospodarstw domowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2305">
<skos:prefLabel>ścieki komunalne</skos:prefLabel>
<skos:definition>ścieki głównie pochodzące z gospodarstw domowych, budynków handlowych, instytucji itp., mogą także zawierać wody ze spływu powierzchniowego, wody gruntowe oraz z kanalizacji burzowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2307">
<skos:prefLabel>dawkowanie</skos:prefLabel>
<skos:definition>odmierzać dawki; używać, podawać w odpowiednich dawkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2308">
<skos:prefLabel>dawka</skos:prefLabel>
<skos:definition>pewna ilość, miarka czegoś, zwłaszcza porcja leku określona wagowo lub objętościowo do jednorazowego użytku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2309">
<skos:prefLabel>zależność dawka-skutek</skos:prefLabel>
<skos:definition>zależność pomiędzy wielkością dawki a dającym się zmierzyć lub zaoobserwować efektem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2315">
<skos:prefLabel>projekt aktu prawnego</skos:prefLabel>
<skos:definition>projekt aktu prawnego, który nie ma mocy obowiązującej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2316">
<skos:prefLabel>ważka</skos:prefLabel>
<skos:definition>Odonata, rząd dużych drapieżnych owadów obejmujący około 4 tysięcy gatunków, występujących w większości w krajach tropikalnych; w Polsce znanych jest około 70 gatunków (np. świtezianki, łątki)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2319">
<skos:prefLabel>osuszanie</skos:prefLabel>
<skos:definition>1) usuwanie wody gruntowej lub powierzchniowej oraz wody z budynków, grawitacyjnie lub przez wypompowanie  2) wypływ wody z gleby dzięki perkolacji (proces polegający na przemieszczaniu wody powierzchniowej w dół przez pęknięcia, szczeliny i pory w glebie i skałach)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2323">
<skos:prefLabel>wody drenujące</skos:prefLabel>
<skos:definition>incydentalnie powstające wody powierzchniowe z takich źródeł jak opady deszczu, topniejący śnieg lub lód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2324">
<skos:prefLabel>zwierzę pociągowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2326">
<skos:prefLabel>szlam z pogłębiania</skos:prefLabel>
<skos:definition>niezestalony materiał usuwany z rzek, strumieni oraz płytkich mórz przy pomocy pogłębiarek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2327">
<skos:prefLabel>pogłębianie</skos:prefLabel>
<skos:definition>usuwanie stałej materii z dna zbiorników wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2330">
<skos:prefLabel>ścieki z wytłaczarni oliwy</skos:prefLabel>
<skos:definition>wodna pozostałość powstająca w procesie ekstrakcji oleju z oliwek; składa się z mieszaniny oleju i wody i powstaje w procesach mycia i ekstrakcji; zawiera również pewną ilosć substancji mineralnych oraz organicznych częściowo tylko biodegradowalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2332">
<skos:prefLabel>połów sieciami dryfującymi</skos:prefLabel>
<skos:definition>połowy z wykorzystaniem sieci o wielkich rozmiarach, nazywanych &quot;ścianami śmierci&quot; ze względu na przypadkowe odławianie znacznej liczby morskich ssaków, ptaków i żółwi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2333">
<skos:prefLabel>wiercenia</skos:prefLabel>
<skos:definition>wiercenie otworów w ziemi w poszukiwaniu wody lub ropy naftowej, przy prowadzeniu poszukiwań geologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2336">
<skos:prefLabel>urządzenia wiertnicze</skos:prefLabel>
<skos:definition>podstawa, na bazie której montuje się urządzenia wiertnicze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2339">
<skos:prefLabel>woda do picia</skos:prefLabel>
<skos:definition>woda nadająca się do picia, nie stwarzająca zagrożenia dla zdrowia, której jakość regulowana jest przepisami prawnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2340">
<skos:prefLabel>strefa ochrony źródła wody do picia</skos:prefLabel>
<skos:definition>obszar otaczający ujęcie wody pitnej, na którym zabronione są lub ograniczone pewne formy działalności w celu ochrony wód podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2343">
<skos:prefLabel>dostarczanie wody do picia</skos:prefLabel>
<skos:definition>zaopatrzenie i magazynowanie wody pitnej w celu jej wykorzystania przez różnych użytkowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2344">
<skos:prefLabel>uzdatnianie wody</skos:prefLabel>
<skos:definition>dyrektywa w sprawie jakości wód powierzchniowych przeznaczonych do picia definiuje trzy kategorie uzdatniania wody (A1, A2, A3) od prostej fizycznej obróbki i dezynfekcji do intensywnych metod fizycznych i chemicznych; technologia uzdatniania zależy od jakości ujmowanej wody; dyrektywa wykorzystuje wartości imperatywne parametrów, o których wiadomo, że powodują negatywny wpływ na zdrowie ludzi oraz wartości zalecane dla parametrów mniej szkodliwych; istnieje również dyrektywa uzupełniająca, określająca metody pomiaru, częstość pobieranych próbek oraz zakres wymaganych analiz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2347">
<skos:prefLabel>susza</skos:prefLabel>
<skos:definition>długotrwały okres bez opadów atmosferycznych lub z dużym ich niedoborem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2350">
<skos:prefLabel>walka z suszą</skos:prefLabel>
<skos:definition>działania podejmowane w celu zapobiegania, łagodzenia lub eliminacji szkód spowodowanych w ekosystemie, w szczególności dotyczy to zbiorów, na skutek długo utrzymującej się suszy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2352">
<skos:prefLabel>niewłaściwe używanie leków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2354">
<skos:prefLabel>lekarstwo</skos:prefLabel>
<skos:definition>substancja chemiczna (lub zespół substancji) mająca pochodzenie roślinne, zwierzęce lub otrzymywana syntetycznie, stosowana w lecznictwie w celu zapobiegania chorobom, ich zwalczania, łagodzenia objawów itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2358">
<skos:prefLabel>pranie chemiczne</skos:prefLabel>
<skos:definition>pranie na sucho środkami innymi niż woda</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2359">
<skos:prefLabel>sucha  depozycja</skos:prefLabel>
<skos:definition>gromadzenie się cząsteczek stałych i gazów wchodzących w kontakt z glebą, wodą lub roślinami na powierzchni ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2360">
<skos:prefLabel>uprawa bez nawadniania</skos:prefLabel>
<skos:definition>system ekstensywnego rolnictwa prowadzonego na obszarach suchych bez irygacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2361">
<skos:prefLabel>suszenie</skos:prefLabel>
<skos:definition>proces częściowego lub całkowitego usuwania wody lub innych cieczy z ciał stałych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2363">
<skos:prefLabel>wysuszanie</skos:prefLabel>
<skos:definition>usuwanie wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2364">
<skos:prefLabel>dwutorowy system gospodarczy</skos:prefLabel>
<skos:definition>gospodarka oparta na dwóch oddzielnych systemach ekonomicznych koegzystujących w tej samej przestrzeni geograficznej; taki dualizm występuje w wielu krajach rozwijających się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2365">
<skos:prefLabel>dwutorowy system zbiórki odpadów</skos:prefLabel>
<skos:definition>w celu zredukowania masy odpadów opakowaniowych i tym samym masy stałych odpadów komunalnych Niemcy wprowadziły daleko idące przepisy oparte o zasadę odpowiedzialności producenta; przemysł zaproponował utworzenie niezależnej organizacji, odpowiedzialnej za zbiórkę i sortowanie odpadów w imieniu wytwórców i detalistów; w rezultacie około 600 firm utworzyło w 1990 r. &quot;Duales System Deutschland&quot; (DSD) (podwójny ponieważ jako pierwszy był już istniejący system oparty na lokalnych władzach); DSD jest obecnie odpowiedzialny za selektywną zbiórkę i recykling opakowań; obecnie DSD jest jedynym systemem tego rodzaju w Niemczech; opakowania uczestniczące w tym systemie oznakowane są zieloną kropką</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2367">
<skos:prefLabel>pozbywanie się odpadów</skos:prefLabel>
<skos:definition>pozbywanie się odpadów w dowolny sposób, często niezgodnie z przepisami chroniącymi środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2372">
<skos:prefLabel>składowanie odpadów na powierzchni ziemi</skos:prefLabel>
<skos:definition>zrzut, składowanie i wstrzykiwanie jakichkolwiek odpadów do gleby lub na powierzchni gleby lub innego gruntu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2377">
<skos:prefLabel>wydma</skos:prefLabel>
<skos:definition>piaszczyste wzniesienie o asymetrycznych stokach, usypane przez wiatr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2380">
<skos:prefLabel>czas nasłonecznienia</skos:prefLabel>
<skos:definition>ilość energii słonecznej (w kaloriach) padająca na jednostkę powierzchni w ciągu określonego czasu np. w ciągu dnia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2381">
<skos:prefLabel>pył</skos:prefLabel>
<skos:definition>drobniutkie, sproszkowane ziarenka ziemi, piasku, różnego rodzaju rozkruszonych lub bardzo rozdrobnionych ciał unoszące się w powietrzu i osiadające na powierzchni przedmiotów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2383">
<skos:prefLabel>imisja pyłu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2384">
<skos:prefLabel>usuwanie pyłu</skos:prefLabel>
<skos:definition>usuwanie pyłu z powietrza przez system wentylacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2386">
<skos:prefLabel>obowiązek ewidencji</skos:prefLabel>
<skos:definition>obowiązek formalnie przypisany agencji rządowej lub władzom polegający na nadawaniu pewnych praw, szczególnie praw do znaków towarowych lub praw autorskich lub pozwolenia na sprzedaż i dystrybucję niektórych wyrobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2387">
<skos:prefLabel>mieszkanie</skos:prefLabel>
<skos:definition>zamknięta przestrzeń całkowicie lub częściowo wykorzystywana lub przeznaczona do wykorzystania na egzystencję, spanie, gotowanie i spożywanie posiłków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2389">
<skos:prefLabel>barwnik</skos:prefLabel>
<skos:definition>substancja barwna pochodzenia naturalnego lub otrzymywana syntetycznie, mająca właściwość trwałego barwienia różnego rodzaju materiałów, jak włókien naturalnych, sztucznych, skóry, futer, papieru, artykułów spożywczych i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2393">
<skos:prefLabel>tama</skos:prefLabel>
<skos:definition>budowla wodna w postaci wału z piasku, kamieni, betonu itp. wznoszona przy regulacji rzeki, np. w celu przesunięcia jej koryta, ochrony brzegu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2394">
<skos:prefLabel>umacnianie grobli</skos:prefLabel>
<skos:definition>dodatek materiałów w celu wzmocnienia struktury grobli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2398">
<skos:prefLabel>system wczesnego ostrzegania</skos:prefLabel>
<skos:definition>szereg procedur i urządzeń zaprojektowanych w celu wykrycia nagłych lub potencjalnych zagrożeń w stosunku do ludzi, mienia lub środowiska na podstawie pierwszych oznak niebezpieczeństwa; w szczególności system wykorzystujący technologię radarową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2400">
<skos:prefLabel>skorupa ziemska</skos:prefLabel>
<skos:definition>zewnętrzna powłoka kuli ziemskiej sięgająca średnio do głębokości 33 km, składająca się z trzech warstw: osadowej, „granitowej”, zbudowanej głównie z granitów i „bazaltowej” zbudowanej ze skał typu bazaltu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2402">
<skos:prefLabel>trzęsienie ziemi</skos:prefLabel>
<skos:definition>wstrząs skorupy ziemskiej wywołany przemieszczeniami mas skalnych w głębi Ziemi, wybuchami wulkanów, zapadaniem się stropów podziemnych jaskiń krasowych; mogący powodować znaczne zniszczenia na powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2405">
<skos:prefLabel>nauka o Ziemi</skos:prefLabel>
<skos:definition>nauka, która zajmuje się ziemią; zawiera między innymi geologię, geografię, oceanografię i meteorologię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2406">
<skos:prefLabel>zależności  Ziemia-Słońce</skos:prefLabel>
<skos:definition>istnienie Ziemi jako planety nadającej się do życia zależy do Słońca, energia słoneczna jest głównym czynnikiem wyznaczającym klimat; stąd warunki na Słońcu i warunki na Ziemi są ściśle powiązane; chociaż światło słoneczne może wydawać się niezmienne, promieniowanie zmienia się; wielu uczonych podejrzewa, że aktywność plam słonecznych ma większy wpływ na zmiany klimatu niż efekt cieplarniany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2408">
<skos:prefLabel>dżdżownica</skos:prefLabel>
<skos:definition>skąposzczety o walcowatym ciele złożonym z licznych pierścieni, spotyka się prawie 1500 gatunków; prowadzi nocny tryb życia, żywi się szczątkami organicznymi, nie strawione części wydala użyźniając glebę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2409">
<skos:prefLabel>skorki</skos:prefLabel>
<skos:definition>owad z rzędu o tej samej nazwie, o ciele walcowatym, spłaszczonym grzbietobrzusznie, z dwoma kleszczami na końcu odwłoka i o dwóch parach skrzydeł (przednie — skórzaste pokrywy, tylne — błoniaste); występuje głównie w krajach tropikalnych, w Polsce znanych kilka gatunków, między innymi skorek pospolity</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2411">
<skos:prefLabel>Afryka Wschodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2412">
<skos:prefLabel>Azja Wschodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2413">
<skos:prefLabel>Europa Wschodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2414">
<skos:prefLabel>stosunki Wschód-Zachód</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2415">
<skos:prefLabel>handel Wschód-Zachód</skos:prefLabel>
<skos:definition>handel pomiędzy państwami i firmami wschodniej półkuli z odpowiednikami z zachodniej półkuli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2419">
<skos:prefLabel>Rada Ministrów Unii Europejskiej</skos:prefLabel>
<skos:definition>główny organ legislacyjny UE; Rada reprezentuje interesy państw członkowskich na szczeblu Wspólnoty w procesie kształtowania i realizacji wspólnej polityki w każdej dziedzinie podlegającej   działalności legislacyjnej Rady; w skład Rady wchodzą, po jednym z każdego państwa, przedstawiciele na szczeblu ministerialnym; państwo członkowskie sprawuje przewodnictwo (prezydencję) w Radzie przez sześć miesięcy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2422">
<skos:prefLabel>dyrektywa Unii Europejskiej</skos:prefLabel>
<skos:definition>najważniejszy akt prawny Wspólnoty Europejskiej o zasięgu ogólnym; wytyczne, dyrektywy stosuje się bezpośrednio i w całości we wszystkich krajach członkowskich, tj. nie podlegają procesowi transformacji w narodowe systemy prawne i bezpośrednio obowiązują mieszkańców, sądy i inne instytucje tak samo jak prawo krajowe; dyrektywy nadają także prawa oraz nakładają obowiązki na organy Wspólnoty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2423">
<skos:prefLabel>dyrektywa Unii Europejskiej dotycząca biocydów</skos:prefLabel>
<skos:definition>dyrektywa Unii Europejskiej dotycząca biocydów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2424">
<skos:prefLabel>Dyrektywa WE w sprawie opakowań</skos:prefLabel>
<skos:definition>dyrektywa Unii Europejskiej dotycząca opakowań i gospodarki opakowaniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2425">
<skos:prefLabel>dyrektywa Unii Europejskiej dotycząca składowania odpadów</skos:prefLabel>
<skos:definition>dyrektywa Unii Europejskiej dotycząca składowania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2426">
<skos:prefLabel>dyrektywa Unii Europejskiej dotycząca ochrony wód</skos:prefLabel>
<skos:definition>dyrektywa Unii Europejskiej dotycząca ochrony wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2427">
<skos:prefLabel>znak ekologiczny</skos:prefLabel>
<skos:definition>Wspólnota Europejska zgłosiła inicjatywę polegającą na zachęcaniu do promocji wyrobów przyjaznych środowisku; plan wszedł w życie w końcu 1992 r.; celem była identyfikacja, dla określonego gatunku, wyrobów mniej szkodliwych dla środowiska, np. znaki ekologiczne są przyznawane wyrobom, które nie zawierają freonów (CFC) niszczących warstwę ozonową, wyrobom, które mogą być lub są poddawane recyklingowi oraz wyrobom energooszczędnym; znaki przyznawane są na podstawie kryteriów środowiskowych ustanowionych przez Wspólnotę; uwzględniają one pełny cykl życia wyrobów, od wydobycia surowców z ziemi, poprzez wytwarzanie wyrobu, jego dystrybucję, wykorzystanie i usuwanie jako odpadu; pierwszymi wyrobami, którym nadano znak ekologiczny były: pralki, ręczniki papierowe, papier do drukarek, żarówki i lakiery do włosów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2428">
<skos:prefLabel>jeżowce</skos:prefLabel>
<skos:definition>zwierzę morskie z gromady o tej samej nazwie, o ciele przeważnie kulistym, okrytym wapiennym pancerzem z kolcami; żyje na dnie mórz i oceanów; w lm gromada tych zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2434">
<skos:prefLabel>ekorozwój</skos:prefLabel>
<skos:definition>1) rozwój zachowawczy oparty na optymalizacji w długiej skali czasowej zasobów biosfery; 2) sposób rozwoju polegający na racjonalnym wykorzystaniu zasobów naturalnych poprzez odpowiednią technologię i system produkcji, który bierze pod uwagę ochronę natury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2435">
<skos:prefLabel>znakowanie produktów przyjaznych dla środowiska</skos:prefLabel>
<skos:definition>Wspólnota Europejska zgłosiła inicjatywę polegającą na zachęcaniu do promocji wyrobów przyjaznych środowisku; plan wszedł w życie w końcu 1992 r.; celem była identyfikacja, dla określonego gatunku, wyrobów mniej szkodliwych dla środowiska; liczono się z tym, że poprzez nabywanie posiadających znak dóbr konsumenci będą mogli wywrzeć presję na wytwórców i detalistów skłaniając ich do wytwarzania i obrotu bardziej przyjaznymi wyrobami; skutki oddziaływania wyrobu na środowisko rozważa się we wszystkich stadiach cyklu życia wyrobu; znaki ekologiczne przyznaje się na podstawie kryteriów ustanowionych przez Wspólnotę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2437">
<skos:prefLabel>obfitość ekologiczna</skos:prefLabel>
<skos:definition>liczba osobników różnych gatunków zwierząt i roślin obserwowana na określonym obszarze w określonym przedziale czasowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2438">
<skos:prefLabel>przystosowanie do warunków środowiska</skos:prefLabel>
<skos:definition>zmiany w organizmie ułatwiające mu przetrwanie i reprodukcję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2440">
<skos:prefLabel>ocena ekologiczna</skos:prefLabel>
<skos:definition>ocena ekologiczna składająca się z monitoringu stanu i zmian zasobów ekologicznych, z których może wyniknąć sukces lub niepowodzenie ekosystemu; pełniejsze zrozumienie struktury i funkcji ekosystemów w celu opracowania lepszych opcji gospodarowania; opracowanie modeli zdolnych do przewidywania odpowiedzi ekosystemu na zmiany wprowadzane w wyniku antropopresji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2441">
<skos:prefLabel>równowaga ekologiczna</skos:prefLabel>
<skos:definition>stan równowagi pomiędzy elementami naturalnej społeczności polegający na ich stałej liczebności i stabilności ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2442">
<skos:prefLabel>księgowość kosztów środowiskowych</skos:prefLabel>
<skos:definition>regularne księgowanie lub rejestracja wpływów przedsiębiorstwa na środowisko lub wydatków ponoszonych na praktyki pro-ekologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2450">
<skos:prefLabel>czynnik ekologiczny</skos:prefLabel>
<skos:definition>czynnik środowiskowy, który w pewnych warunkach może istotnie wpływać na organizmy lub ich  zbiorowiska, powodując zmiany liczebności gatunków lub przekształcenia zbiorowisk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2456">
<skos:prefLabel>nisza ekologiczna</skos:prefLabel>
<skos:definition>1) przestrzeń zajmowana przez gatunek w sensie przestrzeni fizycznej i w aspekcie funkcjonowania gatunku w obrębie tej przestrzeni, 2) pojęcie niszy ekologicznej dotyczy tych właściwości środowiska życia danego organizmu, dzięki którym organizm ten ma zapewnione pożywienie i ochronę przy braku stałych zagrożeń w postaci potencjalnych drapieżników, pasożytów i konkurentów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2457">
<skos:prefLabel>parametr ekologiczny</skos:prefLabel>
<skos:definition>zmienna, mierzalna właściwość, której wartość określa danę cechę ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2461">
<skos:prefLabel>obszar podatny na zakłócenie równowagi ekologicznej</skos:prefLabel>
<skos:definition>obszar, w obrębie którego zmiany istniejących układów ekologicznych wywołają odczuwalne zakłócenia równowagi ekologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2463">
<skos:prefLabel>inwentaryzacja ekologiczna</skos:prefLabel>
<skos:definition>rejestracja wpływów przedsiębiorstwa na środowisko lub postępu w jego działaniach pro-ekologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2469">
<skos:prefLabel>ruchy zielonych</skos:prefLabel>
<skos:definition>działania grupowe i indywidualne mające na celu ochronę środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2470">
<skos:prefLabel>ekologia</skos:prefLabel>
<skos:definition>dziedzina biologii badająca wzajemne stosunki między organizmami a otaczającym je środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2474">
<skos:prefLabel>ekologia poziomów troficznych</skos:prefLabel>
<skos:definition>badanie zależności typu pokarmowego u organizmów w zbiorowiskach i w ekosystemach; połączenia troficzne między populacjami wyrażają się w formie przepływu organizmów, energii organicznej i składników odżywczych; przepływy troficzne mają wielkie znaczenie w dynamice populacji, biogeochemii i energetyce ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2476">
<skos:prefLabel>działalność gospodarcza</skos:prefLabel>
<skos:definition>wysiłek, praca, funkcja lub sfera działania odnosząca się do wytwarzania dóbr, usług i środków pieniężnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2477">
<skos:prefLabel>efektywność gospodarczo-ekologiczna</skos:prefLabel>
<skos:definition>współzawodnictwo w działalności produkcyjnej i handlowej obejmujące związek pomiędzy zasadami finansowymi i środowiskowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2478">
<skos:prefLabel>analiza gospodarcza</skos:prefLabel>
<skos:definition>ilościowa i jakościowa identyfikacja, badanie i ocena właściwości gospodarki, systemu organizacji lub działania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2484">
<skos:prefLabel>rozwój gospodarczy</skos:prefLabel>
<skos:definition>status państw i historyczna droga przebytego przez nie rozwoju oceniana z perspektywy efektywności wykorzystania przez te państwa dostępnych zasobów, pojęcie to obejmuje również zagadnienia zmian społecznych, kulturowych i politycznych, a także wskaźniki dobrobytu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2490">
<skos:prefLabel>wzrost gospodarczy</skos:prefLabel>
<skos:definition>stałe zwiększanie wielkości gospodarczych dotyczących przede wszystkim sił wytwórczych, dochodu narodowego i konsumpcji społeczeństwa w określonym typie stosunków produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2493">
<skos:prefLabel>instrument ekonomiczny</skos:prefLabel>
<skos:definition>narzędzie lub metoda używana przez organizację do osiągnięcia ogólnych celów związanych z rozwojem i dotyczących produkcji, kontroli i zasobów materiałowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2494">
<skos:prefLabel>instrument zarządzania gospodarczego</skos:prefLabel>
<skos:definition>narzędzie lub metoda używana przez organizację w zarządzaniu procesami rozwoju dotyczącymi produkcji, kontroli i zasobów materiałowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2497">
<skos:prefLabel>planowanie gospodarcze</skos:prefLabel>
<skos:definition>gospodarka, w której ceny, przychody itp. są określane centralnie przez rząd a nie w wyniku działania wolnego rynku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2498">
<skos:prefLabel>polityka gospodarcza</skos:prefLabel>
<skos:definition>działalność władzy państwowej, rządu w dziedzie gospodarczej dotycząca spraw wewnętrznych państwa lub stosunków z innymi krajami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2501">
<skos:prefLabel>ekonomia</skos:prefLabel>
<skos:definition>nauka o społecznych prawach rządzących produkcją i podziałem dóbr w społeczeństwie; nauka o racjonalnym gospodarowaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2505">
<skos:prefLabel>sytuacja gospodarcza</skos:prefLabel>
<skos:definition>zespół elementów który, w danym okresie czasu, określa kondycję lub stan kraju lub regionu w odniesieniu do wytwarzania towarów, usług i innych zasobów finansowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2508">
<skos:prefLabel>system gospodarczy</skos:prefLabel>
<skos:definition>określony zbiór procedur stosowanych przez społeczeństwo w celu zarządzania produkcją, dystrybucją dóbr i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2510">
<skos:prefLabel>teoria gospodarcza</skos:prefLabel>
<skos:definition>dziedzina wiedzy obejmująca badania zależności ekonomicznych; jej celem jest analiza i wyjaśnianie zachowania różnych elementów składowych gospodarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2511">
<skos:prefLabel>tendencja gospodarcza</skos:prefLabel>
<skos:definition>zmiany zmiennych i parametrów systemu ekonomicznego analizowane metodą obliczeń statystycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2512">
<skos:prefLabel>żywotność gospodarcza</skos:prefLabel>
<skos:definition>zdolność do rozwoju i przetrwania względnie niezależnej jednostki o charakterze socjalnym, ekonomicznym lub politycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2514">
<skos:prefLabel>strefa gospodarcza</skos:prefLabel>
<skos:definition>plan zagospodarowania terenu w którym wyznaczono granice obszaru na którym zachęca się do określonych rodzajów działalności gospodarczej oraz inwestowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2516">
<skos:prefLabel>gospodarka</skos:prefLabel>
<skos:definition>system działań i zarządzania, którymi posługuje się społeczeństwo w celu użytkowania zasobów i produkcji dóbr; organizacja produkcji, sposoby produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2517">
<skos:prefLabel>ekopediatria</skos:prefLabel>
<skos:definition>gałąź nauk medycznych dotycząca badań i terapii chorób dziecięcych spowodowanych czynnikami środowiskowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2518">
<skos:prefLabel>ekofizjologia</skos:prefLabel>
<skos:definition>badania procesów biofizycznych, biochemicznych i fizjologicznych, dzięki którym organizmy zwierzęce mogą sprostać wpływom środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2519">
<skos:prefLabel>ekosystem</skos:prefLabel>
<skos:definition>powiązane wzajmnymi oddziaływaniami zbiorowiska organizmów wraz z ich fizycznym środowiskiem życia, także fragment przyrody (np. jezioro) stanowiący funkcjonalną całość, w której zachodzi wymiana między jej częścią żywą — biocenozą, a nieożywioną — biotopem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2520">
<skos:prefLabel>analiza ekosystemu</skos:prefLabel>
<skos:definition>szczegółowe badania ekosystemu prowadzone w celu stwierdzenia ich właściwości z punktu widzenia składu gleb, przepływu energii, cyklów biochemicznych, biomasy, organizmów i ich związków ze środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2522">
<skos:prefLabel>degradacja ekosystemów</skos:prefLabel>
<skos:definition>degradacja lub zniszczenie dużych naturalnych środowisk. Gdy ekosystem jest zagrożony katastrofą naturalną lub spowodowaną przez ludzi staje się niezwykle trudnym obliczenie efektów powracającej fali. Gdy dwa lub więcej ekosystemów uległo degradacji, prawdopodobieństwo zniszczeń synergicznych rośnie wielokrotnie. Ekosystemy w wielu regionach są zagrożone pomimo ich bogactwa biologicznego i obietnicy materialnych korzyści</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2523">
<skos:prefLabel>badanie ekosystemu</skos:prefLabel>
<skos:definition>badanie dróg na których rośliny, zwierzęta i mikroby oddziaływują wzajemnie na siebie oraz oddziaływują z fizycznym środowiskiem i procesów obejmujących cyrkulację, transformację i akumulację materiałów odżywczych i energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2525">
<skos:prefLabel>rodzaj ekosystemu</skos:prefLabel>
<skos:definition>w klasyfikacji ekosystemów używa się różnych kryteriów, biorąc pod uwagę źródła energii wyróżnia się dwa główne rodzaje ekosystemów: autotroficzne -z producentami pierwotnymi jako podstawowym elementem systemu i słońcem jako głównym wyjściowym źródłem energii oraz ekosystemy heterotroficzne, zależne od wytworzonej uprzednio materii organicznej, importowanej z ekosystemów autotroficznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2527">
<skos:prefLabel>ekoturystyka</skos:prefLabel>
<skos:definition>wycieczki w tereny o nienaruszonej przyrodzie, obiecujące turystom szansę zaobserwowania dzikiej przyrody i zamieszkujących tam gatunków; branża turystyczna próbuje reklamować przygodę i autentyczność podróżnikom zmęczonym cywilizacją promując podróże w rejony wolne od nowoczesnej techniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2528">
<skos:prefLabel>ekotoksyczność</skos:prefLabel>
<skos:definition>cecha niektórych substancji i/lub preparatów powodująca lub mogąca spowodować natychmiastowe lub opóźnione ryzyko dla jednego lub więcej wycinków środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2529">
<skos:prefLabel>ocena ekotoksyczności</skos:prefLabel>
<skos:definition>ocena szkodliwych skutków pochodzących od substancji chemicznych, czynników fizycznych i produktów naturalnych dla populacji i zbiorowości oraz roślin, zwierząt i ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2530">
<skos:prefLabel>ekotoksykologia</skos:prefLabel>
<skos:definition>nauka zajmująca się szkodliwymi skutkami pochodzącymi od substancji chemicznych, czynników fizycznych i produktów naturalnych na populacje, zbiorowości roślin, zwierząt i ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2531">
<skos:prefLabel>ekotyp</skos:prefLabel>
<skos:definition>jedna z odmiennych populacji tego samego gatunku zwierząt lub roślin, powstała na skutek przystosowania się do życia w różnych warunkach środowiskowych (np. w górach lub na nizinach)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2533">
<skos:prefLabel>polityka Unii Europejskiej</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2534">
<skos:prefLabel>rozporządzenie Unii Europejskiej</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2535">
<skos:prefLabel>przepisy Komisji Europejskiej dotyczące gospodarki w środowisku i audytów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2536">
<skos:prefLabel>przepisy Komisji Europejskiej dotyczące ryzyka od substancji istniejących</skos:prefLabel>
<skos:definition>przepisy Komisji Europejskiej dotyczące ryzyka od substancji istniejących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2539">
<skos:prefLabel>edafologia</skos:prefLabel>
<skos:definition>badania związków pomiędzy glebą i organizmami, w tym sposobu wykorzystania terenu przez ludność</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2542">
<skos:prefLabel>tłuszcz jadalny</skos:prefLabel>
<skos:definition>produkt spożywczy uzyskany z tkanki zwierzęcej lub roślinnej, mający wysoką wartość kaloryczną, niezbędny do odpowiedniego funkcjonowania organizmu, który może stanowić pokarm lub dodatek do żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2547">
<skos:prefLabel>edukacja</skos:prefLabel>
<skos:definition>wychowanie, głównie pod względem rozwoju umysłowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2551">
<skos:prefLabel>instytucja edukacyjna</skos:prefLabel>
<skos:definition>organizacja lub firma założona w celu przekazywania lub rozwijania wiedzy i umiejętności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2554">
<skos:prefLabel>planowanie edukacji</skos:prefLabel>
<skos:definition>proces organizowania porozumień lub przygotowań do prowadzenia szkoleń, instrukcji albo badań które prowadzą do nabycia umiejętności lub wiedzy oraz do rozwoju zrozumienia i znawstwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2555">
<skos:prefLabel>polityka edukacyjna</skos:prefLabel>
<skos:definition>działalność władzy państwowej, rządu w dziedzinie edukacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2557">
<skos:prefLabel>system edukacyjny</skos:prefLabel>
<skos:definition>określona, regularna lub specjalna organizacja szkoleń, kursów  lub udostępnienia wiedzy, w szczególności struktura instytucjonalna podtrzymująca te wysiłki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2562">
<skos:prefLabel>oddziaływanie</skos:prefLabel>
<skos:definition>a) bezpośrednie skutki spowodowane przez działanie i wydarzające się w tym samym miejscu i czasie, b) pośrednie skutki, spowodowane przez działanie i wydarzające się później lub w odległym miejscu ale wciąż możliwe do przewidzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2564">
<skos:prefLabel>wpływ na zdrowie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2565">
<skos:prefLabel>wpływ na człowieka</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2566">
<skos:prefLabel>wpływ na środowisko</skos:prefLabel>
<skos:definition>skutki wydarzeń naturalnych lub działalności człowieka na fizyczne, chemiczne lub biologiczne składniki środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2568">
<skos:prefLabel>badanie oddziaływania</skos:prefLabel>
<skos:definition>badania prowadzone w celu oceny wyników wynikających się z działań lub warunków; ogólny termin stosowany w różnych dziedzinach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2569">
<skos:prefLabel>kryterium efektywności</skos:prefLabel>
<skos:definition>parametr lub zasada oceny kompetencji stosowana przy ocenie efektywności produkcji w stosunku do określonego wsadu zasobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2570">
<skos:prefLabel>poziom efektywności</skos:prefLabel>
<skos:definition>stosunek wartości wyjściowych do wejściowych (w produkcji), podawany w procentach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2571">
<skos:prefLabel>ścieki</skos:prefLabel>
<skos:definition>kanał otwarty lub kryty, odprowadzający zanieczyszczone wody miejskie lub przemysłowe do specjalnych lub naturalnych zbiorników albo cieków wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2573">
<skos:prefLabel>jajo</skos:prefLabel>
<skos:definition>żeńska komórka rozrodcza, komórka jajowa; u gadów i ptaków zawierająca żółtko i białko, otoczone mocną skórką zewnętrzną albo skorupką</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2578">
<skos:prefLabel>dyrektywa dotycząca OOŚ</skos:prefLabel>
<skos:definition>Dyrektywa Rady 85/337 z dnia 27 czerwca 1985 roku w sprawie oceny skutków niektórych publicznych i prywatnych przedsięwzięć, które mogą w sposób istotny oddziaływać na środowisko ze względu na swój rodzaj, rozmiar lub lokalizację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2579">
<skos:prefLabel>ustawodawstwo dotyczące OOŚ</skos:prefLabel>
<skos:definition>przepisy dotyczące ocen oddziaływania na środowisko; przepisy dotyczące skutków w środowisku powodowanych przez niektóre publiczne i prywatne przedsięwzięcia oparte na dyrektywie 85/337</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2580">
<skos:prefLabel>OOŚ</skos:prefLabel>
<skos:definition>identyfikacja, oszacowanie i ocena skutków dla środowiska powodowanych przez przedsięwzięcia w obrębie danej miejscowości lub regionu oraz środki niezbędne do minimalizacji negatywnych oddziaływań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2582">
<skos:prefLabel>elastyczność</skos:prefLabel>
<skos:definition>zdolność powracania ciał do pierwotnego kształtu i wymiarów po usunięciu obciążenia wywołującego odkształcenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2584">
<skos:prefLabel>człowiek w podeszłym wieku</skos:prefLabel>
<skos:definition>osoba dojrzała, która osiagnęła późniejsze stadium swego życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2586">
<skos:prefLabel>inżynieria elektryczna</skos:prefLabel>
<skos:definition>dział nauki i techniki zajmujący się budową i unowocześnianiem urządzeń elektrycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2587">
<skos:prefLabel>przemysł energetyczny (wytwórca energii elektrycznej)</skos:prefLabel>
<skos:definition>przemysł produkujący energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2588">
<skos:prefLabel>urządzenie do akumulacji energii elektrycznej</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2592">
<skos:prefLabel>prąd</skos:prefLabel>
<skos:definition>forma energii w postaci ładunków i pól elektrycznych; także zjawiska związane z wzajemnym oddziaływaniem pól elektrycznych i magnetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2593">
<skos:prefLabel>dostawca energii elektrycznej</skos:prefLabel>
<skos:definition>zakład przemysłowy produkujący i rozsyłający energię elektryczną w danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2594">
<skos:prefLabel>zużycie energii elektrycznej</skos:prefLabel>
<skos:definition>zużycie energii elektrycznej przez urządzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2595">
<skos:prefLabel>wytwarzanie energii elektrycznej</skos:prefLabel>
<skos:definition>proces polegający na przetwarzaniu innych form energii na energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2596">
<skos:prefLabel>koszt wytwarzania energi elektrycznej</skos:prefLabel>
<skos:definition>wartość lub kwota przeznaczona na wytwarzanie i utrzymywanie urządzeń zasilających, używanych jako źródło mocy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2597">
<skos:prefLabel>przemysł energetyczny</skos:prefLabel>
<skos:definition>przemysł dostarczający energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2599">
<skos:prefLabel>linia elektryczna</skos:prefLabel>
<skos:definition>przewody zainstalowane na słupach (metalowych, żelbetowych lub drewnianych), służące do przesyłania energii elektrycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2601">
<skos:prefLabel>moc energii elektrycznej</skos:prefLabel>
<skos:definition>energia elektryczna przypadająca na jednostkę czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2606">
<skos:prefLabel>elektrownia</skos:prefLabel>
<skos:definition>zakład energetyczny przetwarzający różne rodzaje energii (hydrauliczna, pary wodnej, chemiczna lub jądrowa) na prąd elektryczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2608">
<skos:prefLabel>dostarczanie energii elektrycznej</skos:prefLabel>
<skos:definition>zakład energetyczny przetwarzający różne rodzaje energii (hydrauliczna, pary wodnej, chemiczna lub jądrowa) na prąd elektryczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2612">
<skos:prefLabel>pojazd elektryczny</skos:prefLabel>
<skos:definition>pojazd do transportu lądowego zasilany energią elektryczną, charakteryzujący się niskim poziomem hałasu oraz nie powodujący zanieczyszczenia środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2617">
<skos:prefLabel>elektrokinetyka</skos:prefLabel>
<skos:definition>dział fizyki zajmujący się zjawiskami elektryczności w ruchu, z wyłączeniem zjawisk magnetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2618">
<skos:prefLabel>elektroliza</skos:prefLabel>
<skos:definition>proces elektrochemiczny polegający na rozkładzie elektrolitu z wydzieleniem na elektrodach metalu lub gazu — pod wpływem stałego prądu elektrycznego; stosowana do oczyszczania i otrzymywania metali i gazów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2623">
<skos:prefLabel>materiał elektroniczny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2624">
<skos:prefLabel>elektronika</skos:prefLabel>
<skos:definition>dziedzina nauki i techniki zajmująca się praktycznym wyzyskaniem skutków zjawisk ruchu swobodnych elektronów w próżni, w gazach i ciałach stałych; obejmuje właściwości, konstrukcję i technologię przyrządów elektronowych (zwłaszcza lamp elektronowych, tranzystorów)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2625">
<skos:prefLabel>odpady elektroniczne</skos:prefLabel>
<skos:definition>jakikolwiek materiał pochodzący z urządzeń i systemów elektronicznych, który po wykorzystaniu tworzy strumień opadów podlegających przetwarzaniu lub usuwaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2626">
<skos:prefLabel>przepisy dotyczące odpadów elektronicznych</skos:prefLabel>
<skos:definition>akty prawne dotyczące zasad usuwania i wykorzystania odpadów elektronicznych zwłaszcza urządzeń komputerowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2628">
<skos:prefLabel>smog elektromagnetyczny</skos:prefLabel>
<skos:definition>skażenia powstałe w wyniku działania pola magnetycznego i elektrycznego wokół linii wysokiego napięcia, radarów, bezprzewodowych telefonów, domowych urządzeń elektrycznych takich jak kuchenki mikrofalowe, aparaty radiowe, komputery, elektroniczne zegary</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2633">
<skos:prefLabel>urządzenia przemysłu elektrotechnicznego</skos:prefLabel>
<skos:definition>wszystkie urządzenia związane z wykorzystaniem energii elektrycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2634">
<skos:prefLabel>przemysł elektrotechniczny</skos:prefLabel>
<skos:definition>sektor gospodarki, w którym zespół przedsiębiorstw zajmuje się projektowaniem, wytwarzaniem i wprowadzaniem na rynek maszyn i urządzeń, w tym zasilających, służących do wytwarzania, magazynowania i wykorzystywania energii elektrycznej, takich jak urządzenia domowe, sprzęt do odbioru programów radiowych i telewizyjnych, oświetleniowy oraz okablowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2635">
<skos:prefLabel>pierwiastek chemiczny z grupy 0 (gazów jednoatomowych)</skos:prefLabel>
<skos:definition>grupa jednoatomowych pierwiastków gazowych tworzących grupę 8 (dawniej grupa 0) układu okresowego: hel (He), neon (Ne), argon (Ar), krypton (Kr), xenon (Xe) i radon (Rn)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2636">
<skos:prefLabel>pierwiastek chemiczny z grupy I (metali jednowartościowych)</skos:prefLabel>
<skos:definition>każdy jednowartościowy metal: lit, sód, potas, rubid, cez i frans należy do grupy 1A układu okresowego; są to pierwiastki bardzo reaktywne i elektrododatnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2637">
<skos:prefLabel>pierwiastek chemiczny z grupy II (metali dwuwartościowych)</skos:prefLabel>
<skos:definition>każdy dwuwartościowy elektrododatni metal: beryl, magnez, wapń, stront, bar i rad należy do grupy 2A układu okresowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2638">
<skos:prefLabel>pierwiastek chemiczny z grupy III</skos:prefLabel>
<skos:definition>grupa III składa się z dwóch podgrup: grupy IIIb i grupy IIIa; grupa IIIa składa się ze skandu, itru i lantanu, przy którym uwzględnia się grupę lantanowców oraz aktynu, przy którym klasyfikuje się grupę aktynowców; grupa IIIb, która jest grupą główną, obejmuje bor, aluminium, gal, ind i tal</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2639">
<skos:prefLabel>pierwiastek chemiczny z grupy IV</skos:prefLabel>
<skos:definition>grupa IV składa się z dwóch podgrup: grupy IVb czyli grupy głównej i grupy IVa; grupa IVa składa się z tytanu, cyrkonu i hafnu, które klasyfikuje się jako metale przejściowe; główna grupa składa się z węgla, krzemu, germanu, cyny i ołowiu; podstawowa wartościowość pierwiastków wynosi IV, właściwości pierwiastków są rozbieżne: od niemetali do metali przesuwając się w dół grupy, również w tym kierunku wzrasta reakrywność pierwiastków, od węgla do ołowiu; wszystkie reagują z tlenem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2640">
<skos:prefLabel>pierwiastek chemiczny z grupy V</skos:prefLabel>
<skos:definition>grupa V składa się z dwóch podgrup: grupy Vb czyli grupy głównej i grupy Va; grupa Va składa się z wanadu, niobu i tantalu, które klasyfikuje się jako metale przejściowe; główna grupa składa się z azotu, fosforu, arsenu, antymonu i bizmutu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2641">
<skos:prefLabel>pierwiastek chemiczny z grupy VI</skos:prefLabel>
<skos:definition>grupa VI składa się z dwóch podgrup: grupy VIb czyli grupy głównej i grupy Via; grupa VIa składa się z chromu, molibdenu i wolframu; grupa główna składa się z tlenu, siarki, selenu, telluru i polonu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2642">
<skos:prefLabel>pierwiastek chemiczny z grupy VII</skos:prefLabel>
<skos:definition>pierwiastki rodziny fluorowców to: fluor, chlor, brom, jod i astat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2643">
<skos:prefLabel>pierwiastki chemiczne</skos:prefLabel>
<skos:definition>substancja składa się z atomów posiadających tę samą liczbę atomową, przykładem może być wodór, złoto i żelazo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2651">
<skos:prefLabel>emancypacja</skos:prefLabel>
<skos:definition>uwolnienie (się) od zależności w sferze społecznej i politycznej oraz moralnych i towarzyskich konwenansów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2653">
<skos:prefLabel>zarodek</skos:prefLabel>
<skos:definition>wczesne stadium rozwoju organizmów wielokomórkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2654">
<skos:prefLabel>embriogeneza</skos:prefLabel>
<skos:definition>proces rozwoju embrionalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2655">
<skos:prefLabel>regulacje prawne związane z nadzwyczajnymi zagrożeniami</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2656">
<skos:prefLabel>plan na wypadek nadzwyczajnego zagrożenia</skos:prefLabel>
<skos:definition>program procedur, które należy przedsięwziąść w razie nagłego, pilnego i zwykle nieoczekiwanego wydarzenia wymagającego natychmiastowego działania, w szczególności wydarzenia mogącego spowodować potencjalną szkodę dla życia ludzi, własności lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2657">
<skos:prefLabel>pomoc na wypadek awarii</skos:prefLabel>
<skos:definition>pieniądze, żywność lub inna pomoc dostarczana dla ocalałych z nagłej i zwykle nieoczekiwanej katastrofy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2658">
<skos:prefLabel>przedsięwzięcia na wypadek awarii</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2661">
<skos:prefLabel>schronisko dla uchodźców</skos:prefLabel>
<skos:definition>schronisko dla osób pozbawionych podstawowych warunków do życia w wyniku katastrofy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2663">
<skos:prefLabel>emisja</skos:prefLabel>
<skos:definition>zrzut gazów przemysłowych i pyłów unoszonych do atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2665">
<skos:prefLabel>ograniczanie strat wskutek emisji</skos:prefLabel>
<skos:definition>procedury mające na celu redukcję lub zapobieganie szkodom spowodowanym emisjami do powietrza atmosferycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2666">
<skos:prefLabel>dane dotyczące emisji</skos:prefLabel>
<skos:definition>dane dotyczące uwolnień zanieczyszczeń do środowiska z instalacji stacjonarnych i mobilnych lub z produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2667">
<skos:prefLabel>współczynnik emisyjności</skos:prefLabel>
<skos:definition>stosunek wielkości powstających zanieczyszczeń do masy przetworzonych surowców lub zużytego paliwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2668">
<skos:prefLabel>prognozowanie poziomu emisji</skos:prefLabel>
<skos:definition>ostatni krok kończący plan czystego powietrza mający za zadanie opracowanie prognozy jakości powietrza w kontekście dopuszczalnych standardów, w przypadku zastosowania przedsięwzięć przewidzianych planem; najpierw przeprowadza się inwentaryzację emisji oraz prognozę biorąc pod uwagę zmiany w gęstości zaludnienia, budownictwie mieszkaniowym, zatrudnieniu i transporcie; dane uzyskane z różnych źródeł porównuje się do wartości standardowych wprowdzając jednocześnie środki umożliwiające kontrolę; przewiduje się również planowane uruchomienie, modernizację lub zamknięcie dużych przedsiębiorstw; w wyniku inwentaryzacji otrzymuje się prognozę emisji, wyrażoną w tonach na dzień dla poszczególnej substancji w poszczególnych latach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2670">
<skos:prefLabel>redukcja emisji</skos:prefLabel>
<skos:definition>czyn lub proces zmniejszający lub ograniczający zrzut zanieczyszczeń; może to być ustalenie wartości dopuszczalnych emisji lub unowocześnienie źródeł emisji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2671">
<skos:prefLabel>finansowe aspekty ograniczenia emisji</skos:prefLabel>
<skos:definition>system rejestracji ograniczeń oszacowanych ilości emisji do atmosfery w celu ich stosowania w handlu emisjami w różnych układach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2672">
<skos:prefLabel>rejestr emisji</skos:prefLabel>
<skos:definition>spis zanieczyszczeń, według źródeł wielkości, wprowadzanych do atmosfery codziennie przez określoną społeczność</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2673">
<skos:prefLabel>sytuacja w zakresie emisji</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2674">
<skos:prefLabel>źródło emisji</skos:prefLabel>
<skos:definition>proces chemiczny, budowa, piec, fabryka lub inna jednostka odpowiedzialna za wprowadzanie zanieczyszczeń do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2677">
<skos:prefLabel>norma emisji</skos:prefLabel>
<skos:definition>maksymalna ilość zanieczyszczeń wprowadzanych do środowiska legalnie z pojedynczego źródła stacjonarnego lub mobilnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2679">
<skos:prefLabel>zrzut do wód</skos:prefLabel>
<skos:definition>zrzut stałych, ciekłych i gazowych zanieczyszczeń do zbiornika wodnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2683">
<skos:prefLabel>zatrudnienie</skos:prefLabel>
<skos:definition>ogólna liczba miejsc pracy lub osób pracujących w jakiejś jednostce administracyjnej lub w jakiejś dziedzinie gospodarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2684">
<skos:prefLabel>problemy zatrudnienia w aspekcie działań proekologicznych</skos:prefLabel>
<skos:definition>problemy i inicjatywy w zakresie tworzenia nowych stanowisk pracy łącznie z rozwojem pro-ekologicznych gałęzi przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2687">
<skos:prefLabel>skutki w zakresie poziomu zatrudnienia</skos:prefLabel>
<skos:definition>wpływ określonej polityki w regionie lub w kraju na liczbę osób w wieku aktywności zawodowej aktualnie zatrudnionych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2689">
<skos:prefLabel>emulgacja</skos:prefLabel>
<skos:definition>proces tworzenia koloidalnej zawiesiny cieczy w cieczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2691">
<skos:prefLabel>emulsja</skos:prefLabel>
<skos:definition>stabilna mieszanina dyspersyjna jednej cieczy nie mieszającej się w drugiej taka jak mleko (mieszanina tłuszczu w wodzie)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2693">
<skos:prefLabel>enkapsulacja</skos:prefLabel>
<skos:definition>otaczanie substancji zanieczyszczającej materiałem zapobiegajacym jej uwolnienie do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2696">
<skos:prefLabel>encyklopedia</skos:prefLabel>
<skos:definition>dzieło obejmujące zbiór wiadomości (w hasłach ułożonych alfabetycznie lub według zagadnień) ze wszystkich dziedzin wiedzy lub jednej dyscypliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2697">
<skos:prefLabel>gatunek zwierząt zagrożonych wyginięciem</skos:prefLabel>
<skos:definition>gatunki zwierząt, które są zagrożone wyginęciem z powodu zmian środowisku powstających naturalnie lub spowodwanych przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2700">
<skos:prefLabel>gatunek roślin zagrożonych wyginięciem</skos:prefLabel>
<skos:definition>gatunki roślin, które są zagrożone wyginęciem z powodu zmian środowisku powstających naturalnie lub spowodwanych przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2702">
<skos:prefLabel>gatunek zagrożony wyginięciem</skos:prefLabel>
<skos:definition>jeden z trzech stopni określających &quot;rzadkość&quot; gatunku wg kategoryzacji wprowadzonej przez Międzynarodową Unię Ochrony Zasobów Przyrodniczych (IUCN), o ile nie zastosuje się środków by wyeliminować przyczyny zagrożenia gatunki te mogą zniknąć z powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2704">
<skos:prefLabel>zagrożenie wód podziemnych</skos:prefLabel>
<skos:definition>zagrożenie dla jakości i ilości wód podziemnych spowodowane działalnością związaną z wykorzystaniem terenu; niektóre przedsięwzięcia (np.składowisko odpadów) powodują powstanie szczególnego ryzyka zanieczyszczenia środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2705">
<skos:prefLabel>system hormonalny</skos:prefLabel>
<skos:definition>chemiczny system koordynujący funkcjonowanie organizmów zwierzęcych, uczestniczą w nim gruczoły wydzielania wewnętrznego produkujące hormony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2706">
<skos:prefLabel>endokrynologia</skos:prefLabel>
<skos:definition>nauka o gruczołach wydzielania wewnętrznego i wytwarzanych przez nie hormonach oraz o chorobach spowodowanych zaburzeniami czynności tych gruczołów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2707">
<skos:prefLabel>technologia &quot;końca rury&quot;</skos:prefLabel>
<skos:definition>podejście do kontroli zanieczyszczeń koncentrujące się na oczyszczaniu na wylocie, przed zrzutem do środowiska; przeciwną opcją są zmiany w procesie mające na celu ograniczenie zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2712">
<skos:prefLabel>energia</skos:prefLabel>
<skos:definition>energia wytworzona w odniesieniu do energii cieplnej (ciepło), energii promieniowania (światło), energii kinetycznej (ruch), energii chemicznej; mierzona w dżulach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2713">
<skos:prefLabel>bilans energii</skos:prefLabel>
<skos:definition>stan energetyczny systemu w określonym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2714">
<skos:prefLabel>oszczędność energii</skos:prefLabel>
<skos:definition>strategia redukcji nakładu energii na jednostkę produkcji lub innych dóbr bez naruszania rozwoju socjoekonomicznego lub obniżania poziomu życia; w krajach umiarkowanie rozwiniętych większość energii wykorzystuje się na ogrzewanie i oświetlanie budynków przemysłowych i mieszkalnych; procesy przemysłowe, transport i rolnictwo są również głównym użytkownikiem energii; w latach 70-tych wykazano, że zasadnicze oszczędności można osiągnąć poprzez odpowiednie technologie budowlane oraz wykorzystanie energooszczędnych urządzeń do ogrzewania, wentylacji i oświetlenia; większość dóbr wytwarza się w taki sposób, aby ich wykorzystanie prowadziło do oszczędności energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2715">
<skos:prefLabel>zużycie energii</skos:prefLabel>
<skos:definition>ilość energii zużywanej przez pojedynczą osobę lub urządzenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2716">
<skos:prefLabel>przetwarzanie energii</skos:prefLabel>
<skos:definition>proces przemiany energii z jednej formy do innej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2718">
<skos:prefLabel>zapotrzebowanie na energię</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2720">
<skos:prefLabel>straty energetyczne</skos:prefLabel>
<skos:definition>każda strata energii głównie przy zamianie na ciepło</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2722">
<skos:prefLabel>system dystrybucji energii</skos:prefLabel>
<skos:definition>społeczny lub prywatny system rozdziału energii takiej jak energia elektryczna dostarczana do domów i przedsiębiorstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2723">
<skos:prefLabel>energetyka</skos:prefLabel>
<skos:definition>dział gospodarki obejmujący przetwarzanie, przesyłanie, gromadzenie i wykorzystywanie różnych rodzajów energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2724">
<skos:prefLabel>racjonalna gospodarka energetyczna</skos:prefLabel>
<skos:definition>oznacza działania polegające na oszczędzaniu paliw poprzez nowoczesne projektowanie budownictwa, modyfikację procesów produkcji, skuteczniejszą selekcję pojazdów drogowych oraz odpowiednią politykę transportową, zastosowanie planów sieci ciepłowniczej w połączeniu z wytwarzaniem energii elektrycznej, ocieplanie i instalowanie podwójnych okien w budynkach mieszkalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2729">
<skos:prefLabel>ustawodawstwo dotyczące energetyki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2730">
<skos:prefLabel>sterowanie energetyką</skos:prefLabel>
<skos:definition>administrowanie lub dostawa energii pochodzącej z takich źródeł jak: paliwa kopalne, elektryczność i energia słoneczna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2731">
<skos:prefLabel>rynek energetyczny</skos:prefLabel>
<skos:definition>handel lub obrót źródłami energii traktowanymi jako towar (takimi jak paliwa kopalne, elektryczność i energia słoneczna)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2733">
<skos:prefLabel>polityka energetyczna</skos:prefLabel>
<skos:definition>cele i zadania działalności państwa oraz metody realizacji tych zadań w sektorze energetycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2734">
<skos:prefLabel>proces energetyczny</skos:prefLabel>
<skos:definition>naturalne zjawisko lub szereg działań, dzięki którym przekształca się energię lub czyni się ją bardziej użyteczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2735">
<skos:prefLabel>wytwarzanie energii</skos:prefLabel>
<skos:definition>wytwarzanie energii w elektrowni węglowej, elektrowni opalanej ropą, elektrowni jądrowej itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2736">
<skos:prefLabel>odzysk energii</skos:prefLabel>
<skos:definition>forma odzysku zasobów polegająca na przekształcaniu organicznej frakcji odpadów do formy użytecznej energii; odzysk można osiągnąć w procesie spalania przetworzonych odpadów połączonego z wytwarzaniem pary, w procesie pirolizy z wytworzeniem paliwa ciekłego lub gazowego oraz poprzez fermentację beztlenową połączoną z wytwarzaniem metanu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2738">
<skos:prefLabel>zasoby energii</skos:prefLabel>
<skos:definition>potencjalne zasoby energii takie jak węgiel znajdujący się pod ziemią, energia słoneczna, energia wiatru, energia geotermalna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2740">
<skos:prefLabel>oszczędność energii</skos:prefLabel>
<skos:definition>unikanie zbytecznych strat energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2741">
<skos:prefLabel>źródło energii</skos:prefLabel>
<skos:definition>potencjalne źródła energii takie jak paliwo kopalne i paliwo nuklearne jak też energia słoneczna, wodna, energia wiatru, pływów, energia geotermalna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2742">
<skos:prefLabel>nośnik energii</skos:prefLabel>
<skos:definition>źródła, z których można pozyskać energię wykorzystując ją do wytwarzania ciepła, światła i mocy. Zasoby energetyczne takie jak paliwa kopalne i jądrowe, ale także energia słoneczna, hydroenergia, energia pływów oraz geotermalna mogą zostać ujęte lub odzyskane i przetworzone do innych form energii posiadającej szerokie zastosowanie w gospodarstwach domowych, handlu, transporcie i przemyśle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2744">
<skos:prefLabel>zapasy energii</skos:prefLabel>
<skos:definition>zasoby niewykorzystanej energii; często oznacza zapas nieodnawialnego paliwa na przykład ropy naftowej będącego w posiadaniu jakiegoś kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2746">
<skos:prefLabel>zaopatrzenie w energię</skos:prefLabel>
<skos:definition>zaopatrzenie i magazynowanie energii lub ilość zmagazynowanej energii przeznaczonej do wykorzystania przez różnych użytkowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2747">
<skos:prefLabel>technologia energetyczna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2748">
<skos:prefLabel>rodzaj energii</skos:prefLabel>
<skos:definition>stosownie do źródła energię można sklasyfikować jako hydroenergię, energię słoneczną, energię pływów, wiatrową, fal, energię geotermalną itp.. Stosowanie do rodzaju paliwa wykorzystanego do jej wytwarzania energię można sklasyfikować jako jądrową, pochodzącą ze spalania węgla, ropy naftowej, biomasy itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2749">
<skos:prefLabel>wykorzystanie energii</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2751">
<skos:prefLabel>struktura wykorzystania energii</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2753">
<skos:prefLabel>wprowadzanie w życie</skos:prefLabel>
<skos:definition>wprowadzanie w życie, egzekucja prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2754">
<skos:prefLabel>silnik</skos:prefLabel>
<skos:definition>urządzenie przetwarzające na energię mechaniczną inny rodzaj energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2755">
<skos:prefLabel>polityka umocnienia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2756">
<skos:prefLabel>program umocnienia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2758">
<skos:prefLabel>uran wzbogacony</skos:prefLabel>
<skos:definition>uran, w którym stężenie uranu-235 jest wystarczające aby podtrzymać reakcję łańcuchową, wzbogacanie następuje poprzez usuwanie uranu-238</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2759">
<skos:prefLabel>wzbogacenie</skos:prefLabel>
<skos:definition>proces polegający na zwiększeniu ilości określonego izotopu w mieszaninie izotopów; zwykle wykorzystuje się go do zwiększenia ilości U-235 lub dodatkowo Pu-239 w naturalnym uranie w celu wykorzystania go w reaktorach jądrowych lub do produkcji broni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2760">
<skos:prefLabel>dostateczne zaopatrzenie w artykuły żywnościowe</skos:prefLabel>
<skos:definition>dostateczne zaopatrzenie w artykuły żywnościowe odpowiednie do zapotrzebowania społecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2766">
<skos:prefLabel>katastrofa ekologiczna</skos:prefLabel>
<skos:definition>nieoczekiwane wydarzenie, awaria lub szkoda mogące wpłynąć niekorzystnie na ekosystem lub zasoby naturalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2767">
<skos:prefLabel>rachunek ekonomiczny uwzględniający koszty ekologiczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2769">
<skos:prefLabel>doradztwo w zakresie ochrony środowiska</skos:prefLabel>
<skos:definition>konsultacje i zalecenia, przygotowanie wytycznych w zakresie ochrony stanu środowiska i zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2771">
<skos:prefLabel>ekologiczna troska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2772">
<skos:prefLabel>architektura przyjazna środowisku</skos:prefLabel>
<skos:definition>architektura przyjazna środowisku opiera się o pięć zasad: 1) zdrowe środowisko wewnętrzne, 2) efektywność użytkowania energii, 3) materiały ekologicznie przyjazne, 4) formy zgodne ze środowiskiem, 5) prawidłowy projekt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2773">
<skos:prefLabel>ekologiczny aspekt osadnictwa</skos:prefLabel>
<skos:definition>siedliska ludzkie wywierają negatywny wpływ na ekosystemy i na siebie same poprzez wprowadzenie toksycznych lub niebezpiecznych substancji do  litosfery, hydrosfery i atmosfery; głównymi rodzajami zanieczyszczeń środowiskowych są: ścieki kanalizacyjne, metale śladowe, węglowodory petrochemiczne, syntetyczne związki organiczne oraz emisje gazowe; większość, o ile nie wszystkie, powstają w wyniku wzrostu populacji oraz postępu technicznego w społeczeństwach industrialnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2774">
<skos:prefLabel>ocena środowiska</skos:prefLabel>
<skos:definition>oszacowanie lub określenie zasobów środowska przyrodniczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2775">
<skos:prefLabel>kryteria oceny środowiska</skos:prefLabel>
<skos:definition>zasady lub standardy wykorzystywane do oszacowania lub określenia zasobów środowska przyrodniczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2776">
<skos:prefLabel>przegląd środowiskowy</skos:prefLabel>
<skos:definition>określenie rodzaju i rozmiaru ujemnych wpływów i szkód w środowisku (rzeczywistych lub potencjalnych) powodowanych przez różne działania, realizację programów rozwoju, użytkowanie produktu lub przez sam produkt, substancję chemiczną bądź odpady; przeglądy mogą dotyczyć zgodności z wymogami prawa, skuteczności systemów kontroli środowiska, ryzyka lub potrzeby podjęcia działań naprawczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2778">
<skos:prefLabel>świadomość ekologiczna</skos:prefLabel>
<skos:definition>wzrost świadomości, zrozumienia i wyczucia środowiska i związanych z nim problemów, &quot;ekologiczny sposób myślenia&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2779">
<skos:prefLabel>bilans ekologiczny</skos:prefLabel>
<skos:definition>główna część badań wpływu na środowisko, oszacowanie porównujące koszty związane z korzystaniem ze środowiska z korzyściami na bazie jednolitych kryteriów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2782">
<skos:prefLabel>zmiana środowiskowa</skos:prefLabel>
<skos:definition>zmiany, jakie mogą zaistnieć w ekosystemach, klimacie, glebie, siedliskach itp. z powodu presji najróżniejszych czynników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2783">
<skos:prefLabel>ustawodawstwo dotyczące chemikaliów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2784">
<skos:prefLabel>chemia środowiska</skos:prefLabel>
<skos:definition>nauka badająca procesy fizyczne, chemiczne i biochemiczne, którym podlegają zanieczyszczenia wprowadzone do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2786">
<skos:prefLabel>ekologizacja produkcji</skos:prefLabel>
<skos:definition>warunki produkcji oraz programów działań, zakładające ograniczenie wpływu na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2789">
<skos:prefLabel>skutek ekologiczny</skos:prefLabel>
<skos:definition>wynik naturalnych lub spowodowanych przez człowieka zakłóceń w funkcjonowaniu elementów środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2790">
<skos:prefLabel>ochrona środowiska</skos:prefLabel>
<skos:definition>ogół działań ukierunkowanych na utrzymanie i zachowanie tych właściwości środowiska przyrodniczego i zurbanizowanego, które są ważne dla zdrowia fizycznego i psychicznego człowieka oraz dla stanu środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2793">
<skos:prefLabel>procedura działań na wypadek zagrożenia środowiska</skos:prefLabel>
<skos:definition>przygotowanie programowo zorganizowanej procedury działań, które należy przedsięwziąć w wypadku sytuacji awaryjnej, klęski lub zagrożenia dla środowiska, zdrowia ludzi i szkód w zasobach przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2795">
<skos:prefLabel>kontrola ochrony środowiska</skos:prefLabel>
<skos:definition>wykonywanie ochrony środowiska poprzez wdrażanie polityki dotyczącej ograniczania ilości odpadów, poprawy środowiska antropogenicznego, ochrony wartości kulturowych, powoływania parków narodowych i ustanawiania ezerwatów, ochrony flory i fauny, ochrony lasów i krajobrazów, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2796">
<skos:prefLabel>koszty środowiskowe</skos:prefLabel>
<skos:definition>koszty ponoszone w wyniku pogorszenia jakości środowiska, np. przez przedsiębiorstwo, które  wdraża program oczyszczania lub poprawy sytuacji lub przez całe społeczeństwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2797">
<skos:prefLabel>przestępstwo przeciwko środowisku</skos:prefLabel>
<skos:definition>bezprawne działania przciew środowisku, w tym zanieczyszczanie wód, składowanie odpadów niebezpiecznych, zanieczyszczanie powietrza, budowę fabryk bez zezwoleń, rozlewy ropy naftowej, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2798">
<skos:prefLabel>prawo karne z zakresu środowiska</skos:prefLabel>
<skos:definition>zbiór przepisów dotyczących działań lub przypadków zaniedbania ekologicznego uznanych za szkodliwe dla społeczeństwa lub interesów władz i prawnie zabronionych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2799">
<skos:prefLabel>kryteria środowiskowe</skos:prefLabel>
<skos:definition>wartości standardowe fizyczne, chemiczne lub biologiczne (czasami również kryteria społeczne i estetyczne itp.) określające jakość środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2801">
<skos:prefLabel>kultywowanie wartości ekologicznych</skos:prefLabel>
<skos:definition>suma wyuczonych zachowań, postaw, praktyki i wiedzy społecznej w odniesieniu do problemu zachowania i ochrony zasobów przyrodniczych, ekosystemów oraz całości warunków zewnętrznych wpływających na życie ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2802">
<skos:prefLabel>szkoda poniesiona przez środowisko</skos:prefLabel>
<skos:definition>szkoda dla środowiska np. utrata terenów podmokłych, zanieczyszczenie rzek itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2803">
<skos:prefLabel>dane dotyczące środowiska</skos:prefLabel>
<skos:definition>informacja określająca stan środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2805">
<skos:prefLabel>rozwój środowiskowy</skos:prefLabel>
<skos:definition>wzrost, postęp lub rozwój w sprawach związanych z ekologią</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2810">
<skos:prefLabel>zagadnienie związane z ekonomiką środowiska</skos:prefLabel>
<skos:definition>problemy o znaczeniu społecznym obejmujące zarówno gospodarkę finansową jak i ochronę zasobów przyrodniczych w skali państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2811">
<skos:prefLabel>ekonomika środowiska</skos:prefLabel>
<skos:definition>wyspecjalizowana gałąź ekonomiki obejmująca zgadnienia ograniczania emisji i ochrony środowiska, których koszty z trudnością poddają się wycenom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2814">
<skos:prefLabel>edukacja ekologiczna</skos:prefLabel>
<skos:definition>proces kształcenia dotyczący problemu wzajemnych zależności między człowiekiem a środowiskiem, stosujący podejście interdyscyplinarne w rozwiązywaniu problemów oraz wprowadzający odpowiedni system wartości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2817">
<skos:prefLabel>przedsięwzięcie ekologiczne</skos:prefLabel>
<skos:definition>organizacje specjalizujące się w poradnictwie w zakresie ochrony środowiska, np. remediacja zanieczyszczonych gruntów, oczyszczanie powietrza, oceny oddziaływania na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2818">
<skos:prefLabel>etyka ekologiczna</skos:prefLabel>
<skos:definition>ekologiczna świadomość obejmująca przekonania o potrzebie odpowiedzialności za stan środowiska i jego zasobów oraz los obecnych i przyszłych pokoleń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2821">
<skos:prefLabel>fundusze ekologiczne</skos:prefLabel>
<skos:definition>zasoby finansowe z przeznaczeniem na pokrywanie wydatków związanych z pro-ekologicznym zagospodarowaniem i ochroną zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2823">
<skos:prefLabel>zagrożenia środowiska naturalnego</skos:prefLabel>
<skos:definition>czynniki fizyczne lub chemiczne mogące spowodować szkody w ekosystemach lub zasobach przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2824">
<skos:prefLabel>stan zdrowotny środowiska</skos:prefLabel>
<skos:definition>sztuka i nauka w zakresie ochrony zdrowia, promowania wartości estetycznych, zapobiegania chorobom i urazom, wspierające się o pozytywne czynniki środowiskowe z jednoczesnym dążeniem do eliminowania zagrożeń chemicznych, fizycznych, biologicznych i radiologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2825">
<skos:prefLabel>zagrożenie stanu zdrowotnego środowiska</skos:prefLabel>
<skos:definition>czynniki fizyczne lub chemiczne mogące negatywnie oddziaływać na wzajemne zależności między ludźmi a środowiskiem i zagrażające bytowi ludzi i jakości zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2826">
<skos:prefLabel>ocena oddziaływania środowiska na zdrowie</skos:prefLabel>
<skos:definition>oszacowanie oddziaływań powodowanych przez określone działanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2827">
<skos:prefLabel>ochrona stanu zdrowotnego środowiska</skos:prefLabel>
<skos:definition>środki i urządzenia służące ochronie zdrowia ludzi oraz jakości środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2828">
<skos:prefLabel>historia zmian stanu środowiska</skos:prefLabel>
<skos:definition>systematyczny i historyczny opis zmian zewnętrznych czynników środowiska wpływających na warunki życia ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2829">
<skos:prefLabel>oddziaływanie na środowisko</skos:prefLabel>
<skos:definition>zmiana stanu środowiska lub stworzenie nowych warunków środowiskowych na skutek podejmowanych działań, najczęsciej inwestycyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2830">
<skos:prefLabel>ocena oddziaływania na środowisko</skos:prefLabel>
<skos:definition>analiza i oszacowanie wpływu na środowisko przyrodnicze, gospodarcze i społeczne zamierzonego działania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2831">
<skos:prefLabel>oddziaływanie rolnictwa na środowisko</skos:prefLabel>
<skos:definition>rolnictwo wywiera istotny wpływ na jakość wód, zwiększa odpływ sedymentów w ciekach wskutek erozji, zwiększa zanieczyszczenie odpływu przez substancje chemiczne zawarte w środkach ochrony roślin,  nawozach, itp. wywołując niekorzystne zmiany w ekosystemach wodnych i lądowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2832">
<skos:prefLabel>oddziaływanie akwakultury na środowisko</skos:prefLabel>
<skos:definition>hodowla ryb (akwakultura) prowadzi do zanieczyszczenia wód składnikami odżywczymi, np. wapniem, fosforem i azotem, a także metanem i siarkowodorem, które są toksyczne dla ryb i innych organizmów wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2833">
<skos:prefLabel>oddziaływanie energetyki na środowisko</skos:prefLabel>
<skos:definition>energia i problemy środowiskowe są ściśle ze sobą powiązane, z uwagi na fak, że prawie niemożliwe jest wytwarzanie, przesyłanie i wykorzystanie energii bez istotnego wpływu na środowisko; związane  z tym problemy środowiskowe obejmują zanieczyszczenie powietrza, wody, usuwanie stałych odpadów oraz ciepła odpadowego; emisja zanieczyszczeń ze spalania paliw kopalnych jest główną przyczyną zanieczyszczenia powietrza w miastach, zanieczyszczenie wód związane jest w wykorzystaniem energii, a jednym z podstawowych problemów są wycieki ropy naftowej; przy wszystkich operacjach z substancjami ropopochodnymi istnieje skończone prawdopodobieństwo wycieków do gruntu lub do zbiorników wodnych; również górnictwo powoduje zanieczyszczenie wód, a zmiany warunków hydrologicznych wskutek działalności górniczej powodują często napływ czystych wód do miejsc gdzie ulegają zanieczyszczeniu, ponadto, przy wykorzystaniu niektórych form energii powstają niepożądane materiały - odpady stałe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2834">
<skos:prefLabel>oddziaływanie rybołówstwa na środowisko</skos:prefLabel>
<skos:definition>rybołówstwo może wpływać szkodliwie na środowisko z powodu np.: ścieków z gospodarstw rybnych, niezamierzonych przyłowów gatunków, w tym gatunków chronionych, stosowania technik rybołówstwa przemysłowego (trałowania itp.), nielegalnych połowów z użyciem cyjanku i dynamitu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2835">
<skos:prefLabel>oddziaływanie leśnictwa na środowisko</skos:prefLabel>
<skos:definition>oszacowanie bezpośrednich i pośrednich skutków gospodarki leśnej dla środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2836">
<skos:prefLabel>oddziaływanie gospodarstw domowych na środowisko</skos:prefLabel>
<skos:definition>oddziaływania objemują emisje z instalacji ogrzewania, zrzut ścieków, odpady, np. emisje gorącego powietrza, ditlenek węgla, tlenek węgla, para wodna i tlenek azotu, ścieki domowe, odpady gospodarstwa domowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2837">
<skos:prefLabel>oddziaływanie przemysłu na środowisko</skos:prefLabel>
<skos:definition>oddziaływania wynikające z prowadzenia działalności przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2838">
<skos:prefLabel>oddziaływanie rekreacji na środowisko</skos:prefLabel>
<skos:definition>turystyka i rekreacja są często źródłem poważnych szkód w środowisku, szczególnie z uwagi na miejsce lub skalę zjawiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2839">
<skos:prefLabel>oddziaływanie turystyki na środowisko</skos:prefLabel>
<skos:definition>turystyka i rekareacja są często źródłem poważnych szkód w środowisku, szczególnie z uwagi na miejsce lub skalę zjawiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2840">
<skos:prefLabel>oddziaływanie transportu na środowisko</skos:prefLabel>
<skos:definition>transpot odziaływuje na jakość powietrza i klimat akustyczny oraz w znacznym stopniu przyczynia się do fragmentacji ekosystemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2841">
<skos:prefLabel>raport oceny oddzia lywania na środowisko</skos:prefLabel>
<skos:definition>raport, w którym zawarto analizę oddziaływania na środowisko danej inwestycji oraz zaproponowano środki łagodzące te oddziaływania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2844">
<skos:prefLabel>studium oddziaływania na środowisko</skos:prefLabel>
<skos:definition>studium mające na celu stwierdzenie warunków na określonym obszarze przed rozpoczęciem inwestycji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2845">
<skos:prefLabel>bodźce na rzecz ochrony środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2847">
<skos:prefLabel>wskaźnik środowiskowy</skos:prefLabel>
<skos:definition>wynik pomiaru, cecha lub wartość statystyczna, które w sposób przybliżony świadczą o wpływie programów wdrażania gospodarki sprzyjającej ochronie środowiska na jego stan</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2848">
<skos:prefLabel>indeks produktów związanych z ochroną środowiska</skos:prefLabel>
<skos:definition>spis produktów związanych z ochroną środowiska wyprodukowanych od 1972 r, znany jako Roczny Spis Produktów dot. Ochrony Środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2849">
<skos:prefLabel>przemysł chroniący środowisko</skos:prefLabel>
<skos:definition>gałęzie przemysłu rozwijające tzw. &apos;czyste technologie&quot;, zajmujące się m.in.. unieszkodliwianiem odpadów, ich recyklingiem, biotechnologią, ograniczaniem hałasu oraz produkcją dóbr związanych z ochroną środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2850">
<skos:prefLabel>ekoinformatyka</skos:prefLabel>
<skos:definition>zastosowanie technik cyfrowych do gromadzenia i przetwarzania ogólnych danych ekologicznych lub danych dotyczących wybranych ekosystemów, będących efektem monitorowania zjawisk i procesów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2851">
<skos:prefLabel>informacja o środowisku</skos:prefLabel>
<skos:definition>dział nauki zajmujący się opracowywaniem i rozpowszechnianiem danych naukowych dotyczących aktualnego stanu wiedzy w dziedzinie środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2852">
<skos:prefLabel>sieć informacyjna o środowisku</skos:prefLabel>
<skos:definition>system utworzony przez zbiory informacyjne (bazy danych lub systemy informacyjno-wyszukiwawcze), pomiędzy którymi występują określone relacje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2853">
<skos:prefLabel>system informacji o środowisku</skos:prefLabel>
<skos:definition>system informacyjno-wyszukiwawczy, którego zbiorem informacyjnym są zazwyczaj dokumenty (zbiór informacji źródłowej) z dziedziny środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2856">
<skos:prefLabel>inwestycja ochrony środowiska</skos:prefLabel>
<skos:definition>papiery wartościowe jako potencjalne źródło zysku w formie odsetek i dywidend z przenaczeniem na inwestycje w zakresie ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2857">
<skos:prefLabel>determinizm środowiskowy</skos:prefLabel>
<skos:definition>1) troska o środowisko i jego ochronę, 2) teoria podkreślająca podstawowe znaczenie wpływu środowiska na rozwój grupowy lub osobniczy; budowa ciała zwierząt i człowieka oraz zachowania, zgodnie w/w teorią, zależą w dużym stopniu od środowiska fizycznego, biologicznego, psychologicznego lub kulturowego; na podłożu determinizmu środowiskowego wyrosły liczne Partie Zielonych, które stawiają sobie za zadanie &quot;ochronę planety i ludzkości&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2859">
<skos:prefLabel>prawo ochrony środowiska</skos:prefLabel>
<skos:definition>szerokie spektrum opcji legislacyjnych dotyczących ochrony środowiska, od praw rygorystycznych, takich jak porozumienia międzynarodowe i ustawy państwowe do praw mniej rygorystycznych, takich jak zasady, wytyczne, zalecane praktyki lub procedury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2860">
<skos:prefLabel>egzekwowanie prawa ochrony środowiska</skos:prefLabel>
<skos:definition>działalność związana z egzekwowaniem praw i wymuszaniem posłuszeństwa wobec obowiązujących przepisów w zakresie zachowania stanu środowiska, ochrony zasobów przyrodniczych i zdrowia ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2862">
<skos:prefLabel>ustawodawstwo z zakresu ochrony środowiska</skos:prefLabel>
<skos:definition>dziedzina prawa obejmująca problematykę ochrony środowiska, np. ograniczanie emisji, ochrona przyrody i różnorodności biologicznej, bezpieczeństwo środowiskowe, higiena pracy, planowanie ekologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2863">
<skos:prefLabel>ustawodawstwo z zakresu środowiska dotyczące rolnictwa</skos:prefLabel>
<skos:definition>zbiór przepisów ustanowionych przez władze w celu uregulowania wszelkich aspektów produkcji gospodarstw rolnych i produkcji zwierzęcej, które niosą ryzyko zagrożenia jakości środowiska i zdrowia ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2864">
<skos:prefLabel>proces legislacyjny w zakresie prawa ochrony środowiska</skos:prefLabel>
<skos:definition>przebieg prac w postępowaniu legislacyjnym zmierzający do ustanowienia prawa w zakresie ochrony zasobów środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2865">
<skos:prefLabel>odpowiedzialność za środowisko</skos:prefLabel>
<skos:definition>kary uiszczane przez podmioty z tytułu szkód spowodowanych w środowisku przez zanieczyszczenia oraz ich usuwanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2866">
<skos:prefLabel>zezwolenie dotyczące środowiska</skos:prefLabel>
<skos:definition>zezwolenie władz regulujące poziomy emisji zakładów produkcyjnych, handlowych lub usługowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2869">
<skos:prefLabel>substancja niebezpieczna dla środowiska</skos:prefLabel>
<skos:definition>substancje powodujące niepożądane zmiany fizyczne, chemiczne lub biologiczne powietrza, wody lub powierzchni ziemi, które mogą spowodować uszczerbek zdrowia oraz wpłynąć na możliwość przeżycia i działalność ludzi oraz innych organizmów żywych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2870">
<skos:prefLabel>procedury produkcji przyjazne środowisku</skos:prefLabel>
<skos:definition>procesy uzyskiwania produktów oraz usług służących ochronie środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2871">
<skos:prefLabel>produkt przyjazny dla środowiska</skos:prefLabel>
<skos:definition>produkt, który nie jest szkodliwy dla środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2872">
<skos:prefLabel>zarządzenie przyjazne dla środowiska</skos:prefLabel>
<skos:definition>wdrożenie praktyk zintegrowanego gospodarowania w środowisku w celu ograniczenia wpływu działalności przemysłowej i handlowej na środowisko, należy tu m.in. analiza cyklu życia produktów, wdrażanie czystych technologii i minimalizacja produkcji odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2873">
<skos:prefLabel>choroba związana ze stanem środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2874">
<skos:prefLabel>zachowanie przyjazne środowisku</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2876">
<skos:prefLabel>przedsiębiorstwo nie spełniające wymogów ochrony środowiska</skos:prefLabel>
<skos:definition>firmy, które nie spełniają wymagań zawartych w przepisach z zakresu ochrony środowiska, a dotyczących emisji szkodliwych substancji odpadowych, jakie  powstają w cyklu produkcyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2877">
<skos:prefLabel>zarzadzanie środowiskiem</skos:prefLabel>
<skos:definition>środki i systemy kontroli ukierunkowane na wdrażanie ochrony środowiska, racjonalny rozdział i użytkowanie zasobów przyrodniczych, optymalizację wzajemnych zależności między społeczeństwem a środowiskiem oraz poprawę bytu obecnych i przyszłych pokoleń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2880">
<skos:prefLabel>medycyna ekologiczna</skos:prefLabel>
<skos:definition>sztuka i nauka w zakresie ochrony zdrowia, promowania wartości estetycznych, zapobiegania chorobom i urazom, wspierająca się o pozytywne czynniki środowiskowe z jednoczesnym dążeniem do eliminowania zagrożeń chemicznych, fizycznych, biologicznych i radiologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2881">
<skos:prefLabel>powodowanie szkód w środowisku</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2882">
<skos:prefLabel>monitoring środowiska</skos:prefLabel>
<skos:definition>okresowe oraz/lub ciągłe pomiary, oceny oraz oznaczenia parametrów stanu środowiska i stopnia zanieczyszczenia jako podstawa do działań ukierunkowanych na zapobiegania negatywnych i szkodliwych skutków w środowisku; także prognozowanie zmian w ekosystemach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2884">
<skos:prefLabel>hałas przenikający do środowiska</skos:prefLabel>
<skos:definition>dźwięki i właściwości dźwięków pochodzących z różnych źródeł w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2886">
<skos:prefLabel>postrzeganie środowiska</skos:prefLabel>
<skos:definition>intuicyjne rozpoznawanie lub rozumienie pojęcia ekosystemu wraz z zasobami przyrodniczymi, zwykle w oparciu o ludzkie doświadczenia, kulturę lub wierzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2887">
<skos:prefLabel>ocena działań środowiskowych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2889">
<skos:prefLabel>planowanie środowiskowe</skos:prefLabel>
<skos:definition>wskazanie celów w zakresie pożądanego stanu środowiska fizycznego, a także celów społeczno-gospodarczych, oraz stworzenie procedur administracyjnych, jakie należy zastosować by cele te osiagnąć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2890">
<skos:prefLabel>plan ochrony zasobów środowiska</skos:prefLabel>
<skos:definition>ustalone lub usystematyzowane metody ochrony zasobów środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2892">
<skos:prefLabel>polityka środowiskowa</skos:prefLabel>
<skos:definition>oficjalne stwierdzenia zasad, intencji, deklarowanych wartości oraz celów sformułowane w oparciu o obowiązujące prawa i wolę władz państwowych, mające znaczenie jako dokument wiodący w zakresie działania na rzecz ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2893">
<skos:prefLabel>instrument polityki środowiskowej</skos:prefLabel>
<skos:definition>środki technologiczne, ekonomiczne oraz prawne stosowane w celu kontroli (ograniczenia) zanieczyszczenia lub szkód w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2895">
<skos:prefLabel>zanieczyszczenie środowiska</skos:prefLabel>
<skos:definition>wprowadzanie do środowiska, w wyniku działań człowieka, substancji lub energii, które mogą spowodować zagrożenie zdrowia ludzi, szkody w zasobach przyrodniczych i układach ekologicznych, uszkodzenia infrastruktury albo mogą kolidować z prawidłowymi sposobami użytkowania środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2896">
<skos:prefLabel>priorytet ekologiczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2897">
<skos:prefLabel>program ekologiczny</skos:prefLabel>
<skos:definition>zorganizowane działania, zwykle prowadzone przez agencję rządową lub organizację typu &quot;non-profit&quot;, ukierunkowane na ochronę zasobów środowiska oraz wdrażanie rozwiązań pro-ekologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2900">
<skos:prefLabel>ochrona środowiska</skos:prefLabel>
<skos:definition>środki i sposoby kontroli stosowane w celu zapobieżenia szkodom środowiskowym i degradacji środowiska, a także w celu zapewnienia trwałości zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2901">
<skos:prefLabel>agencja ochrony środowiska</skos:prefLabel>
<skos:definition>agencja Ochrony Środowiska (EPA) w USA jest agencją nadzorującą działania w zakresie kontroli zanieczyszczenia powietrza i wody, zagrozeń związanych z pestycydami i promieniowaniem oraz zanieczyszczenia hałasem; Agencja prowadzi także badania skutków zanieczyszczenia środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2902">
<skos:prefLabel>stowarzyszenie ochrony środowiska</skos:prefLabel>
<skos:definition>stowarzyszenia, których statutowe cele dotyczą ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2903">
<skos:prefLabel>koszty ochrony środowiska</skos:prefLabel>
<skos:definition>nakłady na działania w zakresie ochrony zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2905">
<skos:prefLabel>ochrona środowiska w przedsiębiorstwie</skos:prefLabel>
<skos:definition>środki ostrożności, działania, procedury postępowania lub urządzenia stosowane przez jednostki poza-rządowe, handlowe lub przemysłowe, mające na celu zapobieganie lub ograniczenie szkód w ekosystemach i zagrożenia dla zdrowia ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2907">
<skos:prefLabel>decyzja dotycząca ochrony środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2908">
<skos:prefLabel>organizacja ochrony środowiska</skos:prefLabel>
<skos:definition>rządowa agencja, komitet lub grupa osób odpowiedzialnych za zabepieczenie trwałości zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2909">
<skos:prefLabel>rozporządzenie dotyczące ochrony środowiska</skos:prefLabel>
<skos:definition>roporządzenie władz rządowych lub zarządzających (np. obszarem), dotyczące ochrony zasobów przyrodniczych i zapobiegania degradacji ekosystemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2910">
<skos:prefLabel>technologia ochrony środowiska</skos:prefLabel>
<skos:definition>technologie spełniające wymogi ochrony środowiska poprzez włączenie do swoich procedur koncepcji zapobiegania szkodliwym skutkom zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2911">
<skos:prefLabel>psychologia ekologiczna</skos:prefLabel>
<skos:definition>gałąź eksperymentalnej psychologii badającej zależności między zachowaniami z kontekstem środowiskowym, w którym one występują</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2912">
<skos:prefLabel>jakość środowiska</skos:prefLabel>
<skos:definition>właściwości i charakterystyki środowiska w skali ogólnej lub lokalnej w wymiarze ich oddziaływania na ludzi i inne organizmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2913">
<skos:prefLabel>kryteria ochrony środowiska</skos:prefLabel>
<skos:definition>kryteria służące do ustalania norm narażenia na zanieczyszczenia środowiska oraz hałas, w odniesieniu do pestycydów, detergentów, składu ścieków, usuwania odpadów handlowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2914">
<skos:prefLabel>cel ochrony środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2915">
<skos:prefLabel>norma jakości środowiska</skos:prefLabel>
<skos:definition>normatywne dokumenty i wytyczne określające (kwantyfikujące) poziomy warunków środowiska oraz wymogi co do stosowania środków pozwalających na uniknięcie szkodliwych i destrukcyjnych skutków, wpływów i następstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2917">
<skos:prefLabel>raport dotyczący stanu środowiska</skos:prefLabel>
<skos:definition>pisemne sprawozdanie lub oświadczenie zawierające opis zdarzeń, sytuacji lub warunków charakteryzujących dany ekosystem wraz z zasobami przyrodniczymi lub opis innych czynników zewnętrznych mających wpływ na życie ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2918">
<skos:prefLabel>badanie środowiska</skos:prefLabel>
<skos:definition>badania stanu środowiska oraz jego przekształceń spowodowanych przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2920">
<skos:prefLabel>ocena ryzyka środowiskowego</skos:prefLabel>
<skos:definition>jakościowa i ilościowa ocena ryzyka, na jakie narażone jest środowisko z uwagi na aktualne lub potencjalne występowanie w nim substancji zanieczyszczających oraz/lub z powodu ich stosowania przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2921">
<skos:prefLabel>ryzyko środowiskowe</skos:prefLabel>
<skos:definition>prawdopodobieństwo uszkodzenia ciała, choroby lub śmierci wskutek narażenia na potencjalne ryzyko wynikające z zanieczyszczenia środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2922">
<skos:prefLabel>nauki stosowane z zakresu ochrony środowiska</skos:prefLabel>
<skos:definition>badania interdyscyplinarne w zastosowaniu do problemów ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2923">
<skos:prefLabel>bezpieczeństwo ekologiczne</skos:prefLabel>
<skos:definition>środki techniczne i instrumenty polityczne wdrażane w celu ochrony oraz promowania bezpiecznego środowiska życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2924">
<skos:prefLabel>bank okazów ekologicznych</skos:prefLabel>
<skos:definition>miejsca, w których przechowuje się wybrane okazy, np. ryby, muszle, mleko, próby glebowe, ludzkie tkanki, itp. w stanie utrwalonym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2925">
<skos:prefLabel>raport środowiskowy (eko-audyt)</skos:prefLabel>
<skos:definition>ocena dotycząca zysków finansowych oraz strat wynikających z przyjęcia bardziej pro-ekologicznej polityki w określonej firmie lub organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2926">
<skos:prefLabel>statystyka środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2927">
<skos:prefLabel>giełda ochrony środowiska</skos:prefLabel>
<skos:definition>kupno, sprzedaż lub wymiana towarów związanych ochroną środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2928">
<skos:prefLabel>dotacje na rzecz środowiska</skos:prefLabel>
<skos:definition>rządowe wsparcie finansowe na rzecz działań pro-ekologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2929">
<skos:prefLabel>obiekt ekologiczny</skos:prefLabel>
<skos:definition>cenne składniki środowiska, które mogą zostać przekształcone w wypadku realizacji określonej inwestycji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2931">
<skos:prefLabel>edukacja ekologiczna</skos:prefLabel>
<skos:definition>szkolenie, nauczanie i przekazywanie wiedzy na temat warunków środowiska wpływających na jakość życia, rozwoju i przeżywalności organizmów oraz zagrożeń dla ekosystemów i sposobów trwałego podtrzymywania ich jakości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2932">
<skos:prefLabel>technologia ekologiczna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2934">
<skos:prefLabel>terminologia z zakresu ekologii</skos:prefLabel>
<skos:definition>słownik obejmujący terminy techniczne oraz uzus językowy w zastosowaniu odpowiednio do poziomu określonych grup społecznych zainteresowanych ochroną zasobów naturalnych, zachowaniu ekosystemów oraz zdrowia ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2936">
<skos:prefLabel>szkolenie w zakresie ekologii</skos:prefLabel>
<skos:definition>szkolenie specjalistów oraz wykwalifikowanych pracowników w zakresie rozwiązywania problemów środowiskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2938">
<skos:prefLabel>ocena ekonomiczna działań na rzecz środowiska</skos:prefLabel>
<skos:definition>zwartościowanie, oszacowanie, ocena wyników działania przedsiębiorstwa w przedmiocie ochrona środowiska i finanse</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2940">
<skos:prefLabel>wandalizm środowiskowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2941">
<skos:prefLabel>wojna ekologiczna</skos:prefLabel>
<skos:definition>bezpośrednia manipulacja lub niszczenie zasobów środowiskowych stosowane jako groźba polityczna lub w celu osiągnięcia przewagi militarnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2943">
<skos:prefLabel>przyjazny dla środowiska</skos:prefLabel>
<skos:definition>działania, przedsięwzięcia lub produkty, które sprzyjają utrzymaniu odpowiedniego stanu lub jakości środowiska, nie prowadząc do jego degradacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2944">
<skos:prefLabel>środowisko</skos:prefLabel>
<skos:definition>pojęcie obejmujące wszystkie aspekty otoczenia życia ludzi, wpływające na jednostki i społeczności; UE zdefiniowała środowisko jako&quot;kombinację elementów, których skomplikowane powiązania składają się na miejsce,okoliczności, otoczenie oraz warunki życia osób i społeczności&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2945">
<skos:prefLabel>enzymy</skos:prefLabel>
<skos:definition>białka o właściwościach biokatalitycznych, wytwarzane przez każdy żywy organizm, regulujące przebieg procesów życiowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2947">
<skos:prefLabel>epidemia</skos:prefLabel>
<skos:definition>pojawienie się jednocześnie lub w krótkich odstępach czasu dużej liczby zachorowań na określoną chorobę (najczęściej zakaźną) wśród ludności danego terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2949">
<skos:prefLabel>epidemiologia</skos:prefLabel>
<skos:definition>dział medycyny, nauka zajmująca się badaniem czynników i warunków związanych z powstawaniem i szerzeniem się epidemii oraz środkami zapobiegającymi i zwalczającymi epidemie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2955">
<skos:prefLabel>koniowate</skos:prefLabel>
<skos:definition>Equidae, rodzina ssaków z rzędu nieparzystokopytnych, obejmująca m.in. konie, osły i zebry</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2956">
<skos:prefLabel>urządzenie</skos:prefLabel>
<skos:definition>rodzaj mechanizmu lub zespół elementów, przyrządów służący do wykonania określonych czynności, ułatwiający pracę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2958">
<skos:prefLabel>dawka równoważna</skos:prefLabel>
<skos:definition>wielkość dawki używana w ochronie przed promieniowaniem wyrażająca wszystkie rodzaje promieniowania przy użyciu wspólnej skali w celu obliczenia skutecznej dawki pochłoniętej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2960">
<skos:prefLabel>ergonomia</skos:prefLabel>
<skos:definition>nauka zajmująca się badaniem warunków pracy, przystosowaniem środowiska pracy, maszyn i urządzeń technicznych do właściwości fizycznych i psychicznych człowieka z punktu widzenia zapewnienia optymalnych warunków wykonywania pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2963">
<skos:prefLabel>erozja</skos:prefLabel>
<skos:definition>proces niszczenia (żłobienia, rozcinania, rozmywania) skorupy ziemskiej wskutek działania różnych czynników zewnętrznych, np. deszczu, wiatru, wód płynących, lodowców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2964">
<skos:prefLabel>walka z erozją</skos:prefLabel>
<skos:definition>środki stosowane przy pracach budowlanych lub innych działaniach naruszających górotwór w celu zmniejszenia zagrożenia erozją, np. sadzenie drzew i trawy na stokach, urządzenia ograniczające spły wód i zatrzymujące sedymenty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2969">
<skos:prefLabel>biologia estuariów</skos:prefLabel>
<skos:definition>badania naukowe lub charakterystyczne procesy życiowe organizmów majacych swe siedliska a na wpół zamkniętych, połączonych z morzem, zbiorników słonawych wód przybrzeżnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2970">
<skos:prefLabel>obszar chroniony w ujściach rzek</skos:prefLabel>
<skos:definition>obszar prawnie chroniony w ujściu rzeki z uwagi na całość środowiska lub jego część, w celach naukowych, edukacyjnych lub rekreacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2972">
<skos:prefLabel>ekosystem estuarium</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2973">
<skos:prefLabel>oceanografia estuariów</skos:prefLabel>
<skos:definition>badania fizycznych, chemicznych, biologicznych i geologicznych właściwości na wpół zamkniętych, połączonych z morzem, zbiorników słonawych wód przybrzeżnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2974">
<skos:prefLabel>estuarium</skos:prefLabel>
<skos:definition>ujście rzeczne rozszerzające się w lejkowatą zatokę, modelowaną przez przypływ i odpływ morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2977">
<skos:prefLabel>trawienie</skos:prefLabel>
<skos:definition>usuwanie z powierzchni metali rdzę, zgorzelinę itp. działając na nie wodnymi roztworami kwasów, roztworami kwaśnych soli lub alkaliów; także wykonywanie wgłębienia na płycie metalowej lub szklanej za pomocą kwasów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2978">
<skos:prefLabel>substancja wytrawiająca</skos:prefLabel>
<skos:definition>substancja chemicznie niszcząca powierzchnię, np. metalu lub szkła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2979">
<skos:prefLabel>eter etylowy</skos:prefLabel>
<skos:definition>ciecz bezbarwna, lotna o odurzającym zapachu, otrzymywana z etylenu przez działanie stężonym kwasem siarkowym; stosowana w medycynie jako silny narkotyk i środek znieczulający, w przemyśle jako rozpuszczalnik</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2980">
<skos:prefLabel>etyka</skos:prefLabel>
<skos:definition>nauka o moralności zajmująca się opisem, analizą i wyjaśnieniem rzeczywiście istniejącej moralności i ustaleniem dyrektyw moralnego postępowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2982">
<skos:prefLabel>etnologia</skos:prefLabel>
<skos:definition>nauka zajmująca się studiami porównawczo-teoretycznymi dotyczącymi kultur różnych ludów, zwłaszcza pierwotnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2984">
<skos:prefLabel>etologia</skos:prefLabel>
<skos:definition>nauka o zachowaniu się zwierząt; jeden z kierunków badań w zoopsychologii, w zakresie wrodzonych reakcji zwierząt w warunkach naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2989">
<skos:prefLabel>Rada Unii Europejskiej</skos:prefLabel>
<skos:definition>główny organ legislacyjny UE; Rada reprezentuje interesy państw członkowskich na szczeblu Wspólnoty w procesie kształtowania i realizacji wspólnej polityki w każdej dziedzinie podlegającej   działalności legislacyjnej Rady; w skład Rady wchodzą, po jednym z każdego państwa, przedstawiciele na szczeblu ministerialnym; państwo członkowskie sprawuje przewodnictwo (prezydencję) w Radzie przez sześć miesięcy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2991">
<skos:prefLabel>Euratom</skos:prefLabel>
<skos:definition>układ w sprawie utworzenia Euratomu wszedł w życie 1 stycznia 1958 r. równoczesnie z układem o EWG, wielokrotnie modyfikowany; celem układu jest m.in. rozwijanie badań, inwestycji i tworzenie wspólnych instalacji dla rozwoju energii atomowej, tworzenie wspólnego rynku paliw jądrowych, kontrolę właściwego wykorzystania materiałów atomowych dla przeciwdziałania nadużyciom i ochrony zdrowia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2992">
<skos:prefLabel>Europa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2993">
<skos:prefLabel>Komisja Europejska</skos:prefLabel>
<skos:definition>organ wspólny trzech Wspólnot w wyniku Traktatu Fuzyjnego podpisanego 8 kwietnia 1965 r.; posiada prawo inicjatywy ustawodawczej; pełni funkcję organu stanowiącego prawo, funkcję organu zarządzającego i funkcję organu kontrolnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2996">
<skos:prefLabel>Europejski Trybunał Sprawiedliwości</skos:prefLabel>
<skos:definition>organ UE pełniący funkcję sądu międzynarodowego, konstytucyjnego,administracyjnego; jako sąd międzynarodowy rozstrzyga wszelkie spory pojawiające się przy realizacji traktatów i dotyczące naruszenia prawa europejskiego; jest instancją odwoławczą od decyzji Komisji Europejskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2997">
<skos:prefLabel>Europejska Agencja Środowiska</skos:prefLabel>
<skos:definition>ustanowiona jako prawnie niezależna jednostka Wspólnoty Europejskiej w 1990 r.; nadrzędnym celem Agencji jest wspieranie zrównoważonego rozwoju i pomoc w osiąganiu istotnej i wymiernej poprawy stanu środowiska w Europie poprzez dostarczanie aktualnych, istotnych i wiarygodnych informacji o środowisku ośrodkom decyzyjnym i społeczeństwu; „Porozumienie między Wspólnotą Europejską a Rzeczpospolitą Polską w sprawie uczestnictwa w Europejskiej Agencji Środowiska oraz Europejskiej Sieci Informacji i Obserwacji” weszło w życie 1 stycznia 2003 r.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/2998">
<skos:prefLabel>Europejska Rada Ochrony Środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3000">
<skos:prefLabel>europejski rezerwat przyrody</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3002">
<skos:prefLabel>Parlament Europejski</skos:prefLabel>
<skos:definition>dawniej Zgromadzenie Europejskiej Wspólnoty Gospodarczej, obecnie Parlament Europejski składa się z 520 reprezentantów państw członkowskich UE, wybieranych bepośrednio, ma siedzibę w Strasburgu; uczestniczy w procesie stanowienia prawa europejskiego oraz zatwierdzaniu budżetu UE, posiada inicjatywę polityczną, ma wpływ na przyjmowanie do UE nowych członków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3006">
<skos:prefLabel>Unia Europejska</skos:prefLabel>
<skos:definition>15 krajów (Belgia, Dania, Francja, Niemcy, Grecja, Irlandia, Włochy, Luksemburg, Holandia, Portugalia, Hiszpania, Zjednoczone Królestwo Wlk. Bryt., Austria, Finlandia i Szwecja) złączone wspólną polityką monetarną oraz aspiracjami politycznymi i społecznymi; Unia Europejska weszła w życie w wyniku Traktatu z Maastricht z 1 listopada 1993 r. i obejmuje trzy Wspólnoty Europejskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3007">
<skos:prefLabel>eutrofizacja</skos:prefLabel>
<skos:definition>stan biologiczny jeziora, w którym bogactwo substancji pokarmowych, obfita flora i fauna, powodująca często duże zużycie tlenu, prowadzi do zarastania płytkich jezior i powstawania niskich torfowisk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3011">
<skos:prefLabel>ocena</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3012">
<skos:prefLabel>kryterium oceny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3013">
<skos:prefLabel>metoda oceny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3014">
<skos:prefLabel>ocena technologii</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3019">
<skos:prefLabel>parowanie</skos:prefLabel>
<skos:definition>zamiana substancji ciekłej lub stałej w stan pary</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3022">
<skos:prefLabel>ewapotranspiracja</skos:prefLabel>
<skos:definition>przemieszczenie wody z powierzchni ziemi do atmosfery w wyniku odparowania z jezior, strumieni i powierzchni ziemi oraz w wyniku transpiracji z roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3026">
<skos:prefLabel>ewolucja</skos:prefLabel>
<skos:definition>proces przekształcania się organizmów w zmiennych warunkach środowiska, polegający na powstawaniu i utrwalaniu się w ciągu pokoleń nowych, dziedzicznych cech przystosowawczych, różnicowaniu się populacji i wyodrębnieniu nowych gatunków, coraz liczniejszych, wyżej zorganizowanych i wyspecjalizowanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3027">
<skos:prefLabel>nauki ścisłe</skos:prefLabel>
<skos:definition>matematyka i inne nauki oparte o rachunek (obliczenia)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3028">
<skos:prefLabel>wyrobisko</skos:prefLabel>
<skos:definition>pusta przestrzeń w kopalni powstała po wybraniu skał w wyniku robót górniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3029">
<skos:prefLabel>wykopywanie</skos:prefLabel>
<skos:definition>usuwanie naturalnie zalegającej warstwy ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3031">
<skos:prefLabel>zwałowisko materiałów wyrobiskowych</skos:prefLabel>
<skos:definition>zwałowisko ziemi i materiałów wyrobiskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3032">
<skos:prefLabel>teren wyrobiska</skos:prefLabel>
<skos:definition>obszar, na którym pozyskuje się kopaliny na powierzchni ziemi lub pod ziemią</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3033">
<skos:prefLabel>nadmierna wysokość kominów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3040">
<skos:prefLabel>przepis wykonawczy</skos:prefLabel>
<skos:definition>rozporządzenie lub przepis wydany przez zwierzchnika władz administracyjnych lub same władze w celu ustalenia sposobu interpretacji lub egzekucji przepisów wyższego rzędu (konstytucyjnych, ustawowych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3044">
<skos:prefLabel>urządzenie odprowadzające gazy spalinowe</skos:prefLabel>
<skos:definition>urządzenie odprowadzające gazy spalinowe do atmosfery; w pojazdach mechanicznych: rura odprowadzająca gazy spalinowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3045">
<skos:prefLabel>gazy spalinowe</skos:prefLabel>
<skos:definition>mieszanina gazów uchodzących z pieców lub silników spalinowych, będąca produktem spalania paliw (stałych, ciekłych i gazowych) z domieszką tlenu i niepalnych składników powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3047">
<skos:prefLabel>istniejące substancje chemiczne</skos:prefLabel>
<skos:definition>substancje chemiczne istniejące przed 18 września 1981 r.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3048">
<skos:prefLabel>gatunek egzotyczny</skos:prefLabel>
<skos:definition>rośliny, zwierzęta lub mikroorganizmy wprowadzone przez człowieka na obszary, w obrębie których poprzednio gatunki te nie występowały</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3049">
<skos:prefLabel>wydatki</skos:prefLabel>
<skos:definition>pieniądze wydane przez konsumentów, inwestorów lub władze na towary lub usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3050">
<skos:prefLabel>eksperyment</skos:prefLabel>
<skos:definition>celowe wywołanie jakiegoś zjawiska (lub jego zmiany) w sztucznych, zwykle laboratoryjnych warunkach w celu zbadania i wyjaśnienia jego przebiegu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3054">
<skos:prefLabel>system ekspertowy</skos:prefLabel>
<skos:definition>system informacyjno-wyszukiwawczy, na który składają się: 1) zbiór danych faktograficznych z określonej dziedziny (baza danych, zbiór dokumentów) opisanych atrybutami, które będą ułatwiały wnioskowanie i formułowanie zapytań albo symulację zjawisk; 2) logika systemu, czyli zasady wnioskowania właściwe ekspertowi danej dziedziny, posyawione do dyspozycji użytkownika systemu; system realizowany za pomocą specjalnego oprogramowania komputerowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3057">
<skos:prefLabel>poszukiwania geologiczne</skos:prefLabel>
<skos:definition>praca badawcza mająca na celu odkrycie minerałów, rudy, gazu, ropy naftowej lub węgla itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3058">
<skos:prefLabel>wybuch</skos:prefLabel>
<skos:definition>bardzo szybko przebiegająca reakcja chemiczna określonych związków chemicznych lub mieszanin, której towarzyszy powstawanie dużej ilości produktów gazowych, detonacja, gwałtowny wzrost temperatury i ciśnienia, powodująca kruszenie, rozpad materiałów znajdujących się wokoło źródła tej reakcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3061">
<skos:prefLabel>materiał wybuchowy</skos:prefLabel>
<skos:definition>związki chemiczne lub mieszaniny zdolne pod wpływem bodźców zewnętrznych do gwałtownego rozkładu z wydzieleniem dużej ilości ciepła i gazów; używane do wyrobu amunicji, stosowane również w górnictwie, kamieniołomach, przy robotach ziemnych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3063">
<skos:prefLabel>eksport</skos:prefLabel>
<skos:definition>odpłatny wywóz towarów, kapitałów za granicę lub odpłatne świadczenia usług na rzecz kontrahenta zagranicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3064">
<skos:prefLabel>pozwolenie na eksport</skos:prefLabel>
<skos:definition>urzędowe zezwolenie na wywóz i sprzedaż towarów zagranicę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3065">
<skos:prefLabel>eksport odpadów niebezpiecznych</skos:prefLabel>
<skos:definition>wywóz odpadów niebezpiecznych do innych krajów; międzynarodowy obrót odpadami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3067">
<skos:prefLabel>naświetlenie</skos:prefLabel>
<skos:definition>czas naświetlenia i napromieniowania jakiegoś materiału promieniami jonizującymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3071">
<skos:prefLabel>wywłaszczenie</skos:prefLabel>
<skos:definition>pozbawienie kogoś prawa własności (zwykle na rzecz państwa) na mocy ustawy, przeważnie za odszkodowaniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3073">
<skos:prefLabel>ekstensywna gospodarka hodowlana</skos:prefLabel>
<skos:definition>system hodowli na w rozległych terytorialnie gospodarstwach, przy niskim poziomie nakładów  i małej liczbie sztuk bydła na jednostkę powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3079">
<skos:prefLabel>eksternalizacja</skos:prefLabel>
<skos:definition>rozbieżności między kosztami prywatnymi a społecznymi oraz między korzyściami prywatnymi a społecznym, podstawowym pojęciem w eksternalizacji jest wzajemne uzależnienie bez rekompensaty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3081">
<skos:prefLabel>wymieranie</skos:prefLabel>
<skos:definition>masowe umieranie, ginięcie gatunków roślin i zwierząt na Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3084">
<skos:prefLabel>gatunek wymarły</skos:prefLabel>
<skos:definition>gatunki zwierząt i roślin, które całkowicie wyginęły na Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3087">
<skos:prefLabel>wydobycie</skos:prefLabel>
<skos:definition>proces, w wyniku którego uzyskuje się  z rudy czysty metal</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3090">
<skos:prefLabel>przemysł wydobywczy</skos:prefLabel>
<skos:definition>przemysł związany z wydobywaniem kopalin m.in. ze źródeł nieodnawialnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3097">
<skos:prefLabel>tkanina</skos:prefLabel>
<skos:definition>wyrób włókienniczy z nici lnianych, bawełnianych, wełnianych, jedwabnych lub syntetycznych, uzyskiwany w procesie tkania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3099">
<skos:prefLabel>czynnik rynkowy</skos:prefLabel>
<skos:definition>czynniki wpływające na obrót poszczególnymi dobrami lub usługami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3103">
<skos:prefLabel>hodowla na skalę przemysłową</skos:prefLabel>
<skos:definition>wysokonakładowa hodowla zwierząt w warunkach sztucznych, np.w celu przemysłowej produkcji kurczaków; jajek, indyków, wołowiny i wieprzowiny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3106">
<skos:prefLabel>mikroorganizmy w fekaliach</skos:prefLabel>
<skos:definition>mikroorganizmy pochodzące z fekaliów ludzkich i zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3110">
<skos:prefLabel>opad pyłu</skos:prefLabel>
<skos:definition>opadanie zawieszonych w powietrzu cząsteczek stałych lub ciekłych; zjawisko zachodzi gdy siły grawitacji przeważają nad siłami związanymi z ruchem powietrza w górę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3113">
<skos:prefLabel>obszar ugorowany</skos:prefLabel>
<skos:definition>pola w uprawie, czasowo pozostawione bez obsiewu na okres jednego lub więcej sezonów wegetacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3115">
<skos:prefLabel>ugór</skos:prefLabel>
<skos:definition>pole przez jakiś czas nie obsiewane, zwykle w celu przywrócenia żyzności, odchwaszczenia gleby i zatrzymania wilgoci</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3117">
<skos:prefLabel>rodzina</skos:prefLabel>
<skos:definition>małżonkowie i ich dzieci; także: osoby związane pokrewieństwem, powinowactwem; krewni, powinowaci</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3119">
<skos:prefLabel>planowanie rodziny</skos:prefLabel>
<skos:definition>świadome planowanie liczby dzieci w rodzinie z wykorzystaniem środków antykoncepcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3120">
<skos:prefLabel>głód</skos:prefLabel>
<skos:definition>brak żywności spowodowany nieurodzajem, powodzią, trzęsieniem ziemi, wojną itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3122">
<skos:prefLabel>zwierzęta hodowlane</skos:prefLabel>
<skos:definition>zwierzęta hodowane na farmie jako zwierzęta robocze oraz z przeznaczeniem na produkcję mięsa, mleka, jaj</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3123">
<skos:prefLabel>budynki gospodarcze</skos:prefLabel>
<skos:definition>zespół budynków w obrębie gospodarstwa wiejskiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3130">
<skos:prefLabel>gospodarstwo hodowlane</skos:prefLabel>
<skos:definition>gospodarstwo przemysłowo-rolne, teren i zbudowania, w którym prowadzona jest uprawa roślin oraz hodowla zwierząt (np. ryb, krów, świń i ptactwa), a także produkcja, np. mięsa, mleka i jaj</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3133">
<skos:prefLabel>ruch szybki</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3138">
<skos:prefLabel>fauna</skos:prefLabel>
<skos:definition>ogół gatunków zwierząt charakterystyczny dla pewnego terytorium, środowiska lub okresu geologicznego; także ogół gatunków danej grupy systematycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3143">
<skos:prefLabel>władze federalne</skos:prefLabel>
<skos:definition>w państwie związkowym: władze centralne w przeciwieństwie do władz poszczególnych krajów wchodzących w skład związku (stanowych, kantonalnych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3144">
<skos:prefLabel>rząd federalny</skos:prefLabel>
<skos:definition>oparty na zasadach federacji, stanowiący federację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3147">
<skos:prefLabel>prawo federalne</skos:prefLabel>
<skos:definition>zbiór przepisów ustanowionych przez władze ukonstytuowane jako władze centralne w sfederowanej unii niezależnych jednostek politycznych lub stanów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3149">
<skos:prefLabel>opłata</skos:prefLabel>
<skos:definition>regulowana przez przepisy prawne opłata za świadczenia ze strony urzędników państwowych lub za korzystanie z przywilejów pozostających pod kontrolą władz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3151">
<skos:prefLabel>karmienie zwierząt</skos:prefLabel>
<skos:definition>dostarczanie pożywienia zwierzętom i skutki tych działań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3154">
<skos:prefLabel>torfowisko niskie</skos:prefLabel>
<skos:definition>grunty przesycone wodą, gąbczaste zawierające rozkładające się resztki roślinności o charakterze zasadowym, głównie szuwarowej, które przekształcają się w torf</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3159">
<skos:prefLabel>fermentacja</skos:prefLabel>
<skos:definition>enzymatyczne przekształcenie substancji organicznych, zwłaszcza węglowodorów z wydzielaniem się gazu; dzięki fermentacji określone organizmy mogą żyć bez dostępu powietrza; reakcja biochemiczna zachodząca pod katalitycznym wpływem fermentów wytwarzanych przez mikroorganizmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3162">
<skos:prefLabel>paproć</skos:prefLabel>
<skos:definition>naczyniowa roślina zarodnikowa z rzędu Filicales obejmującej kilka tysięcy gatunków, o liściach zwykle pierzastych z kupkami zarodni na spodniej stronie; występuje głównie w lasach i na terenach podmokłych na całej kuli ziemskiej; wiele gatunków uprawia się jako rośliny ozdobne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3169">
<skos:prefLabel>nawóz</skos:prefLabel>
<skos:definition>substancja zawierająca składniki pokarmowe niezbędne do rozwoju roślin, dostarczana roślinom uprawnym najczęściej do gleby np. obornik, kompost, nawóz mineralny, nawóz organiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3171">
<skos:prefLabel>regulacje prawne dotyczące nawozów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3174">
<skos:prefLabel>włókno</skos:prefLabel>
<skos:definition>surowiec naturalny (konopie, len) lub tworzywo sztuczne, z którego wyrabia się przędzę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3175">
<skos:prefLabel>włókno szklane</skos:prefLabel>
<skos:definition>szkło w postaci nitek o różnych długościach i średnicach, używane do wyrobu tkanin szklanych oraz do celów izolacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3176">
<skos:prefLabel>pole</skos:prefLabel>
<skos:definition>obszar ziemi przeznaczony pod uprawę, nadający się do uprawy; ziemia uprawna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3177">
<skos:prefLabel>szkody w rolnictwie</skos:prefLabel>
<skos:definition>szkody materialne w rolnictwie spowodowane różnymi przyczynami m.in. erozją ziemi i zasoleniem gleby, wyrębem lasów, zmianą klimatu, zanieczyszczeniami, postępującą urbanizacją, a także szkody spowodowane przez szkodniki, zwierzynę łowną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3178">
<skos:prefLabel>doświadczenie polowe</skos:prefLabel>
<skos:definition>doświadczenie przeprowadzane na organizmach lub substancjach poza laboratorium</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3179">
<skos:prefLabel>badania terenowe</skos:prefLabel>
<skos:definition>badania naukowe prowadzone poza laboratorium</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3181">
<skos:prefLabel>wypełniacz</skos:prefLabel>
<skos:definition>substancja dodawana do wyrobu w celu nadania mu żądanych właściwości lub obniżenia kosztów produkcji np. wypełniacz farb, tworzyw sztucznych, papieru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3182">
<skos:prefLabel>stacja benzynowa</skos:prefLabel>
<skos:definition>punkt sprzedaży paliwa i smarów dla pojazdów samochodowych, zaopatrzony w odpowiednie urządzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3183">
<skos:prefLabel>film</skos:prefLabel>
<skos:definition>rodzaj widowiska, w którym za pomocą odpowiedniej techniki projekcyjnej rzutuje się na ekran pewną serię obrazów, dających widzowi złudzenie ruchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3184">
<skos:prefLabel>filtr</skos:prefLabel>
<skos:definition>urządzenie służące do oddzielania ciał stałych od cieczy lub gazów, w których są zawieszone, za pomocą przegrody porowatej zatrzymującej ciało stałe, a przepuszczającej płyny i gazy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3186">
<skos:prefLabel>osad filtracyjny</skos:prefLabel>
<skos:definition>substancje stałe, suche lub mokre, gromadzące się na przegrodach filtracyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3190">
<skos:prefLabel>filtracja</skos:prefLabel>
<skos:definition>przepuszczanie przez filtr; filtrowanie, sączenie, przesączanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3192">
<skos:prefLabel>składowanie docelowe</skos:prefLabel>
<skos:definition>metoda, w której materiały obojętne, nie ulegające zmianom w wyniku procesów naturalnych w ciągu długiego okresu czasu są izolowane przez trzy przesłony: naturalny grunt nieprzepuszczalny, sztuczną przesłonę, która może być kontrolowana oraz składowany materiał spełniający rolę przesłon; pojęcie składowania docelowego zawiera możliwość wydobycia materiałów w przyszłości, jeśli są one dostatecznie jednorodne, a przedsięwzięcie jest ekonomiczne opłacalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3194">
<skos:prefLabel>finanse</skos:prefLabel>
<skos:definition>środki pieniężne; fundusze będące wyrazem określonych stosunków produkcyjnych np. finanse państwa, przedsiębiorstwa, organizacji, osób prywatnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3195">
<skos:prefLabel>pomoc finansowa</skos:prefLabel>
<skos:definition>środki finansowe służące wspieraniu kogoś, pomaganiu komuś</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3196">
<skos:prefLabel>odszkodowanie</skos:prefLabel>
<skos:definition>wynagrodzenie, zapłata za wyrządzone szkody, za poniesione przez kogoś straty, doznane krzywdy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3197">
<skos:prefLabel>wkład finansowy</skos:prefLabel>
<skos:definition>wkład finansowy do wspólnego funduszu lub wspólnych zasobów wkład, subsydium, grant lub inny wkład mający na celu wsparcie gospodarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3199">
<skos:prefLabel>instrument finansowy</skos:prefLabel>
<skos:definition>termin ogólny obejmujący wiele różnych form finansowania stosowanych w praktyce byznesu, np. pożyczki, akcje i obligacje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3200">
<skos:prefLabel>prawo finansowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3201">
<skos:prefLabel>rynek finansowy</skos:prefLabel>
<skos:definition>miejsce lub instytucja, na terenie której spotykają się kupujący i sprzedający w celu handlu wartościami pieniężnym, akcjami, obligacjami itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3205">
<skos:prefLabel>finansowanie</skos:prefLabel>
<skos:definition>pokrywanie kosztów lub kredytowanie jakiegoś przedsięwzięcia, jakiejś działalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3207">
<skos:prefLabel>grzywna</skos:prefLabel>
<skos:definition>kara pieniężna nakładana przez organy sądowe lub administracyjne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3209">
<skos:prefLabel>pyły drobne</skos:prefLabel>
<skos:definition>zawieszone w powietrzu cząstki ciał stałych jako wynik działania człowieka lub przyrody, np. w wyniku pożarów lub pylenia gleb;mogą być przyczyną uszkodzenia dróg oddechowych ludzi lub zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3212">
<skos:prefLabel>ogień</skos:prefLabel>
<skos:definition>zjawisko wydzielania się ciepła i światła towarzyszące paleniu się ciał, postrzegane w postaci płomieni i żaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3218">
<skos:prefLabel>prewencja przeciwpożarowa</skos:prefLabel>
<skos:definition>środki, działania lub urządzenia stosowane w celu zapobiegania pożarom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3219">
<skos:prefLabel>środki ograniczające palność</skos:prefLabel>
<skos:definition>substancje chemiczne stosowane do powlekania lub jako dodatek do materiałów palnych w celu ograniczenia możliwości zapłonu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3220">
<skos:prefLabel>ochrona przeciwpożarowa</skos:prefLabel>
<skos:definition>ogół środków jakie należy podjąć by nie dopuścić do powstania pożaru, m.in. sprawdzanie gotowości sprzętu p-poż, szkolenie personelu, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3225">
<skos:prefLabel>wymogi bezpieczeństwa przeciwpożarowego</skos:prefLabel>
<skos:definition>zasady i systemy bezpieczeństwa, które stosuje się w celu zapobiegania lub zwalczania pożarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3226">
<skos:prefLabel>straż pożarna</skos:prefLabel>
<skos:definition>zespół ludzi odpowiednio wyszkolonych i wyposażonych, powołanych do ochrony przeciwpożarowej, a także do przeprowadzania akcji ratowniczych w wypadku klęsk żywiołowych lub katastrof</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3229">
<skos:prefLabel>wypalanie</skos:prefLabel>
<skos:definition>stosowanie obróbki ogniowej lub termicznej w celu utwardzenia lub wytworzenia szkliwa na powierzchni wyrobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3232">
<skos:prefLabel>firma</skos:prefLabel>
<skos:definition>partnerstwo kilku osób na zasadach komercyjnych, zwłaszcza w formie korporacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3236">
<skos:prefLabel>choroba ryb</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3237">
<skos:prefLabel>rybołówstwo</skos:prefLabel>
<skos:definition>przemysłowy połów, przetwarzanie oraz sprzedaż ryb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3238">
<skos:prefLabel>gospodarka zasobami rybnymi</skos:prefLabel>
<skos:definition>administrowanie i sterowanie przemysłem rybnym, od połowu poprzez przetwórstwo do sprzedaży ryb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3241">
<skos:prefLabel>zasoby rybołówstwa</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3243">
<skos:prefLabel>gospodarka rybna</skos:prefLabel>
<skos:definition>organizacja produkcji, dystrybucji i konsumpcji ryb i owoców morza oraz aspekty ekonomiczne przetwórstwa rybnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3246">
<skos:prefLabel>polityka w dziedzinie gospodarki rybnej</skos:prefLabel>
<skos:definition>polityka Wspólnoty Europejskiej w dziedzinie gospodarki rybnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3252">
<skos:prefLabel>ryby</skos:prefLabel>
<skos:definition>zmiennocieplne kręgowce wodne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3254">
<skos:prefLabel>gospodarstwo rybne</skos:prefLabel>
<skos:definition>hodowla ryb w wodach śródlądowych, estuaryjnych lub przybrzeżnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3256">
<skos:prefLabel>wędkarstwo</skos:prefLabel>
<skos:definition>amatorskie łowienie ryb na wędkę, wędkowanie; także dyscyplina sportowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3260">
<skos:prefLabel>przemysł rybny</skos:prefLabel>
<skos:definition>gałąź gospodarki obejmująca przetwórstwo ryb, homarów, langust, ostryg itp. w celach konsumpcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3261">
<skos:prefLabel>regulacje prawne dotyczące rybołówstwa</skos:prefLabel>
<skos:definition>regulacje prawne dotyczące rybołówstwa; międzynarodowe przepisy prawne dotyczące rybołówstwa zawarte zostały w Konwencji o rybołówstwie i ochronie żywych zasobów na morzu otwartym, sporządzonej w Genewie w dniu 24 kwietnia 1958 r.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3263">
<skos:prefLabel>zbiornik wodny do wędkowania</skos:prefLabel>
<skos:definition>wydzielony zbiornik wodny, w którym dozwolone jest wędkowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3266">
<skos:prefLabel>statek rybacki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3269">
<skos:prefLabel>wymieranie ryb</skos:prefLabel>
<skos:definition>choroby ryb obserwowane w ciągu ostatniego trzydziestolecia, przypisywane zanieczyszczeniom wód, w tym krwawienia, guzy, butwienie i deformacje płetw, utrata łusek i ogonów; przypuszcza się, że jedną z głównych przyczyn jest niski poziom rozpuszczonego tlenu, co wynika z nadmiernego zanieczyszczenia ściekami, a ponadto - dopływ pestycydów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3273">
<skos:prefLabel>zasoby ryb</skos:prefLabel>
<skos:definition>zasoby ryb zachowane w celu przyszłego użytkowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3274">
<skos:prefLabel>toksyczność dla ryb</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3277">
<skos:prefLabel>armatura</skos:prefLabel>
<skos:definition>armatura wodno-kanalizacyjna w budownictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3278">
<skos:prefLabel>ustalona opłata</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3281">
<skos:prefLabel>tania bandera</skos:prefLabel>
<skos:definition>praktyka rejestracji statków handlowych w krajach, które mają mniej restrykcyjne podejście do wymogów bezpieczeństwa oraz pobierają niższe opłaty rejestracyjne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3282">
<skos:prefLabel>spalanie gazów na pochodniach</skos:prefLabel>
<skos:definition>1) stosowanie otwartego płomienia do spalania niebezpiecznych odpadów gazowych, 2) urządzenia do spalania niebezpiecznych odpadów gazowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3283">
<skos:prefLabel>szybki test</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3287">
<skos:prefLabel>środek zapachowy</skos:prefLabel>
<skos:definition>substancja chemiczna w postaci ekstraktu lub przyprawy stosowana dla poprawienia smaku lub zapachu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3288">
<skos:prefLabel>pchła</skos:prefLabel>
<skos:definition>Aphaniptera, rząd obejmujący około 1500 gatunków drobnych, bezskrzydłych owadów skaczących, o kłująco-ssących narządach gębowych, pasożytujących na kręgowcach; najbardziej rozpowszechnionym gatunkiem jest pchła ludzka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3291">
<skos:prefLabel>elastyczne podejście w ochronie środowiska</skos:prefLabel>
<skos:definition>elastyczne ujmowanie kontroli całkowitej emisji do środowiska pozwalające na  liberalne traktowanie jednostek, w których nakłady na ograniczanie emisji są zbyt wysokie kosztem innych emitentów, w których ograniczenie emisji odbywa się z mniejszym nakładem środków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3294">
<skos:prefLabel>flokulant</skos:prefLabel>
<skos:definition>substancja, której obecność w roztworze powoduje łączenie się ziarn w zespoły</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3295">
<skos:prefLabel>flokulacja</skos:prefLabel>
<skos:definition>powstawanie dużych skupisk cząsteczek w procesie koagulacji osadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3298">
<skos:prefLabel>powódź</skos:prefLabel>
<skos:definition>zalanie przez wodę terenów nisko położonych, zwykle nadbrzeżnych, głównie w wyniku wezbrania rzeki, powodujące znaczne szkody gospodarcze i społeczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3300">
<skos:prefLabel>ochrona przeciwpowodziowa</skos:prefLabel>
<skos:definition>zabiegi mające na celu zapobieganie lub usunięcie szkodliwych skutków powodzi, w tym budowa zbiorników retencyjnych, kanałów, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3301">
<skos:prefLabel>zalew powodziowy</skos:prefLabel>
<skos:definition>przejściowo występujące warunki częściowego lub całkowietgo zalania wodą obszarów lądu naturalnie suchych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3305">
<skos:prefLabel>flora</skos:prefLabel>
<skos:definition>ogół roślin występujących na określonym obszarze, w określonej epoce geologicznej, a także w określonym środowisku ekologicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3307">
<skos:prefLabel>restytucja flory</skos:prefLabel>
<skos:definition>proces przywracania warunków naturalnych siedliskom i ekosystemom z uwagi na potrzeby życiowe roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3308">
<skos:prefLabel>flotacja</skos:prefLabel>
<skos:definition>rozdzielanie lub wzbogacanie rozdrobnionej kopaliny użytecznej metodą wykorzystującą zjawisko różnej zwilżalności ziarn ciał stałych przez wodę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3311">
<skos:prefLabel>przepływ</skos:prefLabel>
<skos:definition>w hydrologii: ilość wody przepływającej w jednostce czasu przez dany przekrój poprzeczny rzeki, kanału, rowu, wyrażana najczęściej w metrach sześciennych na sekundę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3312">
<skos:prefLabel>kwiat</skos:prefLabel>
<skos:definition>końcowy odcinek pędów roślin, o gęsto skupionych, silnie przeobrażonych liściach, często barwnych, będący organem rozmnażania płciowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3313">
<skos:prefLabel>roślina kwiatowa</skos:prefLabel>
<skos:definition>roślina produkujące ozdobne kwiaty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3315">
<skos:prefLabel>pole prądu</skos:prefLabel>
<skos:definition>prędkość i gęstość cieczy jako funkcja położenia i czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3316">
<skos:prefLabel>woda płynąca</skos:prefLabel>
<skos:definition>wody płynące np. w rzekach i strumieniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3319">
<skos:prefLabel>gazy wylotowe</skos:prefLabel>
<skos:definition>mieszanina gazów uchodzących z pieców lub silników spalinowych, będąca produktem spalania paliw (stałych, ciekłych i gazowych) z domieszką tlenu i niepalnych składników powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3322">
<skos:prefLabel>automatyka strumieniowa</skos:prefLabel>
<skos:definition>technologia sterowania wykorzystująca zjawisko
dynamiki płynów do przeprowadzania odczytów fizycznych, regulacji,
przetwarzania danych i działań uruchamiających bez użycia ruchomych części
mechanicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3324">
<skos:prefLabel>fluidyzacja</skos:prefLabel>
<skos:definition>proces tworzenia się zawiesiny drobnych cząstek ciał stałych w przepływającym z dołu do góry płynie (gazie); ma zastosowanie w reakcjach katalitycznych, suszeniu, kalcynacji itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3325">
<skos:prefLabel>złoże fluidalne</skos:prefLabel>
<skos:definition>1) metoda efektywnego spalania stałych paliw węglopodobnych w warunkach stosunkowo niskiej temperatury, uzyskując w ten sposób niskie wartości emisji zanieczyszczeń; paliwo jest rozdrabniane lub mielone i następnie mieszane z rozdrobnionym materiałem obojętnym; mieszaninę umieszcza się na złożu, przez które przepuszczane jest powietrze z dołu do góry porywając cząsteczki, które zachowują się jak płyn; siły spowodowane cyrkulacją powietrza oraz rozdzielenie niewielkich cząsteczek paliwa zapewnia efektywne spalanie,  2) złoże rozdrobnionej substancji stałej, przez które przepływa w sposób kontrolowany powietrze, dzięki czemu zachowuje się ono jak płyn</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3328">
<skos:prefLabel>fluorkowanie</skos:prefLabel>
<skos:definition>dodatek jonu fluorkowego do wody wodociągowej w stężeniu około 0,8-1,6 ppm, jako środek zapobiegający powstawaniu próchnicy zębów u dzieci</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3334">
<skos:prefLabel>fluor</skos:prefLabel>
<skos:definition>(F) pierwiastek chemiczny o liczbie atomowej 9, należący do grupy fluorowców, gaz o słabym zielonkawożółtym zabarwieniu, o charakterystycznej woni gryzącej, silnie trujący, chemicznie bardzo czynny; znajduje obecnie zastosowanie w przemyśle organicznym, np. do wyrobu chemicznie bardzo odpornych mas plastycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3340">
<skos:prefLabel>zasoby rzeczne</skos:prefLabel>
<skos:definition>zasoby pozyskiwane z rzek, głównie woda używana w celeach gospodarczych i konsumpcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3341">
<skos:prefLabel>transport rzeczny</skos:prefLabel>
<skos:definition>przewóz ludzi i ładunków drogą wodną czyli rzeką</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3342">
<skos:prefLabel>pyły lotne</skos:prefLabel>
<skos:definition>drobne cząsteczki popiołu rozproszone w spalinach; powstają w rezultacie spalania paliw lub innych materiałów; cząsteczki popiołu mogą zawierać resztki niedopalonego paliwa i inne zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3343">
<skos:prefLabel>środek pieniący</skos:prefLabel>
<skos:definition>substancje mogące tworzyć homogeniczną fazę gazową powstającą w wyniku dyspersji w cieczach lub ciałach stałych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3345">
<skos:prefLabel>karma dla zwierząt</skos:prefLabel>
<skos:definition>pasza lub mieszanka pasz przygotowywana dla zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3347">
<skos:prefLabel>mgła</skos:prefLabel>
<skos:definition>zawiesina mikroskopijnych kropelek pary wodnej unosząca się nisko nad ziemią w postaci lekkiego obłoku, powstała w wyniku skroplenia oziębionej pary wodnej zawartej w atmosferze ziemskiej, poważnie ogranicza pole widzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3348">
<skos:prefLabel>mgiełka</skos:prefLabel>
<skos:definition>drobne kropelki wody zawieszone w powietrzu, ograniczające widoczność; o ile widoczność jest poniżej 1000 m, mgła staje się gęsta (zamglenie)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3350">
<skos:prefLabel>ulistnienie</skos:prefLabel>
<skos:definition>zielone liście rośliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3354">
<skos:prefLabel>żywność</skos:prefLabel>
<skos:definition>środki spożywcze, wszelkie produkty pochodzenia roślinnego i zwierzęcego, które w stanie naturalnym lub po obróbce stanowią pokarm człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3356">
<skos:prefLabel>dodatek do żywności</skos:prefLabel>
<skos:definition>substancja chemiczna stosowana jako dodatek do żywności dla poprawienia smaku, zapachu lub zachowania trwałości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3358">
<skos:prefLabel>łańcuch pokarmowy</skos:prefLabel>
<skos:definition>szeregi organizmów zajmujących kolejne poziomy troficzne w obrębie zbiorowiska; pomiędzy tymi poziomami energia przekazywana jest w postaci odżywiania się; energia wchodzi do łańcucha troficznego w procesie asymilacji producentów pierwotnych (głównie rośliny zielone), dalej przechodzi przez poziom roślinożerców (konsumentów I rzędu), a następnie do poziomu mięsożerców (konsumentów II i III rzędu)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3360">
<skos:prefLabel>barwnik spożywczy</skos:prefLabel>
<skos:definition>substancja chemiczna stosowana jako dodatek do żywności dla poprawienia koloru lub wyglądu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3362">
<skos:prefLabel>handel żywnością</skos:prefLabel>
<skos:definition>zbyt produktów żywnościowych, zwykle w dużej skali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3365">
<skos:prefLabel>skażenie żywności</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3367">
<skos:prefLabel>hygiena żywności</skos:prefLabel>
<skos:definition>dziedzina wiedzy stosowanej zajmująca się zasadami i metodami poprawy własności produktów żywnościowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3368">
<skos:prefLabel>przemysł spożywczy</skos:prefLabel>
<skos:definition>komercyjna produkcja i pakowanie żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3370">
<skos:prefLabel>napromieniowanie żywności</skos:prefLabel>
<skos:definition>najnowszym uzupełnieniem technologii konserwacji żywności jest wykorzystanie promieniowania jonizującego; metoda ta ma wyraźną przewagę nad metodami konwencjonalnymi ponieważ żywność może być napromieniowana po zapakowaniu, przez co eliminuje się dodatkową możliwość skażenia, a ponadto konserwowana świeża żywność może być przechowywana dłużej bez zauważalnego spadku jakości; napromieniowanie nie pozostawia żadnych pozostałości, nie powoduje zmian w wartościach odżywczych większych niż zastosowanie konwencjonalnych metod, jest procesem stosującym wysoką energię dzięki czemu następuje sterylizacja wskutek unicestwienia mikroorganizmów, insektów i innych szkodników; do napromieniowania stosuje się promieniowanie gamma, wiązkę elektronów lub promieniowanie X; promieniowanie gamma uzyskuje się i radioaktywnych izotopów takich jak kobalt-60, a wiązkę elektronów uzyskuje się w akceleratorach liniowych przy czym wielkość zastosowanej dawki jest najważniejszym elementem procesu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3372">
<skos:prefLabel>zanieczyszczenie żywności</skos:prefLabel>
<skos:definition>substancje potencjalnie szkodliwe w pożywieniu człowieka lub zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3375">
<skos:prefLabel>konserwacja żywności</skos:prefLabel>
<skos:definition>przetwarzanie żywności w celu jej ochrony przed zepsuciem spwodowanym przez działanie bakterii, enzymów i samoutlenianie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3376">
<skos:prefLabel>przemysł przetwórstwa spożywczego</skos:prefLabel>
<skos:definition>przemysł przetwarzający produkty pochodzenia roślinnego i zwierzęcego na wyroby gotowe, nadające się do spożycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3378">
<skos:prefLabel>produkcja żywności</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3380">
<skos:prefLabel>jakość żywności</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3381">
<skos:prefLabel>podstawowe wymagania pokarmowe</skos:prefLabel>
<skos:definition>minimalna racja żywnościowa zapewniająca podstawowe potrzeby organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3384">
<skos:prefLabel>nauka o żywności</skos:prefLabel>
<skos:definition>dziedzina wiedzy stosowanej zajmująca się chemicznymi, biochemicznymi, fizycznymi, fizykochemicznymi i biologicznymi własnościami żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3387">
<skos:prefLabel>przechowywanie żywności</skos:prefLabel>
<skos:definition>zapasy żywności przechowywane przez państwo dla zapewnienia bezpieczeństwa żywnościowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3388">
<skos:prefLabel>artykuł spożywczy</skos:prefLabel>
<skos:definition>substancja, która może być wykorzystana lub przetworzona jako produkt żywnościowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3391">
<skos:prefLabel>technologia żywności</skos:prefLabel>
<skos:definition>stosowanie nauki i techniki w procesach wytwarzania produktów spożywczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3392">
<skos:prefLabel>transport żywności</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3395">
<skos:prefLabel>ścieżka dla pieszych</skos:prefLabel>
<skos:definition>wąski pas ziemi wytyczony dla pieszych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3396">
<skos:prefLabel>zanieczyszczenie pasz</skos:prefLabel>
<skos:definition>skażenie paszy dla zwierząt przez wprowadzenie substancji niebezpiecznych lub trujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3397">
<skos:prefLabel>regulacje prawne dotyczące przetwórstwa i wykorzystania pasz</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3400">
<skos:prefLabel>prognoza</skos:prefLabel>
<skos:definition>przewidywanie przyszłych faktów, zdarzeń itp.oparte na uzasadnionych przesłankach, formułowane przez specjalistów w danym zakresie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3401">
<skos:prefLabel>wykluczenie</skos:prefLabel>
<skos:definition>zniweczenie, obalenie prawa wykupu obciążonego długiem majątku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3402">
<skos:prefLabel>międzynarodowe stosunki gospodarcze</skos:prefLabel>
<skos:definition>międzynarodowe stosunki gospodarcze w dziedzinie gospodarki i finansów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3404">
<skos:prefLabel>polityka zagraniczna</skos:prefLabel>
<skos:definition>działalność władzy państwowej, rządu w dziedzinach społecznej, gospodarczej, kulturalnej, wojskowej i innych, dotycząca stosunków z innymi krajami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3405">
<skos:prefLabel>handel zagraniczny</skos:prefLabel>
<skos:definition>handel z innymi państwami oraz firmami należącymi do innych państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3406">
<skos:prefLabel>las</skos:prefLabel>
<skos:definition>zbiorowisko roślinne, w którym dominują drzewa i krzewy rosnące w zwarciu tak, że tworzony przez nie wyraźny okap ocienia dno lasu, także: naturalny lub wyhodowany przez człowieka zwarty zespół roślinności z przewagą roślin drzewiastych i ze swoistymi zespołami fauny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3410">
<skos:prefLabel>ochrona lasu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3411">
<skos:prefLabel>niszczenie pokrywy leśnej</skos:prefLabel>
<skos:definition>niszczenie lasów w wielu krajach w celu zdobycia nowej powierzchni pod uprawy i pastwiska; dokonywane jest najczęściej bez właściwego rozpoznania zmienności takich czynników jak klimat i topografia obszaru, na którym np. nachylenie stoków i cechy gleb oraz inne charakterystyki fizjograficzne wskazują, że tereny te powinny być zagospodarowane wyłącznie jako grunty leśne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3414">
<skos:prefLabel>szkody leśne</skos:prefLabel>
<skos:definition>straty w drzewostanach spowodowane przez kwaśne deszcze, pożary leśne, zanieczyszczenia powietrza, odlesianie, szkodniki i choroby drzew, zwierzyny itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3416">
<skos:prefLabel>ekosystem leśny</skos:prefLabel>
<skos:definition>fragment środowiska leśnego, w obrębie którego rośliny i zwierzęta żyją powiązane różnymi współzależnościami między sobą i fizycznym środowiskiem przez nie zamieszkiwanym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3419">
<skos:prefLabel>pożar lasu</skos:prefLabel>
<skos:definition>pożar niszczący pokrywę leśną obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3423">
<skos:prefLabel>przemysł leśny</skos:prefLabel>
<skos:definition>sektor gospodarki obejmujący przedsiębiorstwa zarządzające obszarami leśnymi oraz związane z produkcją leśną i usługami dostarczanymi przez lasy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3425">
<skos:prefLabel>gospodarka leśna</skos:prefLabel>
<skos:definition>planowe użytkowanie lasu w celu pozyskiwania drewna, ochrony fauny i flory, jako bazy rekreacyjnej oraz jako źródła wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3426">
<skos:prefLabel>szkodnik leśny</skos:prefLabel>
<skos:definition>zwierzę wyrządzające szkody gospodarce leśnej (np. gąsienice owadów niszczące drzewa)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3430">
<skos:prefLabel>polityka leśna</skos:prefLabel>
<skos:definition>kierunki działania przyjęte i wdrażane przez rząd, lub inną organizację, jako wytyczne w zakresie wszechstronnego gospodarowania lasami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3431">
<skos:prefLabel>produkcja leśna</skos:prefLabel>
<skos:definition>z lasu pozyskuje się wiele produktów, takich jak drewno opałowe, węgiel drzewny, deski, papier oraz kawa, olej palmowy i guma; niektóre produkty stanowią zasoby odnawialne pod warunkiem trwałego i zrównoważonego gospodarowania lasami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3432">
<skos:prefLabel>produkt leśny</skos:prefLabel>
<skos:definition>produkty lasu będące przedmiotem handlu (drewno, pasze, itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3435">
<skos:prefLabel>rezerwat leśny</skos:prefLabel>
<skos:definition>obszar leśny wyłączony z gospodarowania w celu ochrony fauny i flory</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3437">
<skos:prefLabel>ocena zasobów leśnych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3438">
<skos:prefLabel>zasoby leśne</skos:prefLabel>
<skos:definition>na zasoby leśne składają się powierzchnia leśna oraz drzewa (drewno) rosnące na tej powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3439">
<skos:prefLabel>leśnictwo</skos:prefLabel>
<skos:definition>dział gospodarki narodowej obejmujący całokształt zagadnień związanych z produkcją leśną kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3440">
<skos:prefLabel>ekonomika leśnictwa</skos:prefLabel>
<skos:definition>produkcja, dystrybucja i konsumpcja dóbr i usług pozyskiwanych przez gospodarkę leśną i produkowanych przez przemysł leśny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3441">
<skos:prefLabel>prawo dotyczące leśnictwa</skos:prefLabel>
<skos:definition>zbiór ustanowionych przez władze przepisów prawnych, dotyczących problemów zagospodarowania obszarów leśnych, zwierzyny, drewna i innych zasobów lasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3442">
<skos:prefLabel>prawodawstwo dotyczące leśnictwa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3444">
<skos:prefLabel>praktyka leśna</skos:prefLabel>
<skos:definition>hodowla drzew w celu zapewnienia trwałych dostaw drewna i innych produktów leśnych; również praktyki związane z ochroną lasu i zalesianiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3455">
<skos:prefLabel>forma władzy</skos:prefLabel>
<skos:definition>kształt władzy sprawowanej  przez osobę lub grupę osób nad określoną większością</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3460">
<skos:prefLabel>skamieniałość</skos:prefLabel>
<skos:definition>szczątki dawnych zwierząt lub roślin zachowane w skałach, również ich odlewy, odciski, a nawet nikłe ślady w postaci zabarwienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3462">
<skos:prefLabel>paliwa kopalne</skos:prefLabel>
<skos:definition>palne ciało stałe, ciecz lub gaz znajdujący się w ziemi, wydobywany z niej; wykorzystywane jako źródło energii cieplnej; używane do celów przemysłowych, technicznych, gospodarczych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3465">
<skos:prefLabel>obrastanie statków</skos:prefLabel>
<skos:definition>przyrastanie morskich organizmów do podwodnych części statku, co sprawia, że tracą one na szybkości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3469">
<skos:prefLabel>silnik czterosuwowy</skos:prefLabel>
<skos:definition>tłokowy silnik spalinowy, w którym pełny obieg odbywa się w czasie czterech kolejnych suwów tłoka, co odpowiada dwu pełnym obrotom wału korbowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3474">
<skos:prefLabel>podstawa prawna</skos:prefLabel>
<skos:definition>zbiór ustanowionych przez władze przepisów prawnych, dotyczących powołania nowych programów, agend lub struktur organizacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3477">
<skos:prefLabel>dostęp do informacji</skos:prefLabel>
<skos:definition>prawo dostępu do informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3481">
<skos:prefLabel>transport ładunku</skos:prefLabel>
<skos:definition>przewóz towarów statkiem, samolotem lub innym środkiem transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3483">
<skos:prefLabel>freon</skos:prefLabel>
<skos:definition>nazwa handlowa grupy wielochlorowcowych węglowodorów, tj. zawierających fluor i chlor; przykładem może być trichlorofluorometan</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3485">
<skos:prefLabel>woda słodka</skos:prefLabel>
<skos:definition>woda o względnie niskiej zawartości minerałów, zazwyczaj poniżej 500 mg/l rozpuszczonych składników stałych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3487">
<skos:prefLabel>biologia wód śródlądowych</skos:prefLabel>
<skos:definition>dziedzina wiedzy lub swoiste procesy życiowe organizmów wodnych w zbiornikach naturalnych, takich jak rzeki i jeziora</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3488">
<skos:prefLabel>degradacja wód słodkich</skos:prefLabel>
<skos:definition>zanieczyszczenia powierzchni ziemi włączane są  bezpośrednio lub pośrednio w cykl hydrologiczny; co powoduje zmiany jakości  fizycznej, chemicznej i biologicznej wód oraz innych warunków (np. temperatury);  w wyniku czego wody te mogą zagrażać bezpieczeństwu ludzi i zwierząt oraz zdrowiu publicznemu i rozwojowi gospodarczemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3489">
<skos:prefLabel>ekosystem słodkowodny</skos:prefLabel>
<skos:definition>organizmy śródlądowych wód słodkich wraz z ich nieożywionym otoczeniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3491">
<skos:prefLabel>monitoring wód słodkich</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3492">
<skos:prefLabel>organizm słodkowodny</skos:prefLabel>
<skos:definition>organizmy żyjące w wodach słodkich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3493">
<skos:prefLabel>zanieczyszczenie wód słodkich</skos:prefLabel>
<skos:definition>pośrednie lub bezpośrednie zakłócenie jakości biologicznej, fizycznej, chemicznej lub radiologicznej wód słodkich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3494">
<skos:prefLabel>zasoby wód słodkich</skos:prefLabel>
<skos:definition>sieć rzek, jezior i innych śródlądowych zbiorników wód powierzchniowych, których woda służy do produkcji żywności i zaspokajania innych potrzeb człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3495">
<skos:prefLabel>żaba</skos:prefLabel>
<skos:definition>płaz bezogonowy z rodziny Ranidae, charakteryzujący się gładką skórą, nogami tylnymi dłuższymi niż przednie, skocznymi, odżywiający się głównie owadami, stawonogami i mięczakami; występuje w wielu gatunkach lądowo-wodnych na wszystkich kontynentach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3496">
<skos:prefLabel>szron</skos:prefLabel>
<skos:definition>osad zbitych kryształków lodowych powstały w wyniku bezpośredniej sublimacji na przedmiotach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3498">
<skos:prefLabel>owoc</skos:prefLabel>
<skos:definition>w pełni dojrzała zalążnia rośliny łącznie lub bez innych części kwiatu lub pędu zrośniętych z nią w czasie dojrzewania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3499">
<skos:prefLabel>hodowla owoców</skos:prefLabel>
<skos:definition>hodowla drzew owocowych do użytku domowego lub na sprzedaż</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3502">
<skos:prefLabel>drzewo owocowe</skos:prefLabel>
<skos:definition>drzewo rodzące owoce jadalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3505">
<skos:prefLabel>paliwa</skos:prefLabel>
<skos:definition>palne ciało stałe, ciecz lub gaz wykorzystywane jako źródło energii cieplnej (gaz, benzyna, ropa naftowa, węgiel, drewno); używane do celów przemysłowych, technicznych, gospodarczych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3506">
<skos:prefLabel>dodatek do paliw</skos:prefLabel>
<skos:definition>substancje (takie jak czteroetylek ołowiu) które dodaje się do benzyny jako środki przeciwstukowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3507">
<skos:prefLabel>paliwo alkoholowe</skos:prefLabel>
<skos:definition>alternatywne źródło energii do silników samochodowych; otrzymywane jest w procesie fermentacji trzciny cukrowej przez drożdże Saccharomyces cerevisiae</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3509">
<skos:prefLabel>skład paliwa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3510">
<skos:prefLabel>zapotrzebowanie na paliwo</skos:prefLabel>
<skos:definition>ilość zużywanego paliwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3512">
<skos:prefLabel>paliwo otrzymywane z ropy naftowej</skos:prefLabel>
<skos:definition>ciekły wyrób spalany w celu wytworzenia ciepła, z wyjątkiem paliw o temperaturze zapłonu poniżej 38 st.C; może to być olej opałowy, olej grzewczy, olej bunkrowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3516">
<skos:prefLabel>cysterna paliwowa</skos:prefLabel>
<skos:definition>element systemu paliwowego - zbiornik paliwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3517">
<skos:prefLabel>drewno opałowe</skos:prefLabel>
<skos:definition>drewno wykorzystywane jako opał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3518">
<skos:prefLabel>opary</skos:prefLabel>
<skos:definition>cząstki stałe unoszone w powietrzu, powstałe w wyniku kondensacji par, reakcji chemicznych lub sublimacji; często są to tlenki metali lub metale mające cząstki o średnicy poniżej 1 mikrometra, mogą być toksyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3521">
<skos:prefLabel>odymianie</skos:prefLabel>
<skos:definition>zwalczanie szkodników (owadów, gryzoni) za pomocą gazów, fumigantów, w pomieszczeniach zamkniętych, np. w szklarniach, magazynach, pomieszczeniach mieszkalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3522">
<skos:prefLabel>substancja użytkowa</skos:prefLabel>
<skos:definition>substancja wytworzona w określonym celu, np. składnik farby lub substancja zabezpieczająca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3524">
<skos:prefLabel>funkcja gleby</skos:prefLabel>
<skos:definition>główne funkcje gleby to przekształcanie materii i pośredniczenie w jej migracji, co warunkuje istnienie ekosystemów w warunkach naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3528">
<skos:prefLabel>grzybnia</skos:prefLabel>
<skos:definition>wegetatywne ciało grzybów, mające postać splątanych, nitkowatych strzępek, zwykle przytwierdzone do podłoża chwytnikami; służy wielu grzybom także do rozmnażania wegetatywnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3529">
<skos:prefLabel>grzyb</skos:prefLabel>
<skos:definition>organizmy o strzępkach (niciach plechy) zawierających jądra komórkowe, cudzożywne (pasożytnicze lub saprofityczne), pozbawione chlorofilu, obejmują wiele gatunków, wśród nich także grzyby jadalne rosnące w lesie, na łąkach lub hodowane, mające przeważnie kształt kapelusza osadzonego na nóżce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3530">
<skos:prefLabel>środek grzybobójczy</skos:prefLabel>
<skos:definition>środek chemiczny niszczący grzyby pasożytnicze, zapobiegający ich występowaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3533">
<skos:prefLabel>futro</skos:prefLabel>
<skos:definition>skóra niektórych zwierząt pokryta sierścią, zwłaszcza długą, puszystą, po zdjęciu z nich odpowiednio wyprawiana; także same włosy, sierść na zwierzęciu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3535">
<skos:prefLabel>zwierzę futerkowe</skos:prefLabel>
<skos:definition>dzikie lub hodowane zwierzęta, których skóry są użytkowane na okrycia i galanterię futrzaną, np. króliki, wiewiórki, lisy, norki, nutrie, owce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3536">
<skos:prefLabel>furan</skos:prefLabel>
<skos:definition>bezbarwny, palny toksyczny ciekły związek heterocykliczny wykorzystywany w syntezie nylonu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3538">
<skos:prefLabel>wielkie piece hutnicze</skos:prefLabel>
<skos:definition>urządzenia przemysłowe do przeprowadzania procesów technologicznych, które zachodzą w wysokich temperaturach, uzyskiwanych w wyniku spalania paliw lub elektrycznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3539">
<skos:prefLabel>meble</skos:prefLabel>
<skos:definition>przenośny sprzęt, zwykle z drewna, stanowiący część urządzenia mieszkania, biura itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3540">
<skos:prefLabel>przemysł meblowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3542">
<skos:prefLabel>kuśnierstwo</skos:prefLabel>
<skos:definition>przemysł futrzarski obejmujący szycie futer, odzieży, artykułów konfekcyjnych i galanterii futrzanej z wyprawionych skór futerkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3548">
<skos:prefLabel>kurowate</skos:prefLabel>
<skos:definition>rząd ptaków, obejmujący m.in. głuszca, pardwę, cietrzewia, kuropatwę, bażanta, przepiórkę, bażanta, indyka i pawia, ptaki niezdolne do dalszych lotów, gnieżdżące się na ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3550">
<skos:prefLabel>galwanizacja</skos:prefLabel>
<skos:definition>powlekanie, głównie przedmiotów metalowych, cienką warstwą innego metalu za pomocą elektrolizy, w celu ochrony przed korozją, otrzymania żądanych właściwości fizycznych lub dla ozdoby (złocenie, srebrzenie, niklowanie, chromowanie itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3553">
<skos:prefLabel>zwierzyna łowna</skos:prefLabel>
<skos:definition>dzikie zwierzęta, w tym ptaki i ryby, uznane przez prawo łowieckie za zwierzęta łowne, będące przedmiotem polowań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3554">
<skos:prefLabel>promieniowanie gamma</skos:prefLabel>
<skos:definition>krótkofalowe promieniowanie elektromagnetyczne emitowane m.in. przy wybuchu bomby atomowej, szkodliwe dla zdrowia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3559">
<skos:prefLabel>ogród</skos:prefLabel>
<skos:definition>teren zajęty pod uprawę roślin ozdobnych lub użytkowych z wyznaczonymi alejami, ścieżkami, grządkami, ozdobiony trawnikami, klombami kwiatów itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3562">
<skos:prefLabel>odpady organiczne z ogrodu</skos:prefLabel>
<skos:definition>naturalny organiczny materiał usuwany z ogrodów i trawników składający się z liści, ściętej trawy, gałęzi, krzewów i pni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3564">
<skos:prefLabel>garrigue</skos:prefLabel>
<skos:definition>zbiorowiska śródziemnomorskich niskich kolczastych krzewów i krzewinek oraz licznych roślin zielnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3566">
<skos:prefLabel>gaz</skos:prefLabel>
<skos:definition>ciało lotne, substancja nie mająca własnego kształtu ani objętości, wykazująca zdolność do samorzutnego rozprężania się, poniżej pewnej temperatury, zwanej krytyczną, przechodząca w stan ciekły; jeden z trzech stanów skupienia materii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3567">
<skos:prefLabel>chromatografia gazowa</skos:prefLabel>
<skos:definition>metoda rozdzielania ciekłych lub gazowych mieszanin związków chemicznych na poszczególne składniki, wykorzystująca różne zdolności adsorbowania i osadzania się składników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3570">
<skos:prefLabel>gazownia</skos:prefLabel>
<skos:definition>zakład przemysłowy wytwarzający gaz świetlny i opałowy przez wysokotemperaturowe odgazowanie węgla, rzadziej przez rozkład paliw płynnych lub konwersję gazu ziemnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3571">
<skos:prefLabel>silnik gazowy</skos:prefLabel>
<skos:definition>silnik wewnętrznego spalania, w którym stosuje się paliwo gazowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3572">
<skos:prefLabel>gazowe zanieczyszczenia powietrza</skos:prefLabel>
<skos:definition>nie kondensujące lub lotne gazy, zwykle zawierające różne związki chemiczne usuwane do atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3577">
<skos:prefLabel>stan gazowy</skos:prefLabel>
<skos:definition>stan materii, w którym wypełnia ona cały pojemnik bez względu na jego wielkość</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3578">
<skos:prefLabel>oczyszczanie gazów</skos:prefLabel>
<skos:definition>gaz jest oczyszczany przed wprowadzeniem do obrotu; zakres oczyszczania zależy od jego jakości oraz od ilości zanieczyszczeń takich jak woda, ditlenek węgla i związki siarki oraz od ostatecznego wykorzystania gazu; ditlenek węgla oraz związki siarki powodują zakwaszenie gazu i mogą zostać usunięte przy użyciu rozpuszczalników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3581">
<skos:prefLabel>gazyfikacja</skos:prefLabel>
<skos:definition>1) proces chemiczny lub termiczny, w wyniku którego substancja przekształcana jest w gaz, 2) wytwarzanie paliw gazowych w wyniku reakcji gorących materiałów zawierających węgiel z powietrzem, parą wodną lub tlene; proces przebiega w wysokiej temperaturze, a jego produkt jest mieszaniną palnych gazów i smoły oraz cząsteczek stałych i pary wodnej; w zależności od metody zgazowania różne są zawartości składników; przed wykorzystaniem gaz powinien zostać oczyszczony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3582">
<skos:prefLabel>skraplanie gazu</skos:prefLabel>
<skos:definition>przekształcenie gazu do fazy ciekłej poprzez schładzanie lub kompresję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3585">
<skos:prefLabel>mieszanka gazu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3586">
<skos:prefLabel>sieć gazowa</skos:prefLabel>
<skos:definition>wzajemnie połączony system rur w celu dystrybucji i zaopatrzenia w gaz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3587">
<skos:prefLabel>gazohol</skos:prefLabel>
<skos:definition>mieszanina od 80% do 90% benzyny oraz od 10% do 20% alkoholu etylowego używana jako paliwo w silnikach wewnętrznego spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3589">
<skos:prefLabel>silnik benzynowy</skos:prefLabel>
<skos:definition>silnik wewnętrznego spalania, w którym stosuje się paliwo w postaci mieszaniny powietrza i par benzyny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3591">
<skos:prefLabel>gazociąg</skos:prefLabel>
<skos:definition>system rur, zbiorników i stacji tłoczących, przesyłający gaz palny (np. ziemny, miejski) na duże odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3593">
<skos:prefLabel>ciepłownia na gaz</skos:prefLabel>
<skos:definition>elektrownia spalająca gaz w odróżnieniu od elektrowni nuklearnych oraz opalanych węglem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3594">
<skos:prefLabel>oczyszczanie gazów</skos:prefLabel>
<skos:definition>usuwanie zanieczyszczeń ze spalin powstających w różnych procesach spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3595">
<skos:prefLabel>odzyskiwanie par</skos:prefLabel>
<skos:definition>urządzenie do zawracania par: np.podczas tankowania pary benzyny są odsysane i zawracane do zbiornika zasobnikowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3596">
<skos:prefLabel>zbiornik gazu</skos:prefLabel>
<skos:definition>duży zbiornik do przechowywania gazu powęglowego i ziemnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3599">
<skos:prefLabel>dostawa gazu</skos:prefLabel>
<skos:definition>zaopatrzenie i magazynowanie gazu opałowego dla różnych użytkowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3600">
<skos:prefLabel>ślimak</skos:prefLabel>
<skos:definition>zwierzę należące do klasy Gastropoda, występujące w licznych gatunkach, charakteryzujące się jedną lub dwiema parami czułków, nogą dobrze rozwiniętą, służącą do pełzania i muszlą najczęściej zwiniętą spiralnie, niekiedy całkowicie uwstecznioną; żyje w morzach, wodach śródlądowych i na lądach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3604">
<skos:prefLabel>gazownia</skos:prefLabel>
<skos:definition>zakład przemysłowy wytwarzający gaz świetlny i opałowy przez wysokotemperaturowe odgazowanie węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3611">
<skos:prefLabel>społeczne problemy płci</skos:prefLabel>
<skos:definition>poglądy, kwestie i argumenty dotyczące podziału możliwości, ról społecznych oraz innych podziałów i różnic między kobietami i mężczyznami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3613">
<skos:prefLabel>bank genów</skos:prefLabel>
<skos:definition>definicja EEA - specjalna przechowalnia nasion lub tkanek wegetatywnych w warunkach niskiej wilgotności i temperatury;przechowywanie materiału wspomaga utrzymanie różnorodności genetycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3614">
<skos:prefLabel>zarządzenie o charakterze administracyjnym</skos:prefLabel>
<skos:definition>zarządzenie władz określające w jaki sposób właściwe organizacje lub instytucje rządowe powinny wdrażać koncepcje, plany, rozporządzenia lub ustawy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3615">
<skos:prefLabel>chemia ogólna</skos:prefLabel>
<skos:definition>nauka zajmująca się badaniem właściwości i budowy różnych substancji oraz badaniem przemian jednych substancji w inne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3617">
<skos:prefLabel>gen</skos:prefLabel>
<skos:definition>podstawowa materialna jednostka dziedziczenia, związana z przekazywaniem poszczególnych cech dziedzicznych organizmu, mająca określone i stałe miejsce w chromosomach jądra komórkowego roślin i zwierząt oraz w genoforach (np. u bakterii)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3620">
<skos:prefLabel>zróżnicowanie genetyczne</skos:prefLabel>
<skos:definition>zmienność osobników w populacjach i populacji w obrębie gatunku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3621">
<skos:prefLabel>skutek genetyczny</skos:prefLabel>
<skos:definition>dziedziczone zmiany genetyczne, głównie mutacje wywołane przez działanie promieniowania jonizującego, określonych substancji chemicznych, herbicydów itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3624">
<skos:prefLabel>inżynieria genetyczna</skos:prefLabel>
<skos:definition>1) techniki produkcji nowych genów oraz modyfikacji struktury chromosomów, mające zastosowanie w rolnictwie i medycynie, 2) kierunkowe tworzenie nowych genów i  modyfikacja genomów przez podstawienie lub dodanie nowego materiału genetycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3625">
<skos:prefLabel>prawodawstwo dotyczące inżynierii genetycznej</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3628">
<skos:prefLabel>informacja genetyczna</skos:prefLabel>
<skos:definition>informacje na temat syntezy białek zawarte w sekwencjach nukleotydów ułożonych w polinukleotydowych łańcuchach DNA</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3630">
<skos:prefLabel>zmiany genetyczne</skos:prefLabel>
<skos:definition>dziedziczone zmiany genetyczne wywołane przez działanie promieniowania jonizującego, określonych substancji chemicznych i spożycie niektórych leków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3632">
<skos:prefLabel>zasoby genów</skos:prefLabel>
<skos:definition>pula genowa zawarta w zasobach organizmów występujących tak naturalnie, jak i hodowanych, które mogą być wykorzystywane gospodarczo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3633">
<skos:prefLabel>genetyka</skos:prefLabel>
<skos:definition>dział biologii, nauka zajmująca się badaniem dziedziczności i zmienności organizmów żywych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3634">
<skos:prefLabel>zmienność genetyczna</skos:prefLabel>
<skos:definition>zmiany cech fenotypowych uwarunkowane przez mutacje lub przemieszczenia materiału genetycznego, albo przez wpływ środowiska na geny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3638">
<skos:prefLabel>geodezja</skos:prefLabel>
<skos:definition>nauka o pomiarach Ziemi, zajmująca się wyznaczaniem kształtu i rozmiarów globu ziemskiego lub jego części, sporządzaniem map i planów geodezyjnych, pomiarami gruntów i obiektów dla celów gospodarczych, technicznych, wojskowych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3640">
<skos:prefLabel>czynnik geogeniczny</skos:prefLabel>
<skos:definition>czynniki wywodzące się od gleby, w przeciwieństwie do czynników antropogenicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3645">
<skos:prefLabel>system informacji geograficznej</skos:prefLabel>
<skos:definition>zorganizowany system składający się z komputera, oprogramowania, danych geograficznych i obsługi, zaprojektowany w celu efektywnego przechowywania, uaktualniania, przetwarzania, analizowania i wyświetlania wszystkich form informacji mających odniesienie geograficzne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3646">
<skos:prefLabel>geografia</skos:prefLabel>
<skos:definition>nauka badająca powłokę Ziemi, jej przestrzenne zróżnicowanie pod względem przyrodniczym i społeczno-gospodarczym oraz związki zachodzące między środowiskiem geograficznym a działalnością społeczeństw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3647">
<skos:prefLabel>katastrofa geologiczna</skos:prefLabel>
<skos:definition>katastrofy spowodowane przez ruchy oraz zniekształcenia skorupy ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3648">
<skos:prefLabel>proces geologiczny</skos:prefLabel>
<skos:definition>dynamiczny przebieg wydarzeń na powierzchni Ziemi w związku z działaniem sił naturalnych, takich jak: siła ciężkości, zmiany temperatury, zamarzanie i tajanie, reakcje chemiczne, wstrząsy sejsmiczne, oraz za sprawą wiatru, wody, lodu i śniegu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3650">
<skos:prefLabel>geologia</skos:prefLabel>
<skos:definition>nauka o budowie i dziejach Ziemi, głównie skorupy ziemskiej oraz o zjawiskach i procesach powodujących jej przeobrażenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3652">
<skos:prefLabel>proces geomorfologiczny</skos:prefLabel>
<skos:definition>współzależności fizyczne i chemiczne między powierzchnią Ziemi a siłami naturalnymi modyfikującymi tę powierzchnię; procesy te są zależne od takich zmiennych czynników środowiska, jak m.in. geologia, klimat, roślinność, wyskość nad powierzchnią morza, a ponadto działania człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3654">
<skos:prefLabel>geomorfologia</skos:prefLabel>
<skos:definition>dział geografii fizycznej, nauka o rzeźbie powierzchni Ziemi oraz o procesach kształtujących tę rzeźbę, zajmująca się opisem form na lądach i na dnie mórz, wyjaśniająca ich powstanie, rozwój i rozmieszczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3655">
<skos:prefLabel>geofizyka</skos:prefLabel>
<skos:definition>nauka o Ziemi jako o ciele fizycznym, zajmująca się wszelkimi zjawiskami i procesami fizycznymi zachodzącymi obecnie i w przeszłości geologicznej we wszystkich geosferach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3657">
<skos:prefLabel>geotechnologia</skos:prefLabel>
<skos:definition>gałąź wiedzy, w skład której wchodzą zagadnienia z geologii, górnictwa, geochemii i technologii chemicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3658">
<skos:prefLabel>energia geotermiczna</skos:prefLabel>
<skos:definition>energia wytwarzana w wyniku czerpania wewnętrznego ciepła ziemi; obecnie jedyne dostępne technologie polegają na wydobywaniu ciepła z hydrotermalnych systemów konwekcyjnych, które przenoszą ciepło z głębszych miejsc na powierzchnię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3661">
<skos:prefLabel>zarazek</skos:prefLabel>
<skos:definition>1) drobnoustrój chorobotwórczy (bakteria, wirus, pierwotniak) wywołujący choroby zakaźne w organizmach ludzi i zwierząt, 2) żywa materia, z której może się rozwinąć organ, część lub cały organizm, tkanka pierwotna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3663">
<skos:prefLabel>kiełkowanie</skos:prefLabel>
<skos:definition>o nasionach, bulwach, roślinach: proces puszczania kiełków, wschodzenia, zapoczątkowania rozwoju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3664">
<skos:prefLabel>plazma zarodkowa</skos:prefLabel>
<skos:definition>materiał dziedziczności przekazywany potomstwu w gametach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3673">
<skos:prefLabel>lodowiec</skos:prefLabel>
<skos:definition>wielka masa lodu powstała na lądzie (powyżej linii wiecznego śniegu) z nagromadzonego i przekrystalizowanego śniegu, znajdująca się w stałym, powolnym ruchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3676">
<skos:prefLabel>glacjologia</skos:prefLabel>
<skos:definition>dział geologii i geografii fizycznej zajmujący się badaniem lodowców (powstawaniem, rozwojem, topnieniem), ich wpływu na rzeźbę powierzchni ziemi, a także warunków klimatycznych oraz rodzaju i ukształtowania podłoża, na którym się tworzą i rozwijają</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3678">
<skos:prefLabel>szkło</skos:prefLabel>
<skos:definition>substancja przezroczysta, bezpostaciowa, nierozpuszczalna, stanowiąca mieszaninę zakrzepłych krzemianów, będąca złym przewodnikiem ciepła i elektryczności, łatwo dająca się formować w wyższej temperaturze; powszechnie używana do wyrobu szyb, naczyń, opakowań, przedmiotów artystycznych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3681">
<skos:prefLabel>przemysł szklarski</skos:prefLabel>
<skos:definition>przemysł wytwarzający wyroby ze szkła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3686">
<skos:prefLabel>aspekt globalny</skos:prefLabel>
<skos:definition>różne aspekty dotyczące całego świata w sensie ścisłego powiązania przez nowoczesną telekomunikację i wzajemnie współzależne pod względem gospodarczym, społecznym i politycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3687">
<skos:prefLabel>konwencja globalna</skos:prefLabel>
<skos:definition>umowa międzynarodowa dotycząca spraw politycznych, gospodarczych, ekologicznych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3688">
<skos:prefLabel>Światowy Fundusz na rzecz Środowiska</skos:prefLabel>
<skos:definition>organizacja międzynarodowa powołana w 1990 r. w celu udzielania praktycznej pomocy rządom w ich ich inicjatywach i przedsięwzięciach na rzecz środowiska; zarządzany przez Bank Światowy, który ma 2/3 udziałów oraz UNDP Program Rozwoju ONZ, który kontroluje 1/3 środków finansowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3690">
<skos:prefLabel>model globalny</skos:prefLabel>
<skos:definition>modele symulujące różne aspekty rzeczywistości, które można stosować na poziomie globalnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3692">
<skos:prefLabel>ocieplenie globalne</skos:prefLabel>
<skos:definition>zmiany temperatury na powierzchni Ziemi w wyniku efektu szklarniowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3694">
<skos:prefLabel>słownik</skos:prefLabel>
<skos:definition>zbiór wyrazów ułożonych i opracowanych według pewnej zasady, zwykle alfabetycznie, najczęściej objaśnianych pod względem znaczeniowym i ilustrowanych przykładami użycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3695">
<skos:prefLabel>klej</skos:prefLabel>
<skos:definition>lepka substancja organiczna lub nieorganiczna służąca do łączenia powierzchni jednakowych lub różnych materiałów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3698">
<skos:prefLabel>cel gospodarczy przedsiębiorstwa</skos:prefLabel>
<skos:definition>cele odnoszące się do gospodarki zakładu wytwórczego związego z daną dziedziną przemysłu i rozwojem produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3701">
<skos:prefLabel>golf</skos:prefLabel>
<skos:definition>gra sportowa polegająca na podbijaniu specjalnym, zakrzywionym na końcu kijem małej piłeczki tak, by trafiała kolejno do każdego z dołków rozmieszczonych na boisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3705">
<skos:prefLabel>prawidłowa gospodarka</skos:prefLabel>
<skos:definition>kompetentny, fachowy sposób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3712">
<skos:prefLabel>rządowy organ doradczy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3714">
<skos:prefLabel>budynek rządowy</skos:prefLabel>
<skos:definition>budynki biurowe przeznaczone dla głównych urzędów państwowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3715">
<skos:prefLabel>rząd (gabinet ministrów)</skos:prefLabel>
<skos:definition>naczelny wykonawczy i zarządzający organ państwa, kierujący całym aparatem administracyjnym i zapewniający jednolitość jego działania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3716">
<skos:prefLabel>wydatki z budżetu państwa na środowisko</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3717">
<skos:prefLabel>odpowiedzialność rządu za długi publiczne</skos:prefLabel>
<skos:definition>odpowiedzialność za transakcje przeprowadzone w przeszłości, których skutki należy uregulować i zrefundować należności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3718">
<skos:prefLabel>polityka rządu</skos:prefLabel>
<skos:definition>działalność rządu w dziedzinach społecznej, gospodarczej, kulturalnej, wojskowej i innych, dotycząca spraw wewnętrznych państwa lub stosunków z innymi krajami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3721">
<skos:prefLabel>zboże</skos:prefLabel>
<skos:definition>rośliny uprawne z rodziny traw, a także gryka z rodziny rdestowatych, dostarczające ziarna i słomy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3729">
<skos:prefLabel>trawy</skos:prefLabel>
<skos:definition>przedstawiciele bardzo licznej i bardzo rozpowszechnionej rodziny roślin (Trawy) z klasy Jednoliścienne (Monocotyledoneae) obejmującej głównie gatunki zielne, tylko kilka gatunków wykształca zdrewniałe łodygi, łodygi mają węzły, tzw. kolanka, długie wąskie liście wyrastają z węzłów, kwiaty są niepozorne, zebrane w kłosy, owocem jest ziarniak; trawy są najważniejszym zasobem roślinnym z uwagi na produkcję żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3730">
<skos:prefLabel>pożary traw</skos:prefLabel>
<skos:definition>pożary niszczące zbiorowiska roślin trawiastych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3732">
<skos:prefLabel>świerszcz</skos:prefLabel>
<skos:definition>Grylloidea, nadrodzina owadów prostoskrzydłych, obejmująca około tysiąca gatunków zamieszkujących głównie kraje międzyzwrotnikowe i podzwrotnikowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3733">
<skos:prefLabel>teren trawiasty</skos:prefLabel>
<skos:definition>formacje trawiaste zajmują prawie jedną piątą powierzchni lądów na Ziemi, są wśród nich sawanny, prerie Ameryki Północnej, stepy rosyjskie i środkowo-azjatyckie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3734">
<skos:prefLabel>ekosystem trawiasty</skos:prefLabel>
<skos:definition>układy zbiorowisk w obrębie biomów roślinności trawiastej wraz z ich nieożywionym środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3738">
<skos:prefLabel>żwir</skos:prefLabel>
<skos:definition>luźna skała osadowa, składająca się z obtoczonych okruchów skał, rzadziej minerałów, używana do wyrobu betonu oraz jako materiał drogowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3739">
<skos:prefLabel>wydobycie żwiru</skos:prefLabel>
<skos:definition>pozyskiwanie z ziemi mieszaniny gruboziarnistego piasku, małych otoczaków lub kamieni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3742">
<skos:prefLabel>żwirownia</skos:prefLabel>
<skos:definition>kopalnia odkrywkowa żwiru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3748">
<skos:prefLabel>wypas</skos:prefLabel>
<skos:definition>żywienie zwierząt na pastwisku lub na innych użytkach zielonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3754">
<skos:prefLabel>pas zieleni ochronnej</skos:prefLabel>
<skos:definition>pas zieleni niezabudowanej majacy role klimatotwórcze, ochronne, służący jako teren rekreacyjny itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3755">
<skos:prefLabel>ekologiczna zabudowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3756">
<skos:prefLabel>korytarz ekologiczny</skos:prefLabel>
<skos:definition>szlaki, którymi wędrują zwierzęta o dużym zasiegu terytorialialnym, służące jako kanały wymiany materiału genetycznego oraz rozprzestrzeniania się populacji roślinnych i zwierzęcych, co pozwala im chronić się przed niekorzystnymi zmianami środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3757">
<skos:prefLabel>podatek za korzystanie ze środowiska</skos:prefLabel>
<skos:definition>podatek pobierany przez władze w celu pokrycia kosztów oczyszczania, środowiska, zapobiegania zanieczyszczeniom oraz wdrażania programów edukacyjnych wspierających sprawę zachowania zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3760">
<skos:prefLabel>uprawy szklarniowe</skos:prefLabel>
<skos:definition>uprawy roślin w warunkach kontrolowanych pod szkłem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3761">
<skos:prefLabel>efekt szklarniowy</skos:prefLabel>
<skos:definition>ocieplenie atmosfery ziemskiej w wyniku wzrostu stężenia gazów atmosferycznych, takich jak para wodna i dwutlenek węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3763">
<skos:prefLabel>gazy szklarniowe</skos:prefLabel>
<skos:definition>zbiorcza nazwa składników atmosfery, które wpływają na efekt szklarniowy; są to dwutlenek węgla, metan, tlenki azotu, ozon, freony i para wodna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3766">
<skos:prefLabel>nawóz zielony</skos:prefLabel>
<skos:definition>świeża masa roślinna wprowadzana do gleby jako nawóz organiczny, np. zaorany łubin, seradela</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3768">
<skos:prefLabel>zielona rewolucja</skos:prefLabel>
<skos:definition>termin odnosi się do powszechnego wprowadzenia do rolnictwa wysokoplennych odmian pszenicy, kukurydzy i ryżu w latach 1960 i wczesnych 1970; formalnie akcja ta znana jest jako Światowy Program Rozwoju Rolnictwa po Kongresie FAO z 1963 r.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3770">
<skos:prefLabel>zieleniec</skos:prefLabel>
<skos:definition>obszar z pokrywą roślinną na terenach zurbanizowanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3771">
<skos:prefLabel>warzywa liściaste</skos:prefLabel>
<skos:definition>roślina zielna, której jadalna część liściowa spożywana na surowo jest ważnym źródłem witamin i mikroelementów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3774">
<skos:prefLabel>rozdrabnianie</skos:prefLabel>
<skos:definition>proces dzielenia na drobne części, rozkruszania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3777">
<skos:prefLabel>produkt narodowy brutto</skos:prefLabel>
<skos:definition>produkt narodowy brutto jest to produkt krajowy brutto powiększony o dochody krajowych czynników wytwórczych funkcjonujących za granicą danego kraju i pomniejszony o dochody zagranicznych czynnikow wytwórczych funkcjonujących w danym kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3780">
<skos:prefLabel>woda gruntowa</skos:prefLabel>
<skos:definition>woda znajdująca się w porach i szczelinach skały i gleby pod powierzchnią i nad nieprzepuszczalnym materiałem; podlega grawitacyjnemu przemieszczaniu się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3781">
<skos:prefLabel>pobór wód podziemnych</skos:prefLabel>
<skos:definition>nadmierny pobór wód podziemnych prowadzący do obniżenia poziomu wód gruntowych, zagrażający wyczerpaniem się zasobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3784">
<skos:prefLabel>zanieczyszczenie wód gruntowych</skos:prefLabel>
<skos:definition>zanieczyszczenie wód podziemnych przez substancje przesiąkające, takie jak związki nieorganiczne (chlorki, azotany, metale ciężkie itp.), związki organiczne wytworzone sztucznie (pestycydy, nawozy itp.) oraz patogeny (bakterie, pasożyty itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3785">
<skos:prefLabel>ochrona wód gruntowych</skos:prefLabel>
<skos:definition>działania zapobiegawcze, procedury i urządzenia stosowane w celu ograniczenia zanieczyszczeń zasobów słodkich wód podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3788">
<skos:prefLabel>zachowania grupowe</skos:prefLabel>
<skos:definition>dający się obserwować sposób zachowań, jaki przejawiają osoby jako członkowie grup lub grupy osób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3800">
<skos:prefLabel>zatoka</skos:prefLabel>
<skos:definition>część morza lub jeziora wcinająca się w ląd</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3805">
<skos:prefLabel>nagozalążkowe</skos:prefLabel>
<skos:definition>Gymnospermae, podgromada roślin nasiennych obejmująca około 600 gatunków drzewiastych występujących na całej kuli ziemskiej; zalążki (a potem nasiona) mają nie osłonięte, wyrastające na płaskich łuskach (owocolistkach) zebranych przeważnie w szyszki; obejmuje taksony roślin żyjących obecnie (iglaste, sagowce, miłorzębowe i gniotowce) oraz juz wymarłych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3806">
<skos:prefLabel>gips</skos:prefLabel>
<skos:definition>minerał, uwodniony siarczan wapnia, bezbarwny, biały, szary lub żółtawy, powstający wskutek krystalizacji z wody mórz lub słonych jezior; wypalony i zmielony stosowany do wyrobu materiałów budowlanych, w przemyśle chemicznym, w chirurgii, dentystyce, modelarstwie oraz jako materiał rzeźbiarski i dekoracyjny (alabaster)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3808">
<skos:prefLabel>siedlisko</skos:prefLabel>
<skos:definition>całokształt warunków glebowych, wodnych, klimatycznych, ukształtowanie powierzchni itp. w miejscu występowania danego gatunku roślin, zwierząt lub ich zespołów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3810">
<skos:prefLabel>hematologia</skos:prefLabel>
<skos:definition>nauka o krwi i jej schorzeniach u ludzi i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3812">
<skos:prefLabel>grad</skos:prefLabel>
<skos:definition>opad atmosferyczny w postaci okrągłych, różnej wielkości bryłek lodu (gradzin); tworzy się w silnie rozbudowanych kłębiastych chmurach deszczowych; pada głównie w ciepłej porze roku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3815">
<skos:prefLabel>półokres rozpadu</skos:prefLabel>
<skos:definition>czas wymagany do rozpadu połowy ilości radioaktywnych atomów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3817">
<skos:prefLabel>haloform</skos:prefLabel>
<skos:definition>fluoro-, chloro-, bromo-alkany i jodo-alkany zawierające trzy atomy chlorowców, np. jodoform CHJ3; powstają z ketonu w wyniku reakcji haloformowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3819">
<skos:prefLabel>bifenyle halogenowane</skos:prefLabel>
<skos:definition>halogenowe pochodne bisfenylu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3820">
<skos:prefLabel>węglowodory halogenowane</skos:prefLabel>
<skos:definition>grupa halogenowych pochodnych węglowodorów; grupa zawiera związki monohalogenowe (halogenki alkilowe lub arylowe) oraz polihalogenowe.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3821">
<skos:prefLabel>fenole halogenowane</skos:prefLabel>
<skos:definition>halogenowe pochodne fenolu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3822">
<skos:prefLabel>zanieczyszczenia związkami halogenowanym</skos:prefLabel>
<skos:definition>związki organiczne, do których przyłączono jeden z pięciu pierwiastków halogenowych (astat, brom, chlor, fluor i jod); niektóre z nich przyczyniają się do zmniejszenia stężenia ozonu w stratosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3823">
<skos:prefLabel>terpenyle halogenowane</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3828">
<skos:prefLabel>przemysł rzemieślniczy</skos:prefLabel>
<skos:definition>drobna wytwórczość o charakterze przemysłowym, obejmująca wykonywanie i naprawianie przedmiotów codziennego użytku prostymi narzędziami i maszynami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3830">
<skos:prefLabel>rzemiosło</skos:prefLabel>
<skos:definition>umiejętność wykonywania przedmiotów codziennego użytku prostymi narzędziami i maszynami, fach rzemieślnika wykonującego takie przedmioty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3834">
<skos:prefLabel>port</skos:prefLabel>
<skos:definition>przybrzeżny obszar wodny wraz z przylegającym do niego pasem lądu, wyposażone w urządzenia zapewniające bezpieczny postój i obsługę statków, przeładunek towarów i obsługę ruchu pasażerskiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3837">
<skos:prefLabel>wytrzymałość</skos:prefLabel>
<skos:definition>wytrzymałość ciała stałego na rozciąganie, ściskanie, zginanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3838">
<skos:prefLabel>odpady kłopotliwe</skos:prefLabel>
<skos:definition>niepotrzebny materiał, często niebezpieczny lub w dużych ilościach nie nadający się do składowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3842">
<skos:prefLabel>zanieczyszczenia od ścieków</skos:prefLabel>
<skos:definition>pogorszenie jakości przedziału środowiskowego z powodu wprowadzenia zużytej lub wykorzystanej wody pochodzącej z gospodarstw domowych lub przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3847">
<skos:prefLabel>harmonizacja prawa</skos:prefLabel>
<skos:definition>proces zmiany i ujednolicania systemu prawnego w dwu lub więcej państwach w celu ułatwienia transgranicznego wdrożenia i egzekucji praw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3849">
<skos:prefLabel>żniwo</skos:prefLabel>
<skos:definition>żęcie zbóż, czas żęcia zbóż, zbierania plonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3852">
<skos:prefLabel>niebezpieczeństwo dla zdrowia</skos:prefLabel>
<skos:definition>czynnik natury fizycznej lub chemicznej mogący szkodzić ludziom, majątkowi, zwierzętom, roślinom lub innym zasobom przyrodniczym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3853">
<skos:prefLabel>zagrożenie powodowane przez substancje zanieczyszczające</skos:prefLabel>
<skos:definition>ryzyko lub niebezpieczeństwo dla zdrowia ludzi, majątku lub środowiska spowodowane przez wprowadzenie niebezpiecznych substancji do ekosystemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3854">
<skos:prefLabel>substancja niebezpieczna</skos:prefLabel>
<skos:definition>materiał stwarzający niebezpieczeństwo dla zdrowia ludzi i/lub środowiska; typowe substancje niebezpieczne należą do grupy toksycznych, powodujących korozję, zapalnych, wybuchowych lub chemicznie reaktywnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3855">
<skos:prefLabel>prawodawstwo dotyczące substancji niebezpiecznych</skos:prefLabel>
<skos:definition>regulacje prawne dotyczące substancji niebezpiecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3857">
<skos:prefLabel>usuwanie odpadów niebezpiecznych</skos:prefLabel>
<skos:definition>obiekty,w których niebezpieczne odpady są umieszczane na ziemi lub pod ziemią; właściwie zaprojektowane i eksploatowane składowiska są izolowane w celu uniemożliwienia odpływu odcieków oraz zawierają instalację do odbierania zanieczyszczonej wody ze spływu powierzchniowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3858">
<skos:prefLabel>odpady niebezpieczne</skos:prefLabel>
<skos:definition>odpad lub mieszanina odpadów mogących spowodować szkody dla zdrowia ludzi, żywych organizmów lub środowiska; odpady niebezpieczne zwykle wymagają specjalnego traktowania oraz usuwania, odpowiednie metody określone są przez przepisy prawne poszczególnych państw oraz przepisy prawa miedzynarodowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3859">
<skos:prefLabel>niebezpieczny materiał</skos:prefLabel>
<skos:definition>trucizna, czynnik korozyjny, substancja palna, wybuchowa, związek radioaktywny lub jakikolwiek inny materiał stwarzający zagrożenie dla zdrowia ludzi w wypadku niewłaściwego obchodzenia się z nim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3860">
<skos:prefLabel>zamglenie</skos:prefLabel>
<skos:definition>zawiesina mikroskopijnych kropelek wody w przyziemnej warstwie powietrza, ograniczająca widzialność poziomą od 1 do 10 km</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3864">
<skos:prefLabel>przylądek</skos:prefLabel>
<skos:definition>niewielki występ lądu szerszy u nasady, wysunięty w morze lub jezioro</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3865">
<skos:prefLabel>zdrowie</skos:prefLabel>
<skos:definition>stan żywego organizmu, w którym wszystkie funkcje przebiegają prawidłowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3866">
<skos:prefLabel>troska o zdrowie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3869">
<skos:prefLabel>zależność stan środowiska-zdrowie ludzi</skos:prefLabel>
<skos:definition>związek między jakością środowiska a stanem zdrowotnym ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3871">
<skos:prefLabel>infrastruktura związana z ochroną zdrowia</skos:prefLabel>
<skos:definition>ośrodki usług medycznych, stomatologicznych, chirurgicznych lub pielęgniarskich dla ludzi lub zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3872">
<skos:prefLabel>niebezpieczeństwo dla zdrowia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3874">
<skos:prefLabel>prawodawstwo dotyczące zdrowia</skos:prefLabel>
<skos:definition>proces stanowienia prawa dotyczącego ochrony zdrowia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3877">
<skos:prefLabel>ochrona zdrowia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3878">
<skos:prefLabel>ustawodawstwo dotyczące zdrowia</skos:prefLabel>
<skos:definition>ogół ustaw, przepisów prawnych dotyczących ochrony zdrowia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3880">
<skos:prefLabel>aspekty zdrowotne biotechnologii</skos:prefLabel>
<skos:definition>biotechnologie związane z ochroną zdrowia skupiają się na farmaceutykach opartych na wielkocząsteczkowych białkach, inżynierii genetycznej, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3881">
<skos:prefLabel>służba zdrowia</skos:prefLabel>
<skos:definition>działalność, praca instytucji użyteczności publicznej mająca na celu zapewnienie obywatelom opieki zdrowotnej i sanitarno-epidemiologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3884">
<skos:prefLabel>uszkodzenie słuchu</skos:prefLabel>
<skos:definition>upośledzenie zdolności słyszenia lub inne odchylenia od normy słuchu,  całkowita jego utrata albo uszkodzenie funkcji ucha spowodowane przez chorobę lub uraz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3885">
<skos:prefLabel>procedura rozpraw publicznych</skos:prefLabel>
<skos:definition>określony prawem sposób prowadzenia wstępnych badań przez sędziego w celu ustalenia, czy i jaki rodzaj postępowania sądowego należy wszcząć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3886">
<skos:prefLabel>ochrona słuchu</skos:prefLabel>
<skos:definition>ogół środków i urządzeń mających na celu ochronę słuchu pracowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3887">
<skos:prefLabel>słuch (zmysł)</skos:prefLabel>
<skos:definition>zmysł odbierający wrażenia dźwiękowe, działające na receptory w ślimaku ucha wewnętrznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3890">
<skos:prefLabel>ciepło (fizyka)</skos:prefLabel>
<skos:definition>jedna z form przekazywania energii, zwykle związana z procesem ogrzewania lub chłodzenia; energia kinetyczna cząsteczek lub atomów ciała oraz energia potencjalna ich wzajemnych oddziaływań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3891">
<skos:prefLabel>elektrociepłownia</skos:prefLabel>
<skos:definition>zakład energetyczny spełniający jednocześnie zadanie elektrowni i ciepłowni, wytwarzający energię elektryczną i ciepło wykorzystywane do ogrzewania oraz do celów technologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3892">
<skos:prefLabel>grzejnik</skos:prefLabel>
<skos:definition>urządzenie wypromieniowujące energię cieplną powstającą w nim w wyniku przetwarzania innych rodzajów energii (głównie elektrycznej i chemicznej) lub doprowadzania z zewnątrz za pośrednictwem np. pary wodnej, gorącej wody, gorącego powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3893">
<skos:prefLabel>wrzosowisko</skos:prefLabel>
<skos:definition>teren porosły wiecznie zielonymi krzewinkami, głównie wrzosem (Calluna) i wrzoścem (Erica) ; występuje w klimacie umiarkowanym i wilgotnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3894">
<skos:prefLabel>ogrzewanie</skos:prefLabel>
<skos:definition>instalacja grzewcza doprowadzająca ciepło pochodzące z jednego źródła (np. z ciepłowni) do wielu grzejników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3900">
<skos:prefLabel>ciepłownia</skos:prefLabel>
<skos:definition>zakład energetyczny wytwarzający energię cieplną w postaci gorącej wody lub pary wodnej, do urządzeń przemysłowych oraz do centralnego ogrzewania budynków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3904">
<skos:prefLabel>pompa cieplna</skos:prefLabel>
<skos:definition>urządzenie przenoszące ciepło z chłodniejszego do cieplejszego zbiornika, wydatkując w procesie energię mechaniczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3906">
<skos:prefLabel>akumulacja ciepła</skos:prefLabel>
<skos:definition>gromadzenie ciepła podczas okresu niskiego zużycia i oddawanie go w okresie szczytowym obciążenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3907">
<skos:prefLabel>zaopatrzenie w ciepło</skos:prefLabel>
<skos:definition>zaopatrzenie w paliwo, węgiel i inne materiały grzewcze lub dostarczenie odpowiedniej ilości ciepła do różnych użytkowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3911">
<skos:prefLabel>ruch drogowy pojazdów o dużym tonażu</skos:prefLabel>
<skos:definition>ruch drogowy pojazdów przeznaczonych do przewozu ładunków o dużym tonażu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3914">
<skos:prefLabel>ładunek metali ciężkich</skos:prefLabel>
<skos:definition>wielkość czynnika stresu oddziałującego na ekosystem w wyniku zanieczyszczenia metalami ciężkimi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3915">
<skos:prefLabel>metal ciężki</skos:prefLabel>
<skos:definition>metale nieżelazne oraz ich stopy, o ciężarze właściwym większym niż 4,5 G/cm3 (np. miedź, cynk, ołów, złoto, srebro, platyna)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3917">
<skos:prefLabel>żywopłot</skos:prefLabel>
<skos:definition>ogrodzenie, płot utworzone z gęsto posadzonych i często przycinanych drzew lub krzewów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3933">
<skos:prefLabel>herbicyd</skos:prefLabel>
<skos:definition>substancje chemiczne używane do niszczenia chwastów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3935">
<skos:prefLabel>gatunek roślinożerny</skos:prefLabel>
<skos:definition>zwierzę, którego głównym pożywieniem jest pokarm roślinny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3937">
<skos:prefLabel>związki heterocykliczne</skos:prefLabel>
<skos:definition>związki w których sktruktura pierścienia jest kombinacją więcej niż jednego rodzaju atomów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3945">
<skos:prefLabel>kształcenie na poziomie wyższym</skos:prefLabel>
<skos:definition>kształcenie studentów do poziomu pracowników naukowych i wyżej kwalifikowanych pracowników w poszczególnych zawodach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3947">
<skos:prefLabel>ekosystem górski</skos:prefLabel>
<skos:definition>układy zbiorowisk roślinnych i zwierzęcych wraz z ich nieożywionym siedliskiem, spotykane na terenach wyniesionych względnie wysoko nad poziom morza, gdzie panują warunki wysokogórskie, obniżone ciśnienie powietrza, niższe temperatury, zmniejszona ilość tlenu oraz większe nasłonecznienie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3949">
<skos:prefLabel>wysoka góra</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3953">
<skos:prefLabel>żywność o wysokiej zawartości białka</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3954">
<skos:prefLabel>wysokościowiec</skos:prefLabel>
<skos:definition>budynek wysoki, wielopiętrowy z windami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3955">
<skos:prefLabel>linia superszybkiej koleji</skos:prefLabel>
<skos:definition>linie kolejowe, po których kursują pociągi z szybkością powyżej 200 km/godzinię; na terenach w obrębie których obowiązują ograniczenia są to także linie kolejowe, po których pociągi jadą z szybkością równą 200 km/godzinę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3956">
<skos:prefLabel>superszybka kolej</skos:prefLabel>
<skos:definition>pociągi o maksymalnej szybkości podróżnej 300 km/godzinę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3958">
<skos:prefLabel>maksymalny poziom przypływu</skos:prefLabel>
<skos:definition>najwyższy poziom przypływu wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3960">
<skos:prefLabel>linia wysokiego napięcia</skos:prefLabel>
<skos:definition>przewody wysokiego napięcia zainstalowane na słupach (metalowych, żelbetowych lub drewnianych), służące do przesyłania energii elektrycznej z miejsca wytwarzania (elektrowni) do miejsc rozdziału (stacji transformatorowo-rozdzielczych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3963">
<skos:prefLabel>autostrada</skos:prefLabel>
<skos:definition>szeroka i długa arteria komunikacyjna łącząca ważne ośrodki danego terenu, zwykle omijająca miasta, przeznaczona i dostosowana do szybkiego ruchu samochodowego; ma dwie wielotorowe jezdnie, osobne dla obu kierunków ruchu, oraz kilkupoziomowe skrzyżowania z innymi drogami i bezkolizyjne wjazdy i zjazdy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3966">
<skos:prefLabel>wzgórze</skos:prefLabel>
<skos:definition>nieduże, łagodne wzniesienie terenu, niewysoka góra</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3971">
<skos:prefLabel>proces ewolucyjny</skos:prefLabel>
<skos:definition>proces przeobrażeń, zmian, przechodzenia do stanów bardziej złożonych lub doskonalszych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3974">
<skos:prefLabel>zabytek</skos:prefLabel>
<skos:definition>obiekt lub zespół obiektów stanowiący świadectwo epoki lub zdarzenia, mający wartość historyczną, naukową, kulturalną, artystyczną i podlegający ochronie prawnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3975">
<skos:prefLabel>badania historyczne</skos:prefLabel>
<skos:definition>badania ujmujące fakty, wydarzenia z punktu widzenia ich rozwoju; traktujące o dziejach rozwoju życia społecznego lub przyrody, mające za przedmiot te dzieje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3977">
<skos:prefLabel>obiekt zabytkowy</skos:prefLabel>
<skos:definition>obiekt lub zespół obiektów stanowiący świadectwo epoki lub zdarzenia, mający wartość historyczną, naukową, kulturalną, artystyczną i podlegający ochronie prawnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3983">
<skos:prefLabel>historia</skos:prefLabel>
<skos:definition>opisane dzieje, proces rozwoju życia społecznego lub przyrody, przebieg wydarzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3984">
<skos:prefLabel>infrastruktura turystyczna</skos:prefLabel>
<skos:definition>teren zagospodarowany jako kamping z miejscami noclegowymi i urządzeniami rekreacyjnymi dla wczasowiczów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3986">
<skos:prefLabel>wakacje</skos:prefLabel>
<skos:definition>1) okres przerwy w pracy, nauce przeznaczony na wypoczynek i rekreację, 2) prawnie lub zwyczajowo ustalony dzień świąteczny, wolny od pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3995">
<skos:prefLabel>koń</skos:prefLabel>
<skos:definition>Equus caballus, ssak nieparzystokopytny z rodziny koniowatych, roślinożerny; hodowany jako zwierzę domowe, pociągowe lub wierzchowe; żyje także w stanie dzikim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3998">
<skos:prefLabel>ogrodnictwo</skos:prefLabel>
<skos:definition>gałąź gospodarki rolnej obejmująca warzywnictwo, sadownictwo oraz uprawę i hodowlę roślin ozdobnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/3999">
<skos:prefLabel>szpital</skos:prefLabel>
<skos:definition>zakład lecznictwa zamkniętego służący badaniu i leczeniu chorych wymagających stałej opieki lekarskiej i pielęgniarskiej, odpowiednich zabiegów leczniczych i diagnostycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4002">
<skos:prefLabel>odpady szpitalne</skos:prefLabel>
<skos:definition>odpady stałe, biologiczne oraz inne powstające w szpitalach i po usunięciu nie przeznaczone do dalszego wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4008">
<skos:prefLabel>przemysł hotelarski</skos:prefLabel>
<skos:definition>prowadzenie przedsiębiorstwa hotelowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4009">
<skos:prefLabel>gorąca woda</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4015">
<skos:prefLabel>gospodarstwo domowe</skos:prefLabel>
<skos:definition>grupa osób zamieszkujących wspólnie w jednym mieszkaniu, mająca wspólny budżet, dzieląca się dochodami, itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4016">
<skos:prefLabel>środki chemiczne w gospodarstwie domowym</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4018">
<skos:prefLabel>wyroby gospodarstwa domowego</skos:prefLabel>
<skos:definition>wyroby niezbędne w gospodarstwie domowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4022">
<skos:prefLabel>budownictwo mieszkaniowe</skos:prefLabel>
<skos:definition>1) zespół mieszkaniowy wraz z infrastrukturą usługową, 2) miejsce zamieszkania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4027">
<skos:prefLabel>zagęszczenie mieszkań</skos:prefLabel>
<skos:definition>liczba mieszkań lub mieszkańców przypadająca na określony teren</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4029">
<skos:prefLabel>finansowanie budownictwa mieszkaniowego</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4030">
<skos:prefLabel>poprawa stanu osiedli mieszkaniowych</skos:prefLabel>
<skos:definition>rozbudowa, remont lokali mieszkalnych w celu zwiększenia ich wartości estetycznych, funkcjonalnych lub finansowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4032">
<skos:prefLabel>prawodawstwo dotyczące budownictwa mieszkaniowego</skos:prefLabel>
<skos:definition>ogół ustaw, przepisów prawnych dotyczących budownictwa mieszkaniowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4034">
<skos:prefLabel>potrzeby mieszkaniowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4037">
<skos:prefLabel>program mieszkaniowy</skos:prefLabel>
<skos:definition>planowy system programów, usług i działań ukierunkowany na wspomaganie pojedynczych osób oraz rodzin potrzebujących mieszkań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4039">
<skos:prefLabel>standard jakości budynków mieszkalnych</skos:prefLabel>
<skos:definition>norma lub miara stosowana w przypadkach prawnych, odzwierciedlająca stopień lub poziom doskonałości w budowie, konserwacji, funkcjonowaniu, zamieszkaniu, wykorzystaniu lub wyglądzie jednostki mieszkalnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4040">
<skos:prefLabel>restauracja budowli</skos:prefLabel>
<skos:definition>przywrócenie dawnej wartości i wyglądu obiektom architektonicznym, które z jakichś powodów (np. wskutek zaniedbania) straciły</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4045">
<skos:prefLabel>biologia człowieka</skos:prefLabel>
<skos:definition>nauka o życiu człowieka, jego przejawach i właściwościach, o jego powstaniu i rozwoju na Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4046">
<skos:prefLabel>choroba człowieka</skos:prefLabel>
<skos:definition>proces patologiczny objawiający się zaburzeniem czynności ludzkiego organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4047">
<skos:prefLabel>ekologia człowieka</skos:prefLabel>
<skos:definition>badania rozwoju, rozmieszczenia i organizacji zbiorowisk ludzkich w aspekcie ich stosunków z innymi ludźmi, z innymi gatunkami, a także z otaczającym je środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4049">
<skos:prefLabel>ekspozycja człowieka na działanie zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4051">
<skos:prefLabel>zdrowie człowieka</skos:prefLabel>
<skos:definition>stan organizmu człowieka, w którym wszystkie funkcje przebiegają prawidłowo; pełna sprawność i dobre samopoczucie fizyczne i psychiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4052">
<skos:prefLabel>katastrofa wywołana przez człowieka</skos:prefLabel>
<skos:definition>gwałtowna, nagła i destruktywna zmiana w środowisku spowodowana przez ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4054">
<skos:prefLabel>patologia człowieka</skos:prefLabel>
<skos:definition>nauka o chorobach człowieka, ich przyczynach, formach, objawach i skutkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4055">
<skos:prefLabel>fizjologia człowieka</skos:prefLabel>
<skos:definition>nauka o czynnościach organizmu człowieka, jego narządów, tkanek, komórek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4056">
<skos:prefLabel>ludność</skos:prefLabel>
<skos:definition>grupa ludzi mających wspólne cechy i zamieszkujących określony obszar</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4060">
<skos:prefLabel>prawa człowieka</skos:prefLabel>
<skos:definition>prawo człowieka do wolności i sprawiedliwości itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4062">
<skos:prefLabel>osadnictwo</skos:prefLabel>
<skos:definition>miasta, wsie lub inne skupiska ludzkie na określonym obszarze; siedliska ludzkie związane są z problemami środowiska, zanieczyszczeń i jakości życia, rozwiązywanymi poprzez planowanie i zarządzanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4065">
<skos:prefLabel>gospodarka osadnicza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4070">
<skos:prefLabel>próchnica</skos:prefLabel>
<skos:definition>materia organiczna w różnym stopniu rozkładu stanowiąca część składową gleb, zwiększa ona ich żyzność; dostarcza większości składników mineralnych potrzebnych do życia roślin oraz poprawia strukturę gleby i retencjonuje wodę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4072">
<skos:prefLabel>polowanie</skos:prefLabel>
<skos:definition>tropienie, chwytanie i zabijanie zwierzyny łownej traktowane jako sport</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4073">
<skos:prefLabel>rezerwat łowiecki</skos:prefLabel>
<skos:definition>obszar, na którym dozwolone jest tropienie, chwytanie lub zabijanie zwierzyny łownej lub dzikich zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4077">
<skos:prefLabel>huragan</skos:prefLabel>
<skos:definition>niezwykle gwałtowny, porywisty wiatr o średniej prędkości 120 km/godz.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4080">
<skos:prefLabel>krzyżowanie</skos:prefLabel>
<skos:definition>kojarzenie płciowe dwóch genetycznie różnych organizmów, np. ras, odmian, czy też form różniących się tylko kilkoma genami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4082">
<skos:prefLabel>budowla hydrotechniczna</skos:prefLabel>
<skos:definition>budowla służąca ukierunkowaniu płynącej wody lub utrzymywaniu ciężaru i ciśnienia masy wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4084">
<skos:prefLabel>inżynieria hydrotechniczna</skos:prefLabel>
<skos:definition>dział nauki i techniki zajmujący się budową i unowocześnianiem urządzeń i konstrukcji hydrotechnicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4085">
<skos:prefLabel>hydraulika</skos:prefLabel>
<skos:definition>nauka badająca doświadczalnie prawa równowagi i ruchu cieczy, korygująca teoretyczne wzory hydromechaniki oraz tworząca własne, oparte na doświadczeniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4088">
<skos:prefLabel>hydrobiologia</skos:prefLabel>
<skos:definition>dział biologii zajmujący się organizmami żyjącymi w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4091">
<skos:prefLabel>węglowodory</skos:prefLabel>
<skos:definition>organiczne związki węgla z wodorem występujące w postaci gazów, cieczy i ciał stałych, otrzymywane z ropy naftowej, gazu ziemnego oraz z węgla kamiennego i brunatnego; stosowane jako paliwo, używane jako opał oraz jako podstawowy surowiec w przemyśle organicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4094">
<skos:prefLabel>kwas solny</skos:prefLabel>
<skos:definition>kwas nieorganiczny, wodny roztwór chlorowodoru, bezbarwna ciecz dymiąca na powietrzu; stosowany w przemyśle, m.in. do ekstrakcji rud metali, syntezy organicznej, do trawienia metali, lutowania i w hydrolizie drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4095">
<skos:prefLabel>kultura wodna</skos:prefLabel>
<skos:definition>uprawa roślin z użyciem piasku lub wermikulitu jako podłoża oraz roztworami soli mineralnych jako pożywki, stosowana głównie w szklarniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4098">
<skos:prefLabel>hydroelektrownia</skos:prefLabel>
<skos:definition>elektrownia przetwarzająca energię mechaniczną wody w energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4103">
<skos:prefLabel>wodór</skos:prefLabel>
<skos:definition>(H) pierwiastek chemiczny, liczba atomowa 1, najlżejszy ze wszystkich pierwiastków gaz, bezbarwny, bezwonny, bez smaku, chemicznie czynny, palny — z tlenem tworzy mieszaninę wybuchową (w temp. powyżej 500°C); w przyrodzie występuje w dużych ilościach w postaci związków, głównie — wody; jest składnikiem wszystkich kwasów i prawie wszystkich związków organicznych; ma zastosowanie w przemyśle chemicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4107">
<skos:prefLabel>siarkowodór</skos:prefLabel>
<skos:definition>związek nieorganiczny siarki z wodorem, bezbarwny, silnie trujący, palny gaz o przykrym zapachu zgniłych jaj; wyczuwalny w powietrzu w rozcieńczeniu 0,002 mg/l; występuje w przyrodzie w gazach ziemnych i wulkanicznych, w niektórych wodach mineralnych oraz powstaje podczas gnicia substancji białkowych; stosowany w analizie chemicznej jako odczynnik, bardzo niebezpieczny - zatrucie prowadzi do zapaści, śpiączki i śmierci w kilka sekund po wchłonięciu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4108">
<skos:prefLabel>hydrogeologia</skos:prefLabel>
<skos:definition>dział geologii stosowanej zajmujący się badaniem występowania wód powierzchniowych i podziemnych, ich użytkowania, rozmieszczenia, występowania na powierzchni Ziemi, ruchu w warstwach skalnych oraz przemian, jakim ulegały w historii Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4109">
<skos:prefLabel>hydrografia</skos:prefLabel>
<skos:definition>nauka zajmująca się pomiarami i opisywaniem właściwości fizycznych ocanów, jezior, rzek i obszarów przybrzeżnych ze szczgólnym uwzględnieniem zarządzania oraz użytkowania wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4111">
<skos:prefLabel>katastrofa hydrologiczna</skos:prefLabel>
<skos:definition>gwałtowna, nagła i destruktywna zmiana jakości, rozmieszczenia lub ruchu wody na lądzie, pod powierzchnią ziemi lub w atmosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4115">
<skos:prefLabel>równowaga hydrologiczna</skos:prefLabel>
<skos:definition>bilans dopływu, odpływu i oraz ilości zgromadzonej wody w jednostce hydrologicznej takiej jak zlewnia wód, formacja wodonośna, strefa glebowa, jezioro lub zbiornik; związki pomiędzy odparowaniem, opadem, spływem powierzchniowym i zmianami zapasu wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4116">
<skos:prefLabel>cykl hydrologiczny</skos:prefLabel>
<skos:definition>przemieszczanie się wody pomiędzy oceanami, powierzchnią gruntu i atmosferą w wyniku odparowania, opadów atmosferycznych oraz aktywności organizmów żywych, w ciągu dnia woda odparowuje z oceanów i jest przenoszona w powietrzu nad lądy, na powierzchnię których wraca w formie opadów atmosferycznych, a następnie dopływa rzekami na powrót do morza, zamykając w ten sposób cykl</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4118">
<skos:prefLabel>hydrologia</skos:prefLabel>
<skos:definition>nauka o wodach występujących w przyrodzie, badająca zjawiska i procesy zachodzące w hydrosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4119">
<skos:prefLabel>hydroliza</skos:prefLabel>
<skos:definition>1) rozkład substancji chemicznych, organicznych i nieorganicznych pod wpływem wody, 2) w wodnych roztworach elektrolitów: reakcja kationów z wodą dająca w wyniku słabą zasadę, lub anionów z wodą - dająca w wyniku słaby kwas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4120">
<skos:prefLabel>hydrometeorologia</skos:prefLabel>
<skos:definition>dziedzina meteorologii zajmująca się problematyką hydrologiczną, głównie ochroną przeciwpowodziową elektrowniami wodnymi, nawadnianiem itp. działami robót inżynieryjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4122">
<skos:prefLabel>energia wodna</skos:prefLabel>
<skos:definition>energia uzyskana z naturalnych lub sztucznych wodospadów, bezpośrednio przez obracanie koła wodnego albo turbiny, lub pośrednio z wytworzeniem energii elektrycznej przez prądnicę napędzaną turbiną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4124">
<skos:prefLabel>hydrosfera</skos:prefLabel>
<skos:definition>wodna powłoka Ziemi, przenikająca skorupę i atmosferę ziemską, obejmująca wody powierzchniowe, wody podziemne, lodowce oraz parę wodną w atmosferze i skorupie ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4126">
<skos:prefLabel>higiena</skos:prefLabel>
<skos:definition>dział medycyny, nauka zajmująca się ogółem praktyk chroniących zdrowie ludzkie, dążąca do eliminowania czynników ujemnych, a wprowadzania dodatnich w celu ochrony zdrowia oraz zapewnienia jednostce i społeczeństwu najlepszych warunków rozwoju psychicznego i fizycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4127">
<skos:prefLabel>błonkówki</skos:prefLabel>
<skos:definition>Hymenoptera, rząd owadów, mających dwie pary błoniastych skrzydeł, narządy gębowe gryząco-liżące lub ssące; przechodzą przeobrażenie zupełne, liczne gatunki tworzą tzw. społeczeństwa owadzie, np. pszczoły, mrówki, termity</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4131">
<skos:prefLabel>lód</skos:prefLabel>
<skos:definition>woda w stałym stanie skupienia, substancja utworzona przez zamrożenie wody do postaci ciała stałego; tworzy heksagonalną formę kryształów; także zamarznięta powierzchnia wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4132">
<skos:prefLabel>góra lodowa</skos:prefLabel>
<skos:definition>pływająca po morzu lub oceanie potężna bryła lodu, oderwana od czoła lądolodu lub lodowca uchodzącego do morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4133">
<skos:prefLabel>pak lodowy</skos:prefLabel>
<skos:definition>skupisko wieloletniej kry lodowej na obszarach mórz polarnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4136">
<skos:prefLabel>identyfikacja zanieczyszczeń</skos:prefLabel>
<skos:definition>oznaczenie substancji lub grupy substancji powodujących zanieczyszczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4137">
<skos:prefLabel>ideologia</skos:prefLabel>
<skos:definition>system poglądów, idei, pojęć politycznych, socjologicznych, prawnych, etycznych, religijnych lub filozoficznych jednostki albo grupy ludzi, związany z epoką, miejscem i stosunkami społecznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4140">
<skos:prefLabel>przetwarzanie obrazu</skos:prefLabel>
<skos:definition>działania cyfrowe wykonywane na obrazie cyfrowym, których celem jest poprawa jego geometrii, czytelności, wyróżnienie pewnych obiektów itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4141">
<skos:prefLabel>kod IMCO</skos:prefLabel>
<skos:definition>kodeks IMCO w zakresie międzynarodowego transportu morskiego, w szczególności bezpieczeństwa na morzu i ochrony mórz przed zanieczyszczeniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4143">
<skos:prefLabel>degeneracja fauny</skos:prefLabel>
<skos:definition>jeden ze skutków wprowadzenia obcego gatunku fauny na danym obszarze; łączy się to z ryzykiem nadmiernej jego ekspansywności kosztem rodzimych gatunków i ich zespołów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4144">
<skos:prefLabel>degeneracja flory</skos:prefLabel>
<skos:definition>jeden ze skutków wprowadzenia obcego gatunku rośliny na obszarze, na którym gatunek ten dotąd nie występował</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4146">
<skos:prefLabel>imisja</skos:prefLabel>
<skos:definition>wchłonięcie zanieczyszczeń przez środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4147">
<skos:prefLabel>ograniczanie imisji</skos:prefLabel>
<skos:definition>procedury prawne i administracyjne mające na celu ograniczenie strat spowodowanych przez emisje. Programy kontroli zanieczyszczeń zwykle opierają się akceptowalnych dawkach w odniesieniu do ludzi. Bardzo ważnym zadaniem jest organizacja inwentaryzacji emisji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4148">
<skos:prefLabel>regulacje prawne dotyczące ograniczania imisji</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4149">
<skos:prefLabel>szkody od imisji</skos:prefLabel>
<skos:definition>szkoda spowodowana przez zanieczyszczenie pochodzące z określonego źródła emisji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4150">
<skos:prefLabel>prognozowanie wielkości imisji</skos:prefLabel>
<skos:definition>prognozę imisji oblicza się na podstawie ładunku zanieczyszczenia, wysokości źródła emisji, szybkości wiatru i współczynnika dyspersji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4151">
<skos:prefLabel>maksymalny poziom imisji</skos:prefLabel>
<skos:definition>maksymalny poziom stężenia dowolnego zanieczyszczenia, który skutkuje nieakceptowalną jakością powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4152">
<skos:prefLabel>ładunek imisji</skos:prefLabel>
<skos:definition>całkowity ładunek imisji wprowadzony do danego środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4154">
<skos:prefLabel>system odpornościowy</skos:prefLabel>
<skos:definition>układ, dzięki któremu organizm może przeciwstawiać się chorobom, uwarunkowany działaniem wyspecjalizowanych komórek krwi lub przeciwciał przez nie produkowanych jako odpowiedź na naturalne narażenie lub zaszczepienie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4155">
<skos:prefLabel>odporność</skos:prefLabel>
<skos:definition>zdolność organizmu do przeciwstawiania się chorobom lub toksynom przy użyciu środków naturalnych lub sztucznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4156">
<skos:prefLabel>próba immunologiczna</skos:prefLabel>
<skos:definition>metody jakościowego oznaczenia takich substancji chemicznych jak hormony, lekarstwa oraz niektóre białka, opierające się o wykorzystanie wysokospecyficznych powiązań między antygenem i przeciwciałem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4157">
<skos:prefLabel>choroba systemu immunologicznego</skos:prefLabel>
<skos:definition>zakłócenie złożonego układu współzależnych komórek, produktów komórkowych i tkanek komórkotwórczych, który chroni organizm przed czynnikami patogennymi, niszczy komórki złośliwe oraz usuwa wydaliny komórkowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4158">
<skos:prefLabel>immunologia</skos:prefLabel>
<skos:definition>nauka o sposobach swoistego reagowania organizmu na antygeny; powstała w drugiej połowie XIX w. jako dyscyplina medyczna traktująca o odporności organizmu na bakterie, wirusy, pierwotniaki, grzyby itp. czynniki chorobotwórcze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4159">
<skos:prefLabel>ocena oddziaływania</skos:prefLabel>
<skos:definition>ocena oddziaływania projektu na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4160">
<skos:prefLabel>minimizacja oddziaływania</skos:prefLabel>
<skos:definition>działania, procedury lub urządzenia mające na celu zredukowanie rozmiaru lub stopnia negatywnego wpływu na zdrowie ludzi i na ekosystemy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4161">
<skos:prefLabel>impaktor</skos:prefLabel>
<skos:definition>przyrząd do wychwytywania cząstek zawieszonych w powietrzu atmosferycznym; składa się z obudowy naprowadzającej strumień powietrza na czujnik w postaci płytki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4162">
<skos:prefLabel>przeciwdziałanie negatywnemu oddziaływaniu na środowisko</skos:prefLabel>
<skos:definition>środki zapobiegawcze; działania lub urządzenia mające przeciwdziałać negatywnym wpływom na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4163">
<skos:prefLabel>źródło negatywnego oddziaływania</skos:prefLabel>
<skos:definition>czynniki wywołujące szkody w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4164">
<skos:prefLabel>prawo wdrożeniowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4167">
<skos:prefLabel>import</skos:prefLabel>
<skos:definition>przywóz towarów z zagranicy w drodze wymiany towarowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4169">
<skos:prefLabel>licencja importowa</skos:prefLabel>
<skos:definition>pozwolenie rządowe na przywóz i sprzedaż w obrębie kraju towarów wyprodukowanych zagranicą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4171">
<skos:prefLabel>zubożenie</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4172">
<skos:prefLabel>środek impregnujący</skos:prefLabel>
<skos:definition>substancja stosowana do uszczelniania szpar w drewnie, gipsie itp.podłożach przed położeniem warstwy farby lub lakieru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4173">
<skos:prefLabel>impregnacja (tworzywo)</skos:prefLabel>
<skos:definition>wypełnianie substancją ciekłą szczelin w porowatych ciałach stałych w celu zmiany ich właściwości, np. impregnowanie drewna kreozotem w celu uodpornienia na niszczące działanie wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4176">
<skos:prefLabel>poprawa skuteczności</skos:prefLabel>
<skos:definition>korzystny rozwój lub poprawa efektywności produkcji albo maksymalizacja uzysku w odniesieniu do dostępnych lub przydzielonych zasobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4177">
<skos:prefLabel>wypadkowy zrzut zanieczyszczeń</skos:prefLabel>
<skos:definition>nagły zrzut zanieczyszczeń do powietrza, wody lub gruntu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4178">
<skos:prefLabel>szum impulsowy</skos:prefLabel>
<skos:definition>przejściowe, krótkotrwałe zakłócenia rozłożone w miarę jednorodnie w użytecznym paśmie systemu przekazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4182">
<skos:prefLabel>spopielenie</skos:prefLabel>
<skos:definition>kontrolowane spalanie stałych, ciekłych lub gazowych palnych odpadów z wytworzeniem spalin, przy czym pzostałość stała jest pozbawiona substancji palnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4183">
<skos:prefLabel>palenie odpadów</skos:prefLabel>
<skos:definition>kontrolowane spalanie stałych, ciekłych lub gazowych palnych odpadów z wytworzeniem spalin i pozostałości stałej pozbawionej substancji palnych w celu zmniejszenia objętości odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4184">
<skos:prefLabel>spalarnie odpadów</skos:prefLabel>
<skos:definition>urządzenie do spalania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4186">
<skos:prefLabel>stok</skos:prefLabel>
<skos:definition>nachylona powierzchnia różnych form ukształtowania terenu, np. skłon wzgórza, również rozległe części kontynentów opadające do oceanów, np. skłon Pacyfiku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4187">
<skos:prefLabel>dochód</skos:prefLabel>
<skos:definition>dochód czerpany z kapitału, pracy lub innej aktywności lub obu tych form łącznie włączając zysk i dochód z zamiany kapitału</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4189">
<skos:prefLabel>włączenie</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4194">
<skos:prefLabel>odszkodowanie</skos:prefLabel>
<skos:definition>rekompensata finansowa, zwrot kosztów lub ubezpieczenie od szkód i strat proponowane przez rząd, politykę ubezpieczeniową lub umowę kontraktową na uzgodnionych warunkach i obejmujące określone nieszczęśliwe wypadki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4195">
<skos:prefLabel>Ocean Indyjski</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4196">
<skos:prefLabel>wskaźnik</skos:prefLabel>
<skos:definition>coś, co wskazuje, szczególnie wskazuje na tendencje (zmian)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4200">
<skos:prefLabel>wskaźnik zagospodarowania środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4202">
<skos:prefLabel>wskaźnik jakości środowiska</skos:prefLabel>
<skos:definition>parametr jakościowy lub ilościowy stosowany jako miara warunków środowiska, np. jakość powietrza lub wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4204">
<skos:prefLabel>las naturalny</skos:prefLabel>
<skos:definition>lasy naturalnie występujące na danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4205">
<skos:prefLabel>prosta technologia</skos:prefLabel>
<skos:definition>stosowane przez ludność miejscową techniki, które stanowią istotny element dziedzictwa kulturowego i z tego powodu powinny podlegać ochronie przed ich wyzyskiwaniem przez kraje uprzemysłowione; problem ochrony dorobku kulturowego ludności miejscowej rozważano podczas konferencji w Rio, choć nie został on specjalnie uwzględniony w konwencji o różnorodności biologicznej; art. 8 konwencji stanowi, że Strony &quot;szanują, chronią i wspierają praktyki stosowane przez ludność miejscową, te zwłaszcza, które odzwierciedlają tradycyjny sposób życia... oraz promują ich szerokie stosowanie za zgodą i przy udziale właścicieli tego dorobku, a także innowacje i praktyki, a ponadto działają na rzecz sprawiedliwego podziału korzyści z tego wynikających&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4206">
<skos:prefLabel>odprowadzający ścieki</skos:prefLabel>
<skos:definition>nie domowe źródła ścieków odprowadzające zanieczyszczenia do publicznego systemu oczyszczania wód; mogą to być zakłady handlowe lub przemysłowe odprowadzające ścieki do lokalnej kanalizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4209">
<skos:prefLabel>zanieczyszczenie powietrza w pomieszczeniach zamkniętych</skos:prefLabel>
<skos:definition>chemiczne, fizyczne lub biologiczne zanieczyszczenia powietrza wewnątrz budynków i innych przestrzeni zamkniętych, w których przebywają ludzie; zanieczyszczenia te mogą wynikać z obecności dymu tytoniowego, pestycydów, środków czyszczących, gazów uwalnianych z materiałów budowlanych, wykładzin, produktów gospodarstwa domowego, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4211">
<skos:prefLabel>środowisko wewnątrz pomieszczenia</skos:prefLabel>
<skos:definition>środowisko wewnątrz domu lub innego budynku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4213">
<skos:prefLabel>teren przemysłowy</skos:prefLabel>
<skos:definition>obszary przeznaczone pod działalność przemysłową w planie zagospodarowania przestrzennego miasta lub planie ochrony; rodzaje przemysłu uwzględnione w planie mogą obejmować przemysł lekki, usługowy, wielobranżowy, niebezpieczny, uciążliwy lub szkodliwy; w odniesieniu do obszarów przemysłowych normy określa się zazwyczaj w aspektach: dostępu i dróg, odwodnienia, parkingów, względów estetycznych i krajobrazowych, stref buforowych, poziomów hałasu, powietrza i wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4216">
<skos:prefLabel>stowarzyszenie przemysłowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4217">
<skos:prefLabel>budynki przemysłowe</skos:prefLabel>
<skos:definition>budynki bezpośrednio wykorzystywane do wytwarzania i produkcji technicznej; b.p. są dostępne w zasadzie tylko dla pracowników; b.p. obejmują zabudowania używane bezpośrednio w procesie produkcji elektryczności, wytwarzania produktów, w kopalnictwie surowców oraz magazynowaniu tekstyliów, produktów ropopochodnych, produktów z drewna i papieru, chemikaliów, tworzyw sztucznych i metali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4224">
<skos:prefLabel>roślina przemysłowa</skos:prefLabel>
<skos:definition>rośliny dostarczające surówców do produkcji przemysłowej takie jak soja, bawełna (puch i nasiona), len i tytoń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4226">
<skos:prefLabel>rozwój gospodarczy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4227">
<skos:prefLabel>składowanie odpadów przemysłowych</skos:prefLabel>
<skos:definition>składowanie odpadów produkcyjnych przez podmioty wytwarzające odpady</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4228">
<skos:prefLabel>gospodarka przemysłowa</skos:prefLabel>
<skos:definition>produkcja, dystrybucja i konsumpcja dóbr i usług dostarczanych przez przemysł wytwórczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4229">
<skos:prefLabel>ścieki poprodukcyjne</skos:prefLabel>
<skos:definition>substancje usuwane jako ścieki w procesach produkcyjnych lub wynikające z prowadzenia procesów produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4231">
<skos:prefLabel>emisja z procesów przemysłowych</skos:prefLabel>
<skos:definition>zanieczyszczenia gazowe usuwane do atmosfery przez kominy zakładów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4233">
<skos:prefLabel>polityka środowiskowa w dziedzinie przemysłu</skos:prefLabel>
<skos:definition>procedury, zasady lub działania ukierunkowane na ochronę zasobów naturalnych przed zanieczyszczeniami powstającymi przy okazji działalności produkcyjnej i handlowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4234">
<skos:prefLabel>urządzenia przemysłowe</skos:prefLabel>
<skos:definition>urządzenia związane z działalnością przemysłową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4238">
<skos:prefLabel>gazy przemysłowe</skos:prefLabel>
<skos:definition>wyziewy dymu lub pary z  substancji, szczególnie wyziewy odorowe lub szkodliwe wynikające z procesów handlowych lub produkcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4242">
<skos:prefLabel>instalacje przemysłowe</skos:prefLabel>
<skos:definition>urządzenie, układ lub część wyposażenia zainstalowanego w zakładzie przemysłowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4244">
<skos:prefLabel>uprzemysłowienie</skos:prefLabel>
<skos:definition>proces, w wyniku którego przemysł wytwórczy zaczyna dominować w gospodarce narodowej lub regionalnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4247">
<skos:prefLabel>ustawodawstwo dotyczące przemysłu</skos:prefLabel>
<skos:definition>zespół obowiązujących przepisów ustalonych przez rząd w celu uregulowania warunków pracy lub kwestii pozyskiwania, przetwarzania i składowania materiałów przez wszystkie zakłady, firmy i przedsiębiorstwa w jednej lub  w wielu gałęziach produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4249">
<skos:prefLabel>materiały przemysłowe</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4250">
<skos:prefLabel>medycyna przemysłowa</skos:prefLabel>
<skos:definition>gałąź medycyny zajmująca się zależnościami &quot;ludzie a wykonywana przez nich praca&quot; w celu zapobiegania chorobom i obrażeniom oraz w celu promowania optymalnych uregulowań zdrowotnych, produkcyjnych i społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4251">
<skos:prefLabel>hałas przemysłowy</skos:prefLabel>
<skos:definition>hałas powstający w wyniku działań przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4253">
<skos:prefLabel>planowanie przemysłu</skos:prefLabel>
<skos:definition>proces ustaleń lub przygotowań mający na celu ułatwienie działań wytwórczych, produkcyjnych i przetwórczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4254">
<skos:prefLabel>zabudowa przemysłowa</skos:prefLabel>
<skos:definition>budynki, w których prowadzone są prace związane z procesami produkcyjnymi w przemyśle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4255">
<skos:prefLabel>polityka przemysłowa</skos:prefLabel>
<skos:definition>kierunek działania przyjety przez rząd w celu wspierania i promowania przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4256">
<skos:prefLabel>zanieczyszczenie pochodzenia przemysłowego</skos:prefLabel>
<skos:definition>zanieczyszczenie pochodzenia przemysłowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4257">
<skos:prefLabel>technologia przemysłowa</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4258">
<skos:prefLabel>produkcja przemysłowa</skos:prefLabel>
<skos:definition>procesy przerobu lub przetwarzania surowców i innych zasobów na wartościowe wyroby i usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4259">
<skos:prefLabel>statystyka produkcji przemysłowej</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4260">
<skos:prefLabel>produkt przemysłowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4262">
<skos:prefLabel>prawo własności przemysłowej</skos:prefLabel>
<skos:definition>uzasadnione prawo gwarantowane przez rząd lub inną władzę, której zadaniem jest ochranianie własności lub wykluczenie prawa innych do tworzenia, używania lub sprzedaży określonych wynalazków, unikatowych wzorów artykułów lub innych wytworów oraz odkryć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4267">
<skos:prefLabel>teren przemysłowy</skos:prefLabel>
<skos:definition>teren, na którym znajduje się określony zakład przemysłowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4268">
<skos:prefLabel>osad ze ścieków przemysłowych</skos:prefLabel>
<skos:definition>osady powstające w  wyniku prowadzenia procesów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4269">
<skos:prefLabel>społeczeństwo przemysłowe</skos:prefLabel>
<skos:definition>wielkie społeczeństwo związane z wieloma sektorami wytwórczości oraz infrastrukturą i gospodarką opierającą się na nauce, technice i instrumentalnej racjonalności nowoczesnego Zachodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4273">
<skos:prefLabel>odpady przemysłowe</skos:prefLabel>
<skos:definition>materiały odpadowe usuwane podczas procesów produkcji przemysłowej lub powstające podczas procesów wytwórczych; odpady mają postać stałą, szlamu (mokre części stałe) lub ciekłą, ponadto mogą (lub nie) być klasyfikowane jako niebezpieczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4274">
<skos:prefLabel>odpady gazowe</skos:prefLabel>
<skos:definition>gazy odpadowe powstające podczas wytwarzania i innych procesów przemysłowych, można je oczyszczać i uwalniać do atmosfery, kierować do powtórnego użycia lub uwalniać bez oczyszczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4276">
<skos:prefLabel>ścieki przemysłowe</skos:prefLabel>
<skos:definition>woda odpadowa pochodząca z procesów przemysłowych i wytwórczych; do kanalizacji zrzucana jest bezpośrednio albo w formie ścieków sanitarnych lub mieszanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4278">
<skos:prefLabel>strefowanie przemysłu</skos:prefLabel>
<skos:definition>system planowania przestrzennego, w którym wydziela się strefy i odgranicza obszary  przeznaczone do wyłącznego użytkowania przez przemysł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4279">
<skos:prefLabel>przemysł</skos:prefLabel>
<skos:definition>gałąź przemysłu to grupa firm (przedsiębiorstw), zajmujących się jednakowymi lub podobnymi działaniami gospodarczymi; przemysł wytwarza towary w celu ich sprzedaży na zasadzie opłacalności; pojedyncza gałąź przemysłu może być producentem różnych towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4282">
<skos:prefLabel>unieruchomianie substancji niebezpiecznych</skos:prefLabel>
<skos:definition>dezaktywacja odpadów obejmuje zestalanie i stabilizację; stabilizacja jest procesem mającym na celu zmniejszenie ryzyka zagrożenia od odpadów przez przekształcenie substancji zanieczyszczających do form najmniej rozpuszczalnych, najmniej ruchliwych lub najmniej toksycznych; zestalanie polega na fizycznym związaniu odpadów lub ich otoczkowaniu tak by tworzyły lite ciało stałe; zestalanie można stosować do odpadów w formie rozdrobnionego ciała, cieczy lub gazów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4283">
<skos:prefLabel>odpady nieaktywne</skos:prefLabel>
<skos:definition>odpady nie ulegające podczas składowania procesom fizycznej, chemicznej lub biologicznej degradacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4284">
<skos:prefLabel>umieralność noworodków</skos:prefLabel>
<skos:definition>wskaźnik umieralności noworodków w pierwszym roku życia, w przeliczeniu na określoną populację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4286">
<skos:prefLabel>zakażenie</skos:prefLabel>
<skos:definition>inwazja i rozwój lub namnażanie się czynników zakaźnych w żywym organizmie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4288">
<skos:prefLabel>choroba zakaźna</skos:prefLabel>
<skos:definition>warunki chorobowe powstałe w organizmie gospodarza wskutek inwazji czynnika patogenetycznego, który rozmnaża się wywołując zakażenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4289">
<skos:prefLabel>zakażenie zbiorów</skos:prefLabel>
<skos:definition>inwazja organizmów pasożytniczych na roślinach uprawnych;  wśród zwierząt kręgowych wiele szkodników roślin to ssaki, szczególnie gryzonie, także ptaki; wśród bezkręgowców - są to niektóre gatunki ślimaków i liczne gatunki obleńców z gromady nicieni; najbardziej różnorodną i liczną grupę szkodników roślin stanowią stawonogi - owady, pajęczaki oraz niektóre gatunki krocionogów i skorupiaków; choroby roślin są zróżnicowane - od wirusowych i bakteryjnych, poprzez choroby związane z odżywianiem do grzybic, chorób środowiskowych oraz innych niespecyficznych; FAO ocenia, że w skali światowej roczne straty plonów spowodowane przez szkodniki i choroby roślin sięgają około 20-25% potencjalnego zbioru roślin konsumpcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4290">
<skos:prefLabel>zakażenie żywności</skos:prefLabel>
<skos:definition>żywność zanieczyszczona, której jakość pogorszyła się w  wyniku oddziaływania szkodników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4291">
<skos:prefLabel>przenikanie</skos:prefLabel>
<skos:definition>przesiąkanie wody przez powierzchnię gleby do gruntu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4296">
<skos:prefLabel>substancja łatwopalna</skos:prefLabel>
<skos:definition>substancja łatwo ulegająca zapłonowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4300">
<skos:prefLabel>dopływ</skos:prefLabel>
<skos:definition>woda, poza ściekami dopływającymi do kanalizacji (wraz z jej doprowadzeniami), odprowadzana z takich źródeł (ale nie tylko) jak: rynny dachowe, drenaż piwniczny, podwórzowy, obszarowy, drenaż terenów źródliskowych i podmokłych, włazów, skrzyżowań kanałów burzowych i sanitarnych, wież chłodniczych, odpływu burzowego, powierzchniowego, wody po spłukiwaniu ulic lub z odwodnień; dopływ nie obejmuje infiltracji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4301">
<skos:prefLabel>rozmowa nieformalna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4302">
<skos:prefLabel>informatyka</skos:prefLabel>
<skos:definition>nauka i technika opracowania danych oraz automatycznej obróbki informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4303">
<skos:prefLabel>informacja</skos:prefLabel>
<skos:definition>każda myśl, fakt, wyobrażenie, które mogą być przekazane w procesie komunikowania się w sposób formalny lub nieformalny, w dowolnym formacie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4307">
<skos:prefLabel>przetwarzanie informacji</skos:prefLabel>
<skos:definition>proces przetwarzania dowolnych danych przez ludzi i komputery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4309">
<skos:prefLabel>obsługa informacyjna</skos:prefLabel>
<skos:definition>zorganizowany system obsługi użytkowników informacji przy wykorzystaniu zasobów informacyjnych (zasobów bibliotecznych, systemów informacyjno-wyszukiwawczych, baz danych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4310">
<skos:prefLabel>źródło informacji</skos:prefLabel>
<skos:definition>jednostka zasobu informacyjnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4311">
<skos:prefLabel>system informacyjny</skos:prefLabel>
<skos:definition>system, którego zbiór wyszukiwawczy składa się z baz danych lub systemów informacyjno-wyszukiwawczych z zakresu danej dziedziny wiedzy; określony pewnymi zasadami przepływ informacji zapewnia jak najlepszy do niej dostęp</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4313">
<skos:prefLabel>technika informacyjna</skos:prefLabel>
<skos:definition>narzędzia, programy, środki materialne i umiejętności posługiwania się nimi umożliwiające człowiekowi celową działalność w każdej dziedzinie aktywności dzięki wykorzystaniu elektronicznych itp. zasobów informacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4315">
<skos:prefLabel>zerwanie kontraktu</skos:prefLabel>
<skos:definition>zerwanie, pogwałcenie lub naruszenie ustawy, kontraktu, prawa lub obowiązku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4317">
<skos:prefLabel>promieniowanie podczerwone</skos:prefLabel>
<skos:definition>promieniowanie elektromagnetyczne o długościach fal w zakresie od 0.75 lub 0.8 mikrometra do 1000 mikrometrów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4320">
<skos:prefLabel>poddźwięk</skos:prefLabel>
<skos:definition>drgania powietrza o częstotliwościach zbyt niskich by człowiek mógł je odbierać jako dźwięk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4321">
<skos:prefLabel>infrastruktura</skos:prefLabel>
<skos:definition>podstawowy system, urządzenia lub inwestycje społeczne, niezbędne dla funkcjonowania gospodarki i innych działań społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4323">
<skos:prefLabel>mieszkaniec</skos:prefLabel>
<skos:definition>osoba zamieszkująca dany region, miasto, dom, kraj, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4325">
<skos:prefLabel>uraz</skos:prefLabel>
<skos:definition>uraz organizmu zakłócający jego strukturę lub funkcjonowanie i wywołujący procesy chorobowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4326">
<skos:prefLabel>atrament</skos:prefLabel>
<skos:definition>zawiesina pigmentu lub roztwór barwnika w cieczy nośnej dający w wyniku płyn, pastę lub proszek stosowane i wysuszane na powierzchniach; tuszów do pisania, znaczenia, rysowania i druku używa się na kilka sposobów na papierze, metalu, tworzywie sztucznym, drewnie, szkle, tkaninie lub innych podłożach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4329">
<skos:prefLabel>rybactwo śródlądowe</skos:prefLabel>
<skos:definition>obszary połowów na jeziorach, rzekach, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4331">
<skos:prefLabel>żegluga śródlądowa</skos:prefLabel>
<skos:definition>żegluga drogami wodnymi na śródlądziu, na rzekach, kanałach, jeziorach, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4333">
<skos:prefLabel>wody śródlądowe</skos:prefLabel>
<skos:definition>jeziora, rzeki lub inne zbiorniki wodne w obrębie danego państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4336">
<skos:prefLabel>śródlądowy transport wodny</skos:prefLabel>
<skos:definition>transport osób i towarów łodziami pływającymi po rzekach, kanałach lub jeziorach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4340">
<skos:prefLabel>nowatorstwo</skos:prefLabel>
<skos:definition>coś, co zostało wprowadzone po raz pierwszy, np. nowe metody lub urządzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4342">
<skos:prefLabel>chemia nieorganiczna</skos:prefLabel>
<skos:definition>gałąź chemii zajmująca się chemicznymi reakcjami i właściwościami wszystkich substancji nieorganicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4345">
<skos:prefLabel>nawóz nieorganiczny</skos:prefLabel>
<skos:definition>nieorganiczny środek chemiczny poprawiajacy wzrost roślin dzięki dostarczeniu podstawowych składników odżywczych, takich jak siarczan amonu lub wapń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4347">
<skos:prefLabel>zanieczyszczenia nieorganiczne</skos:prefLabel>
<skos:definition>zanieczyszczenia pochodzenia mineralnego, nie mające szkieletu węglowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4350">
<skos:prefLabel>substancja nieorganiczna</skos:prefLabel>
<skos:definition>substancje chemiczne nie zawierające węgla jako podstawowego elementu (z wyjątkiem węglanów, cjanków i cjanianów) tj. materia inna niż roślinna lub zwierzęca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4354">
<skos:prefLabel>środek owadobójczy</skos:prefLabel>
<skos:definition>czynniki chemiczne stosowane do niszczenia szkodników nieorganicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4356">
<skos:prefLabel>zwierzę owadożerne</skos:prefLabel>
<skos:definition>zwierzęta łożyskowe z rzędu Owadożernych (Insectivora), zazwyczaj bardzo małych rozmiarów, o prostym uzębieniu, żywiące się bezkręgowcami; do grupy tej należą ryjówki, krety i jeże</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4358">
<skos:prefLabel>owad</skos:prefLabel>
<skos:definition>przedstawiciel gromady Stawonogów (Arthropoda) o charakterystycznie członowanej budowie ciała, chitynowej pokrywie zewnętrznej, z parą oczu złożonych, parą czułków, trzema parami narządów gębowych i dwoma parami skrzydeł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4359">
<skos:prefLabel>w miejscu</skos:prefLabel>
<skos:definition>na miejscu naturalnego bądź normalnego występowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4360">
<skos:prefLabel>sprawdzenie</skos:prefLabel>
<skos:definition>oficjalne sprawdzenie i ocena zakresu w jakim osiągnięto wytyczone cele, normy lub wywiązano się z zadań określonych przez politykę lub procedury obowiązujące w firmie, organizacji, zakładzie lub określonej jednostce organizacyjnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4361">
<skos:prefLabel>sprawdzenie zapisów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4362">
<skos:prefLabel>służba nadzoru</skos:prefLabel>
<skos:definition>organizacja powołana w celu oceny, nadzoru i sprawozdawania nt. jakości prac osób i instytucji lub zakładów, lub nt. zgodności z ustawami i przepisami prawa działań prowadzonych przez określony sektor społeczny lub inną grupę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4363">
<skos:prefLabel>urządzenie wymagające zezwolenia</skos:prefLabel>
<skos:definition>oficjalne zezwolenie, którego uzyskanie jest niezbędne by zmontować i posadowić w określonym miejscu aparaturę, urzadzenia, wojskowe posterunki lub maszyny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4365">
<skos:prefLabel>instytucjonalizacja</skos:prefLabel>
<skos:definition>ustanowienie i unormowanie praw, obyczajów, praktyki, zasad lub systemów uregulowań w działaniach lub celach działań określonej grupy lub organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4369">
<skos:prefLabel>wytwarzanie sprzętu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4371">
<skos:prefLabel>izolacja dźwiękowa</skos:prefLabel>
<skos:definition>materiał stosowany w celu ograniczenia przewodnictwa dźwięków do/lub z jakiegoś ciała, urządzenia, pomieszczenia itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4374">
<skos:prefLabel>ubezpieczenie</skos:prefLabel>
<skos:definition>ustawa, system lub agencja, której zadaniem jest finansowe ubezpieczenie od wypadków takich jak śmierć, utrata lub zniszczenie własności; ubezpieczenie polega na regularnym wypłacaniu kwot premii w zamian za wykupioną polisę ubezpieczeniową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4375">
<skos:prefLabel>działalność ubezpieczeniowa</skos:prefLabel>
<skos:definition>usługa handlowa polegająca na gwarancji wypłaty odszkowania w wypadku różnego rodzaju strat, uszczerbku dla osób, własności i firm w zamian za wpłacane składki ubezpieczeniowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4376">
<skos:prefLabel>ochrona ubezpieczenia</skos:prefLabel>
<skos:definition>ochrona w obliczu przypadków różnego rodzaju ryzyka, zwykle wyszczególnionych w zależności od rodzaju ochrony ubezpieczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4378">
<skos:prefLabel>kompleksowy rezerwat przyrody</skos:prefLabel>
<skos:definition>obszary wyznaczone w celu ochrony określonych gatunków roślin i zwierząt (def.ang.); co różni je od parków narodowych, które służą w znacznej mierze rekreacji publicznej, rezerwaty natomiast powoływane są w celu ochrony gatunków jako takich; w rezerwatach przyrody coraz częściej chroni się gatunki zagrożone by zapobiec ich wymarciu; rezerwaty przyrody są miejscem schronienia, rozmnażania i zimowania także dla gatunków licznie reprezentowanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4382">
<skos:prefLabel>kompleksowa technika ochrony środowiska</skos:prefLabel>
<skos:definition>techniki odpowiadające wymogom ochrony środowiska z uwagi na włączenie elementów zapobiegania zanieczyszczeniom na etapie projektowania; włączenie procedur ochrony środowiska na wczesnym etapie projektowania, w przeciwieństwie do opcji kontroli na &quot;końcu rury&quot;; poprawia aspekty techniczne i ekonomiczne procesu technologicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4383">
<skos:prefLabel>biologiczna metoda zwalczania szkodników</skos:prefLabel>
<skos:definition>systematyczne, wszechstronne podejście do zwalczania szkodników, polegające na wykorzystaniu wiedzy o biologii i zachowaniu owadów lub gryzoni w celu wynalezienia najmniej toksycznych i najtańszych metod zwalczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4386">
<skos:prefLabel>kompleksowy monitoring zanieczyszczeń</skos:prefLabel>
<skos:definition>procedura biorąca pod uwagę wszystkie podstawowe rodzaje emisji do gleby, powietrza i wody jednocześnie po to, by uniknąć sytuacji, w której badania jednego z mediów wpływają ujemnie na inne media</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4390">
<skos:prefLabel>intensywna hodowla zwierząt</skos:prefLabel>
<skos:definition>wyspecjalizowany system hodowli zwierząt, polegający na trzymaniu ich w zamknięciu i podawaniu paszy w postaci koncentratów, z częstym stosowaniem lekarstw w celu zapobiegania chorobom, jakie stanowią stałe zagrożenie w powyższych warunkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4391">
<skos:prefLabel>gospodarka intensywna</skos:prefLabel>
<skos:definition>rolnictwo, w którym dąży się do maksymalizacji użytków z określonego terenu poprzez uprawianie łączne roślin zbożowych lub uprawę kilku gatunków w ciągu roku lub też stosowanie nawozów w wielkich ilościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4395">
<skos:prefLabel>synergizm działania pestycydów</skos:prefLabel>
<skos:definition>wzmożone oddziaływanie pestycydów w wypadku, kiedy stosuje się je łącznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4398">
<skos:prefLabel>wymiana danych elektronicznych</skos:prefLabel>
<skos:definition>przesyłanie informacji w kodzie binarnym pomiędzy przynajmniej dwoma komputerami przy użyciu kanału komunikacyjnego zdolnego do przenoszenia sygnałów elektromagnetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4400">
<skos:prefLabel>badania interdyscyplinarne</skos:prefLabel>
<skos:definition>wykorzystanie, łączenie i koordynowanie jednej lub więcej dyscyplin, technologii i nauk humanistycznych w ramach kompleksowego podejścia do problemów ekologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4401">
<skos:prefLabel>odsetki</skos:prefLabel>
<skos:definition>kwota wypłacona lub naliczona w zamian za obrót pieniędzmi lub ich pożyczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4402">
<skos:prefLabel>grupa interesu</skos:prefLabel>
<skos:definition>grupa ludzi, zjednoczonych przez wspólne cechy, postawy, wierzenia lub cele, którzy utworzyli formalną organizację w celu zadbania o specyficzny interes grupy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4404">
<skos:prefLabel>tymczasowa decyzja</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4408">
<skos:prefLabel>interkalibracja laboratoriów</skos:prefLabel>
<skos:definition>badania prowadzone w tym samym czasie przez różne laboratoria w celu oceny jakości wyników badań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4410">
<skos:prefLabel>półprodukt</skos:prefLabel>
<skos:definition>produkty częściowo skończone lub kierowane do dalszego przetwarzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4412">
<skos:prefLabel>hałas nieciągły</skos:prefLabel>
<skos:definition>hałas powstający w odstępach regularnych bądź nieregularnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4414">
<skos:prefLabel>europejski rynek wewnętrzny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4415">
<skos:prefLabel>internalizacja kosztów środowiskowych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4417">
<skos:prefLabel>porozumienie międzynarodowe</skos:prefLabel>
<skos:definition>międzynarodowa współpraca na rzecz osiągania globalnych celów ekologicznych; wszystkie zainteresowane kraje muszą uczestniczyć w rozwiązywaniu ekologicznych problemów takich, jak transgraniczny transport zanieczyszczeń powietrza i wód, zubażanie warstwy ozonowej oraz zmiany klimatu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4419">
<skos:prefLabel>konkurencyjność międzynarodowa</skos:prefLabel>
<skos:definition>zdolność firm do konkurowania w zakresie produkcji i sprzedaży dóbr na rynkach światowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4420">
<skos:prefLabel>konwencja międzynarodowa</skos:prefLabel>
<skos:definition>układ lub umowa międzynarodowa zawierana przez różne kraje lub organizacje rządowe stanowiąca zobowiązania i prawa dla stron zawierających umowę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4421">
<skos:prefLabel>współpraca międzynarodowa</skos:prefLabel>
<skos:definition>współpraca pomiędzy rządami, przedsiębiorstwami lub osobami polegająca na opracowaniu wspólnej strategii szczególnie w zakresie badań lub regulacji standardów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4422">
<skos:prefLabel>Międzynarodowy Trybunał Sprawiedliwości</skos:prefLabel>
<skos:definition>międzynarodowy Trybunał Sprawiedliwości - główny organ ONZ; działa na podstawie statutu będącego integralną częścią Karty Narodów Zjednoczonych; rozpatruje spory pomiędzy państwami członkowskimi, jeżeli zostały mu one przekazane za zgodą wszystkich stron sporu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4423">
<skos:prefLabel>rozmieszczenie zasobów</skos:prefLabel>
<skos:definition>alokacja zasobów na świecie lub podział dóbr na rynku światowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4424">
<skos:prefLabel>międzynarodowy podział pracy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4427">
<skos:prefLabel>stosunki międzynarodowe w zakresie ochrony środowiska</skos:prefLabel>
<skos:definition>polityczne lub dyplomatyczne oddziaływanie i stosunki pomiędzy niezależnymi państwami w zakresie ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4429">
<skos:prefLabel>uzgodnienia międzynarodowe</skos:prefLabel>
<skos:definition>harmonizacja wzajemnych stosunków rządów poszczególnych państw poprzez wprowadzanie w życie podstawowych zasad uznawanych w świecie cywilizowanym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4430">
<skos:prefLabel>prawo międzynarodowe</skos:prefLabel>
<skos:definition>ogół norm określających stosunki między państwami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4432">
<skos:prefLabel>ekosystem o znaczeniu międzynarodowym</skos:prefLabel>
<skos:definition>ekosystemy, których znaczenie uznano na szczeblu międzynarodowym, niekiedy również chronione przez międzynarodowe konwencje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4434">
<skos:prefLabel>organizacja międzynarodowa</skos:prefLabel>
<skos:definition>stowarzyszenie niezależnych państw, których przedstawiciele działają we wspólnym interesie dotyczącym m.in. obrony i handlu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4436">
<skos:prefLabel>polityka międzynarodowa</skos:prefLabel>
<skos:definition>stosowanie metod, strategii, intryg, podejmowanie decyzji i używanie siły przez rządy i ich reprezentantów w celu osiągnięcia celów o zasięgu międzynarodowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4438">
<skos:prefLabel>stosunki międzynarodowe</skos:prefLabel>
<skos:definition>wzajemne oddziaływanie niezależnych państw na niwie politycznej i dyplomatycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4440">
<skos:prefLabel>dorzecze międzynarodowe</skos:prefLabel>
<skos:definition>obszar odwadniany przez określoną rzekę wraz z dopływami, wody których znajdują się na terenie dwóch lub więcej krajów i są przez nie użytkowane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4441">
<skos:prefLabel>bezpieczeństwo międzynarodowe</skos:prefLabel>
<skos:definition>ochrona ludzi, mienia i środowiska przed niebezpieczeństwem wynikającym z transgranicznego przemieszczania się zagrożeń, np. transgranicznym zanieczyszczaniem powietrza na dalekie odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4442">
<skos:prefLabel>standaryzacja międzynarodowa</skos:prefLabel>
<skos:definition>ustalenie lub ujednolicenie norm lub miar uznawanych na terytorium wielu państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4444">
<skos:prefLabel>handel międzynarodowy</skos:prefLabel>
<skos:definition>zorganizowana wymiana dóbr, obrót towarów pomiędzy państwami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4448">
<skos:prefLabel>międzynarodowy ciek wodny</skos:prefLabel>
<skos:definition>obszary geograficzne stanowiące wydzieloną jednostkę hydrograficzną, np.zlewnia rzeki, która leży w obrębie jurysdykcji dwóch lub kilku państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4451">
<skos:prefLabel>sposoby interpretacji</skos:prefLabel>
<skos:definition>metoda stosowana do oceny znaczenia i wartości danych, wyników, faktów, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4454">
<skos:prefLabel>strefa międzypływowa</skos:prefLabel>
<skos:definition>1) obszar między lądem a morzem regularnie odsłaniany ze względu na morskie pływy; organizmy morskie zamieszkujące strefy pływów muszą się przystosować do okresów ekspozycji na przemienne działanie powietrza i falowania wody, jest to jedno z najbardziej krytycznych środowisk z uwagi na uwarunkowania fizyczne, 2) strefa brzegowa między najwyższym zasięgiem przypływu i najniższym odpływu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4455">
<skos:prefLabel>interwencja w krajobraz przyrodniczy</skos:prefLabel>
<skos:definition>włączanie się do lub uczestnictwo w rozwiązywaniu sytuacji konfliktowych związanych z przyrodą lub krajobrazem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4466">
<skos:prefLabel>inwentaryzacja</skos:prefLabel>
<skos:definition>szczegółowa lista artykułów, dóbr, własności, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4468">
<skos:prefLabel>inwentaryzacja szkód leśnych</skos:prefLabel>
<skos:definition>przegląd i badanie obszarów leśnych w celu określenia ubytków (szkód) w lasach; celem inwentaryzacji leśnej jest uzyskanie oceny stanu lasu, w tym szczególnie wykrycie zmian tego stanu, ponadto wynikiem inwentaryzacji powinien być obraz rozmieszczenia szkód leśnych oraz stwierdzenie zależności między stanem siedlisk a stanem drzewostanów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4473">
<skos:prefLabel>inwersja</skos:prefLabel>
<skos:definition>odwrócenie zwykłego kierunku procesu, np. w wypadku zmiany gęstości wody przy temp. 4oC</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4474">
<skos:prefLabel>warstwa inwersyjna</skos:prefLabel>
<skos:definition>warstwa atmosfery w obrębie której zachodzi inwersja</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4475">
<skos:prefLabel>bezkręgowce</skos:prefLabel>
<skos:definition>zwierzęta bez kręgosłupa, zalicza się do nich wszystkie gatunki nie sklasyfikowane jako kręgowce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4478">
<skos:prefLabel>inwestycja</skos:prefLabel>
<skos:definition>obiekt wartościowy nabyty w celu osiagnięcia dochodu, odsetek lub zysków kapitałowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4480">
<skos:prefLabel>analiza in vitro</skos:prefLabel>
<skos:definition>analiza wykonywana w środowisku sztucznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4481">
<skos:prefLabel>analiza in vivo</skos:prefLabel>
<skos:definition>analiza wykonywana w środowisku naturalnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4483">
<skos:prefLabel>jod</skos:prefLabel>
<skos:definition>niemetaliczny pierwiastek halogenowy; trujące, powodujące korozję płytki lub granulki łatwo sublimujące, nierozpuszczalne w wodzie, natomiast rozpuszczalne w rozpuszczalnikach powszechnego zastosowania; stosowane w celach bakteriobójczych i antyseptycznych, jako barwniki, nalewki i środki farmaceutyczne, w litografii oraz jako katalizatory i odczynniki analityczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4485">
<skos:prefLabel>wymiana jonowa</skos:prefLabel>
<skos:definition>proces wymiany jonów miedzy roztworem a fazą nierozpuszczalną, zazwyczaj żywicą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4487">
<skos:prefLabel>wymieniacz jonowy</skos:prefLabel>
<skos:definition>trwały materiał nierozpuszczalny (zazwyczaj żywica syntetyczna), który zawiera jony ulegające  wymianie z jonami z otaczającego roztworu; zarówno wymieniacze anionowe jak kationowe (anionity i kationity) stosuje się do uzdatniania wody; objętość wymieniacza jonowego mierzy się w decymetrach sześciennych wymieniacza po dokładnym przemyciu, osuszeniu i osadzeniu złoża</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4491">
<skos:prefLabel>promieniowanie jonizujące</skos:prefLabel>
<skos:definition>promieniowanie zdolne do pobudzenia atomów tak, by wytrącić z nich elektrony; w tym stanie atomy stają się bardziej reaktywne; promieniowanie jonizujące zwiększa aktywność chemiczną i wywołuje skutki biologiczne łącznie ze zmianami w DNA; tylko promieniowanie elektromagnetyczne &quot;X&quot; i &quot;gamma&quot; wywołuje jonizację materiałów biologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4492">
<skos:prefLabel>jonosfera</skos:prefLabel>
<skos:definition>strefa ziemskiej atmosfery na wysokości od 60 do 1000 km nad powierzchnią globu, w której pod wpływem promieniowania jonizującego z przestrzeni kosmicznej występują wysokie stężenia wolnych elektronów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4493">
<skos:prefLabel>jon</skos:prefLabel>
<skos:definition>elektrycznie naładowany atom lub grupa atomów utworzona w wyniku utraty lub uzyskania jednego lub więcej elektronów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4498">
<skos:prefLabel>żelazo</skos:prefLabel>
<skos:definition>kowalny, plastyczny, srebrzysto biały, metaliczny pierwiastek ferromagnetyczny występujący głównie w hematytach i magnetytach, używany powszechnie w budownictwie i technice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4501">
<skos:prefLabel>hutnictwo żelaza</skos:prefLabel>
<skos:definition>sektor gospodarki obejmjący kopalnie i huty rud żelaza produkujące surówkę żelazną, żelazo zgrzewne i stal</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4503">
<skos:prefLabel>przemysł wyrobów żelaznych</skos:prefLabel>
<skos:definition>przemysł produkujacy artykuły żelazne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4504">
<skos:prefLabel>naświetlanie</skos:prefLabel>
<skos:definition>potraktowanie światłem lub innym promieniowaniem elektromagnetycznym lub wiązką cząsteczek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4505">
<skos:prefLabel>nawadnianie</skos:prefLabel>
<skos:definition>doprowadzenie wody niezbędnej do wzrostu lub poprawy wzrostu, np. roślin uprawnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4507">
<skos:prefLabel>kanał nawadniający</skos:prefLabel>
<skos:definition>trwały kanał doprowadzający wodę od źródła do jednego lub kilku gospodarstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4509">
<skos:prefLabel>uprawa na obszarach nawadnianych</skos:prefLabel>
<skos:definition>uprawa rolna w oparciu o wodę sztucznie dostarczaną do gleb uprawnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4514">
<skos:prefLabel>wyspa</skos:prefLabel>
<skos:definition>obszar lądu, o rozmiarach zwykle mniejszych niż kontynent, całkowicie ootoczony wodą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4515">
<skos:prefLabel>ekosystem wysp</skos:prefLabel>
<skos:definition>unikatowe, kruche i wrażliwe ekosystemy z uwagi na fakt, że ewolucja ich flory i fauny przebiegała w izolacji; wiele izolowanych wysp ma najbardziej unikatową florę w skali świata, na niektórych wyspach występują gatunki roślin i zwierząt, które wyewoluowały w specjalnych warunkach, bez ostrej konkurencji, która panuje na lądzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4518">
<skos:prefLabel>izolacja (proces)</skos:prefLabel>
<skos:definition>zapobieganie lub redukcja przesyłu elektryczności, ciepła lub dźwięku między jakimś ciałem a jego otoczeniem poprzez otoczenie go materiałem nieprzewodzącym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4519">
<skos:prefLabel>izomer</skos:prefLabel>
<skos:definition>1) jeden lub więcej związków mających taki sam wzór cząsteczkowy lecz inny układ atomów wewnątrz cząsteczki; 2) jedna lub więcej substancji chemicznych mających taki sam procentowy udział składników i taki sam ciężar objętościowy lecz różniących się budową, a w związku z tym właściwościami; różnice strukturalne mogą się przejawiać na różne sposoby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4523">
<skos:prefLabel>izotopy</skos:prefLabel>
<skos:definition>atomy tego samego pierwiastka chemicznego mające jednakową liczbę protonów, lecz różne liczby neutronów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4524">
<skos:prefLabel>kość słoniowa</skos:prefLabel>
<skos:definition>drobnoziarnista, śmietankowo-biała dentyna budująca zęby lub ciosy niektórych dużych zwierząt takich jak mors; od dawna ceniona jako surowiec na wyroby zdobnicze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4530">
<skos:prefLabel>zbiorowy dłużnik</skos:prefLabel>
<skos:definition>osoby mające wspólne zobowiązania lub wierzytelności; dwie lub więcej osób będących dłużnikami wobec jednej wierzytelności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4531">
<skos:prefLabel>wprowadzenie w życie postanowień Konferencji w Rio de Janeiro</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4534">
<skos:prefLabel>opieka prawna państwa</skos:prefLabel>
<skos:definition>program finansowany lub administrowany przez rząd, mający na celu prowadzenie i reprezentowanie w postępowaniu sądowym tych osób, których nie stać na opłacenie adwokata</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4535">
<skos:prefLabel>sądownictwo</skos:prefLabel>
<skos:definition>organizacja publiczna lub wydzielona część administracji rządowej odpowiedzialna za wymiar sprawiedliwości lub egzekwowanie prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4538">
<skos:prefLabel>reguła postępowania prawnego</skos:prefLabel>
<skos:definition>normy, uregulowania i precedensy które ustalają sposób postępowania, procedury i organizację systemu prawnego, jego podział i funkcje urzędników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4541">
<skos:prefLabel>jurysdykcja</skos:prefLabel>
<skos:definition>władza sądu do rozpoznawania i rozsądzania spraw lub wydawania rozporządzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4543">
<skos:prefLabel>prawodawstwo</skos:prefLabel>
<skos:definition>nauka lub filozofia prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4545">
<skos:prefLabel>kras</skos:prefLabel>
<skos:definition>1) niemieckie brzmienie terminu serbochorwackiego używanego na określenie terenu zbudowanego z rozpuszczalnych wapieni, pozbawionego odpływu powierzchniowego; szereg zagłębień powierzchni ziemi, depresje i szczeliny, struktury zapadliskowe i rozległy system odpływu podziemnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4548">
<skos:prefLabel>nagromadzenie zapasów</skos:prefLabel>
<skos:definition>przechowywanie zapasów na przyszłość</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4550">
<skos:prefLabel>kerozyna</skos:prefLabel>
<skos:definition>rzadki olej destylowany z ropy naftowej lub olej łupkowy używany do ogrzewania i gotowania, w lampach i jako środek skażający alkohol</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4566">
<skos:prefLabel>oznakowanie</skos:prefLabel>
<skos:definition>opatrywanie produktu lub pojemnika informacją na temat zawartości tychże, sposobie użytkowania, wytwórcy i ostrzeżenia o ewentualnych zagrożeniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4567">
<skos:prefLabel>laboratorium</skos:prefLabel>
<skos:definition>pomieszczenie lub budynek z wyposażeniem badawczym przeznaczony do badań naukowych lub nauczania, lub miejsce produkcji substancji chemicznych lub lekarstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4571">
<skos:prefLabel>doświadczenia laboratoryjne</skos:prefLabel>
<skos:definition>doświadczenia lub badania prowadzone w laboratorium</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4573">
<skos:prefLabel>badanie laboratoryjne</skos:prefLabel>
<skos:definition>badania prowadzone w laboratorium w zakresie testowania substancji chemicznych, kultur tkankowych, doświadczeń mikrobiologicznych, biochemicznych, hematologicznych, mikroskopowych, immunologicznych, parazytologicznych i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4574">
<skos:prefLabel>metoda laboratoryjna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4575">
<skos:prefLabel>odpady laboratoryjne</skos:prefLabel>
<skos:definition>materiały odpadowe powstające podczas badań analitycznych i działalności naukowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4577">
<skos:prefLabel>praca</skos:prefLabel>
<skos:definition>jeden z czynników produkcji; włącza się tu wysiłek fizyczny i umysłowy osób ukierunkowany na produkcję dóbr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4580">
<skos:prefLabel>prawo pracy</skos:prefLabel>
<skos:definition>ogół przepisów regulujących stosunki między pracownikami a pracodawcą, określających obowiązki i uprawnienia pracownika i pracodawcy względem siebie; reguły dotyczące stosunków ze związkami zawodowymi; zasady interwencji państwa, które zabezpieczają ochronę pracowników w określonych sytuacjach, w których taka interwencja jest wymagana</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4581">
<skos:prefLabel>rynek pracy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4584">
<skos:prefLabel>stosunki pracy</skos:prefLabel>
<skos:definition>dynamika lub stan związków (zależności) pomiędzy zarządem a pracownikami w przedsiębiorstwie, przemyśle lub w kraju, przy uwzględnieniu kontraktów, porozumień zbiorowych i statusu związków zwodowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4586">
<skos:prefLabel>lakier</skos:prefLabel>
<skos:definition>materiał zawierający znaczne ilości pochodnej celulozy, najczęściej nitrocelulozy, czasem też estrów celulozowych, takich jak: octan celulozy, maślan celulozy lub eter celulozy - etyloceluloza, używany w celu uzyskania połysku na mosiądzu lub innych powierzchniach metalicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4589">
<skos:prefLabel>laguna</skos:prefLabel>
<skos:definition>zbiornik wodny powstały przez odcięcie fragmentu otwartego morza przez rafy koralowe lub mierzeję piaszczystą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4590">
<skos:prefLabel>zlewnia jezior</skos:prefLabel>
<skos:definition>1) obniżenie na powierzchni Ziemi zajmowane przez jezioro; 2) obszar, z którego wody spływają do jeziora</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4593">
<skos:prefLabel>zanieczyszczenie jezior</skos:prefLabel>
<skos:definition>bezpośrednie lub pośrednie zmiany biologicznego, fizycznego, chemicznego lub radiologicznego stanu wód jeziornych lub ekosystemu jeziora</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4594">
<skos:prefLabel>jezioro</skos:prefLabel>
<skos:definition>odgraniczony zbiornik wodny, zazwyczaj z wodą słodką, nie obejmuje mórz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4597">
<skos:prefLabel>lampa oświetleniowe</skos:prefLabel>
<skos:definition>urządzenie dające światło, takie jak lampa elektryczna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4599">
<skos:prefLabel>ląd</skos:prefLabel>
<skos:definition>wydzielony fragment powierzchni Ziemi wraz z jego geologią, warstwami osadów powierzchniowych, topografią, hydrologią, glebami, florą i fauną oraz cechami działalności człowieka w przeszłości i współcześnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4600">
<skos:prefLabel>dostęp do terenów</skos:prefLabel>
<skos:definition>zezwolenie na użytkowanie, wejście lub przejście przez wycinek terenu, często stanowiący własność prywatną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4602">
<skos:prefLabel>parcelacja gruntów</skos:prefLabel>
<skos:definition>podział wielkich majątków ziemskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4603">
<skos:prefLabel>rejestr ziemi i własności</skos:prefLabel>
<skos:definition>system rejestrowania majątków lub udziałów we własności terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4607">
<skos:prefLabel>chłonność terenu</skos:prefLabel>
<skos:definition>maksymalny zasięg (możliwość) eksploatacji określonego wycinka terenu bez powodowania jego degradacji lub ubożenia zasobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4609">
<skos:prefLabel>oczyszczanie gruntów</skos:prefLabel>
<skos:definition>usuwanie drzew, krzewów, itp. w celu przygotowania terenu do uprawy rolnej, budowy, itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4610">
<skos:prefLabel>ochrona gruntów</skos:prefLabel>
<skos:definition>opieka, ochrona i wtórne użytkowanie terenów, szczególnie tych, które odznaczają się cennymi zasobami glebowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4611">
<skos:prefLabel>scalenie gruntów</skos:prefLabel>
<skos:definition>łączenie drobnych fragmentów terenu w wieksze, np. scalanie pól</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4612">
<skos:prefLabel>pokrywa terenu</skos:prefLabel>
<skos:definition>stan fizyczny powierzchni terenu łącznie z roślinością, glebą, skałą macierzystą, wodami i budowlami, które łącznie tworzą krajobraz; pokrywa terenu tworzy przejście między skorupą ziemską a atmosferą wpływając na obieg materii i przepływ energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4614">
<skos:prefLabel>rozwój przestrzenny</skos:prefLabel>
<skos:definition>planowanie rozwoju infrastruktury, usług i zabudowy przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4615">
<skos:prefLabel>ekologia systemów lądowych</skos:prefLabel>
<skos:definition>badanie zależności między organizmami lądowymi i ich środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4621">
<skos:prefLabel>składowisko odpadów</skos:prefLabel>
<skos:definition>metoda pozbywania się odpadów, najczęściej odpady składowane były w piaskowniach i żwirowniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4623">
<skos:prefLabel>zabezpieczanie składowisk odpadów</skos:prefLabel>
<skos:definition>zabezpieczenie ochronne najczęściej w postaci warstwy gleby nakładanej na powierzchnię odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4624">
<skos:prefLabel>odgazowanie składowiska</skos:prefLabel>
<skos:definition>odprowadzanie gazu składowiskowego przy zastosowaniu czynnej lub biernej wentylacji złoża odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4625">
<skos:prefLabel>biogaz</skos:prefLabel>
<skos:definition>gaz składowiskowy powstający w wyniku rozkładu odpadów z gospodarstw domowych; jest to bezbarwna mieszanka gazów o przykrym zapachu z uwagi na obecność organicznych związków siarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4626">
<skos:prefLabel>odciek ze składowiska</skos:prefLabel>
<skos:definition>ciecz powstająca po przesączeniu wód opadowych przez warstwę odpadów na składowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4628">
<skos:prefLabel>kształtowanie terenu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4629">
<skos:prefLabel>ukształtowanie terenu</skos:prefLabel>
<skos:definition>wyodrębniona fizyczna forma powierzchni Ziemi, o charakterystycznym kształcie i uwarunkowana przez przyczyny naturalne; wyróżnia się większe formy, takie jak: równina, wyżyna i góra oraz mniejsze, takie jak: wzgórze, dolina, stok, oz i wydma, łącznie formy te decydują o konfiguracji powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4632">
<skos:prefLabel>ssak lądowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4633">
<skos:prefLabel>użytkowanie terenu</skos:prefLabel>
<skos:definition>użytkowanie lub własność obszarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4635">
<skos:prefLabel>planowanie przestrzenne</skos:prefLabel>
<skos:definition>projektowanie, organizacja lub przygotowanie terenów do przyszłego użytkowania, szczególnie terenów w regionach cennych ze względu na zasoby naturalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4636">
<skos:prefLabel>zanieczyszczenie gruntu</skos:prefLabel>
<skos:definition>obecność zanieczyszczeń na danym obszarze lub w którejś z jego części składowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4641">
<skos:prefLabel>rekultywacja gruntów</skos:prefLabel>
<skos:definition>uproduktywnienie terenu poprzez wprowadzenie zasadniczych zmian jego ogólnego charakteru, np. odwodnienie terenów podmokłych, nawodnienie terenów suchych lub odzyskiwanie terenów przez osuszanie morza, jezior lub rzek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4644">
<skos:prefLabel>klasyfikacja użytkowania gruntów</skos:prefLabel>
<skos:definition>rejestr gruntów zawierający m.in. informację nt. powierzchni własności, nazwisk właścicieli i historycznej struktury podziału własności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4646">
<skos:prefLabel>renaturalizacja terenu</skos:prefLabel>
<skos:definition>przywracanie produktywnych funkcji na nieużytkach, wyrównywanie powierzchni itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4648">
<skos:prefLabel>krajobraz</skos:prefLabel>
<skos:definition>cechy charakterystyczne, układ i budowa wydzielonego obszaru geograficznego wraz z jego składnikami biologicznymi i środowiskiem fizycznym oraz cechami wynikającymi z działalności człowieka; obszar, na którym występują ekosystemy powiązane ze sobą różnymi interakcjami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4649">
<skos:prefLabel>krajobraz terenów pokopalnianych</skos:prefLabel>
<skos:definition>krajobraz zdegradowany przez kopalnictwo, charakteryzuje się m.in. zdewastowaniem gleb, naruszeniem stosunków wodnych, zniszczeniem potencjału produkcyjnego gruntów rolnych i leśnych oraz zakłóceniem wartości estetycznych i społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4650">
<skos:prefLabel>zmiany krajobrazu</skos:prefLabel>
<skos:definition>zmiany krajobrazu wynikające z działalności człowieka lub procesów przyrodniczych (pożary, klęski żywiołowe, itp.); mogą one być zdalnie rejestrowane z samolotów i satelitów przy użyciu teledetekcji i GIS</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4651">
<skos:prefLabel>architektura krajobrazu</skos:prefLabel>
<skos:definition>przekształcanie przestrzeni zewnętrznych poprzez tworzenie ogrodów, parków i innych kompozycji roślinnych i krajobrazowych w celu poprawy walorów estetycznych krajobrazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4652">
<skos:prefLabel>element krajobrazu</skos:prefLabel>
<skos:definition>w ocenie wizualnej krajobrazu bierze się pod uwagę cztery główne elementy różnicujące: formy, linie, barwy i teksturę, czyli wizualne odczucie krajobrazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4653">
<skos:prefLabel>ochrona krajobrazu</skos:prefLabel>
<skos:definition>ochrona krajobrazu i jego wartości w celu zapewnienia warunków rekreacji społeczeństwa, turystyki itp. również wprowadzanie nowych urządzeń rekreacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4654">
<skos:prefLabel>polityka ochrony krajobrazu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4657">
<skos:prefLabel>ekologia krajobrazu</skos:prefLabel>
<skos:definition>nauka o krajobrazie uwzględniająca ekologię populacji gatunków stanowiących biologiczną składową krajobrazu; obejmuje geomorfologię i ekologię; stosowana jest w projektowaniu i architekturze krajobrazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4658">
<skos:prefLabel>racjonalne zagospodarowanie krajobrazu</skos:prefLabel>
<skos:definition>zabiegi ukierunkowane na zachowanie krajobrazu lub ograniczanie zasięgu zmian wywoływanych przez człowieka lub siły przyrody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4659">
<skos:prefLabel>planowanie krajobrazu</skos:prefLabel>
<skos:definition>aspekt procesu planowania przestrzennego dotyczący wartości fizycznych, biologicznych, estetycznych, kulturowych i historycznych oraz zależności między tymi wartościami a użytkowaniem terenu i środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4660">
<skos:prefLabel>ochrona krajobrazu</skos:prefLabel>
<skos:definition>opracowanie i wdrożenie strategii oraz przedsięwzięć w celu ochrony, zachowania, racjonalnego użytkowania i odnowy zasobów przyrodniczych oraz krajobrazów naturalnych i kulturowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4661">
<skos:prefLabel>strefa chronionego krajobrazu</skos:prefLabel>
<skos:definition>obszar krajobrazu chronionego z uwagi na jego szczególne właściwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4662">
<skos:prefLabel>przekształcanie krajobrazu</skos:prefLabel>
<skos:definition>nadmierne użytkowanie elementów krajobrazu prowadzące od ich przekształcenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4663">
<skos:prefLabel>użytkowanie krajobrazu</skos:prefLabel>
<skos:definition>użytkowanie krajobrazu lub jego elementów w celach turystycznych, sportowych lub rolniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4666">
<skos:prefLabel>planowanie regionalne</skos:prefLabel>
<skos:definition>formułowanie regionalnych celów, planów i programów oraz harmonizacja skutków regionalnych w planowaniu sektorowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4668">
<skos:prefLabel>osuwisko</skos:prefLabel>
<skos:definition>przemieszczanie się powierzchni ziemi i procesy takie jak transport materiału w dół stoku pod wpływem grawitacyjnego oddziaływania mas gleby i skał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4676">
<skos:prefLabel>transport lądowy</skos:prefLabel>
<skos:definition>transport osób i dóbr samochodami i koleją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4678">
<skos:prefLabel>planowanie przestrzenne</skos:prefLabel>
<skos:definition>przestrzenne aspekty wszelkich działań człowieka łącznie ze sposobem przekształcania powierzchni terenu w celu zaspokojenia potrzeb społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4680">
<skos:prefLabel>klasyfikacja użytkowania gruntów</skos:prefLabel>
<skos:definition>system kategorii jednostek przestrzennych utworzony w oparciu o właściwości terenu i jego przydatność do określonych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4682">
<skos:prefLabel>planowanie przestrzenne</skos:prefLabel>
<skos:definition>interdyscyplinarny proces oceny, organizacji oraz ukierunkowania w zagospodarowaniu terenów i ich zasobów, obecnie i w przyszłości, przy uwzględnieniu przydatności tych terenów oraz zasady trwałej i zrównoważonej produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4684">
<skos:prefLabel>zagospodarowanie gleb</skos:prefLabel>
<skos:definition>zagospodarowanie i użytkowanie gleb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4686">
<skos:prefLabel>wycena gruntów</skos:prefLabel>
<skos:definition>wycena materialnej wartości handlowej terenu lub gruntów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4691">
<skos:prefLabel>spalarnia przemysłowa</skos:prefLabel>
<skos:definition>urządzenie, które zamienia energię uwalnianą przy spalaniu mieszaniany paliwa z powietrzem na  energię mechaniczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4698">
<skos:prefLabel>laser</skos:prefLabel>
<skos:definition>akronim utworzony ze sformułowania Light Amplification by Stimulated Emission of Radiation (wzmacnianie światła przez wymuszoną emisję promieniowania)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4702">
<skos:prefLabel>pranie</skos:prefLabel>
<skos:definition>czynności takie jak pranie i prasowanie odzieży bielizny, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4707">
<skos:prefLabel>ustawa</skos:prefLabel>
<skos:definition>akt prawny o charakterze ogólnym, powszechnie obowiązujący, uchwalony w szczególnym trybie przez najwyższy organ władzy państwowej, parlament</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4708">
<skos:prefLabel>ustawa o zmianie ustawy</skos:prefLabel>
<skos:definition>zmiana obowiązującej ustawy (przepisu), która po zatwierdzeniu przez władzę ustawodawczą zmienia istniejące przepisy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4710">
<skos:prefLabel>przestrzeganie prawa</skos:prefLabel>
<skos:definition>działania mające na celu ochronę porządku prawnego i wymuszenie przestrzegania obowiązujących przepisów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4715">
<skos:prefLabel>prawo dotyczące stosunków sąsiedzkich</skos:prefLabel>
<skos:definition>zestaw obowiązujących przepisów chroniących zdrowie ludzi i środowiska, regulujących zagadnienia rozwoju obszaru w aspekcie poprawy jakości życia na niewielkich obszarach zmieszkiwanych przez  wspólnoty lokalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4719">
<skos:prefLabel>prawo (dyscyplina naukowa)</skos:prefLabel>
<skos:definition>ogół przepisów, norm prawnych regulujących stosunki między ludźmi danej społeczności, określających zasady ich postępowania lub zawierających zakazy, których naruszenie zagrożone jest karą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4722">
<skos:prefLabel>ługowanie</skos:prefLabel>
<skos:definition>1) proces oddzielania cząstek stałych od cieczy np. przy perkolacji ścieków przez warstwę gleby; 2) ekstrakcja rozpuszczalnych składników z mieszaniny ciał stałych z zastosowaniem perkolacji rozpuszczalnikiem; 3) usuwanie składników rozpuszczalnych przy zastosowaniu cieczy perkolującej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4723">
<skos:prefLabel>ołów</skos:prefLabel>
<skos:definition>ciężki pierwiastek metaliczny, toksyczny, o niebiesko-białej barwie, bardzo łatwo kowalny, w przyrodzie występuje głównie w postaci rud galenowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4724">
<skos:prefLabel>związki ołowiu</skos:prefLabel>
<skos:definition>związki ołowiu obecne są np. w domieszkach do benzyny, w farbach, produktach ceramicznych, pokryciu dachów, uszczelniaczach rur, urządzeniach elektrycznych, rurach lub kontenerach; ołów w powietrzu pochodzi przede wszystkim ze spalania paliw z zawartością ołowiu, odpadów stałych, węgla, ropy oraz wielu innych źródeł sztuczncyh i naturalnych, takich jak wulkany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4725">
<skos:prefLabel>skażenie ołowiem</skos:prefLabel>
<skos:definition>obecność i uwalnianie ołowiu do powietrza, wody i gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4727">
<skos:prefLabel>norma dotycząca zawartości ołowiu w benzynie</skos:prefLabel>
<skos:definition>przepisy dotyczące ograniczenia lub eliminacji zawartości ołowiu w paliwach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4728">
<skos:prefLabel>poziom ołowiu we krwi</skos:prefLabel>
<skos:definition>miara ilości ołowiu lub jego soli zaabsorbowanej przez organizm jako oznaka przypuszczalnego zatrucia ostrego lub chronicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4730">
<skos:prefLabel>liść (botanika)</skos:prefLabel>
<skos:definition>główny organ fotosyntetyzujący i transpirujący u roślin wyższych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4731">
<skos:prefLabel>przeciek</skos:prefLabel>
<skos:definition>przypadkowy, niekontrolowany zrzut lub wyciek cieczy, gazów i innych substancji zagrażający zdrowiu ludzi i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4735">
<skos:prefLabel>skóra wyprawiona</skos:prefLabel>
<skos:definition>wyprawiona, wygarbowana skóra zwierzęcia, zazwyczaj pozbawiona sierści</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4738">
<skos:prefLabel>przemysł skórzany</skos:prefLabel>
<skos:definition>przemysł produkukujący wyroby skórzane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4739">
<skos:prefLabel>podstawa prawna</skos:prefLabel>
<skos:definition>podstawowe prawo, ustawa lub precedens prawny, na którym opiera się decyzje lub działania jednostek władzy, spółek lub osób prywatnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4741">
<skos:prefLabel>prawo zastrzeżone przepisem ustawy</skos:prefLabel>
<skos:definition>roszczenie mające wsparcie prawne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4743">
<skos:prefLabel>uregulowanie prawne</skos:prefLabel>
<skos:definition>rozporządzenie lub przepis wydany przez rząd określający procedury tworzenia, wykonywania i rozstrzygania prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4744">
<skos:prefLabel>środek prawny</skos:prefLabel>
<skos:definition>środki służące egzekwowaniu prawa lub zapobieganiu jego pogwałcenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4746">
<skos:prefLabel>tekst aktów prawnych</skos:prefLabel>
<skos:definition>dokładne sformułowanie lub język prawa lub innego dokumentu, zgodny z obowiązującym prawem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4749">
<skos:prefLabel>ustawodawstwo</skos:prefLabel>
<skos:definition>akt lub proces tworzenia prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4752">
<skos:prefLabel>prawodawstwo w zakresie emisji zanieczyszczeń</skos:prefLabel>
<skos:definition>normy prawne w zakresie ograniczenia emisji zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4758">
<skos:prefLabel>prawodawstwo w zakresie zasobów wodnych</skos:prefLabel>
<skos:definition>normy prawne regulujące zagadnienia ochrony i gospodarki zasobami wodnymi na danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4759">
<skos:prefLabel>władza ustawodawcza</skos:prefLabel>
<skos:definition>władza obradującego zespołu osób do nadawania sprawom, uchwałom, rezolucjom i ustawom statusu formalnego, prawnie obowiązującego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4760">
<skos:prefLabel>kompetencje ustawodawcze</skos:prefLabel>
<skos:definition>wiedza, kwalifikacje, uprawnienia i władza do stanowienia prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4762">
<skos:prefLabel>informacja dotycząca ustawodawstwa</skos:prefLabel>
<skos:definition>informacje nt obowiązujących i proponowanych aktów prawnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4764">
<skos:prefLabel>zgromadzenie ustawodawcze</skos:prefLabel>
<skos:definition>wydział, zgromadzenie lub grupa osób stanowiących prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4767">
<skos:prefLabel>aktywny wypoczynek</skos:prefLabel>
<skos:definition>uprawianie sportu i inne działania rekreacyjne w czasie wolnym od pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4771">
<skos:prefLabel>wypoczynek</skos:prefLabel>
<skos:definition>czas wolny od pracy i innych obowiązków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4777">
<skos:prefLabel>motyle</skos:prefLabel>
<skos:definition>rząd owadów łuskoskrzydłych, takich jak motyle, ćmy; osobniki dorosłe mają dwie pary błoniastych skrzydeł i trąbkowato wydłużone ssące narządy gębowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4782">
<skos:prefLabel>białaczka</skos:prefLabel>
<skos:definition>postępujaca choroba narządów krwiotwórczych o charakterze złośliwym; zaburzona proliferacja i rozwój leukocytów oraz ich prekursorów we krwi i szpiku kostnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4784">
<skos:prefLabel>podatek</skos:prefLabel>
<skos:definition>podlegająca opodatkowaniu część produktu powstałego w oparciu o wartości majątku lub pracę obywateli państwa, pobierana przez państwo w celu wykonywania swoich praw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4785">
<skos:prefLabel>leksykon</skos:prefLabel>
<skos:definition>zasób słów dotyczący określonej dziedziny działalności, określonego regionu, klasy społecznej lub jednostki, lub też kompletny zestaw morfemów albo znaczeniowych jednostek języka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4787">
<skos:prefLabel>odpowiedzialność</skos:prefLabel>
<skos:definition>przyjęcie zobowiązań wynikających z prawa; odpowiedzialność może być cywilna lub kryminalna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4788">
<skos:prefLabel>odpowiedzialność za katastrofy morskie</skos:prefLabel>
<skos:definition>zobowiązanie do rekompensaty szkód ekologicznych oraz innych szkód, wskutek których ucierpiały osoby i mienie, a wynikających z zanieczyszczenia i degradacji mórz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4789">
<skos:prefLabel>odpowiedzialność za szkodę jądrową</skos:prefLabel>
<skos:definition>zobowiązanie do rekompensaty szkód w środowisku oraz innych szkód, wskutek których ucierpiały osoby i mienie podczas produkcji, użytkowania i transportu materiałów radiaktywnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4790">
<skos:prefLabel>ustawodawstwo dotyczące odpowiedzialności cywilnej</skos:prefLabel>
<skos:definition>ustawa lub pakiet ustaw odnoszący się do zagadnień odpowiedzialności cywilnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4793">
<skos:prefLabel>biblioteka</skos:prefLabel>
<skos:definition>pomieszczenie, w któym chroni się książki i inne zapisy literaturowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4796">
<skos:prefLabel>wydawanie zezwoleń</skos:prefLabel>
<skos:definition>proces wydawania zezwoleń i certyfikatów lub pozwoleń administracyjnych na prowadzenie określonego rodzaju działalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4797">
<skos:prefLabel>procedura wydawania zezwoleń</skos:prefLabel>
<skos:definition>procedyry wydawania zezwoleń przez władze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4798">
<skos:prefLabel>obowiązek posiadania zezwoleń</skos:prefLabel>
<skos:definition>obowiązek posiadania zezwolenia na wykonywanie określonych zawodów i prowadzenie działalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4799">
<skos:prefLabel>porosty</skos:prefLabel>
<skos:definition>organizmy o charakterze złożonym utrzymujące się dzięki symbiozie grzyba i glonu; tworzą skorupiaste narośla lub skupiska krzaczków na pniach drzew, murach, dachach i glebie; porosty mogą być stosowane jako organizmy wskaźnikowe w monitoringu zanieczyszczeń powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4802">
<skos:prefLabel>cykl życia</skos:prefLabel>
<skos:definition>fazy, przemiany lub stadia, przez które organizm przechodzi w ciągu życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4805">
<skos:prefLabel>przyrodoznawstwo</skos:prefLabel>
<skos:definition>nauka dotycząca zbiorów żywych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4808">
<skos:prefLabel>styl życia</skos:prefLabel>
<skos:definition>swoiste postawy, zwyczaje lub zachowania wykazywane przez pojedyncze osobniki lub ich grupy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4810">
<skos:prefLabel>światło</skos:prefLabel>
<skos:definition>promieniowanie elektromagnetyczne, wywołujące wrażenia wzrokowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4814">
<skos:prefLabel>oświetlenie</skos:prefLabel>
<skos:definition>oświetlenie uliczne lub mieszkaniowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4815">
<skos:prefLabel>separator lekkich cieczy</skos:prefLabel>
<skos:definition>urządzenie mechaniczne służące do oddzielania i usuwania pozostałości paliw i olejów od wody ściekowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4818">
<skos:prefLabel>węgiel brunatny</skos:prefLabel>
<skos:definition>węgiel stosunkowo współczesnego pochodzenia, utworzony z częściowo rozłożonej roślinności; o charakterze pośrednim między torfem a węglem kamiennym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4819">
<skos:prefLabel>górnictwo węgla brunatnego</skos:prefLabel>
<skos:definition>wydobycie węgla brunatnego z naturalnych pokładów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4820">
<skos:prefLabel>wapno</skos:prefLabel>
<skos:definition>mineralne i przemysłowe formy tlenku wapnia, różniące się zawartością wody i udziałem takich składników jak krzem, glin i żelazo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4823">
<skos:prefLabel>skała wapienna</skos:prefLabel>
<skos:definition>skała osadowa składająca się głównie z węglanu wapnia, na ogół w postaci mineralnego kalcytu z domieszką lub bez węglanu magnezu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4828">
<skos:prefLabel>wartość graniczna</skos:prefLabel>
<skos:definition>kryterium narażenia lub norma określająca czy stężenie substancji w urzadzeniach lub w pomieszczeniu nie powoduje szkodliwych skutków u większości narażonych pracowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4830">
<skos:prefLabel>limnologia</skos:prefLabel>
<skos:definition>nauka o zbiornikach wód słodkich z uwzględnieniem ich świata roślinnego i zwierzęcego, właściwości fizycznych, cech geograficznych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4832">
<skos:prefLabel>linearne źródło dźwięku</skos:prefLabel>
<skos:definition>punktowe źródła hałasu ustawione szeregowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4834">
<skos:prefLabel>materiał wykładzinowy</skos:prefLabel>
<skos:definition>warstwa materiałów sztucznych lub naturalnych wyściełająca zbocza lub spąg składowiska odpadów albo zbiorniki retencyjne, ograniczająca wgłębny lub boczny dopływ odcieków do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4835">
<skos:prefLabel>źródło liniowe</skos:prefLabel>
<skos:definition>jednowymiarowe źródło emisji, np. droga gruntowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4838">
<skos:prefLabel>lipid</skos:prefLabel>
<skos:definition>rodzaj związków zawierających długołańcuchowe węglowodory alifatyczne i ich pochodne, takie jak kwasy tłuszczowe, alkohole, aminy, aminoalkohole i aldehydy; należą do nich woski, tłuszcze i związki pochodne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4839">
<skos:prefLabel>substancja lipofilowa</skos:prefLabel>
<skos:definition>substancje wykazujące powinowactwo do lipidów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4840">
<skos:prefLabel>ciekły gaz</skos:prefLabel>
<skos:definition>związek gazowy lub mieszanina przekształcona do fazy ciekłej w procesie schładzania lub kompresji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4843">
<skos:prefLabel>gnojowica</skos:prefLabel>
<skos:definition>nawóz o zawartości wody powyżej 90 procent, zazwyczaj składający się z fekaliów zwierzęcych z dodatkiem wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4844">
<skos:prefLabel>stan ciekły</skos:prefLabel>
<skos:definition>stan przejściowy materii między substancją krystaliczną a gazem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4845">
<skos:prefLabel>odpady płynne</skos:prefLabel>
<skos:definition>odpady ciekłe składające się ze ścieków kanalizacyjnych i bytowych, wody zużytej, lub innych cieczy powstałych podczas procesów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4850">
<skos:prefLabel>literatura</skos:prefLabel>
<skos:definition>teksty pisane, takie jak poezje, powieści, szkice, szczególnie utwory wyobraźni literackiej charakteryzujące się doskonałością stylu i wyrazu oraz ogólną lub ponadczasową tematyką</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4851">
<skos:prefLabel>bank danych literaturowych</skos:prefLabel>
<skos:definition>zasoby informacji dot. określonej tematyki, w postaci zbioru dokumentów, najczęściej przechowywany i użytkowany w systemie komputerowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4852">
<skos:prefLabel>krytyka literacka</skos:prefLabel>
<skos:definition>ocena i krytyka tekstów pisanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4853">
<skos:prefLabel>badanie literatury</skos:prefLabel>
<skos:definition>identyfikacja, opis, analiza i klasyfikacja książek i innych tekstów literaturowych, użytkowanych i konsultowanych podczas przygotowywania prac</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4855">
<skos:prefLabel>litosfera</skos:prefLabel>
<skos:definition>zewnętrzna, najbardziej sztywna warstwa kuli ziemskiej, także: sama skorupa ziemska, w zestawieniu z atmosferą i litosferą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4857">
<skos:prefLabel>ściółka</skos:prefLabel>
<skos:definition>słoma, siano lub podobny materiał stosowany jako podściółka na legowiskach zwierząt gospodarskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4858">
<skos:prefLabel>strefa przybrzeżna</skos:prefLabel>
<skos:definition>strefa międzypływowa na brzegu morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4862">
<skos:prefLabel>zwierzę domowe</skos:prefLabel>
<skos:definition>bydło, konie i inne zwierzęta hodowane w gospodarstwie wiejskim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4863">
<skos:prefLabel>hodowla zwierząt domowych</skos:prefLabel>
<skos:definition>hodowanie zwierząt gospodarskich z zastosowaniem krzyżowania różnych odmian w celu uzyskania nowych o pożądanych właściwościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4864">
<skos:prefLabel>hodowla zwierząt</skos:prefLabel>
<skos:definition>hodowla bydła, koni i innych zwierząt gospodarskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4866">
<skos:prefLabel>warunki życia</skos:prefLabel>
<skos:definition>czynniki lub właściwości miejsca zamieszkania oceniane z punktu widzenia ich znaczenia dla zachowania zdrowia i dobrobytu jego mieszkańców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4867">
<skos:prefLabel>morskie zasoby żywe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4868">
<skos:prefLabel>przestrzeń życiowa</skos:prefLabel>
<skos:definition>pomieszczenia, budowle lub obszary stanowiące otoczenie życia mieszkańców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4871">
<skos:prefLabel>jaszczurka</skos:prefLabel>
<skos:definition>gady z podrzędu Lacertilia, szczególnie z rodziny Lacertidae, charakteryzujące się wydłużonym ciałem, czterema kończynami i niewielkim ogonem; należą tu gekony, legwany, kamelony, warany i padalce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4873">
<skos:prefLabel>wytrzymałość na obciążenie</skos:prefLabel>
<skos:definition>maksymalne obciążenie, które wytrzyma dany układ zanim przestanie funkcjonować</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4879">
<skos:prefLabel>władza lokalna</skos:prefLabel>
<skos:definition>władza agencji rządowej lub administracji rządowej w zakresie wdrażania prawa i polityki rządu na określonym terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4884">
<skos:prefLabel>finansowanie na poziomie lokalnym</skos:prefLabel>
<skos:definition>teoria i praktyka całokształtu spraw finansowych w odniesieniu do wydzielonych obszarów, takich jak:  dzielnica, miasto lub małych zarządów regionalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4885">
<skos:prefLabel>polityka władz miejscowych</skos:prefLabel>
<skos:definition>kierunki działania przyjęte i stosowane przez rządzące władze polityczne lub system, który określa stan panujący w dzielnicy, mieście lub w regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4886">
<skos:prefLabel>doprowadzenie energii cieplnej</skos:prefLabel>
<skos:definition>doprowadzanie ciepła w postaci pary wodnej lub gorącej wody z centralnego źródła do grupy budynków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4888">
<skos:prefLabel>lokalny materiał budowlany</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4889">
<skos:prefLabel>lokalny transport pasażerski</skos:prefLabel>
<skos:definition>system transportu pasażerów na wydzielonym terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4891">
<skos:prefLabel>lokalny ruch komunikacyjny</skos:prefLabel>
<skos:definition>komunikacja wewnątrz zurbanizowanego obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4893">
<skos:prefLabel>rozmieszczenie przemysłu</skos:prefLabel>
<skos:definition>teren przydatny do zlokalizowania określonego zakładu przemysłowego, wybór miejsca zależy od wielu czynników gospodarczych i środowiskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4897">
<skos:prefLabel>lokomotywa</skos:prefLabel>
<skos:definition>samoporuszający się silnik napędzany parą, elektrycznością lub olejem napędowym służący jako siła pociągowa na torach kolejowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4902">
<skos:prefLabel>ruch komunikacyjny dalekobieżny</skos:prefLabel>
<skos:definition>komunikacja na znacznych przestrzeniach, pokonująca wielkie odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4903">
<skos:prefLabel>transport dalekobieżny</skos:prefLabel>
<skos:definition>przewóz materiałów i dóbr lądem i wodą lub w powietrzu na znacznych odległościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4906">
<skos:prefLabel>skutek długofalowy</skos:prefLabel>
<skos:definition>skutek, którego trwałość utrzymuje się długo po ustaniu przyczyny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4907">
<skos:prefLabel>długofalowy skutek zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4908">
<skos:prefLabel>eksperyment długofalowy</skos:prefLabel>
<skos:definition>1) długotrwałe doświadczenie; 2) doświadczenie, którego wyniki uzyskuje się po dłuższym okresie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4909">
<skos:prefLabel>prognozowanie długofalowe</skos:prefLabel>
<skos:definition>prognozowanie i obliczanie prawdopodopodobieństwa zaistnienia określonych warunków lub pojawów w oddalonym punkcie czasowym w przyszłości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4911">
<skos:prefLabel>tendencja długofalowa</skos:prefLabel>
<skos:definition>dominująca tendencja lub panujący kierunek zmian w dłuższej perspektywie czasowej ustalony na podstawie badań i analiz danych statystycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4913">
<skos:prefLabel>samochód ciężarowy</skos:prefLabel>
<skos:definition>duży pojazd silnikowy przeznaczony do przwozu ciężkich ładunków, zazwyczaj z platformą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4914">
<skos:prefLabel>strata</skos:prefLabel>
<skos:definition>wynik operacji w działaniach handlowych, w których koszty własne są większe niż wpływy lub zyski</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4916">
<skos:prefLabel>utrata siedlisk</skos:prefLabel>
<skos:definition>niszczenie siedlisk w wyniku degradacji środowiska, która z kolei wywołana jest przez zanieczyszczenie powietrza lub wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4917">
<skos:prefLabel>głośność</skos:prefLabel>
<skos:definition>natężenie fizjologicznego odczucia spowodowanego przez dźwięk, zależne bezpośrednio od fizycznego natężenia dźwięku oraz od częstotliwości i kształtu fali dźwiękowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4918">
<skos:prefLabel>tanie budownictwo mieszkaniowe</skos:prefLabel>
<skos:definition>mieszkania wybudowane systemem oszczędnościowym w celu utrzymania niskich stawek wynajmu lub niskich cen, ustalonych na podstawie kryterium poziomu rocznego dochodu poniżej lokalnej wielkości mediany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4920">
<skos:prefLabel>izba niższa parlamentu</skos:prefLabel>
<skos:definition>organ dwuizbowego ciała ustawodawczego, w którego skład wchodzą przedstawiciele wybrani przez ogół elektoratu zorganizowany w okręgach wyborczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4921">
<skos:prefLabel>gatunek nie zagrożony wyginięciem</skos:prefLabel>
<skos:definition>zwierzęta, rośliny i inne organizmy żywe uznane za niezagrożone wyginięciem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4922">
<skos:prefLabel>przepływ niżówkowy</skos:prefLabel>
<skos:definition>faza najniższego poziomu wody w cieku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4929">
<skos:prefLabel>lot na niskim pułapie</skos:prefLabel>
<skos:definition>latania na małych wyskościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4931">
<skos:prefLabel>technologia uproszczona</skos:prefLabel>
<skos:definition>względnie nieskomplikowane urządzenia techniczne lub metoda charakteryzująca się stopniem funkcjonalności niższym niż w innych podobnych układach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4934">
<skos:prefLabel>smar</skos:prefLabel>
<skos:definition>substancja używana do zmniejszenia tarcia na powierzchniach ciernych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4937">
<skos:prefLabel>jaskrawość subiektywna</skos:prefLabel>
<skos:definition>funkcyjna zależność między wielkością gwiazd a liczbą i rozkładem gwiazd w przedziałach wielkości gwiezdnej; także współczynnik jaskrawości względnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4942">
<skos:prefLabel>ług</skos:prefLabel>
<skos:definition>zasadowy roztwór uzyskiwany przez ługowaniu popiołu drzewnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4945">
<skos:prefLabel>system limfatyczny</skos:prefLabel>
<skos:definition>układ naczyń i węzłów przewodzacych limfę (chłonkę) w organizmach kręgowców; od naczyń włosowatych w przestrzeniach międzytkankowych do kanałów piersiowych uchodzących do żył podobojczykowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4948">
<skos:prefLabel>pomiar lizymetryczny</skos:prefLabel>
<skos:definition>pomiar wody przesiąkającej przez warstwę gleby i oznaczenie substancji w niej rozpuszczonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4952">
<skos:prefLabel>produkcja maszyn</skos:prefLabel>
<skos:definition>wytwarzanie urządzeń mechanicznych w celach przemysłowych i handlowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4956">
<skos:prefLabel>cel makrogospodarczy</skos:prefLabel>
<skos:definition>cele odnoszące się do produkcji, dystrybucji i użytkowania dochodów, dóbr i wyrobów na określonym obszarze, zazwyczaj związane z fiskalną i monetarną polityką rządu w aspekcie jej wpływu na poziomy zatrudnienia, konsumpcji, inwestycji i rozwoju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4957">
<skos:prefLabel>makroekonomia</skos:prefLabel>
<skos:definition>nowoczesna analiza gospodarcza przetwarzająca dane zbiorcze, w przeciwieństwie do oddzielnych analiz dochodu narodowego, konsumpcji i inwestycji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4964">
<skos:prefLabel>taśma magnetyczna</skos:prefLabel>
<skos:definition>taśma plastikowa, papierowa lub metalowa powlekana lub impregnowana cząsteczkami magnesującego się tlenku żelaza, stosowana do zapisu magnetycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4965">
<skos:prefLabel>magnetyzm</skos:prefLabel>
<skos:definition>rodzaj zjawisk fizycznych związanych z przemieszczaniem się elektryczności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4967">
<skos:prefLabel>lista wysyłkowa</skos:prefLabel>
<skos:definition>lista obejmująca adresy lub adresy poczty komputerowej, na które wysyła się przesyłki pocztowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4969">
<skos:prefLabel>zachowanie stanu środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4971">
<skos:prefLabel>poważna awaria</skos:prefLabel>
<skos:definition>nieoczekiwany wypadek, awaria, strata na skalę przekraczajacą pewien poziom normalny lub określony, mogąca stanowić szczególne zagrożenie dla życia ludzil lub prowadzić do strat materialnych lub szkód w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4974">
<skos:prefLabel>malaria</skos:prefLabel>
<skos:definition>grupa chorób febrycznych u ludzi o długotrwałym cyklu nawrotów, wywoływana przez krwinkowcowe, pasożyty krwi z rodzaju Plasmodium roznoszone przez komary Anopheles</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4977">
<skos:prefLabel>wada rozwojowa</skos:prefLabel>
<skos:definition>trwała zmiana budowy, która może mieć negatywny wpływ na przetrwanie, rozwój i funkcjonowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4979">
<skos:prefLabel>niedożywienie</skos:prefLabel>
<skos:definition>wadliwe odżywianie spowodowane niewłaściwym pobieraniem składników lub ich wadliwym trawieniem, przyswajaniem lub metabolizmem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4982">
<skos:prefLabel>ssak</skos:prefLabel>
<skos:definition>Mammalia, gromada ciepłokrwistych zwierząt kręgowych; główne cechy to gruczoły mlekowe u samic, przepona oddzielajaca klatkę piersiową i czterokomorowe serce; obejmuje około 8 tysięcy gatunków; należą do niej m.in. walenie, mięsożerne, gryzonie, nietoperze, naczelne i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4984">
<skos:prefLabel>człowiek (społeczeństwo)</skos:prefLabel>
<skos:definition>przedstawiciel rasy ludzkiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4985">
<skos:prefLabel>zarządzanie</skos:prefLabel>
<skos:definition>wykonywanie czynności administracyjnych, sprawowanie władzy, kontrolowanie, nadzorowanie; akt zarządzania, kierowania, administrowania przedsiębiorstwem, gospodarstwem, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4987">
<skos:prefLabel>umowa o pracę</skos:prefLabel>
<skos:definition>umowa prawna między dwoma lub więcej stronami pracodawców i pracowników, określająca zakres i charakter prac administracyjnych lub nadzorczych do wykonania w zamian za płacę i zapewnienie warunków pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4988">
<skos:prefLabel>gospodarowanie zasobami naturalnymi</skos:prefLabel>
<skos:definition>planowe użytkowanie zasobów naturalnych, głównie nieodnawialnych, w zgodzie z zasadami, które zapewniają uzyskiwanie optymalnych i długotrwałych korzyści gospodarczych i społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4990">
<skos:prefLabel>plan zarządzania</skos:prefLabel>
<skos:definition>program działania, ukierunkowany na osiągnięcie wytyczonych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4994">
<skos:prefLabel>mandat</skos:prefLabel>
<skos:definition>rozkaz lub zezwolenie na działanie powadzone zgodnie z określonym trybem postępowania wydane przez przełożonego podlegającemu mu pracownikowi, przez sąd - sądowi niższego szczebla lub przez elektorat - swojemu przedstawicielowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4998">
<skos:prefLabel>namorzyn</skos:prefLabel>
<skos:definition>zbiorowiska roślinne oraz drzewa porastające bagna w strefie pływów morskich, błotniste, muliste i piaszczyste brzegi w okolicy ujść rzecznych i inne nisko położone tereny podlegające systematycznemu zalewowi przez morze lecz chronione od wysokich fal i silnych prądów; namorzyny to jedyne gatunki drzewiaste przystosowane do zalewania przez morze; zbiorowiska namorzynowe stanowią ostoję wieluset gatunków lądowych i morskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/4999">
<skos:prefLabel>bagna namorzynowe</skos:prefLabel>
<skos:definition>podmokłe, grząskie obszary lądu wzdłuż wybrzeży w klimatach tropikalnych porośnięte przez namorzyny drzewiaste i krzewiaste, szczególnie przez namorzyny czerwone (Rhizophora), czarne (Avicennia) i białe (Laguncularia)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5000">
<skos:prefLabel>zmiany klimatu spowodowane przez człowieka</skos:prefLabel>
<skos:definition>zmiany klimatu wynikają prawdopodobnie z efektu szklarniowego i są ubocznymi skutkami innych działań ludzkich; zmiany albedo lądów spowodowane przez pustynnienie i odlesienie wpływają na zmiany ilości energii słonecznej zaabsorbowanej przez powierzchnię Ziemi; sztuczne aerosole utworzone ze związków siarki emitowanych do atmosfery przez elektrownie mogą mieć modyfikujący wpływ na chmury, zmiany w ilości stratosferycznego ozonu w wyniku dopływu chlorofluorowęglowodrów (CFC) mają wpływ na klimat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5003">
<skos:prefLabel>wzajemne oddziaływanie człowieka i przyrody</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5004">
<skos:prefLabel>siła robocza</skos:prefLabel>
<skos:definition>1) siła wytwarzana przez pracującego człowieka; 2) liczba pracowników mogących wykonać jakąś pracę, usługę, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5005">
<skos:prefLabel>działalność produkcyjna</skos:prefLabel>
<skos:definition>działania mające na celu przetworzenie surowców na wyroby skończone, szczególnie w wyniku operacji wielkoprzemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5006">
<skos:prefLabel>handel wyrobami przemysłowymi</skos:prefLabel>
<skos:definition>proces lub akt wymiany, kupna lub sprzedaży wytworzonych wyrobów lub surowców do przerobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5007">
<skos:prefLabel>nawóz zwierzęcy</skos:prefLabel>
<skos:definition>odchody zwierzęce usuwane ze stajni i obór, łącznie ze ściółką lub bez niej, stosowane do nawożenia gleb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5009">
<skos:prefLabel>produkcja nawozu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5011">
<skos:prefLabel>mapa</skos:prefLabel>
<skos:definition>odwzorowanie, zazwyczaj na płaskim podłożu, cech fizycznych i administracyjnych powierzchni Ziemi, ukazujące ich formy, rozmiary i zależności, według przyjętej konwencji odwzorowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5013">
<skos:prefLabel>kartowanie</skos:prefLabel>
<skos:definition>proces sporządzania mapy danego obszaru, szczególnie prace terenowe z tym związane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5014">
<skos:prefLabel>kartowanie porostów</skos:prefLabel>
<skos:definition>sporządzanie map rozmieszczenia porostów wskazujących na jakość powietrza; porosty krzaczkowate są bardziej wrażliwe na uszkodzenia z powodu SO2 niż porosty liściaste, a obie te grupy porostów są mniej odporne niż porosty skorupiaste; typy morfologiczne porostów stosuje się więc jako wskaźniki stężeń zanieczyszczeń powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5018">
<skos:prefLabel>marmur</skos:prefLabel>
<skos:definition>skała przeobrażona, składająca się z rekrystalizowanego kalcytu lub dolomitu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5023">
<skos:prefLabel>grunty nieurodzajne</skos:prefLabel>
<skos:definition>niskiej jakości grunty, z których produkcja zaledwie wystarcza na pokrycie poniesionych na nią nakładów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5024">
<skos:prefLabel>hodowla organizmów morskich</skos:prefLabel>
<skos:definition>hodowla morskich organizmów w ich środowisku naturalnym, zwykle w celach komercyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5025">
<skos:prefLabel>przystań</skos:prefLabel>
<skos:definition>mały port, użytkowany głównie w celach rekreacyjnych, często z hotelami, restauracjami i barami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5027">
<skos:prefLabel>biologia morza</skos:prefLabel>
<skos:definition>gałąź biologii zajmująca się organizmami zamieszkującymi morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5029">
<skos:prefLabel>morski obszar chroniony</skos:prefLabel>
<skos:definition>fragment obszaru morza lub oceanu objęty specjalną ochroną, głównie w celu zmniejszenia ryzyka  zniszczenia żyjących tam zwierząt i ekosystemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5030">
<skos:prefLabel>ekologia morska</skos:prefLabel>
<skos:definition>kompleksowa dziedzina wiedzy zajmująca się badaniami podstawowych zależności strukturalnych i funkcjonalnych w obrębie populacji organizmów oraz między tymi populacjami a ich fizyczno-chemicznym środowiskiem w ekosystemach morskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5031">
<skos:prefLabel>ekosystem morski</skos:prefLabel>
<skos:definition>dowolne środowisko morskie, od stawu do oceanu, w którym na rośliny i zwierzęta oddziaływa zespół właściwości chemicznych i fizycznych środowiska morskiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5032">
<skos:prefLabel>technika okrętowa</skos:prefLabel>
<skos:definition>projektowanie, budowa, wyposażenie, działanie i konserwacja urządzeń energetycznych oraz towarzyszącego im zaplecza technicznego niezbędnego do napędu statków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5033">
<skos:prefLabel>środowisko morskie</skos:prefLabel>
<skos:definition>środowisko morskie obejmujące estuaria, przybrzeżne i okołobrzegowe strefy morza, a także regiony otwartego oceanu oraz głębi morskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5034">
<skos:prefLabel>fauna morska</skos:prefLabel>
<skos:definition>zwierzęta żyjące w morzach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5035">
<skos:prefLabel>rybołówstwo morskie</skos:prefLabel>
<skos:definition>odławianie zwierząt i roślin z oceanu dla zaspokojenia potrzeb konsumpcyjnych i rekreacyjnych, pozyskania pokarmu dla zwierząt oraz różnorodnych materiałów organicznych dla przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5038">
<skos:prefLabel>geologia morska</skos:prefLabel>
<skos:definition>wycinek badań oceanu dotyczący problematyki dna oceanicznego i granicy ocean-kontynent, łącznie z badaniami podmorskiego reliefu, geochemii i petrografii osadów i skał na dnie oceanu oraz wpływu wód morskich i falowania na dno oceanu i jego budowę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5042">
<skos:prefLabel>monitoring środowiska morskiego</skos:prefLabel>
<skos:definition>ocena zanieczyszczeń morza na drodze zintegrowanych badań chemicznych, ekologicznych i toksykologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5043">
<skos:prefLabel>organizm morski</skos:prefLabel>
<skos:definition>organizm żyjący w wodzie morskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5044">
<skos:prefLabel>zanieczyszczenie mórz</skos:prefLabel>
<skos:definition>negatywne zmiany środowiska morskiego spowodowane przez umyślne lub nieumyślne uwolnienie substancji niebezpiecznych lub toksycznych, np. ścieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5046">
<skos:prefLabel>rezewat morski</skos:prefLabel>
<skos:definition>obszar morski, w obrębie którego całość przyrody podlega ochronie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5048">
<skos:prefLabel>ochrona zasobów morza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5049">
<skos:prefLabel>osady morskie</skos:prefLabel>
<skos:definition>rozdrobnione fragmenty ciał stałych pochodzące z wietrzenia skał osadzające się na dnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5051">
<skos:prefLabel>prawo morskie</skos:prefLabel>
<skos:definition>system prawny odnoszący się do handlu i żeglugi morskiej, interesów handlowych na morzu, statków, transportu morskiego, marynarzy itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5052">
<skos:prefLabel>żegluga morska</skos:prefLabel>
<skos:definition>podróże morskie łodziami, statkami itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5055">
<skos:prefLabel>transport morski</skos:prefLabel>
<skos:definition>przewóz towarów i ludzi statkami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5056">
<skos:prefLabel>marker</skos:prefLabel>
<skos:definition>1) niewielka ilość łatwo wykrywalnej substancji, którą stosuje się do śledzenia i oszacowania przepływu substancji lub przemieszczania się organizmów inaczej niedostrzegalnych; 2) izotop pierwiastka, który w minimalnej ilości dodaje się do badanego materiału w celu prześledzenia drogi tego pierwiastka w procesach chemicznych, biologicznych lub fizycznych, a przez to do zbadania drogi badanego większego obiektu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5057">
<skos:prefLabel>wskaźnik (izotopowy)</skos:prefLabel>
<skos:definition>minimalna ilość radioaktywnego izotopu stosowanego w medycynie i biologii w celu zbadania zmian chemicznych w żywych tkankach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5058">
<skos:prefLabel>rynek</skos:prefLabel>
<skos:definition>miejsce działalności handlowej, gdzie sprzedaje się i kupuje towary; także kupno i sprzedaż; w sensie zawężonym rynek to zakresy stawek i cen zgłaszanych przez pośredników handlowych i maklerów na rynku papierów wartościowych; także zapotrzebowanie na określony towar</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5061">
<skos:prefLabel>gospodarka rynkowa</skos:prefLabel>
<skos:definition>system gospodarczy , w którym alokacja zasobów (pracy, ziemi, kapitału) oraz podział wytworzonych produktów pomiędzy poszczególne jednostki dokonuje się za pośrednictwem rynku; podstawą gospodarki rynkowej jest prywatna własność czynników wytwórczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5062">
<skos:prefLabel>organizacja rynku</skos:prefLabel>
<skos:definition>całokształt stosunków handlowo-gospodarczych, obejmujący sprzedaż i kupno towarów, wymianę kapitałów, usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5064">
<skos:prefLabel>marketing</skos:prefLabel>
<skos:definition>działalność polegająca na kształtowaniu stosunków między producentem a odbiorcą towarów, dotycząca w szczególności ustalania wielkości produkcji, określania cen, metod dystrybucji, usług gwarancyjnych, reklamy i promocji sprzedaży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5068">
<skos:prefLabel>badania rynku</skos:prefLabel>
<skos:definition>systematyczne gromadzenie, przetwarzanie i analiza danych o problemach związanych ze sprzedażą i dystrybucją towarów i usług w określonych przedziałach czasowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5076">
<skos:prefLabel>workowate</skos:prefLabel>
<skos:definition>rodzaj australijskich ssaków z (podgromady) rzędu torbaczy; samice mają fałd skóry brzucha tworzący torbę, w której noszone są młode zanim staną się zdolne do samodzielnego życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5079">
<skos:prefLabel>środek masowego przekazu</skos:prefLabel>
<skos:definition>środki komunikacji docierające do szerokich kręgów społecznych, takie jak telewizja, gazety, radio, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5080">
<skos:prefLabel>masowy wypoczynek</skos:prefLabel>
<skos:definition>sposób spędzania czasu, rozrywki, sporty i inne rodzaje wypoczynku, w których uczestniczy wielka liczba osób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5081">
<skos:prefLabel>rozdział</skos:prefLabel>
<skos:definition>oddzielenie zmieszanych substancji od siebie, np.oddzielenie oleju od wody, lub gazu od oleju, itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5083">
<skos:prefLabel>przemieszczanie się materii</skos:prefLabel>
<skos:definition>przemieszczanie się materii w ośrodku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5086">
<skos:prefLabel>materiał</skos:prefLabel>
<skos:definition>substancja, z której jest wytworzony lub z której składa się określony wyrób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5088">
<skos:prefLabel>bilans materii</skos:prefLabel>
<skos:definition>rachunek mający na celu określenie ilości materii wchodzącej i wychodzącej z układu w trakcie procesu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5090">
<skos:prefLabel>cykl życia materiału</skos:prefLabel>
<skos:definition>wszystkie stadia życia materiału w czasie jego przetwarzania, rozprowadzania, sprzedaży, użytkowania i wtórnego użytkowania, recyklingu oraz zagospodarowania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5093">
<skos:prefLabel>materiałoznawstwo</skos:prefLabel>
<skos:definition>dziedzina wiedzy dotycząca charakteru, zachowania i użytkowania materiałów w zastosowaniu do nauki i techniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5094">
<skos:prefLabel>badanie materiału</skos:prefLabel>
<skos:definition>kompleksowe badania w celu ustalenia charakterystyki i zachowania się materiałów; testy dzielą się na fizyczne, chemiczne, mechaniczne i technologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5097">
<skos:prefLabel>analiza matematyczna</skos:prefLabel>
<skos:definition>gałąź matematyki związana z procesami granicznymi lub koncepcją konwergencji; obejmuje teorie różniczkowania, całkowania i miary, serie nieskończone i funkcje analityczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5099">
<skos:prefLabel>modelowanie</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5107">
<skos:prefLabel>maksymalne stężenie dopuszczalne</skos:prefLabel>
<skos:definition>największe możliwe narażenie na czynniki chemiczne lub fizyczne dopuszczalne w ciągu 8 godzinnego dnia pracy bez obawy wystąpienia choroby lub urazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5108">
<skos:prefLabel>maksymalne stężenie zanieczyszczeń powietrza</skos:prefLabel>
<skos:definition>najwyższe stężenie substancji zanieczyszczających powietrze w środowisku, którego wpływ w określonym przedziale czasu i przy określonej częstotliwości występowania nie powoduje negatywnych skutków u człowieka, zwierząt i roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5110">
<skos:prefLabel>łąka</skos:prefLabel>
<skos:definition>fragment gruntu trwale pokryty trawą użytkowany w celu produkcji siana, także obszary nadwodnych pastwisk nie nadające się pod uprawy rolne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5116">
<skos:prefLabel>środek transportu</skos:prefLabel>
<skos:definition>pojazdy używane do przewożenia ludzi i towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5119">
<skos:prefLabel>mierzenie</skos:prefLabel>
<skos:definition>potencjał analitycznej metody lub protokółu w zakresie możliwości oceny ilościowej i ustalenia obecności badanej substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5122">
<skos:prefLabel>metoda pomiaru</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5123">
<skos:prefLabel>program pomiaru</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5124">
<skos:prefLabel>mięso</skos:prefLabel>
<skos:definition>jadalne części (mięśnie) zwierzęce, szczególnie pochodzące od ssaków, w odróżnieniu np. od części jadalnych ryb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5128">
<skos:prefLabel>inżynieria mechaniczna</skos:prefLabel>
<skos:definition>gałąź inżynierii dotycząca projektowania, budowy i działania maszyn</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5135">
<skos:prefLabel>wiedza medyczna</skos:prefLabel>
<skos:definition>gałąź wiedzy dotycząca sztuki leczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5137">
<skos:prefLabel>przeterminowany lek</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5138">
<skos:prefLabel>roślina lecznicza</skos:prefLabel>
<skos:definition>roślina o właściwościach leczniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5144">
<skos:prefLabel>lecznictwo</skos:prefLabel>
<skos:definition>nauka i praktyka leczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5145">
<skos:prefLabel>obszar śródziemnomorski</skos:prefLabel>
<skos:definition>zbiór wysp i krajów morza śródziemnego leżącego pomiędzy Europą, Afryką i Azją, połączonego z Oceanem Atlantyckim przez Cieśninę Gibraltarską na zachodzie; obejmuje morza: Tyrreńskie, Adriatyckie, Egejskie i Jońskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5147">
<skos:prefLabel>śródziemnomorskie zarośla</skos:prefLabel>
<skos:definition>formacja roślinna na obszarze śródziemnomorskim; składają się na nią głównie niskie, sucholubne, wiecznie zielone drzewa i krzewy; powstała w wyniku zniszczenia pierwotnej roślinności przez jej wypalanie i wypas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5148">
<skos:prefLabel>topnienie</skos:prefLabel>
<skos:definition>zmiana stanu substancji z fazy stałej do ciekłej, zwana też fuzją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5150">
<skos:prefLabel>membrana</skos:prefLabel>
<skos:definition>cienka struktura błoniasta otaczająca lub wyścielająca komórki, organy lub inne struktury; składa się z podwójnej warstwy lipidowej z wewnętrzną wkładką zbudowaną z cząsteczek białkowych; membrany są przepuszczalne dla wody i substancji rozpuszczalnych w tłuszczach a nieprzepuszczalne dla cząstek spolaryzowanych takich jak cukry</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5152">
<skos:prefLabel>skutki umysłowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5154">
<skos:prefLabel>rtęć</skos:prefLabel>
<skos:definition>(Hg) pierwiastek chemiczny z grupy cynkowców, liczba atomowa 80, metal o barwie srebrzystej z silnym połyskiem metalicznym, ciekły w normalnej temperaturze, używany m.in. do produkcji termometrów, barometrów i lamp rtęciowych, stosowany w amalgamatach dentystycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5155">
<skos:prefLabel>skażenie rtęcią</skos:prefLabel>
<skos:definition>obecność i uwalnianie rtęci do powietrza, wody i gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5161">
<skos:prefLabel>metabolizm</skos:prefLabel>
<skos:definition>wszelkie reakcje chemiczne zachodzące w organizmie żywym; są to reakcje anaboliczne i kataboliczne; metabolizm podstawowy polega na wymianie energii zwierzęcia w stadium spoczynku; anabolizm to reakcje syntezy skomplikowanych cząsteczek z prostszych molekuł, katabolizm to reakcje rozkładu dużych cząstek na protsze z wydzieleniem energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5163">
<skos:prefLabel>metabolizm pestycydów</skos:prefLabel>
<skos:definition>suma reakcji chemicznych, zarówno syntezy jak i rozpadu, zachodzących w substancjach i ich mieszankach stosowanych w celu zniszczenia lub ograniczenia występowania szkodników, które bezpośrednio lub pośrednio zagrażają plonom i innym gospodarczym interesom człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5165">
<skos:prefLabel>metabolit</skos:prefLabel>
<skos:definition>produkt metabolizmu pośredniego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5167">
<skos:prefLabel>metadane</skos:prefLabel>
<skos:definition>dane zgromadzone w celu opisania lub zdefiniowania innego zestawu danych; dokument lub inny element składowy informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5169">
<skos:prefLabel>obróbka powierzchniowa metali</skos:prefLabel>
<skos:definition>proces, w którym substancję chemiczną lub inną stosuje się na powierzchni metali w celu jej oczyszczenia, ochrony, zmiany wyglądu lub właściwości fizycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5171">
<skos:prefLabel>minerał metaliczny</skos:prefLabel>
<skos:definition>minerał zawierający metale, np. boksyt, piryt i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5175">
<skos:prefLabel>przemysł metalurgiczny</skos:prefLabel>
<skos:definition>przemysł zajmujący się wydobywaniem, oczyszczaniem, wytwarzaniem stopów i produkcją metali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5178">
<skos:prefLabel>tlenek metalu</skos:prefLabel>
<skos:definition>związek dwuskładnikowy, w którym tlen występuje w połączeniu z jednym lub więcej atomami metalu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5179">
<skos:prefLabel>galwanotechnika</skos:prefLabel>
<skos:definition>powlekanie wyrobu cienką warstewką metaliczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5180">
<skos:prefLabel>produkt metalowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5181">
<skos:prefLabel>przemysł metalowy</skos:prefLabel>
<skos:definition>przemysł zajmujący się obróbką metali i produkcją wyrobów metalowych przetworzonych; produkty przemysłu metalowego znajdują główne zastosowania w produkcji samochodów, maszyn, urządzeń, sprzętu elektrycznego, budownictwie, meblarstwie i produkcji kontenerów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5184">
<skos:prefLabel>metal</skos:prefLabel>
<skos:definition>nieprzejrzysty materiał krystaliczny, zazwyczaj bardzo wytrzymały, wykazujący dobre przewodnictwo elektryczne i cieplne oraz dużą ciągliwość i współczynnik odbicia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5186">
<skos:prefLabel>wytapianie metali</skos:prefLabel>
<skos:definition>proces metalurgiczny polegający na podgrzewaniu mieszaniny rud do poziomu powyżej punktu topnienia w celu wydobycia lub wytopienia metalu surowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5187">
<skos:prefLabel>obróbka metali</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5193">
<skos:prefLabel>katastrofa meteorologiczna</skos:prefLabel>
<skos:definition>gwałtowna, nagła i niszcząca zmiana środowiska związana z procesami pogodotwórczymi, powstała w ich wyniku, lub mająca wpływ na stan atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5194">
<skos:prefLabel>prognoza meteorologiczna</skos:prefLabel>
<skos:definition>dziedzina wiedzy zajmująca się badaniem dynamiki atmosfery i bezpośrednich wpływów atmosferycznych na powierzchnię Ziemi, oceany i mieszkańców planety, skupiając się szczególnie na pogodzie i warunkach pogodowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5195">
<skos:prefLabel>parametr meteorologiczny</skos:prefLabel>
<skos:definition>zmienne, takie jak: ciśnienie, temperatura, siła wiatru, wilgotność itp., na podstawie których wnioskuje się o stanie pogody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5196">
<skos:prefLabel>zjawisko meteorologiczne</skos:prefLabel>
<skos:definition>zjawiska zachodzące w troposferze i stratosferze, takie jak opady, wiatry itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5197">
<skos:prefLabel>meteorologia</skos:prefLabel>
<skos:definition>nauka zajmująca się atmosferą i zachodzącymi w niej zjawiskami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5199">
<skos:prefLabel>metan</skos:prefLabel>
<skos:definition>gaz bezbarwny, bezwonny i nie mający smaku, lżejszy od powietrza i gwałtownie reagujący z chlorem i bromem pod wpływem światła słonecznego; główny składnik gazu naturalnego, stosowany jako źródło metanolu, acetylenu i tlenku węgla; znany też jako wodorek metylu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5203">
<skos:prefLabel>metodologia</skos:prefLabel>
<skos:definition>system metod i zasad stosowany w danej dyscyplinie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5210">
<skos:prefLabel>metropolia</skos:prefLabel>
<skos:definition>termin stosowany w odniesieniu do dużego miasta, szczególnie do takiego, w którym znajduje się siedziba rządu danego kraju, siedziba władz kościelnych lub ośrodek handlu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5215">
<skos:prefLabel>zasoby mikrobiologiczne</skos:prefLabel>
<skos:definition>materiały uzyskiwane z mikroorganizmów, użytkowane do takich celów jak produkcja żywności i leków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5216">
<skos:prefLabel>analiza mikrobiologiczna</skos:prefLabel>
<skos:definition>analiza wykonywana w celu określenia rodzaju wirusów, bakterii, grzybów i pasożytów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5218">
<skos:prefLabel>mikrobiologia</skos:prefLabel>
<skos:definition>nauka i badania dotyczące mikroorganizmów, takich jak: pierwotniaki, glony, grzyby, bakterie, wirusy i riketsje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5219">
<skos:prefLabel>mikroklimat</skos:prefLabel>
<skos:definition>lokalny, dość jednolity układ klimatyczny panujący na danym terenie, w odróżnieniu od klimatu większego obszaru, którego ten teren jest częścią</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5220">
<skos:prefLabel>efekt mikroklimatyczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5221">
<skos:prefLabel>mikroklimatologia</skos:prefLabel>
<skos:definition>badania mikroklimatu obejmujące badania profilów temperatury, wilgotności oraz wiatrów w najniższej warstwie powietrza, wpływu roślinności i pasów ochronnych oraz modyfikującego wpływu miast i zabudowań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5222">
<skos:prefLabel>mikrokomputer</skos:prefLabel>
<skos:definition>mikroprocesor połączony z urządzeniami współpracującymi (interface) na wejściu i wyjściu, wraz z  pamięcią zewnętrzną oraz innymi elementami potrzebnymi do stworzenia działającego systemu komputerowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5224">
<skos:prefLabel>mikroekosystem</skos:prefLabel>
<skos:definition>uproszczony ekosystem w małej skali, laboratoryjny lub w terenie; pochodzenia: a) naturalnego (np. w wypadku, kiedy próba wody ze stawu utrzymywana jest w warunkach sztucznego oświetlenia i wymiany gazowej; lub b) z rozwoju kultur aksenicznych (kultur jednego organizmu - wolnych od innych domieszkowych organizmów) do wytworzenia określonych warunków w obrębie układu zwanego też mikrokosmosem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5225">
<skos:prefLabel>mikroelektronika</skos:prefLabel>
<skos:definition>stosowanie technik budowania obwodów i urządzeń w skrajnie małych pakietach, oznacza także mikrominiaturyzację, elektronikę mikrosystemową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5226">
<skos:prefLabel>mikrofiltracja</skos:prefLabel>
<skos:definition>oddzielanie lub usuwanie z cieczy cząstek i mikroorganizmów o średnicy rzędu 0,1 do 0,2 mikrona</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5228">
<skos:prefLabel>drobnoustrój</skos:prefLabel>
<skos:definition>mikroskopijne organizmy, takie jak: bakterie, pierwotniaki, drożdże, wirusy i glony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5229">
<skos:prefLabel>śladowe zanieczyszczenie wody</skos:prefLabel>
<skos:definition>zanieczyszczenie wody, występujace w bardzo małych, śladowych ilościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5230">
<skos:prefLabel>badanie mikroskopowe</skos:prefLabel>
<skos:definition>interpretacyjne zastosowanie powiększenia mikroskopowego w badaniach materiałów niedokładnie rozpoznawalnych okiem nieuzbrojonym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5231">
<skos:prefLabel>mikrofale</skos:prefLabel>
<skos:definition>fale ektromagnetyczne o długościach między 0,3 a 30 centymetrów, co odpowiada częstotliwościom od 1 do 100 gigahertzów; brak jednak ostrych granic między mikrofalami a podczerwienią i falami radiowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5234">
<skos:prefLabel>robotnicy sezonowi</skos:prefLabel>
<skos:definition>czasowo zatrudnieni robotnicy, zmieniający miejsca pracy, np. zatrudnieni w rolnictwie w zależności od tempa prac rolnych i dojrzewania plonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5236">
<skos:prefLabel>migracje ludności</skos:prefLabel>
<skos:definition>ciągłe lub częste zmiany miejsca zamieszkania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5237">
<skos:prefLabel>wędrówka zwierząt</skos:prefLabel>
<skos:definition>regularne przemieszczanie się niektórych gatunków zwierząt od miejsc lęgowych do zimowisk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5238">
<skos:prefLabel>wędrowny gatunek ptaków</skos:prefLabel>
<skos:definition>ptaki wędrujące w grupach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5239">
<skos:prefLabel>gatunki wędrowne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5241">
<skos:prefLabel>aspekt wojskowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5243">
<skos:prefLabel>działania wojskowe</skos:prefLabel>
<skos:definition>działania prowadzone przez siły zbrojne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5245">
<skos:prefLabel>lotnictwo wojskowe</skos:prefLabel>
<skos:definition>użycie lotnictwa w siłach zbrojnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5253">
<skos:prefLabel>strefa wojskowa</skos:prefLabel>
<skos:definition>obszary użytkowane wyłącznie przez siły zbrojne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5254">
<skos:prefLabel>mleko</skos:prefLabel>
<skos:definition>białawy płyn wydzielany przez gruczoły mlekowe w celu odżywiania młodych, składający się z węglowodanów, białek, tłuszczów, soli mineralnych i przeciwciał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5258">
<skos:prefLabel>młyn</skos:prefLabel>
<skos:definition>pomieszczenie, w którym ziarno jest mielone na mąkę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5261">
<skos:prefLabel>kopalnia</skos:prefLabel>
<skos:definition>odkrywka lub wydrążenie w głębi ziemi w celu wydobycia minerałów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5262">
<skos:prefLabel>wypełnianie wyrobisk kopalnianych</skos:prefLabel>
<skos:definition>wypełnianie wyrobisk kopalnianych kruszywem lub materiałami odpadowymi w celu przywrócenia właściwości geologicznych, krajobrazowych i użytkowych np. rolniczych danego obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5264">
<skos:prefLabel>złoże surowców mineralnych</skos:prefLabel>
<skos:definition>nagromadzenie minerałów naturalnie występujące w przyrodzie np. rud metalicznych lub niemetalicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5265">
<skos:prefLabel>wydobycie minerałów</skos:prefLabel>
<skos:definition>wydobycie metalicznych lub niemetalicznych kopalin z głębi Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5267">
<skos:prefLabel>włókno mineralne</skos:prefLabel>
<skos:definition>włókno wyrabiane ze szkła, skał lub żużla stosowane do produkcji materiału izolującego ciepło</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5268">
<skos:prefLabel>przemysł mineralny</skos:prefLabel>
<skos:definition>przemysł związany z wydobywaniem kopalin ze złóż podziemnych w wyrobiskach pod ziemią lub odkrywkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5269">
<skos:prefLabel>mineralizacja</skos:prefLabel>
<skos:definition>proces powstawania skamienielin, w czasie którego materia nieorganiczna zastępuje organiczne składniki organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5270">
<skos:prefLabel>mineralogia</skos:prefLabel>
<skos:definition>nauka zajmująca się substancjami nieorganicznymi spotykanymi w przyrodzie czyli minerałami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5274">
<skos:prefLabel>surowiec mineralny</skos:prefLabel>
<skos:definition>złoża cennych minerałów wydobywanych obecnie i w przyszłości; pojęcie stosuje się do znanych złóż oraz potencjalnych surowców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5277">
<skos:prefLabel>minerał</skos:prefLabel>
<skos:definition>substancja występująca w przyrodzie, o charakterystycznym składzie chemicznym dającym się przedstawić w postaci wzoru chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5279">
<skos:prefLabel>woda mineralna</skos:prefLabel>
<skos:definition>woda zawierająca naturalne lub sztuczne domieszki mineralne lub gazowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5282">
<skos:prefLabel>planowanie kosztów minimalnych</skos:prefLabel>
<skos:definition>proces przygotowania warunków do takiej produkcji lub dostarczania usług, które wymagają jak najmniejszego nakładu kosztów, czasu lub pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5283">
<skos:prefLabel>minimalizacja szkód</skos:prefLabel>
<skos:definition>działania ukierunkowane na zmniejszenie szkód i strat w środowisku lub w ekosystemie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5286">
<skos:prefLabel>okręg górniczy</skos:prefLabel>
<skos:definition>obszary, na których wydobywa się kopaliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5287">
<skos:prefLabel>inżynieria górnicza</skos:prefLabel>
<skos:definition>inżynieria związana z odkrywaniem złóż, budową kopalń i eksploatacją węgla, rud i innych kopalin, a także z procesami oczyszczania, formowania i wzbogacania produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5288">
<skos:prefLabel>geologia górnicza</skos:prefLabel>
<skos:definition>badania geologicznych właściwości złóż kopalin, w szczególności w związku z problemami kopalnictwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5289">
<skos:prefLabel>przemysł górniczy</skos:prefLabel>
<skos:definition>dział gospodarki obejmujący przedsiębiorstwa przemysłowe zajmujące się wydobyciem kopalin ze złóż naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5290">
<skos:prefLabel>prawo górnicze</skos:prefLabel>
<skos:definition>akty prawne regulujące potencjalne szkodliwe skutki działalności przedsiębiorstw zajmujących się wydobyciem i przetwarzaniem rud metali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5293">
<skos:prefLabel>przepisy dotyczące górnictwa</skos:prefLabel>
<skos:definition>akty prawne regulujące zagadnienia bezpieczeństwa, praworządności i odpowiedzialności ekologicznej przemysłu wydobywczego i przetwórstwa rud metali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5295">
<skos:prefLabel>renaturyzacja terenów pokopalnianych</skos:prefLabel>
<skos:definition>programy działań mające na celu przywrócenie potencjału lub właściwości fizycznych, chemicznych i biologicznych określonego terenu zdegradowanego przez górnictwo do akceptowalnego stanu, jak najbardziej zbliżonego do stanu wyjściowego, sprzed okresu działań przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5297">
<skos:prefLabel>odpady z górnictwa</skos:prefLabel>
<skos:definition>odpady powstające podczas  wydobywania surowców z Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5298">
<skos:prefLabel>rozporządzenie ministra</skos:prefLabel>
<skos:definition>formalne orzeczenie lub upoważnienie dotyczące określonego zagadnienia wydane przez ministerstwo, zazwyczaj pod auspicjami ministra</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5299">
<skos:prefLabel>ministerstwo</skos:prefLabel>
<skos:definition>urząd państwowy, przy pomocy którego minister zarządza wyodrębnionym organizacyjnie działem administracji państwowej i sprawuje władzę nad określoną gałęzią gospodarki narodowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5303">
<skos:prefLabel>mniejszość narodowa</skos:prefLabel>
<skos:definition>odłam społeczeństwa, grupa wyróżniająca się rasą, preferencjami politycznymi itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5305">
<skos:prefLabel>różne produkty</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5307">
<skos:prefLabel>zdolność mieszania się</skos:prefLabel>
<skos:definition>tendencja lub zdolność dwu lub więcej cieczy do tworzenia jednolitej mieszaniny, tj. do wzajemnego rozpuszczania się w sobie; może być stopniowana, od całkowitej, poprzez częściową do braku zdolności do mieszania się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5310">
<skos:prefLabel>roztocze</skos:prefLabel>
<skos:definition>małe pajęczaki, rząd Arachnidae o ciałach zaokrąglonych; roztocza występują pospolicie w glebie, odżywiając się pokarmem roślinnym i zwierzętami bezkręgowymi; roztocza pasożytnicze (np. przędziorki) wyrządzają wiele szkód wśród roślin uprawnych, inne wywołują choroby zwierząt, ssące krew kleszcze przenoszą choroby na ludzi i bydło</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5311">
<skos:prefLabel>środek łagodzący (skutki zagrożeń)</skos:prefLabel>
<skos:definition>działania lub procedury podejmowane w celu zmniejszenia szkodliwych wpływów działalności człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5313">
<skos:prefLabel>gospodarka mieszana uprawowo-hodowlana</skos:prefLabel>
<skos:definition>gospodarka mieszana uprawowo-hodowlana, uprawa zbóż i hodowla bydła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5316">
<skos:prefLabel>las mieszany</skos:prefLabel>
<skos:definition>las z kilkugatunkowym drzewostanem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5317">
<skos:prefLabel>teren użytkowany wielostronnie</skos:prefLabel>
<skos:definition>obszar użytkowany wielokierunkowo, np. jako pastwiska, w celu ochrony zlewni, ochrony przyrody, rekreacji, produkcji drewna itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5318">
<skos:prefLabel>mieszanie</skos:prefLabel>
<skos:definition>proces mieszania różnych substancji w celu uzyskania jednolitej mieszaniny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5322">
<skos:prefLabel>dom ruchomy</skos:prefLabel>
<skos:definition>pomieszczenia mieszkalne na kołach, przewożone przy użyciu samochodów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5325">
<skos:prefLabel>model</skos:prefLabel>
<skos:definition>1) przedstawienie, zwykle w mniejszej skali, określonego urządzenia, budowli itp.; 2) ilościowe lub matematyczne przedstawienie lub symulacja komputerowa, która ma celu opis właściwości lub zależności pomiędzy zdarzeniami fizycznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5327">
<skos:prefLabel>modelowanie</skos:prefLabel>
<skos:definition>technika badawcza wykorzystująca matematyczne lub fizyczne przedstawienia układu lub teorii wyjaśniającej jego właściwości; modele stosowane są do testowania skutków zmian wprowadzanych w elementach układu w całym badanym układzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5332">
<skos:prefLabel>wilgoć</skos:prefLabel>
<skos:definition>1) zawartość pary wodnej w atmosferze lub całkowita zawartość wody w postaci gazowej, ciekłej lub stałej obecna w określonej objętości powietrza; 2) woda zdyspergowana w gazie w formie pary wodnej lub małych kropelek, zdyspergowana w ciałach stałych lub skroplona na ich powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5333">
<skos:prefLabel>biologia molekularna</skos:prefLabel>
<skos:definition>badania chemicznych struktur i procesów stanowiących podłoże zjawisk biologicznych na poziomie molekularnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5339">
<skos:prefLabel>mięczak</skos:prefLabel>
<skos:definition>bezkręgowiec o ciele niesegmentowanym, często chronionym przez muszlę utworzoną z wydzieliny fałdu skórnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5343">
<skos:prefLabel>wycena pieniężna</skos:prefLabel>
<skos:definition>oszacowanie finansowe w celu wyliczenia opodatkowania, opłaty lub wymierzenia kary</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5346">
<skos:prefLabel>monitoring</skos:prefLabel>
<skos:definition>systematyczna kontrola w celu oceny zmian ilościowych i jakościowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5347">
<skos:prefLabel>kryterium monitoringu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5348">
<skos:prefLabel>dane monitoringowe</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5349">
<skos:prefLabel>sprzęt używany w telededekcji</skos:prefLabel>
<skos:definition>specyficzny sprzęt stosowany w teledetekcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5350">
<skos:prefLabel>sieć monitoringu</skos:prefLabel>
<skos:definition>grupa stacji pomiaru zanieczyszczeń pozostających w ścisłej łączności ze sobą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5352">
<skos:prefLabel>stacja monitoringowa</skos:prefLabel>
<skos:definition>stacja, na której prowadzi się ilościowe i jakościowe pomiary obecności i wpływu substancji zanieczyszczających powietrze i wodę, hałasu i wybuchów, promieniowania, transportu rumoszu, osiadania gruntu oraz zmian charakteru roślinności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5353">
<skos:prefLabel>układ urządzeń (aparatury) monitoringowych</skos:prefLabel>
<skos:definition>skoordynowany zestaw urządzeń do wykrywania i przekazywania, którego zadaniem jest wykrywanie i zapis wyników działania układów naturalnych lub sztucznych, celem ustalenia faktów w przyszłości lub ich rekonstrukcji historycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5354">
<skos:prefLabel>metoda monitoringowa</skos:prefLabel>
<skos:definition>techniki używane w procesie sprawdzania, obserwacji i pomiarów zjawisk fizycznych, chemicznych, biologicznych i ekologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5357">
<skos:prefLabel>monopol</skos:prefLabel>
<skos:definition>warunki powstające na rynku wówczas, kiedy istnieje tylko jeden sprzedawca określonego wyrobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5360">
<skos:prefLabel>pomniki</skos:prefLabel>
<skos:definition>obiekt, najczęściej dużych rozmiarów, zbudowany z kamienia w celu upamiętnienia i uhonorowania jakiejś osoby lub grupy ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5362">
<skos:prefLabel>wrzosowisko</skos:prefLabel>
<skos:definition>teren pokryty roślinnością, w której dominuje wrzos, trawy, paprocie i mchy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5364">
<skos:prefLabel>moralna perswazja</skos:prefLabel>
<skos:definition>odwołanie się do zasad etycznych lub wierzeń partnera lub społeczeństwa po to, by wpłynąć na zmianę jego sposobu zachowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5369">
<skos:prefLabel>morfologia</skos:prefLabel>
<skos:definition>gałąź biologii zajmująca się formą i budową organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5371">
<skos:prefLabel>umieralność</skos:prefLabel>
<skos:definition>liczba zgonów w określonej populacji w określonym przedziale czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5377">
<skos:prefLabel>mech</skos:prefLabel>
<skos:definition>rośliny z klasy mszaków (Bryophyta) występujące prawie we wszystkich siedliskach wilgotnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5384">
<skos:prefLabel>motocykl</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5388">
<skos:prefLabel>paliwo silnikowe</skos:prefLabel>
<skos:definition>paliwa ciekłe lub gazowe spalane w silnikach spalinowych wewnętrznego spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5390">
<skos:prefLabel>pojazd samochodowy</skos:prefLabel>
<skos:definition>pojazd drogowy napędzany silnikiem, szczególnie silnikiem spalinowym wewnętrznego spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5391">
<skos:prefLabel>spaliny pojazdów silnikowych</skos:prefLabel>
<skos:definition>tworzenie się i uwalnianie do środowiska samochodowych zanieczyszczeń gazowych i cząsteczkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5392">
<skos:prefLabel>emisja spalin motoryzacyjnych</skos:prefLabel>
<skos:definition>gazy uwalniane do atmosfery przez pojazdy napędzane silnikiem spalinowym wewnętrznego spalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5393">
<skos:prefLabel>przemysł samochodowy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5395">
<skos:prefLabel>autostrada</skos:prefLabel>
<skos:definition>szeroka droga dla pojazdów poruszających się z dużą prędkością i na dalekie odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5401">
<skos:prefLabel>góra</skos:prefLabel>
<skos:definition>rys, twór powierzchni Ziemi, wyniesiony wysoko nad swą podstawę, mający strome zbocza i względnie mały obszar w partii szczytowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5403">
<skos:prefLabel>ekosystem górski</skos:prefLabel>
<skos:definition>ekosystemy spotykane w wysokich partiach gór, bardzo wrażliwe, podatne na erozję, osuwanie się gruntów i utratę różnorodności biologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5404">
<skos:prefLabel>turystyka górska</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5405">
<skos:prefLabel>obszar górski</skos:prefLabel>
<skos:definition>obszary wysokich szczytów, grzbietów i łańcuchów górskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5409">
<skos:prefLabel>masyw górski</skos:prefLabel>
<skos:definition>wielkie, oddzielne pasmo górskie składające się z szeregu gór lub grzbietów górskich położonych blisko siebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5412">
<skos:prefLabel>koszenie</skos:prefLabel>
<skos:definition>cięcie trawy i zbóż przy pomocy kosy lub kosiarki mechanicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5415">
<skos:prefLabel>równina błotna</skos:prefLabel>
<skos:definition>względnie płaska równina wzdłuż wybrzeża, pokryta drobnym iłem, zalewana i odsłaniana przez wody morskie w rytm przypływów i odpływów, lub pokryta płytkimi wodami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5417">
<skos:prefLabel>muł (osad)</skos:prefLabel>
<skos:definition>mieszanina pyłów i iłów z wodą tworząca plastyczną masę o średnicy ziaren zasadniczo poniżej 0,06 mm; osadza się w jeziorach, estuariach i na lagunach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5418">
<skos:prefLabel>ściółka</skos:prefLabel>
<skos:definition>warstwa materiału organicznego wyłożona na powierzchni gruntu w celu ochrony przed parowaniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5419">
<skos:prefLabel>porozumienie wielostronne</skos:prefLabel>
<skos:definition>wielostronne podejmowanie inicjatyw i porozumień stanowi rzadko osiągany ideał sposobu załatwiania problemów międzynarodowych, jest to rodzaj demokracji w międzynarodowym społeczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5421">
<skos:prefLabel>spółka wielonarodowa</skos:prefLabel>
<skos:definition>duża spółka prowadząca interesy w wielu krajach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5424">
<skos:prefLabel>wielofunkcyjne zagospodarowanie terenu</skos:prefLabel>
<skos:definition>1) wielokierunkowe, skoordynowane zagospodarowanie terenu uwzględniajace aspekty trwałego i zrównowazonego rozwoju; 2) wieloaspektowe użytkowanie terenu, np. w celach rolniczych i ochronnych np. ochrona zlewni i przyrody; w celach rekreacji i produkcji drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5428">
<skos:prefLabel>oczyszczanie komunalne</skos:prefLabel>
<skos:definition>usługi świadczone przez miasto tj. oczyszczanie ulic, wywóz śmieci, odkażanie wody, gleby i innych zasobów naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5429">
<skos:prefLabel>miejskie przedsiębiorstwo oczyszczania</skos:prefLabel>
<skos:definition>usuwanie w celu oczyszczenia lub składowania pozostałości odpadowych z miejsc użytkowanych publicznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5430">
<skos:prefLabel>polityka władz miejskich dotycząca środowiska</skos:prefLabel>
<skos:definition>procedury, zasady, kierunki działania w zakresie ochrony zasobów naturalnych w skali lokalnej miasta lub dzielnicy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5431">
<skos:prefLabel>zarząd miasta</skos:prefLabel>
<skos:definition>miasto, dzielnica i inne zarządy mające władzę samorządu lokalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5432">
<skos:prefLabel>prawo miejskie</skos:prefLabel>
<skos:definition>w szerokim ujęciu - prawa dotyczące wyłącznie obywateli i mieszkańców państwa; w wąskim ujęciu - prawa dotyczące samorządów lokalnych miast i wsi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5433">
<skos:prefLabel>szczebel komunalny</skos:prefLabel>
<skos:definition>jurysdykcja, pozycja  lub status miasta lub zarządu lokalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5437">
<skos:prefLabel>odpady komunalne</skos:prefLabel>
<skos:definition>całość komunalnych materiałów odpadowych, bytowych i handlowych powstających na określonym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5439">
<skos:prefLabel>komunalny system dystrybucji wody</skos:prefLabel>
<skos:definition>publicznie lub prywatnie zorganizowany system centralnego uzdatniania wody i dostarczania jej odbiorcom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5440">
<skos:prefLabel>komunalna gospodarka wodna</skos:prefLabel>
<skos:definition>komunalna gospodarka wodna obejmuje zagadnienia dostarczania wody i technologii wodnej w zakresie planowania, oczyszczania, budowania i produkcji; dotyczy też kanalizacji, składowania osadów ściekowych, oczyszczania wody na obszarach wiejskich, oszczędzania wody i gospodarowania zasobami wodnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5443">
<skos:prefLabel>układ mięśniowy</skos:prefLabel>
<skos:definition>komórki i tkanki mięśniowe i organy odpowiedzialne za ruch u kręgowców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5444">
<skos:prefLabel>muzeum</skos:prefLabel>
<skos:definition>pomieszczenie, w którym wystawia się, przechowuje i bada obiekty o znaczeniu historycznym, artystycznym lub naukowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5448">
<skos:prefLabel>grzyb kapeluszowy</skos:prefLabel>
<skos:definition>przedstawiciel podklasy podstawczaków Basidiomycetes, którego owocniki wytwarzają zarodniki na blaszkowatych podstawkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5449">
<skos:prefLabel>muzyka</skos:prefLabel>
<skos:definition>artystyczna kompozycja (układ) dźwięków lub tonów, wyrażająca myśli i odczucia poprzez składniki rytmu, melodii, harmonii i zabarwienie tonalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5450">
<skos:prefLabel>hodowla małży</skos:prefLabel>
<skos:definition>hodowanie małży jako źródła pożywienia i w celach komercyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5452">
<skos:prefLabel>łasicowate</skos:prefLabel>
<skos:definition>duża i różnorodna grupa ssaków mięsożernych o długich tułowiach i krótkich kończynach, do której zalicza się norki, łasice i borsuki; wyróżnia je obecność tylko jednego trzonowca w każdej ze szczęk górnych i najwyżej dwóch trzonowców w żuchwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5456">
<skos:prefLabel>mutageniczność</skos:prefLabel>
<skos:definition>właściwość czynników chemicznych i fizycznych związana z wywoływaniem takich zmian w materiale genetycznym, które mogą być przekazywane podczas podziału komórki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5457">
<skos:prefLabel>badanie mutageniczności</skos:prefLabel>
<skos:definition>badania właściwości substancji po względem możliwości wywoływania przez nią mutacji genetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5459">
<skos:prefLabel>mutagen</skos:prefLabel>
<skos:definition>czynnik zwiększający częstotliwość mutacji powyżej poziomu charakterystycznego dla mutacji spontanicznych, takie właściwości mają niektóre substancje chemiczne i pewne formy promieniowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5460">
<skos:prefLabel>mutant</skos:prefLabel>
<skos:definition>osobnik mający zmutowany allel, co znalazło odzwierciedlenie w jego fenotypie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5461">
<skos:prefLabel>uwolnienie zmutowanego organizmu do środowiska</skos:prefLabel>
<skos:definition>uwolnienie do środowiska zmutowanego mikroorganizmu połączone jest z ryzykiem przejawiania przez ten organizm nieznanej uprzednio patogenetyczności wskutek przejęcia własności od naturalnie występujących bakterii lub przekazania tym bakteriom cech niepożądanych; istnieje obawa, że na drodze naturalnej mutacji taki zmutowany organizm może się przekształcić w formę niebezpieczną dla środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5462">
<skos:prefLabel>mutacja</skos:prefLabel>
<skos:definition>zmiana w chemicznym układzie DNA w chromosomach określonego organizmu; zmiany dotyczą zasadniczo poszczególnych genów, w niektórych wypadkach mogą to być zmiany całych chromosomów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5463">
<skos:prefLabel>mykologia</skos:prefLabel>
<skos:definition>gałąź botaniki zajmująca się badaniem grzybów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5465">
<skos:prefLabel>mikoryza</skos:prefLabel>
<skos:definition>symbiotyczny związek korzeni roślin wyższych z grzybem; w mikoryzie ektotroficznej  (np. wrzosu i sosny) mycelium grzybowe pokrywa korzenie z zewnątrz; w mikoryzie endotroficznej (np.u storczyków) grzyb wrasta między komórki kory korzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5474">
<skos:prefLabel>krajowy program ochrony</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5475">
<skos:prefLabel>gospodarka narodowa</skos:prefLabel>
<skos:definition>zasoby finansowe państwa i efektywne zarządzanie tymi zasobami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5476">
<skos:prefLabel>rachunek ekonomiczny uwzględniający koszty ekologiczne</skos:prefLabel>
<skos:definition>zbiór i przetwarzanie informacji finansowych dotyczących ekologicznych kosztów prowadzenia gospodarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5478">
<skos:prefLabel>dobra kultury</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5479">
<skos:prefLabel>nacjonalizacja</skos:prefLabel>
<skos:definition>przekazanie prawa własności prywatnej na rzecz administracji państwowej, przez wywłaszczenie lub przymusową sprzedaż po cenach wyznaczonych przez rząd</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5482">
<skos:prefLabel>ustawodawstwo krajowe</skos:prefLabel>
<skos:definition>ustawa lub zbiór praw ustalonych przez rząd suwerennego państwa w obrębie jego jurysdykcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5484">
<skos:prefLabel>park narodowy</skos:prefLabel>
<skos:definition>definicja EEA - obszar wyróżniający się pięknem przyrody, ustanowiony w celu ochrony flory, fauny i krajobrazu oraz na potrzeby rekreacji, o ile nie pozostają one w konflikcie z celami ochronnymi; działalność gospodarcza na tym terenie podlega ograniczeniom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5486">
<skos:prefLabel>planowanie na szczeblu krajowym</skos:prefLabel>
<skos:definition>stopniowy proces obejmujący definiowanie, wypracowanie i określanie kierunków działań w celu zaspokojenia potrzeb bieżących i przyszłych oraz osiągania celów w skali kraju i poszczególnych grup społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5489">
<skos:prefLabel>rezerwat przyrody o znaczeniu krajowym</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5494">
<skos:prefLabel>obszar przyrody niezakłóconej przez człowieka</skos:prefLabel>
<skos:definition>obszar, na którym dominują naturalne procesy przyrodnicze, a interwencja człowieka jest minimalna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5496">
<skos:prefLabel>klęska naturalna</skos:prefLabel>
<skos:definition>nagła, gwałtowna i niszcząca zmiana środowiska, nie spowodowana przez człowieka, wywołana przez takie zjawiska przyrodnicze jak powodzie, trzęsienia ziemi, pożary i huragany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5497">
<skos:prefLabel>system naturalnego odwodnienia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5498">
<skos:prefLabel>środowisko naturalne</skos:prefLabel>
<skos:definition>kompleksowy układ właściwości atmosferycznych, geologicznych i biologicznych spotykany w obrębie określonego obszaru z pominięciem obiektów sztucznych i wpływu rozwiniętej działalności technologicznej człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5500">
<skos:prefLabel>nawóz naturalny</skos:prefLabel>
<skos:definition>organiczny materiał stosowany jako dodatek do gleby w celu uzupełnienia składników chemicznych niezbędnych dla żywienia roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5501">
<skos:prefLabel>włókno naturalne</skos:prefLabel>
<skos:definition>włókno pochodzenia mineralnego, roślinnego lub zwierzęcego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5502">
<skos:prefLabel>las naturalny</skos:prefLabel>
<skos:definition>obszar lasu, który powstał bez udziału człowieka i nie podlega wyraźnym wpływom antropogenicznym; pojęcie &quot;las naturalny&quot; może obejmować, ale nie musi, pojęcie &quot;lasu pierwotnego&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5503">
<skos:prefLabel>gaz ziemny</skos:prefLabel>
<skos:definition>paliwo naturalne zawierające metan i węglowodory, które występuje w niektórych formacjach geologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5504">
<skos:prefLabel>poszukiwanie gazu ziemnego</skos:prefLabel>
<skos:definition>podziemne poszukiwania prowadzone wieloma metodami w celu odkrycia złóż naturalnego gazu ziemnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5505">
<skos:prefLabel>wydobycie gazu ziemnego</skos:prefLabel>
<skos:definition>wydobycie gazu ziemnego ze złóż podmorskich oraz, ogólnie, wydobycie z lokalizacji podziemnych, często występujących razem ze złożami ropy naftowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5506">
<skos:prefLabel>zagrożenia związane ze zjawiskami przyrody</skos:prefLabel>
<skos:definition>prawdopodobieństwo wystąpienia potencjalnie groźnego zjawiska przyrodniczego w określonym przedziale czasu i na określonym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5507">
<skos:prefLabel>dziedzictwo przyrodnicze</skos:prefLabel>
<skos:definition>światowe zasoby przyrodnicze, którymi gospodaruje pokolenie współcześnie żyjące na Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5508">
<skos:prefLabel>prawa naturalne</skos:prefLabel>
<skos:definition>ogół przepisów wywodzących się z funkcjonowania przyrody i uważanych za obowiązujące w społeczeństwie, w przeciwieństwie do przepisów ustalonych przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5510">
<skos:prefLabel>materiał pochodzenia naturalnego</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5511">
<skos:prefLabel>pomnik przyrody</skos:prefLabel>
<skos:definition>definicja EEA - cecha, twór przyrody lub kultury wyróżniający się unikatowymi wartościami z uwagi na rzadkość występowania, cechy estetyczne lub znaczenie kulturowe; klasyfikując obiekt jako pomnik przyrody należy uwzględniać m.in.: a) wielość walorów wyróżniających dany obszar, takich jak wodospady, jaskinie, kratery, skamienieliny, wydmy piszczyste wraz z ich unikatową florą i fauną, b) obszar powinien być wystarczająco duży by ochronić całość walorów i ich otoczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5513">
<skos:prefLabel>park przyrodniczy</skos:prefLabel>
<skos:definition>definicja EEA - obszar ochronny ustanowiony w celu ochrony zasobów przyrody ze względu na piękno krajobrazu przyrodniczego, wartości naukowe i kulturowe, a także edukacyjne; działalność gospodarcza na tym obszarze podlega ograniczeniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5518">
<skos:prefLabel>promieniowanie naturalne</skos:prefLabel>
<skos:definition>promieniowanie pochodzące głównie z uranu obecnego w małych ilościach w wielu rodzajach skał, w glebach, budynkach i materiałach, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5520">
<skos:prefLabel>ochrona zasobów naturalnych</skos:prefLabel>
<skos:definition>zagospodarowanie ożywionych i nieożywionych zasobów przyrody w taki sposób, by zachować maksymalne korzyści dla współczesnych i przyszłych pokoleń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5521">
<skos:prefLabel>zasoby naturalne</skos:prefLabel>
<skos:definition>utwory lub składniki środowiska przyrodniczego reprezentujące istotne wartości dla człowieka, np. gleba, woda, roślinność, zwierzęta itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5523">
<skos:prefLabel>krajobraz naturalny</skos:prefLabel>
<skos:definition>obszar, w obrębie którego wpływy działań ludzkich nie są zauważalne na poziomie krajobrazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5524">
<skos:prefLabel>nauki przyrodnicze</skos:prefLabel>
<skos:definition>dziedziny wiedzy zajmujące się obiektywnie mierzalnymi zjawiskami w zakresie przekształcania i zależności energii i materii, obejmują: biologię, fizykę i matematykę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5529">
<skos:prefLabel>kamień naturalny</skos:prefLabel>
<skos:definition>naturalny kamień szlachetny w odróżnieniu od sztucznych substytutów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5530">
<skos:prefLabel>wartość przyrodnicza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5536">
<skos:prefLabel>instytucja zajmująca się ochroną przyrody</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5537">
<skos:prefLabel>ochrona przyrody</skos:prefLabel>
<skos:definition>definicja EEA - gospodarowanie zasobami przyrodniczymi Ziemi oraz  środowiskiem w celu utrzymania ich walorów jakościowych i zagwarantowania, że będą one prawidłowo użytkowane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5538">
<skos:prefLabel>prawo ochrony przyrody</skos:prefLabel>
<skos:definition>zbiór przepisów ustalonych przez rząd w celu ochrony, zachowania i odnowy naturalnych siedlisk lub ekosystemów na danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5539">
<skos:prefLabel>program ochrony przyrody</skos:prefLabel>
<skos:definition>skoordynowany zespół działań i procedur, zwykle prowadzonych przez organizację rządową lub organizację &quot;non-profit&quot; w celu ochrony i zachowania składników świata przyrody, takich jak góry, drzewa, zwierzęta lub rzeki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5544">
<skos:prefLabel>ochrona przyrody</skos:prefLabel>
<skos:definition>działania zapobiegawcze, procedury lub urządzenia służące zapobieganiu lub zmniejszeniu ryzyka szkód w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5545">
<skos:prefLabel>rezerwat przyrody</skos:prefLabel>
<skos:definition>defnicja EEA - obszary przeznaczone do zachowania i ochrony określonych gatunków zwierząt i roślin; różnią się one od parków narodowych, które są przede wszystkim terenami publicznej rekreacji tym, że obejmuje się je ochroną ze względu wyłącznie na przyrodę oraz gatunki, które mogą być zagrożone</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5547">
<skos:prefLabel>mdłości</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5550">
<skos:prefLabel>zagrożenie nawigacyjne</skos:prefLabel>
<skos:definition>przeszkody spotykane na szlaku nawigacyjnym, stwarzające zagrożenie dla statku, jego ładunku lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5554">
<skos:prefLabel>obumieranie tkanek</skos:prefLabel>
<skos:definition>zamieranie komórek lub tkanek wskutek choroby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5555">
<skos:prefLabel>potrzeba</skos:prefLabel>
<skos:definition>odczuwanie braku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5559">
<skos:prefLabel>program poprawy stosunków w sąsiedztwie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5565">
<skos:prefLabel>nicienie</skos:prefLabel>
<skos:definition>grupa niesegmentowanych robaków (Nematoda) ujmowana w klasyfikacji różnie, jako rząd, klasa lub typ</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5569">
<skos:prefLabel>system nerwowy</skos:prefLabel>
<skos:definition>układ koordynujący i integrujący, od którego zależy przystosowanie się organizmu do środowiska życia; u kręgowców układ ten składa się z mózgu, pnia mózgowego, rdzenia kręgowego, nerwów czaszkowych i peryferyjnych oraz zwojów nerwowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5571">
<skos:prefLabel>wyczerpanie zasobów</skos:prefLabel>
<skos:definition>całkowite wyczerpanie zasobów przyrodniczych niezbędnych dla ludzi i innych istot żywych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5572">
<skos:prefLabel>tworzenie sieci emitorów</skos:prefLabel>
<skos:definition>wszystkie źródła emisji na określonym terenie stanowiące własność jednej firmy lub kontrolowane przez nią i traktowane jako jedno wielkie źródło emisji; pozwala to na elastyczne podejście do kontroli poszczególnych pojedynczych źródeł emisji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5575">
<skos:prefLabel>neurotoksyczność</skos:prefLabel>
<skos:definition>występowanie szkodliwych skutków w układzie nerwowym po narażeniu na substancje chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5577">
<skos:prefLabel>zobojętnianie, neutralizacja</skos:prefLabel>
<skos:definition>dodawanie zasady do roztworu kwasu lub odwrotnie w celu zobojętnienia roztworu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5580">
<skos:prefLabel>nowa społeczność</skos:prefLabel>
<skos:definition>grupa ludzi stanowiących wspólnotę w oparciu o poglądy społeczno-polityczne, religijne i zawodowe  w odróżnieniu od dotychczasowych grup wspólnotowych funkcjonujących w oparciu o wspólne miejsce zamieszkania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5581">
<skos:prefLabel>nowe urządzenia</skos:prefLabel>
<skos:definition>urządzenie, układ lub fragment aparatury nowo zainstalowany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5583">
<skos:prefLabel>nowoczesne materiały</skos:prefLabel>
<skos:definition>nowoczesne wysokowydajne materiały uzyskiwane na drodze interdyscyplinarnych badań chemicznych, chemii stosowanej oraz inżynierii chemicznej i mechanicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5585">
<skos:prefLabel>nowa technologia</skos:prefLabel>
<skos:definition>narzędzia i urządzenia elektroniczne ostatniej generacji obecnie wprowadzane do przemysłu; nowe techniki często wprowadzano do tej pory bez uwzględniania ich wpływu na układy biologiczne i ekologiczne, które ulegały z tego powodu różnym zakłóceniom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5589">
<skos:prefLabel>nikiel</skos:prefLabel>
<skos:definition>kowalny, ciągliwy, srebrzysto-biały pierwiastek metaliczny, odporny na korozję; występuje głównie w pentlandycie i nikolicie, używany do produkcji stopów, szczególnie w celu wzmocnienia stali, w galwanotechnice oraz jako katalizator w syntezach organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5593">
<skos:prefLabel>azotan</skos:prefLabel>
<skos:definition>sól lub ester kwasu azotowego, np. azotan sodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5598">
<skos:prefLabel>nitryfikacja</skos:prefLabel>
<skos:definition>proces, w którym związki amonowe, w tym także nawozy sztuczne i humus z materii organicznej, są przekształcane w azotyny, a następnie w azotany; azotany jako składniki odżywcze pobierane są przez rośliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5601">
<skos:prefLabel>azotyn</skos:prefLabel>
<skos:definition>sól lub ester kwasu azotawego, np. azotyn potasu, azotyn sodu i azotyn butylu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5604">
<skos:prefLabel>nitrozwiązki</skos:prefLabel>
<skos:definition>związki organiczne zawierające jednowartościową grupę NO2 (rodnik lub grupę nitrową) połączoną z atomem węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5605">
<skos:prefLabel>azot</skos:prefLabel>
<skos:definition>podstawowy składnik odżywczy w żywieniu roślin oraz w diecie zwierząt; zwierzęta pobierają go w postaci związków zawierających azot, głównie  aminokwasów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5607">
<skos:prefLabel>krążenie azotu</skos:prefLabel>
<skos:definition>kompleksowy układ procesów w trakcie którego rośliny pobierają azot i przetwarzają go na białka, kwasy nukleinowe i inne składniki  biochemiczne niezbędne do budowy organizmów, a w dalszej części cyklu główną rolę odgrywają procesy powrotu azotu do atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5608">
<skos:prefLabel>dwutlenek azotu</skos:prefLabel>
<skos:definition>czerwonawo-brązowy gaz; występuje w różnych stężeniach w równowadze z innymi tlenkami azotu; używany do produkcji kwasu azotowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5611">
<skos:prefLabel>wiązanie azotu</skos:prefLabel>
<skos:definition>asymilacja azotu atmosferycznego przez różne mikroorganizmy glebowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5612">
<skos:prefLabel>podtlenek azotu</skos:prefLabel>
<skos:definition>bezbarwny gaz, rozpuszczalny w wodzie, etanolu i eterze; powstaje podczas różnych reakcji, np. podczas redukcji kwasu azotowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5613">
<skos:prefLabel>tlenki azotu</skos:prefLabel>
<skos:definition>tlenki, które tworzą się i uwalniają podczas spalania; powstają w wyniku utleniania azotu atmosferycznego w wysokich temperaturach; wprowadzane są do atmosfery ze spalin samochodowych oraz z obiektów spalających paliwa i odpady; tlenki azotu składają się z podtlenku azotu, tlenku azotu, dwutlenku azotu i pięciotlenku azotu oraz kwasu azotowego; tlenki azotu wchodzą w wiele reakcji w atmosferze tworząc fotochemiczny smog</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5618">
<skos:prefLabel>nitrozamina</skos:prefLabel>
<skos:definition>oleiste związki obojętne,często zabarwione na żółto, zawierające dwuwartościową grupę NNO</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5623">
<skos:prefLabel>NOEL</skos:prefLabel>
<skos:definition>akronim powstały z pierwszych liter No Observed Effects Level - poziom obecności (stężenie) substancji nie powodujący skutków, które mogłyby być zaobserwowane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5624">
<skos:prefLabel>hałas</skos:prefLabel>
<skos:definition>dźwięk niepożądany z uwagi albo na jego negatywny wpływ na ludzi lub na jego interferencję z odbieraniem innych dźwięków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5626">
<skos:prefLabel>ograniczenie hałasu</skos:prefLabel>
<skos:definition>zabiegi zmierzające do ograniczenia hałasu u źródła, do wprowadzania bardziej wyciszonych technik, odpowiedniego sprzętu lub ograniczenie możliwości rozchodzenia się dźwięku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5629">
<skos:prefLabel>analiza hałasu</skos:prefLabel>
<skos:definition>oznaczenie składowych częstotliwości odpowiedzialnych za badany rodzaj hałasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5630">
<skos:prefLabel>bariera antyhałasowa</skos:prefLabel>
<skos:definition>bariery ograniczające rozchodzenie się dźwięków, często sytuowane wzdłuż szlaków komunikacyjnych i w przemyśle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5631">
<skos:prefLabel>walka z hałasem</skos:prefLabel>
<skos:definition>proces ograniczania słyszalnych i uciążliwych dźwięków do akceptowalnego poziomu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5633">
<skos:prefLabel>zakłócenie spodowane przez hałas</skos:prefLabel>
<skos:definition>zaburzenia jakości życia spowodowane przez hałas - zakłócanie procesów komunikacji, procesów myślowych, snu itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5635">
<skos:prefLabel>emisja hałasu</skos:prefLabel>
<skos:definition>emisja hałasu do środowiska ze źródeł takich jak np. transport, przemysł, codzienna działalność bytowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5637">
<skos:prefLabel>opłaty za zanieczyszczenie środowiska hałasem</skos:prefLabel>
<skos:definition>kwota opłat karnych nałożona przez władze administracyjne na jednostki emitujące uciążliwy i niepożądany hałas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5638">
<skos:prefLabel>technologia bezhałasowa</skos:prefLabel>
<skos:definition>stosowanie różnych środków technicznych w celu ograniczenia emisji hałasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5640">
<skos:prefLabel>imisja hałasu</skos:prefLabel>
<skos:definition>imisje hałasu do środowiska w postaci wibracji negatywnie oddziałujących na zdrowie ludzi, zwierząt, roślin lub innych obiektów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5643">
<skos:prefLabel>ustawodawstwo dotyczące hałasu</skos:prefLabel>
<skos:definition>prawodawstwo dotyczące zapobiegania i ograniczania emisji hałasu ze źródeł przemysłowych, usługowych i domowych, pojazdów i samolotów oraz sprzętu domowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5644">
<skos:prefLabel>natężenie hałasu</skos:prefLabel>
<skos:definition>fizyczna wielkość określająca poziom mierzonego hałasu, zwykle w decybelach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5646">
<skos:prefLabel>pomiar hałasu</skos:prefLabel>
<skos:definition>proces ilościowego oszacowania właściwości hałasu akustycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5648">
<skos:prefLabel>monitoring poziomu hałasu</skos:prefLabel>
<skos:definition>regularny układ pomiarowy do wykrywania oraz określania ilościowego i jakościowego poziomu i skutków hałasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5650">
<skos:prefLabel>hałas jako zanieczyszczenie środowiska</skos:prefLabel>
<skos:definition>zakłócenia hałasowe w środowisku, potencjalnie szkodliwe dla człowieka, zwierząt i roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5651">
<skos:prefLabel>zagrożenie hałasem</skos:prefLabel>
<skos:definition>zakłócenia hałasowe w środowisku, które można zmierzyć w określonych lokalizacjach, a następnie  uśredniać wyniki pomiarów w czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5653">
<skos:prefLabel>ochrona przed hałasem</skos:prefLabel>
<skos:definition>stosowanie środków w celu zapobiegania i ograniczania emisji hałasu ze źródeł przemysłowych, usługowych i domowych, pojazdów i samolotów oraz sprzętu domowego, takich jak bariery antyhałasowe, strefy buforowe, montaż wyciszaczy urządzeń itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5654">
<skos:prefLabel>redukcja hałasu</skos:prefLabel>
<skos:definition>zmniejszanie poziomu hałasu lub natężenia dźwięków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5658">
<skos:prefLabel>rodzaj hałasu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5662">
<skos:prefLabel>ludność koczownicza</skos:prefLabel>
<skos:definition>1) członek grupy ludzkiej lub plemienia przemieszczającego się z miejsca na miejsca w poszukiwaniu pastwisk i żywności; 2) koczownikami są Romowie, plemiona pustynne takie jak Beduini i wiele plemion żyjących na kontynentach amerykańskich, w Azji i Australii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5664">
<skos:prefLabel>nomenklatura</skos:prefLabel>
<skos:definition>system nazw lub terminów odnoszących się do określonej dziedziny wiedzy lub sztuki lub nazewnictwo obiektów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5666">
<skos:prefLabel>zanieczyszczenia nie podlegające rozkładowi biologicznemu</skos:prefLabel>
<skos:definition>związki organiczne, zwykle syntetyczne, które nie są rozkładane ani mineralizowane przez mikroorganizmy lub w innych procesach biologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5667">
<skos:prefLabel>obszar niezabudowany</skos:prefLabel>
<skos:definition>obszary charakteryzujące się mało intensywną zabudową o charakterze mieszkalnym, handlowym, przemysłowym itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5668">
<skos:prefLabel>odnawialne źródło energii</skos:prefLabel>
<skos:definition>energia odnawialna i bezpieczna ekologicznie taka jak energia pływów, wiatru itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5669">
<skos:prefLabel>przemysł metali nieżelaznych</skos:prefLabel>
<skos:definition>przemysł wytwarzający metale inne niż żelazo oraz oparte na żelazie stopy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5670">
<skos:prefLabel>metale nieżelazne</skos:prefLabel>
<skos:definition>wszystkie metale inne niż żelazo i jego stopy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5671">
<skos:prefLabel>organizacja pozarządowa</skos:prefLabel>
<skos:definition>prywatne, ochotnicze nie generujące zysku organizacje działające jako grupy nacisku; w świecie istnieje ponad 5000 tego rodzaju organizacji zajmujących się środowiskiem i rozwojem i miliony ich zwolenników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5674">
<skos:prefLabel>promieniowanie niejonizujące</skos:prefLabel>
<skos:definition>promieniowanie, które nie zmienia struktury atomów, ale może powodować szkodliwe skutki biologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5675">
<skos:prefLabel>minerał niemetaliczny</skos:prefLabel>
<skos:definition>minerały zawierające niemetale, takie jak kwarc, granaty itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5676">
<skos:prefLabel>niemetal</skos:prefLabel>
<skos:definition>pierwiastki niemetaliczne takie jak arsen lub krzem, mające niektóre własności metali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5677">
<skos:prefLabel>czyste źródło energii</skos:prefLabel>
<skos:definition>energia, która jest bezpieczna z ekologicznego punktu widzenia i odnawialna, najczęściej jest to energia hydroelektryczna obecnie zabezpieczająca około 6,6% światowego zapotrzebowania na energię; inne tego rodzaju źródła to energia słoneczna, pływów, fal i wiatru; większość tego rodzaju inwestycji wymaga dużych nakładów kapitałowych lecz charakteryzuje się niskimi kosztami eksploatacyjnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5679">
<skos:prefLabel>czyste paliwo</skos:prefLabel>
<skos:definition>czyste paliwo, które nie powoduje emisji zanieczyszczeń do środowiska, takie jak metan</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5684">
<skos:prefLabel>nieodnawialne źródło energii</skos:prefLabel>
<skos:definition>nieodnawialne źródła energii powstawały lub ewoluowały na przestrzeni okresów geologicznych, stąd ich użytkowanie wiąże się z problemem krańcowego wyczerpywania z uwagi na bardzo wolne tempo odnowy tych zasobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5685">
<skos:prefLabel>surowiec nieodnawialny</skos:prefLabel>
<skos:definition>naturalne zasoby, które w skali istnienia ludzkości znajdują się na Ziemi w określonej ilości i dlatego mogą być wykorzystane tylko raz w dającej się przewidzieć przyszłości (chociaż mogą również zostać poddane recyklingowi); obejmuje to paliwa kopalne oraz także zasoby mineralne i czasami wody podziemne, chociaż woda oraz wiele minerałów mogą mieć charakter odnawialny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5686">
<skos:prefLabel>zabudowa nie-mieszkaniowa</skos:prefLabel>
<skos:definition>obszar zawierający obiekty o charakterze handlowym, przemysłowym i publicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5687">
<skos:prefLabel>pojemnik nienadający się do ponownego wykorzystania</skos:prefLabel>
<skos:definition>jakikolwiek pojemnik, dla którego nie ma obowiązku powtórnego wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5689">
<skos:prefLabel>organizm nie będący obiektem zwalczania</skos:prefLabel>
<skos:definition>roślina lub zwierzę inne niż to, do którego zastosowano pestycyd</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5690">
<skos:prefLabel>norma</skos:prefLabel>
<skos:definition>ustalony wzorzec, wytyczna lub przepis; zasada lub przepis ustanowiony przez władze, nakazujący lub polecający określone działanie lub zakazujący go</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5691">
<skos:prefLabel>normalizacja</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5692">
<skos:prefLabel>Afryka Północna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5693">
<skos:prefLabel>Ameryka Północna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5694">
<skos:prefLabel>Ocean Atlantycki Północny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5695">
<skos:prefLabel>Ocean Spokojny Północny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5701">
<skos:prefLabel>żywność genetycznie zmodyfikowana</skos:prefLabel>
<skos:definition>żywność wyprodukowana z zastosowaniem inżynierii genetycznej; wymagane jest precyzyjne oznakowanie jej składu na etykietach wskazujące na rodzaj zmian, jakie zostały wprowadzone</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5708">
<skos:prefLabel>awaria jądrowa</skos:prefLabel>
<skos:definition>wypadek w elektrowni jądrowej lub gdziekolwiek indziej, gdzie wykorzystuje się, składuje lub przewozi materiały radioaktywne, pociągający za sobą uwolnienie ich do środowiska w ilościach mogących spowodować zagrożenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5712">
<skos:prefLabel>energia jądrowa</skos:prefLabel>
<skos:definition>energia uwalniana w wyniku rozczepienia jądra atomowego lub syntezy jądrowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5713">
<skos:prefLabel>prawo atomowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5714">
<skos:prefLabel>użytkowanie energii jądrowej</skos:prefLabel>
<skos:definition>energia jądrowa wykorzystywana jest w sektorze przemysłowym do wytwarzania innych rodzajów energii, w medycynie i badaniach naukowych, w transporcie, do produkcji broni jądrowej itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5718">
<skos:prefLabel>eksplozja jądrowa (wypadek)</skos:prefLabel>
<skos:definition>niezamierzone uwolnienie się energii w wyniku raptownej reakcji jądrowej skutkujące powstaniem wysokich temperatur oraz radiacji szkodliwych dla zdrowia ludzi i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5721">
<skos:prefLabel>obiekt jądrowy</skos:prefLabel>
<skos:definition>miejsce, wraz z budynkami, gdzie prowadzi się działalność związaną z badaniami jądrowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5722">
<skos:prefLabel>rozszczepienie jądra</skos:prefLabel>
<skos:definition>podział jądra na części o porównywalnej masie; zwykle ograniczony do cięższych jąder takich jak izotopy uranu, plutonu i toru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5724">
<skos:prefLabel>paliwo jądrowe</skos:prefLabel>
<skos:definition>paliwo jądrowe uzyskuje się z minerałów nieorganicznych wydobywanych w kopalniach; chociaż częściowo są one zużywane podczas wykorzystywania w reaktorach wytwarzających energię, różnią się od paliw kopalnych sposobem wyzwalania energii; spalanie paliw kopalnych, takich jak węgiel, ropa naftowa czy gaz jest reakcją chemiczną; paliwa jądrowe, takie jak uran, zużywają się w procesie spontanicznego rozpadu, zwanego rozczepieniem; jeżeli proces zachodzi w sposób naturalny we wzbogaconych w uran skałach, szybkość zmian jest niedostrzegalna, w reaktorze jądrowym proces wyzwalania energii w wyniku  rozpadu, który w naturze zachodzi powoli, w ciągu tysięcy milionów lat, zredukowany jest do minut; uwolniona energia wytwarza parę, która napędza prądnice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5727">
<skos:prefLabel>jednostka paliwa jądrowego</skos:prefLabel>
<skos:definition>kawałek paliwa jądrowego uformowany i otulony osłoną umieszcza się w zespole paliwowym reaktora</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5728">
<skos:prefLabel>synteza termojądrowa</skos:prefLabel>
<skos:definition>łączenie dwóch lekkich jąder w celu utworzenia cięższego jądra, któremu towarzyszy uwolnienie energii wiązania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5733">
<skos:prefLabel>fizyka jądrowa</skos:prefLabel>
<skos:definition>badania właściwości, zachowania się i budowy jąder atomowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5734">
<skos:prefLabel>debata dotycząca energii jądrowej</skos:prefLabel>
<skos:definition>trwająca dyskusja publiczna i spory dotyczące wykorzystania energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5736">
<skos:prefLabel>elektrownia jądrowa</skos:prefLabel>
<skos:definition>elektrownia, w której energia jądrowa jest przekształcana w ciepło wykorzystywane do wytwarzania pary napędzającej turbiny, które napędzają generatory wytwarzające energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5737">
<skos:prefLabel>usuwanie odpadów radioaktywnych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5741">
<skos:prefLabel>reakcja jądrowa</skos:prefLabel>
<skos:definition>reakcja pociągająca za sobą zmianę w strukturze atomu, może być to rozczepienie, synteza jądrowa, wychwyt radiacyjny neutronów lub rozpad radioaktywny; różni się ona od reakcji chemicznej, która ogranicza się do zmian w strukturze elektronów otaczających jądro</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5742">
<skos:prefLabel>reaktor jądrowy</skos:prefLabel>
<skos:definition>urządzenie, które wytwarza ciepło i energię dzięki rozpoczęciu i dalszemu kontrolowaniu rozczepienia atomów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5745">
<skos:prefLabel>ośrodek badań jądrowych</skos:prefLabel>
<skos:definition>obiekt, w którym naukowcy badają zachowanie się i własności jąder atomowych poprzez badania testowe i inne formy eksperymentów, często odkrywając nowe technologie stosowane w innych badaniach do celów medycznych lub przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5746">
<skos:prefLabel>bezpieczeństwo jądrowe</skos:prefLabel>
<skos:definition>środki i techniki wdrażane w celu zredukowania możliwości wypadku pociągającego za sobą możliwe szkody spowodowane przez substancje radioaktywne wykorzystywane jako źródło energii, materiał badawczy lub broń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5747">
<skos:prefLabel>próba jądrowa</skos:prefLabel>
<skos:definition>test wykonywany w celu oceny broni jądrowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5750">
<skos:prefLabel>broń jądrowa</skos:prefLabel>
<skos:definition>każda bomba, głowica lub pocisk, w których wykorzystano materiał radioaktywny w celu wywołania reakcji łańcuchowej powodującej detonację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5751">
<skos:prefLabel>kwas nukleinowy</skos:prefLabel>
<skos:definition>kwasy organiczne w połączeniu z białkami (DNA lub RNA) występujące w jądrze i protoplazmie komórek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5752">
<skos:prefLabel>uciążliwość</skos:prefLabel>
<skos:definition>wszystko co wywiera wpływ na zdrowie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5755">
<skos:prefLabel>szkółka</skos:prefLabel>
<skos:definition>teren, na którym hoduje się rośliny przed wysadzeniem ich na miejsca docelowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5756">
<skos:prefLabel>środowisko odżywcze</skos:prefLabel>
<skos:definition>środowisko dostarczające składników odżywczych lub sprzyjające odżywianiu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5757">
<skos:prefLabel>bilans składników odżywczych</skos:prefLabel>
<skos:definition>warunki, w których występuje równowaga pomiędzy pobieraniem i wydalaniem składników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5758">
<skos:prefLabel>zawartość składników odżywczych</skos:prefLabel>
<skos:definition>zawartość białek, weglowodanów, tłuszczów, soli nieorganicznych, minerałów i wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5759">
<skos:prefLabel>cykl odżywczy</skos:prefLabel>
<skos:definition>cykl biochemiczny, podczas którego składniki nieorganiczne przemieszczają się z gleby do żywych organizmów, powietrza i wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5761">
<skos:prefLabel>usuwanie składników odżywczych</skos:prefLabel>
<skos:definition>oczyszczanie ścieków ze składników takich jak fosfor, azot w celu zapobiegania eutrofizacji zbiorników wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5763">
<skos:prefLabel>składnik odżywczy</skos:prefLabel>
<skos:definition>składniki chemiczne niezbędne do budowy żywych tkanek; najważniejsze ilościowo to węgiel, wodór i tlen</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5765">
<skos:prefLabel>odżywianie się</skos:prefLabel>
<skos:definition>proces pobierania składników przez rośliny i zwierzęta i ich asymilacja w procesach budowy tkanek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5769">
<skos:prefLabel>wartość odżywcza żywności</skos:prefLabel>
<skos:definition>miara ilości lub dostępności składników spotykanych w pożywieniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5774">
<skos:prefLabel>dąb</skos:prefLabel>
<skos:definition>drzewa z rodzaju Quercus w rzędzie Fagales, o liściach prostych, zwykle wrębnych, łuskowatych pąkach zimujących, promienistym rysunku tkanek na przekroju poprzecznym, twardym drewnie; owocem jest żołędź (orzech)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5776">
<skos:prefLabel>sprzeciw</skos:prefLabel>
<skos:definition>żądanie ponownego rozpoznania sprawy przez stronę, w której sprawie zapadło orzeczenie podczas jej nieobecności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5777">
<skos:prefLabel>obowiązek informowania</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5778">
<skos:prefLabel>obowiązek etykietowania</skos:prefLabel>
<skos:definition>odpowiedzialność prawna lub obowiązek umieszczania etykiet informujących o produktach zgodnie ze stosownymi przepisami i normami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5781">
<skos:prefLabel>choroba zawodowa</skos:prefLabel>
<skos:definition>choroba pozostająca w związku z wykonywaną pracą, wywołana długotrwałym oddziaływaniem na organizm szkodliwych warunków pracy zawodowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5783">
<skos:prefLabel>grupa zawodowa</skos:prefLabel>
<skos:definition>zespół ludzi, który zarabia na życie wykonując podobną lub taką samą pracę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5784">
<skos:prefLabel>higiena pracy</skos:prefLabel>
<skos:definition>zespół warunków zapewniających wyeliminowanie lub maksymalne ograniczenie ujemnego wpływu środowiska pracy na organizm pracujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5785">
<skos:prefLabel>zakładowa higiena pracy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5786">
<skos:prefLabel>medycyna pracy</skos:prefLabel>
<skos:definition>dział medycyny zajmujący się badaniem wpływu wszelkich czynników związanych z pracą na zdrowie człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5787">
<skos:prefLabel>bezpieczeństwo pracy</skos:prefLabel>
<skos:definition>zespół warunków zapewniających wyeliminowanie lub maksymalne ograniczenie ujemnego wpływu środowiska pracy na organizm pracujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5788">
<skos:prefLabel>akty prawne dotyczące bezpieczeństwa pracy</skos:prefLabel>
<skos:definition>regulacje prawne dotyczące bezpieczeństwa pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5789">
<skos:prefLabel>ocean</skos:prefLabel>
<skos:definition>masy wód zajmujące całą powierzchnię Ziemi poza powierzchnią lądów, z wyłączeniem jezior i mórz śródlądowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5791">
<skos:prefLabel>krążenie wód w oceanach</skos:prefLabel>
<skos:definition>krążenie wód w oceanach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5792">
<skos:prefLabel>prąd oceaniczny</skos:prefLabel>
<skos:definition>ruchy poziome znacznych mas wodnych w oceanach i morzach spowodowane działaniem wiatrów, różnicą temperatur i gęstości wód, zmianami ciśnienia atmosferycznego nad wodami itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5793">
<skos:prefLabel>zrzut odpadów do oceanu</skos:prefLabel>
<skos:definition>zatapianie w śwatowym oceanie zanieczyszczeń będących ściekami, odpadami przemysłowymi, odpadami pokonsumpcyjnymi oraz spływem z terenów rolniczych i zurbanizowanych; zanieczyszczenia te pochodzą ze źródeł rozproszonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5794">
<skos:prefLabel>Oceania</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5797">
<skos:prefLabel>oceanografia</skos:prefLabel>
<skos:definition>badania i eksploracja oceanów i mórz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5799">
<skos:prefLabel>temperatura oceanów</skos:prefLabel>
<skos:definition>standaryzowana miara ciepłoty wód oceanicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5802">
<skos:prefLabel>ważka</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5804">
<skos:prefLabel>zapach</skos:prefLabel>
<skos:definition>właściwość danej substancji do drażnienia zmysłu węchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5806">
<skos:prefLabel>uciążliwość zapachowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5809">
<skos:prefLabel>przestępstwo ekologiczne</skos:prefLabel>
<skos:definition>niezgodne z prawem działanie na szkodę środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5810">
<skos:prefLabel>urząd</skos:prefLabel>
<skos:definition>pomieszczenie lub zespół budynków, w którym znajdują się instytucje, biura, kancelaria służące organom władzy publicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5811">
<skos:prefLabel>obowiązek urzędowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5812">
<skos:prefLabel>publiczna rozprawa</skos:prefLabel>
<skos:definition>publiczne posiedzenie sądu lub innego organu orzekającego, wyznaczone dla rozpoznania określonej sprawy w celu jej rozstrzygnięcia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5814">
<skos:prefLabel>dojazdy do pracy poza szczytem</skos:prefLabel>
<skos:definition>regularne podróże tam i z powrotem na określonym dystansie poza godzinami szczytu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5816">
<skos:prefLabel>podróżowanie poza szczytem</skos:prefLabel>
<skos:definition>podróżowanie poza godzinami szczytu dla uniknięcia tłoku w publicznych środkach transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5818">
<skos:prefLabel>usuwanie nierównomierności w rokładzie godzin pracy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5820">
<skos:prefLabel>handel emisjami</skos:prefLabel>
<skos:definition>polityka polegająca na tym, że niska emisja z nowoczesnego urządzenia jest równoważona redukcją emisji z innych źródeł, stabilizując tym samym emisję całkowitą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5823">
<skos:prefLabel>wydobycie z dna morza</skos:prefLabel>
<skos:definition>wydobycie ropy naftowej z platform usytuowanych niedaleko wybrzeża</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5824">
<skos:prefLabel>wydobycie ropy naftowej z dna morza</skos:prefLabel>
<skos:definition>wydobywanie ropy naftowej z pokładów pod dnem oceanu lub innego dużego zbiornika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5825">
<skos:prefLabel>działania poza określonym terenem</skos:prefLabel>
<skos:definition>działania mające miejsce lub usytuowane poza określonym terenem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5827">
<skos:prefLabel>sorbent rozlewu ropy naftowej</skos:prefLabel>
<skos:definition>środki o właściwościach absorpcyjnych wykorzystywane w celu usunięcia na drodze fizycznej plam ropy naftowej powstałych w wyniku wycieków w zbiornikach wodnych, na terenach przemysłowych, drogach itp.; materiały nadające się do tego celu są różnrodne: od naturalnych,  takich jak słoma, trociny oraz torf do syntetycznych, takich jak pianka poliuretanowa lub pył polistyrenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5829">
<skos:prefLabel>energia uzyskiwana z ropy</skos:prefLabel>
<skos:definition>energia wytwarzana z ropy naftowej jako paliwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5830">
<skos:prefLabel>pływająca zapora do wychwytywania plam ropy</skos:prefLabel>
<skos:definition>pływające urządzenie wykorzystywane do łapania ropy naftowej w zbiornikach wodnych; gdy zostaje nadmuchane holuje się je w kierunku gładzi ropy z wiatrem i formuje, nadając mu  kształt litery U; pod naporem wiatru plama ropy zostaje złapana przez zaporę i następnie wypompowana</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5831">
<skos:prefLabel>katastrofalny rozlew ropy</skos:prefLabel>
<skos:definition>katastrofa spowodowana przez przypadkowe wycieki ropy naftowej ze statków lub urządzeń znajdujących się na nadbrzeżu; zanieczyszczenie akwenu olejem powoduje zniszczenie ekosystemu wodnego oraz dzikiej przyrody, np. ptaków; uniemożliwia zaopatrzenie w wodę oraz tworzy niebezpieczeństwo pożaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5833">
<skos:prefLabel>poszukiwanie ropy naftowej</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5834">
<skos:prefLabel>wydobycie ropy naftowej</skos:prefLabel>
<skos:definition>wydobywanie ropy naftowej powierzchniowo, np. z piasków smołowych lub łupków bitumicznych lub tunelowo, w płytkich zbiornikach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5835">
<skos:prefLabel>przemysł tłuszczowy</skos:prefLabel>
<skos:definition>przemysł produkujący i przetwarzający oleje i tłuszcze jadalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5836">
<skos:prefLabel>rurociąg naftowy</skos:prefLabel>
<skos:definition>rurociąg, system rur połączonych z zasuwami i innymi urządzeniami kontrolnymi, do przesyłania ropy naftowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5837">
<skos:prefLabel>zanieczyszczenia ropą naftową</skos:prefLabel>
<skos:definition>zanieczyszczenie ekosystemu, szczególnie ekosystemów wodnych olejami i produktami ropopochodnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5838">
<skos:prefLabel>usuwanie rozlewów ropy naftowej</skos:prefLabel>
<skos:definition>różne systemy usuwania plam ropy naftowej z powierzchni morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5839">
<skos:prefLabel>statek do usuwania rozlewów ropy naftowej</skos:prefLabel>
<skos:definition>statki służące do odzyskiwania ropy rozlanej na morzu przez tankowce; procedura polega na ograniczaniu rozlewu na powierzchni wody i fizycznym zebraniu olejów z zastosowaniem, lub bez, adsorbentów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5840">
<skos:prefLabel>zakład przetwórstwa ropy naftowej</skos:prefLabel>
<skos:definition>układ jednostek procesowych stosowany do przerobu ropy naftowej na paliwa, smary i inne produkty ropopochodne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5841">
<skos:prefLabel>przetwórstwo ropy naftowej</skos:prefLabel>
<skos:definition>podział mieszaniny ropy na części składowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5843">
<skos:prefLabel>odzyskiwanie ropy naftowej z pozostałości</skos:prefLabel>
<skos:definition>odzysk ropy pozostawionej po przeprowadzeniu awaryjnej operacji likwidacji wycieku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5847">
<skos:prefLabel>łupki bitumiczne</skos:prefLabel>
<skos:definition>skały osadowe lub metamorficzne o budowie cienkowarstwowej, z których metodą destylacji uzyskuje się węglowodory gazowe i ciekłe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5848">
<skos:prefLabel>plama ropy naftowej</skos:prefLabel>
<skos:definition>ropa naftowa, pływająca na powierzchni wody, zrzucana awaryjnie, w sposób naturalny lub planowo, porywana przez prądy, pływy i wiatr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5852">
<skos:prefLabel>wyciek ropy naftowej</skos:prefLabel>
<skos:definition>niekontrolowany duży wypływ ropy naftowej lub innych produktów petrochemicznych, zwykle do ekosystemów słodkowodnych lub morskich;wyciek może być kontrolowany przez dyspersję, spalanie, zbieranie mechaniczne lub absoprcję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5854">
<skos:prefLabel>tankowiec</skos:prefLabel>
<skos:definition>wielki statek wyposażony w zbiorniki, rurociągi oraz silne pompy, służący do przewozu ładunków płynnych (zwykle ropy naftowej, benzyny, oleju)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5856">
<skos:prefLabel>stare składowisko odpadów</skos:prefLabel>
<skos:definition>wypełnione odpadami składowisko, pokryte warstwą gleby i obsiane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5857">
<skos:prefLabel>obszar zanieczyszczony</skos:prefLabel>
<skos:definition>miejsce zanieczyszczone stanowiące zagrożenie z powodu zrzutów substancji niebezpiecznych lub szkodliwych do wody, gleby lub atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5858">
<skos:prefLabel>przestarzałe instalacje</skos:prefLabel>
<skos:definition>stare instalacje, które nie spełniają nowych przepisów dotyczących przeciwdziałania zanieczyszczaniu środowiska i których unowocześnienie wymaga nakładów inwestycyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5862">
<skos:prefLabel>zanieczyszczenie lotne</skos:prefLabel>
<skos:definition>zanieczyszczenie powodowane emisjami gazowymi do atmosfery, które emitowane nawet w niewielkich ilościach mogą być szkodliwe dla zdrowia lub powodowć choroby u ludzi mieszkających w sąsiedztwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5863">
<skos:prefLabel>olfaktometria</skos:prefLabel>
<skos:definition>badanie czułości narządu węchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5869">
<skos:prefLabel>onchocerciasis</skos:prefLabel>
<skos:definition>infekcja nicieniem Onchocerca volvulus; powoduje w rezultacie raka skóry, papular dermatitis i komplikacje oczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5870">
<skos:prefLabel>onkologia</skos:prefLabel>
<skos:definition>dział medycyny, nauka o nowotworach i metodach ich leczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5874">
<skos:prefLabel>górnictwo odkrywkowe</skos:prefLabel>
<skos:definition>eksploatacja kopalin, która odbywa się na otwartej przestrzeni, w warstwie powierzchniowej ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5875">
<skos:prefLabel>rybołówstwo dalekomorskie</skos:prefLabel>
<skos:definition>rybołówstwo prowadzone na obszarach najgłębszych mórz i oceanów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5877">
<skos:prefLabel>dane eksploatacyjne</skos:prefLabel>
<skos:definition>praktyczne dane odnoszące się do prowadzenia procesu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5880">
<skos:prefLabel>opinia</skos:prefLabel>
<skos:definition>mniemanie, sąd, przekonanie, pogląd; nie musi być poparta dowodami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5885">
<skos:prefLabel>optymalizacja urządzeń</skos:prefLabel>
<skos:definition>działania mające na celu uzyskanie optymalnych (najlepszych) wyników pracy urządzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5887">
<skos:prefLabel>ruda</skos:prefLabel>
<skos:definition>surowiec mineralny, z którego, w wyniku procesów hutniczych, uzyskuje się na skalę przemysłową metale; w praktyce także niektóre surowce będące źródłem niemetali (np.ruda siarki, azbestu)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5890">
<skos:prefLabel>organ</skos:prefLabel>
<skos:definition>w pełni wydzielona jednostka o charakterze strukturalnym lub funkcjonalnym, taka jak wątroba lub korzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5892">
<skos:prefLabel>węgiel organiczny</skos:prefLabel>
<skos:definition>węgiel pochodzenia zwierzęcego lub roślinnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5893">
<skos:prefLabel>chemia organiczna</skos:prefLabel>
<skos:definition>dział chemii zajmujący się budową, właściwościami, metodami otrzymywania i zastosowania związków organicznych, w których skład wchodzi przede wszystkim pierwiastek węgiel</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5897">
<skos:prefLabel>rolnictwo ekologiczne</skos:prefLabel>
<skos:definition>rolnictwo prowadzone bez stosowania nawozów wytwarzanych przemysłowo lub pestycydów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5902">
<skos:prefLabel>zanieczyszczenia organiczne</skos:prefLabel>
<skos:definition>zanieczyszczenia pochodzące z uprawy roślin i hodowli zwierząt zawierające głównie węgiel, wodór i tlen</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5910">
<skos:prefLabel>rozpuszczalnik organiczny</skos:prefLabel>
<skos:definition>substancje organiczne, zawierające rozcieńczalniki, występujące w formie ciekłej w warunkach normalnych, stosowane jako rozpuszczalniki, substancje zmniejszające lepkość lub środki czyszczące</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5912">
<skos:prefLabel>substancja organiczna</skos:prefLabel>
<skos:definition>związki chemiczne, o budowie opartej na łańcuchu lub pierścieniu węglowym i zawierające również wodór; mogą zawierać również tlen, azot lub inne pierwiastki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5913">
<skos:prefLabel>odpady organiczne</skos:prefLabel>
<skos:definition>odpady zawierające związki będące połączeniami z węglem; pochodzą z materiałów zwierzęcych i roślinnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5914">
<skos:prefLabel>organizm</skos:prefLabel>
<skos:definition>twór roślinny lub zwierzęcy, którego poszczególne części i struktury tworzą zharmonizowaną pod względem funkcjonalnym całość, wykazującą wszelkie cechy życia (przemianę materii, zdolność wzrostu, rozwoju i rozmnażania się)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5921">
<skos:prefLabel>związki chloroorganiczne</skos:prefLabel>
<skos:definition>związki organiczne zawierające wiązania C-Cl</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5924">
<skos:prefLabel>związki halogenoorganiczne</skos:prefLabel>
<skos:definition>związki organiczne zawierające wiązania C-fluorowiec</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5927">
<skos:prefLabel>związki metaloorganiczne</skos:prefLabel>
<skos:definition>cząsteczki zawierające połączenie węgiel-metal; związki zawierające rodniki alkilowe lub arylowe przyłączone do metalu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5928">
<skos:prefLabel>związki azotoorganiczne</skos:prefLabel>
<skos:definition>związki organiczne zawierające wiązanie C-N</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5929">
<skos:prefLabel>związki tlenoorganiczne</skos:prefLabel>
<skos:definition>związki alifatyczne i aromatyczne posiadające wiązanie C-O (alkohole, aldehydy itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5931">
<skos:prefLabel>związki fosforoorganiczne</skos:prefLabel>
<skos:definition>pestycydy zawierające fosfor; krótkożyciowe, niektóre mogą przy pierwszym zastosowaniu wykazywać właściwości toksyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5935">
<skos:prefLabel>związki krzemoorganiczne</skos:prefLabel>
<skos:definition>substancje pochodzenia naturalnego złożone z dwóch lub więcej nietypowych atomów połączonych razem wiązaniami chemicznymi i zawierających krzem, pierwiastki niemetaliczne; często występują w skałach i minerałach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5936">
<skos:prefLabel>związki siarkoorganiczne</skos:prefLabel>
<skos:definition>jedna z grup substancji zawierająca węgiel i siarkę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5937">
<skos:prefLabel>związki cynoorganiczne</skos:prefLabel>
<skos:definition>związki chemiczne wykorzystywane w farbach przeciwporostowych (okrętowych) w celu ochrony kadłubów łodzi i statków, boi i pali przed organizmami morskimi takimi jak wąsonogi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5942">
<skos:prefLabel>ornitologia</skos:prefLabel>
<skos:definition>dział zoologii, nauka o budowie, życiu i systematyce ptaków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5944">
<skos:prefLabel>orografia</skos:prefLabel>
<skos:definition>dział geomorfologii, nauka zajmująca się opisywaniem form powierzchni Ziemi na podstawie ich wyglądu zewnętrznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5945">
<skos:prefLabel>prostoskrzydłe</skos:prefLabel>
<skos:definition>niejednorodny rząd uogólnionych insektów ulegających częściowemu przeobrażeniu, posiadających żujące narządy gębowe i cztery skrzydła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5949">
<skos:prefLabel>osmoza</skos:prefLabel>
<skos:definition>samorzutne przenikanie rozpuszczalnika przez półprzepuszczalną błonę z roztworu o mniejszym stężeniu do roztworu o stężeniu większym; odgrywa dużą rolę w procesach biologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5953">
<skos:prefLabel>rekreacja w miejscu zamieszkania</skos:prefLabel>
<skos:definition>rozrywka, ćwiczenia i inne działania przynoszące radość i relaks prowadzone w centrach miast, małych miastach i dzielnicach miast</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5956">
<skos:prefLabel>nadkład</skos:prefLabel>
<skos:definition>skały płonne leżące nad złożem kopaliny użytecznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5957">
<skos:prefLabel>zagęszczenie</skos:prefLabel>
<skos:definition>nadmierna liczba mieszkańców przypadająca na określony teren</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5958">
<skos:prefLabel>nadmierne użytkowanie</skos:prefLabel>
<skos:definition>intensywne wykorzystywanie surowców bez rozważenia ekologicznego wpływu w skali długoterminowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5959">
<skos:prefLabel>przenawożenie</skos:prefLabel>
<skos:definition>stosowanie zbyt dużej masy nawozów na jednostkę powierzchni terenu; spływ powierzchniowy z terenów przenawożonych może powodować zanieczyszczenie wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5960">
<skos:prefLabel>odławianie nadmierne</skos:prefLabel>
<skos:definition>wybieranie z morza większej liczby osobników niż wynosi naturalny przyrost populacji; przełowienie ma wiele przyczyn, najbardziej bezlitosna to nowoczesna flota rybacka, która może łowić więcej ryb niż wynika to z możliwości ich naturalnego odtwarzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5962">
<skos:prefLabel>wypas nadmierny</skos:prefLabel>
<skos:definition>intensywny wypas zwierząt, np. krów, owiec lub kóz; nadmierny wypas staje się poważnym zagrożeniem dla ekosystemów trawiastych i prowadzi do degradacji gleb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5963">
<skos:prefLabel>napowietrzne linie energetyczne</skos:prefLabel>
<skos:definition>wiszące linie eneregetyczne, którymi prąd jest rozsyłany po kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5965">
<skos:prefLabel>przeludnienie</skos:prefLabel>
<skos:definition>gęstość populacji, która przekracza możliwości środowiskowe zapewnienia zdrowia indywidualnych osobników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5967">
<skos:prefLabel>miksja</skos:prefLabel>
<skos:definition>miesznie się warstw powierzchniowych wody jeziornej lub morskiej z wodami przydennymi, szczególnie wiosną i jesienią, spowdowane różnicami gęstości wody pod wpływem zmian temperatury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5968">
<skos:prefLabel>przezimowanie</skos:prefLabel>
<skos:definition>przetrwanie niekorzystnych warunków zimowych w określonym miejscu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5971">
<skos:prefLabel>własność</skos:prefLabel>
<skos:definition>prawo do rozporządzania rzeczą i korzystania z niej, wraz z prawem do zbycia jej komuś innemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5972">
<skos:prefLabel>utlenianie</skos:prefLabel>
<skos:definition>reakcja chemiczna zwiększająca zawartość tlenu w cząsteczce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5976">
<skos:prefLabel>utlenianie-redukcja</skos:prefLabel>
<skos:definition>utlenianie chemiczne, w wyniku którego wzrasta wartościowość dodatnia (utrata elektronów) i towarzyszy mu równoczesna redukcja (pobranie elektronów)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5978">
<skos:prefLabel>tlenek</skos:prefLabel>
<skos:definition>dwuskładnikowy związek chemiczny prawie wszystkich pierwiastków z tlenem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5980">
<skos:prefLabel>utleniacz</skos:prefLabel>
<skos:definition>substancja powodująca utlenianie drugiego ciała, jednocześnie sama ulegająca redukcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5982">
<skos:prefLabel>tlen</skos:prefLabel>
<skos:definition>pierwiastek chemiczny z grupy tlenowców, najbardziej rozpowszechniony w przyrodzie, liczba atomowa 8, gaz bezbarwny, bezwonny, występujący w stanie wolnym w powietrzu, niezbędny do oddychania dla większości organizmów żywych, bardzo aktywny chemicznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5983">
<skos:prefLabel>natlenianie</skos:prefLabel>
<skos:definition>nasycanie tlenem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5986">
<skos:prefLabel>zawartość tlenu</skos:prefLabel>
<skos:definition>masa tlenu zawarta w roztworze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5987">
<skos:prefLabel>niedobór tlenu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5991">
<skos:prefLabel>ozon</skos:prefLabel>
<skos:definition>alotropowa odmiana tlenu, gaz o charakterystycznym, orzeźwiającym zapachu i błękitnej barwie, powstający w powietrzu pod wpływem wyładowań elektrycznych lub działania promieni ultrafioletowych, używany głównie do odkażania wody i odświeżania powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5992">
<skos:prefLabel>potencjał zubażania warstwy ozonowej</skos:prefLabel>
<skos:definition>czynnik odzwierciedlający potencjalne możliwości związku do zubażania warstwy ozonowej w porównaniu z potencjałem freonu CFC-11</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5993">
<skos:prefLabel>warstwa ozonowa</skos:prefLabel>
<skos:definition>warstwa w górnej części atmosfery charakteryzująca się znacznym stężeniem ozonu; ozon w tej warstwie odgrywa ważną rolę w bilansie promieniowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5994">
<skos:prefLabel>zubożenie warstwy ozonowej</skos:prefLabel>
<skos:definition>delikatna osłona ozonowa niszczona jest przez substancje chemiczne uwalniane na ziemi; są to głównie chlorofluorowęglowodory wykorzystywane w chłodziarkach, aerozole oraz czynniki czyszczące</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5995">
<skos:prefLabel>ozonowanie</skos:prefLabel>
<skos:definition>proces oddziaływania oraz nasycania ozonem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/5999">
<skos:prefLabel>opakowanie</skos:prefLabel>
<skos:definition>wszystkie produkty wykonane z różnych materiałów i wykorzystywane do obudowy, ochrony, operowania, dostarczania i prezentacji towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6001">
<skos:prefLabel>odpady opakowaniowe</skos:prefLabel>
<skos:definition>odpady składające się z materiałów oraz jednostek służących jako opakowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6007">
<skos:prefLabel>farba</skos:prefLabel>
<skos:definition>mieszanina barwników i pigmentów ze spoiwami, tworząca barwną substancję używaną do malowania, barwienia lub ochrony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6009">
<skos:prefLabel>usługi malarskie</skos:prefLabel>
<skos:definition>usługi polegające na wykonywaniu malowania wnętrz i ścian zewnętrznych budynków i konstrukcji, z wykorzystaniem farb lub innych temu podobnych środków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6012">
<skos:prefLabel>sklep z farbami</skos:prefLabel>
<skos:definition>sklep, w którym sprzedaje się farby i inne pokrewne produkty i akcesoria</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6013">
<skos:prefLabel>paleontologia</skos:prefLabel>
<skos:definition>nauka zajmująca się badaniem kopalnych organizmów zwierzęcych i roślinnych zachowanych w warstwach skorupy ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6023">
<skos:prefLabel>papier</skos:prefLabel>
<skos:definition>materiał w postaci wstęgi lub arkuszy otrzymywany w wyniku procesu produkcyjnego z włókien, głównie roślinnych, z odpowiednimi dodatkami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6026">
<skos:prefLabel>przemysł papierniczy</skos:prefLabel>
<skos:definition>przemysł związany z produkcją papieru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6033">
<skos:prefLabel>parametr</skos:prefLabel>
<skos:definition>zmienna, którą (podkreślając odmienną jej rolę w porównaniu z innymi zmiennymi) przyjmuje się za stałą w danej sytuacji; może być inna w zmienionych warunkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6035">
<skos:prefLabel>pasożyt</skos:prefLabel>
<skos:definition>organizm zwierzęcy albo roślinny bytujący w ciągu całego życia lub w pewnych jego okresach na powierzchni albo wewnątrz ciała innego organizmu — żywiciela, czerpiący z niego pokarm i zakłócający jego procesy życiowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6038">
<skos:prefLabel>parazytologia</skos:prefLabel>
<skos:definition>nauka o organizmach prowadzących pasożytniczy tryb życia (głównie o pasożytach zwierząt i człowieka) oraz o pasożytnictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6046">
<skos:prefLabel>parking</skos:prefLabel>
<skos:definition>miejsce wyznaczone do czasowych postojów pojazdów samochodowych na placu lub ulicy albo na wydzielonych placach położonych na pasie przydrożnym przy większych arteriach komunikacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6047">
<skos:prefLabel>miejsce parkingowe</skos:prefLabel>
<skos:definition>miejsce wyznaczone dla czasowych postojów pojazdów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6048">
<skos:prefLabel>parlament</skos:prefLabel>
<skos:definition>naczelny organ władzy państwowej, pochodzący z reguły z wyborów powszechnych, sprawujący funkcję ustawodawczą, uchwalający budżet państwa oraz kontrolujący organy wykonawcze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6051">
<skos:prefLabel>uczestnictwo</skos:prefLabel>
<skos:definition>branie udziału, uczestniczenie w życiu społecznym i publicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6052">
<skos:prefLabel>cząstka</skos:prefLabel>
<skos:definition>1) bardzo mała cząstka materii taka jak molekuła, atom, elektron; 2) niewielka częśc materii o średnicy od kilku angstremów do kilku milimetrów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6055">
<skos:prefLabel>separator cząstek</skos:prefLabel>
<skos:definition>urządzenie do segregacji cząstek stałych według średnicy; przesiewanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6059">
<skos:prefLabel>transport pasażerski</skos:prefLabel>
<skos:definition>transport pasażerski lądowy, wodny, morski, powietrzny za pomocą pojazdów samochodowych, autobusów, pociągów, statków, samolotów i innych środków transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6061">
<skos:prefLabel>pastwisko</skos:prefLabel>
<skos:definition>teren porośnięty zwartą okrywą roślinną, głównie trawą, na którym pasą się zwierzęta gospodarskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6062">
<skos:prefLabel>odpady maziste</skos:prefLabel>
<skos:definition>odpady pochodzące z różnych źródeł o konsystencji pasty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6063">
<skos:prefLabel>patogen</skos:prefLabel>
<skos:definition>czynnik chorobotwórczy (np. bakterie, wirusy, pasożyty zwierzęce, promieniowanie jonizujące) działający na ludzi, zwierzęta i rośliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6064">
<skos:prefLabel>organizm chorobotwórczy</skos:prefLabel>
<skos:definition>czynnik wywołujący chorobę lub potencjalnie zdolny do wywołania choroby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6065">
<skos:prefLabel>efekt patologiczny</skos:prefLabel>
<skos:definition>efekt odnoszący się do patologii, odbiegający od stanu normalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6066">
<skos:prefLabel>patologia</skos:prefLabel>
<skos:definition>nauka o chorobach (człowieka, zwierząt i roślin), część medycyny badająca przyczyny chorób, ich formy, objawy i skutki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6068">
<skos:prefLabel>model rozwoju urbanizacji</skos:prefLabel>
<skos:definition>zespół aktów, tendencji i charakterystyk obrazujących rozwój obszarów zurbanizowanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6081">
<skos:prefLabel>torf</skos:prefLabel>
<skos:definition>skała osadowa barwy brunatnej lub czarnej powstała z obumarłej roślinności bagiennej w wyniku zachodzących w niej biochemicznych procesów, przebiegających w warunkach nadmiernej wilgotności przy słabym dostępie powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6084">
<skos:prefLabel>wydobywanie torfu</skos:prefLabel>
<skos:definition>eksploatacja złóż torfu, głównie w celu zdobycia opału</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6092">
<skos:prefLabel>strefa dla pieszych</skos:prefLabel>
<skos:definition>ulica, miejsce spacerowe, zamknięte dla ruchu kołowego i przeznaczone wyłącznie dla pieszych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6094">
<skos:prefLabel>pedosfera</skos:prefLabel>
<skos:definition>zewnętrzna warstwa skorupy ziemskiej, w której zachodzą procesy glebowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6102">
<skos:prefLabel>kara za szkody wyrządzone środowisku</skos:prefLabel>
<skos:definition>konsekwencje finansowe, ponoszone na rzecz państwa, związane z wyrządzeniem szkód w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6103">
<skos:prefLabel>kara</skos:prefLabel>
<skos:definition>określona w normie prawnej konsekwencja naruszenia prawa, np. grzywna, kara pozbawienia wolności, odpowiedzialność cywilna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6106">
<skos:prefLabel>pięciochlorofenol</skos:prefLabel>
<skos:definition>jedna z toksycznych pochodnych fenolu;wykorzystywany jako fungicyd, herbicyd i melluskocyd</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6107">
<skos:prefLabel>w przeliczeniu na głowę</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6109">
<skos:prefLabel>perchloroetylen</skos:prefLabel>
<skos:definition>trwała, bezbarwna ciecz, niepalna i stabilna o niskiej toksyczności. Wykorzytywany jako środek do czyszczenia chemicznego oraz jako rozpuszczalnik</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6111">
<skos:prefLabel>woda przesiąkająca</skos:prefLabel>
<skos:definition>woda podpowierzchniowa przepływająca pod działaniem siły ciężkości przez skały lub glebę wzdłuż linii najmniejszego oporu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6115">
<skos:prefLabel>perifiton</skos:prefLabel>
<skos:definition>organizmy roślinne lub zwierzęce bytujące na powierzchni liści i łodyg roślin wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6118">
<skos:prefLabel>ekosystem wiecznej zmarzliny</skos:prefLabel>
<skos:definition>ekosystem związany z obszrami, gdzie występuje wieczna zmarzlina</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6120">
<skos:prefLabel>przepuszczalność</skos:prefLabel>
<skos:definition>zdolność membrany lub innego materiału do przepuszczania substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6123">
<skos:prefLabel>dopuszczalny poziom oddziaływania</skos:prefLabel>
<skos:definition>graniczne narażenie ustalone dla substancji niebezpiecznych lub czynników szkodliwych wdrożone przez OSHA jako standardy prawne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6124">
<skos:prefLabel>pozwolenie</skos:prefLabel>
<skos:definition>licencja na działalność w określonej dziedzinie, zwykle uznawana jako prawnie i proceduralnie prawidłowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6128">
<skos:prefLabel>azotan nadtlenku acetylu</skos:prefLabel>
<skos:definition>zanieczyszczenie powstające na skutek działania światła słonecznego na węglowodory i tlenki azotu w powietrzu;jeden ze składników smogu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6129">
<skos:prefLabel>trwałość</skos:prefLabel>
<skos:definition>zdolność substancji do bycia stabilną chemicznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6131">
<skos:prefLabel>trwałość pestycydów w środowisku</skos:prefLabel>
<skos:definition>zdolność substancji chemicznej do zachowania molekularnej niepodzielności i tym samym własności fizycznych, chemicznych i innych w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6133">
<skos:prefLabel>odpowiedzialność osobista</skos:prefLabel>
<skos:definition>obowiązek osobistego odpowiadania za swoje czyny i ponoszenia za nie konsekwencji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6138">
<skos:prefLabel>szkodnik</skos:prefLabel>
<skos:definition>organizm wyrządzający szkody gospodarce ludzkiej (np. gąsienice owadów niszczące lasy, zboża)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6139">
<skos:prefLabel>zwalczanie szkodników</skos:prefLabel>
<skos:definition>zmniejsznie liczby szkodników poprzez wyniszczanie lub ochronę przed ich atakami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6140">
<skos:prefLabel>środek przeciw szkodnikom</skos:prefLabel>
<skos:definition>ogólna nazwa dla substancji chemicznych stosowanych do niszczenia niechcianych roślin, szkodników pochodzenia zwierzęcego lub powodujących choroby grzybów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6143">
<skos:prefLabel>norma użycia pestycydów</skos:prefLabel>
<skos:definition>normy lub miary stosowane w przepisach prawnych odnoszące się do wytwarzania, rozpowszechniania lub wykorzystania substancji eliminujących insekty szkodliwe dla roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6144">
<skos:prefLabel>drogi krążenia pestycydów w środowisku</skos:prefLabel>
<skos:definition>szczególna lub znana droga lub kierunek, w którym przemieszczają się pestycydy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6146">
<skos:prefLabel>pozostałości pestycydów</skos:prefLabel>
<skos:definition>pestycydy pozostające w środowisku w stanie skuteczności przez długi okres czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6149">
<skos:prefLabel>masowy atak szkodników</skos:prefLabel>
<skos:definition>nagłe pojawienie się znacznej liczby szkodników; niebezbieczny wzrost ich populacji na pewnym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6152">
<skos:prefLabel>zwierzę domowe</skos:prefLabel>
<skos:definition>zwierzę trzymane w domu dla przyjemności i towarzystwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6154">
<skos:prefLabel>przemysł petrochemiczny</skos:prefLabel>
<skos:definition>przemysł obejmujący produkcję i przeróbkę surowców chemicznych otrzymywanych z ropy naftowej i gazu ziemnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6156">
<skos:prefLabel>benzyna</skos:prefLabel>
<skos:definition>mieszanina ciekłych węglowodorów, bezbarwna ciecz otrzymywana głównie przez destylację i krakowanie ropy naftowej albo syntetycznie, łatwopalna, używana jako paliwo silników spalinowych i jako rozpuszczalnik</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6161">
<skos:prefLabel>ropa naftowa</skos:prefLabel>
<skos:definition>ciekły surowiec kopalny barwy brunatnej, będący mieszaniną węglowodorów; przez destylację otrzymuje się z niego benzynę, wazelinę, parafinę, wosk, naftę itp.; w stanie naturalnym ma zastosowanie jako paliwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6162">
<skos:prefLabel>zużycie ropy naftowej</skos:prefLabel>
<skos:definition>ilość zużywanej ropy naftowej, przetwarzanej na produkty ropopochodne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6163">
<skos:prefLabel>inżynieria petrochemiczna</skos:prefLabel>
<skos:definition>część geologii, badająca powstawanie i nagromadzenie ropy i gazu oraz określająca techniczne i ekonomiczne warunki wydobycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6164">
<skos:prefLabel>przemysł naftowy</skos:prefLabel>
<skos:definition>przemysł obejmujący przetwórstwo nafty, produktów pochodnych oraz jej otrzymywanie i eksploatację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6169">
<skos:prefLabel>ochrona zasobów ropy naftowej</skos:prefLabel>
<skos:definition>kontrolowane użytkowanie, ochrona i rozwój eksploatowanych i potencjalnych zasobów ropy naftowej w celu zaspokojenia bieżącego i przyszłego zapotrzebowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6176">
<skos:prefLabel>rośliny kwiatowe</skos:prefLabel>
<skos:definition>rośliny kwiatowe, dzielą się na nagosienne i okrytonasienne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6177">
<skos:prefLabel>przemysł farmaceutyczny</skos:prefLabel>
<skos:definition>przemysł obejmujący produkcję leków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6179">
<skos:prefLabel>odpady farmaceutyczne</skos:prefLabel>
<skos:definition>odpady farmaceutyczne z aptek, szpitali, klinik i przemysłu farmaceutycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6180">
<skos:prefLabel>farmakokinetyka</skos:prefLabel>
<skos:definition>badanie szybkości absorpcji, dystrybucji, biotransformacji i wydalania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6181">
<skos:prefLabel>farmakologia</skos:prefLabel>
<skos:definition>nauka o substancjach, które mogą być stosowane jako leki ze względu na ich swoisty wpływ na organizm</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6183">
<skos:prefLabel>fenole</skos:prefLabel>
<skos:definition>pochodne benzenu, grupa związków aromatycznych zawierających jedną grupę wodorotlenową lub więcej tych grup; otrzymywane syntetycznie lub ze smoły węglowej, używane do wyrobu tworzyw sztucznych, barwników, leków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6184">
<skos:prefLabel>feromon</skos:prefLabel>
<skos:definition>rodzaj substancji wydzielanej przez zwierzęta, która wpływa na zmiane zachowania innych osobników tego samego gatunku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6187">
<skos:prefLabel>filozofia</skos:prefLabel>
<skos:definition>nauka zajmująca się ogólnymi rozważaniami na temat istoty i struktury bytu, ludzkiego poznania, zasad wartościowania, miejsca człowieka w świecie i jego poglądu na świat, obejmująca tradycyjnie historię filozofii, teorię poznania, ontologię, logikę, psychologię, etykę, estetykę i metodologię nauk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6189">
<skos:prefLabel>fosforan</skos:prefLabel>
<skos:definition>sól kwasu fosforowego, niezbędny składnik mineralnego pożywienia roślin i zwierząt, wprowadzany do gleby głównie w postaci nawozów sztucznych, zwłaszcza superfosfatu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6190">
<skos:prefLabel>usuwanie fosforanów</skos:prefLabel>
<skos:definition>zastępowanie fosforanów w detergentach przez substancje bezpieczniejsze dla środowiska (np. zeolity)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6192">
<skos:prefLabel>zamiennik fosforanów</skos:prefLabel>
<skos:definition>substancje, które są w stanie zastąpić fosforany w detergentach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6196">
<skos:prefLabel>fosfor</skos:prefLabel>
<skos:definition>pierwiastek chemiczny, liczba atomowa 15, niemetal z grupy azotowców, trujący, czynny chemicznie, występujący w przyrodzie wyłącznie w postaci związków, ważny składnik komórek zwierzęcych i roślinnych; ma szerokie zastosowanie w przemyśle i agrotechnice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6197">
<skos:prefLabel>czynnik fotochemiczny</skos:prefLabel>
<skos:definition>czynnik, który wyzwala reakcje fotochemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6198">
<skos:prefLabel>reakcja fotochemiczna</skos:prefLabel>
<skos:definition>wynik lub skutek reakcji chemicznej spowodowanej światłem lub promieniowaniem UV</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6199">
<skos:prefLabel>utleniacz fotochemiczny</skos:prefLabel>
<skos:definition>substancja chemiczna wchodząca do reakcji utleniania zachodzącej w obecności światła lub innej energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6200">
<skos:prefLabel>produkty fotochemiczne</skos:prefLabel>
<skos:definition>produkty degradacji, które są wytwarzane w wyniku działania światła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6201">
<skos:prefLabel>reakcja fotochemiczna</skos:prefLabel>
<skos:definition>chemiczna reakcja inicjowana przez światło o określonej długości fali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6202">
<skos:prefLabel>smog fotochemiczny</skos:prefLabel>
<skos:definition>połączenie mgły i substancji chemicznych emitowanych z samochodów i przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6204">
<skos:prefLabel>fotogrametria</skos:prefLabel>
<skos:definition>metoda wyznaczania kształtu i wielkości przedmiotu lub terenu na podstawie zdjęć fotograficznych wykonywanych z ziemi lub z samolotu; stosowana głównie w geodezji, poza tym w architekturze, górnictwie, archeologii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6205">
<skos:prefLabel>fotografia</skos:prefLabel>
<skos:definition>sposób wytwarzania trwałych obrazów przedmiotów, za pomocą klisz lub błon światłoczułych, na które działają promienie świetlne odbite od danych przedmiotów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6211">
<skos:prefLabel>fotosynteza</skos:prefLabel>
<skos:definition>proces przyswajania przez rośliny dwutlenku węgla przy zużytkowaniu energii światła słonecznego oraz udziale chlorofilu i niektórych barwników karotenoidowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6221">
<skos:prefLabel>warunki fizyczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6222">
<skos:prefLabel>geografia fizyczna</skos:prefLabel>
<skos:definition>część geografii, badająca własnośći i relacje zachodzące w środowisku przyrodniczym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6224">
<skos:prefLabel>oceanografia fizyczna</skos:prefLabel>
<skos:definition>część oceanografii, badająca fizyczne aspekty oceanów, a także ich relacje z atmosferą i dnem oceanicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6225">
<skos:prefLabel>planowanie przestrzenne</skos:prefLabel>
<skos:definition>planowanie rozmieszczenia na określonym terenie zakładów produkcyjnych, domów mieszkalnych, urządzeń i obiektów socjalnych i usługowych zgodnie z potrzebami społecznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6226">
<skos:prefLabel>zanieczyszczenia fizyczne</skos:prefLabel>
<skos:definition>zanieczyszczenia charakteryzujące się oddziaływaniem fizycznym na warunki środowiskowe (np. hałas, promieniowanie mikrofalowe, wibracja)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6227">
<skos:prefLabel>zanieczyszczenie fizyczne</skos:prefLabel>
<skos:definition>wprowadzenie lub obecność szkodliwych substancji do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6228">
<skos:prefLabel>proces fizyczny</skos:prefLabel>
<skos:definition>kolejno następujące po sobie zmiany (serie zmian), które wpływają na fizyczną formę materii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6229">
<skos:prefLabel>właściwość fizyczna</skos:prefLabel>
<skos:definition>to, co jest charakterystyczne z fizycznego punktu widzenia; może ulec zmianie bez zmian składu chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6230">
<skos:prefLabel>nauki fizyczne</skos:prefLabel>
<skos:definition>nauki badające nieożywioną materię, energię i właściwości fizyczne wszechświata, takie jak fizyka, chemia, astronomia i geologia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6233">
<skos:prefLabel>proces fizykochemiczny</skos:prefLabel>
<skos:definition>procesy powodujące zmiany we własnościach fizycznych i strukturze chemicznej substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6234">
<skos:prefLabel>oczyszczanie fizykochemiczne</skos:prefLabel>
<skos:definition>wykorzystywane do zatężania odpadowych roztworów solanki, usuwania stałych zanieczyszczeń pochodzenia organicznego lub amoniaku z wodnych roztworów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6235">
<skos:prefLabel>fizyka</skos:prefLabel>
<skos:definition>nauka przyrodnicza zajmująca się badaniem ogólnych właściwości materii i zjawisk w niej zachodzących oraz wykrywaniem ogólnych praw, którym te zjawiska podlegają</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6240">
<skos:prefLabel>fizjologia</skos:prefLabel>
<skos:definition>nauka o czynnościach organizmów żywych, ich narządów, tkanek, komórek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6243">
<skos:prefLabel>fitomasa</skos:prefLabel>
<skos:definition>biomasa roślin, szacowana ilościowo jako całkowita masa roślin (populacji) na danym obszarze, w określonym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6244">
<skos:prefLabel>fitopatologia</skos:prefLabel>
<skos:definition>nauka o chorobach roślin, objawach, przyczynach i warunkach ich powstawania oraz metodach i sposobach zapobiegania i zwalczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6247">
<skos:prefLabel>fitoplankton</skos:prefLabel>
<skos:definition>plankton złożony z organizmów roślinnych, przeważnie z glonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6248">
<skos:prefLabel>fitosocjologia</skos:prefLabel>
<skos:definition>nauka o zbiorowiskach roślinnych i prawidłowościach grupowania się roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6250">
<skos:prefLabel>toksyczność względem roślin</skos:prefLabel>
<skos:definition>toksyczne działanie substancji chemicznych na organizmy roślinne, wykorzystywane w rolnictwie do niszczenia chwastów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6252">
<skos:prefLabel>wytrawialnia</skos:prefLabel>
<skos:definition>zakład, w którym usuwa się kamień kotłowy z powierzchni żelaznych lub stalowych, zwykle przez zanurzenie w gorącej kąpieli kwasu chlorowodorowego lub siarkowego; odpadami są zużyty płyn wytrawiający, osady i woda spłukująca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6253">
<skos:prefLabel>obraz satelitarny</skos:prefLabel>
<skos:definition>obraz danych przedstawionych w dwuwymiarowej siatce pól (tzw. pixeli), pozyskiwanych przez sztuczne satelity</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6261">
<skos:prefLabel>pilotowanie</skos:prefLabel>
<skos:definition>kierowanie statkiem powietrznym; przeprowadzanie statków wodnych przez miejsca trudne do żeglugi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6262">
<skos:prefLabel>urządzenia pilotażowe</skos:prefLabel>
<skos:definition>mała wersja urządzenia przemysłowego stosowana w celu sprawdzenia i nabycia doświadczeń przed uruchomieniem instalacji w skali przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6263">
<skos:prefLabel>przedsięwzięcie pilotowe</skos:prefLabel>
<skos:definition>niewielki eksperyment lub zestaw obserwacji podjętych w celu określenia w jaki sposób i czy w ogóle należy podejmować realizację projektu w pełnym wymiarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6268">
<skos:prefLabel>płetwonogie</skos:prefLabel>
<skos:definition>Pinnipedia, rząd ssaków morskich, obejmujący około 30 gatunków dużych zwierząt o ciele wrzecionowatym i kończynach przekształconych w płetwy; należą do niego 3 rodziny: uchatki, morsy i foki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6270">
<skos:prefLabel>rurociąg</skos:prefLabel>
<skos:definition>przewód rurowy (wraz z armaturą) do przesyłania cieczy lub gazów na znaczne odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6274">
<skos:prefLabel>rura</skos:prefLabel>
<skos:definition>długi przewód o przekroju pierścieniowym, używany najczęściej do transportu cieczy i gazów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6276">
<skos:prefLabel>równina</skos:prefLabel>
<skos:definition>obszar o powierzchni płaskiej lub lekko falistej; płaska przestrzeń ziemi, rozległa płaszczyzna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6279">
<skos:prefLabel>plan</skos:prefLabel>
<skos:definition>program zadań i prac, które mają być wykonane w pewnym określonym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6280">
<skos:prefLabel>powierzchniowe źródło zanieczyszczeń</skos:prefLabel>
<skos:definition>zanieczyszczenia powstające w wyniku różnego rodzaju działalności nie wyodrębnionej jako źródła emisji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6282">
<skos:prefLabel>mikroorganizmy wodne</skos:prefLabel>
<skos:definition>niewielkie organizmy unoszące się w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6285">
<skos:prefLabel>urbanizacja planowa</skos:prefLabel>
<skos:definition>planowy rozwój obszarów metropolitalnych i związane z tym planowane zmiany w użytkowaniu ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6287">
<skos:prefLabel>planowanie</skos:prefLabel>
<skos:definition>określenie szczegółowego programu zadań i prac, które mają być wykonane w pewnym określonym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6290">
<skos:prefLabel>ustawodawstwo dotyczące planowania</skos:prefLabel>
<skos:definition>zespół aktów i praw ustanowionych przez rząd, regulujących zasady użytkowania ziemi, planowania przestrzennego, rozwoju i rozbudowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6291">
<skos:prefLabel>procedura planowania</skos:prefLabel>
<skos:definition>procedura podejmowana w celu opracowania i zaprojektowania działań na przyszłość</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6292">
<skos:prefLabel>zezwolenie planistyczne</skos:prefLabel>
<skos:definition>licencja lub podobny oficjalny dokument zatwierdzający szczególowe metody i procesy definiowania i rozwoju możliwych działan mających na celu zapewnienie osiągnięcia założonych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6293">
<skos:prefLabel>system planowania - projektowania - określania kosztów</skos:prefLabel>
<skos:definition>system osiągania długoterminowych celów, oparty na analizie i ocenie rozwiązań alternatywnych, zaprojektowany w celu rozwiązywania problemów poprzez znalezienie najbardziej efektywnych rozwiązań w świetle obiektywnych kryteriów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6298">
<skos:prefLabel>hodowla roślin</skos:prefLabel>
<skos:definition>hodowla pewnych typów roślin poprzez krzyżowanie odmian między sobą w celu uzyskania nowych o najbardzoiej pożądanych cechach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6299">
<skos:prefLabel>zbiorowisko roślinne</skos:prefLabel>
<skos:definition>zgrupowanie roślin reprezentujących różne gatunki, występujących razem na jednym siedlisku lub obszarze, oddziałujących na siebie poprzez zależności troficzne i przestrzenne, zazwyczaj charakteryzowane na podstawie obecności jednego lub większej ilości gatunków dominujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6300">
<skos:prefLabel>części roślin</skos:prefLabel>
<skos:definition>części roślin składające się na całość organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6304">
<skos:prefLabel>choroba roślin</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6305">
<skos:prefLabel>wyposażenie zakładu</skos:prefLabel>
<skos:definition>wyposażenie w postaci maszyn, narzędzi, instrumentów niezbędnych do produkcji w zakładzie pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6307">
<skos:prefLabel>genetyka roślin</skos:prefLabel>
<skos:definition>badania materiału roślinnego stanowiącego podłoże dziedziczności w celu hybrydyzacji roślin, poprawy zasobów żywieniowych i zwiększenia produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6309">
<skos:prefLabel>ochrona roślin</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6310">
<skos:prefLabel>środek ochrony roślin</skos:prefLabel>
<skos:definition>substancja lub mieszanina substancji, która poprzez oddziaływanie fizjologiczne chroni roślinę przed pasożytami, grzybami, wirusami i innym szkodliwymi czynnikami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6312">
<skos:prefLabel>stopochodne</skos:prefLabel>
<skos:definition>zwierzęta dotykające całą stopą podłoża podczas chodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6313">
<skos:prefLabel>sadzenie drzew</skos:prefLabel>
<skos:definition>sadzenie sadzonek, szczepów, zrzezów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6316">
<skos:prefLabel>fizjologia roślin</skos:prefLabel>
<skos:definition>nauka o czynnościach, funkcjach życiowych roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6318">
<skos:prefLabel>ochrona roślin</skos:prefLabel>
<skos:definition>ochrona rzadkich, zagrożonych lub innych o szczególnym znaczeniu gatunków roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6319">
<skos:prefLabel>badanie środków ochrony roślin</skos:prefLabel>
<skos:definition>badania prowadzone w celu oceny skuteczności środków ochrony roślin w różnych warunkach klimatycznych i glebowych oraz ich wpływu na człowieka, zwierzęta i rośliny, a także trwałości środków w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6323">
<skos:prefLabel>roślina</skos:prefLabel>
<skos:definition>organizm o komórkach okrytych ścianą komórkową, samożywny, wytwarzający dzięki ciałkom zieleni w drodze asymilacji złożone związki organiczne z prostych związków nieorganicznych lub cudzożywny, odżywiający się związkami organicznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6325">
<skos:prefLabel>włókno naturalne</skos:prefLabel>
<skos:definition>surowiec naturalny (np. wełna, konopie, len), z którego wyrabia się przędzę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6326">
<skos:prefLabel>handel roślinami</skos:prefLabel>
<skos:definition>handel roślinami podlegający przepisom zawartym w Konwencji o międzynarodowym handlu dzikimi zwierzętami i roślinami gatunków zagrożonych wyginięciem, sporządzonej w Waszyngtonie dnia 3 marca 1973 r. Polska ratyfikowała Konwencję 15 stycznia 1991 r.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6327">
<skos:prefLabel>technologia plazmowa</skos:prefLabel>
<skos:definition>wspólna nazwa dla wielu przemysłowych zastosowań plazmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6329">
<skos:prefLabel>tworzywo sztuczne</skos:prefLabel>
<skos:definition>materiał otrzymywany zwykle z surowców organicznych, dający się dowolnie formować</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6336">
<skos:prefLabel>odpady z tworzyw sztucznych</skos:prefLabel>
<skos:definition>odpady z tworzyw sztucznych wytwarzane w procesach przemysłowych oraz przez indywidualnych konsumentów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6338">
<skos:prefLabel>platyna</skos:prefLabel>
<skos:definition>pierwiastek chemiczny z podgrupy platynowców, liczba atomowa 78, metal szlachetny, srebrzystobiały, trudno topliwy, używany do wyrobu naczyń laboratoryjnych, narzędzi, biżuterii i jako katalizator</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6339">
<skos:prefLabel>plac zabaw</skos:prefLabel>
<skos:definition>wolna przestrzeń, przeznaczona do zabaw i gier ruchowych, często wyposażona w piaskownice, huśtawki itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6346">
<skos:prefLabel>pluton</skos:prefLabel>
<skos:definition>promieniotwórczy pierwiastek chemiczny, liczba atomowa 94, srebrzystobiały metal dość aktywny, bardzo trujący, występujący w naturze w ilościach minimalnych, otrzymywany sztucznie w reaktorach jądrowych; użyty jako źródło energii w bombie atomowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6355">
<skos:prefLabel>kłusownictwo</skos:prefLabel>
<skos:definition>polowanie na zwierzynę lub łowienie ryb bez posiadania uprawnień, w sposób niedozwolony lub w niedozwolonym czasie i miejscu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6357">
<skos:prefLabel>punktowe źródło zanieczyszczeń</skos:prefLabel>
<skos:definition>zanieczyszczenie z jednostkowego źródła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6358">
<skos:prefLabel>trucizna</skos:prefLabel>
<skos:definition>substancja, która po dostaniu się w odpowiedniej dawce do organizmu może zakłócić jego funkcje życiowe, a nawet spowodować śmierć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6359">
<skos:prefLabel>zatrucie</skos:prefLabel>
<skos:definition>ostre lub przewlekłe zaburzenie czynności narządów i tkanek lub ich uszkodzenie wskutek działania trucizny lub trujących substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6362">
<skos:prefLabel>ekosystem polarny</skos:prefLabel>
<skos:definition>ekosystem związany z obszarami, gdzie stale panuje temperatura poniżej 10° C, przede wszystkim w pobliżu bieguna północnego i południowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6364">
<skos:prefLabel>region polarny</skos:prefLabel>
<skos:definition>region związany z obszarami znajdującymi się pomiędzy kołami podbiegunowymi a odpowiadającymi im biegunami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6365">
<skos:prefLabel>polder</skos:prefLabel>
<skos:definition>osuszone depresyjne tereny nadmorskie, otoczone groblami w celu ochrony przed zalaniem przez rzekę, jezioro itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6367">
<skos:prefLabel>policja</skos:prefLabel>
<skos:definition>organ państwowy powołany do ochrony porządku i bezpieczeństwa publicznego, zorganizowany w sposób wojskowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6368">
<skos:prefLabel>ustawodawstwo dotyczące policji</skos:prefLabel>
<skos:definition>obwiązujące prawo regulujące zadania i sposoby ich realizacji przez policję lub inne organizacje cywiilne, powołane do utrzymywania porządku, przeciwdziałania i wykrywania przestępstw itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6370">
<skos:prefLabel>polityka</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6371">
<skos:prefLabel>polityka</skos:prefLabel>
<skos:definition>1)działalność władzy państwowej, rządu w dziedzinach społecznej, gospodarczej, kulturalnej, wojskowej i innych, dotycząca spraw wewnętrznych państwa lub stosunków z innymi krajami; 2)działalność grupy społecznej, partii, osoby mająca zwykle na celu zdobycie i utrzymanie władzy państwowej; 3)Cele i zadania takiej działalności, metody realizacji tych zadań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6372">
<skos:prefLabel>wytyczne do polityki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6373">
<skos:prefLabel>instrument polityczny</skos:prefLabel>
<skos:definition>metoda lub mechanizm stosowany przez rządy, partie polityczne, przedsiębiorców lub osoby prywatne w celu osiągnięcia zamierzonego efektu, poprzez środki prawne i ekonomiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6374">
<skos:prefLabel>polityka integracyjna</skos:prefLabel>
<skos:definition>proces koordynowania, harmonizowania i ujednolicania celów i procedur stosowanych przez różne oddziały i biura należące do jednej organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6376">
<skos:prefLabel>polityka ochrony zasobów</skos:prefLabel>
<skos:definition>wytyczne, filozofia lub kierunek działań mających na celu ochronę i odnowę zasobów ludzkich i przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6378">
<skos:prefLabel>planowanie polityki</skos:prefLabel>
<skos:definition>proces uzgodnień i przygotowań ułatwiających wyznaczenie kierunku działań, które mogą być przyjęte i wykonywane przez rząd, przedsiębiorstwo lub inną organizację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6380">
<skos:prefLabel>doradztwo polityczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6381">
<skos:prefLabel>doktryna polityczna</skos:prefLabel>
<skos:definition>polityka, stanowisko lub zasada głoszona lub realizowana, dotycząca rządzenia i administrowania w państwie i społeczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6382">
<skos:prefLabel>ekologia polityczna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6383">
<skos:prefLabel>geografia polityczna</skos:prefLabel>
<skos:definition>badanie wpływu działań politycznych na środowisko geograficzne człowieka wraz z analizą przestrzenną zjawisk politycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6384">
<skos:prefLabel>organizacja polityczna</skos:prefLabel>
<skos:definition>organizacja zrzeszająca ludzi mających te same poglądy na ustrój państwa, reprezentujących interesy jakiejś grupy społeczeństwa i popierająca kandydatów na określone stanowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6387">
<skos:prefLabel>partia polityczna</skos:prefLabel>
<skos:definition>organizacja społeczna jednocząca swoich członków na podstawie programu politycznego (doktryny, ideologii) wyrażającego interesy określonych grup (klas, warstw) społecznych i dążąca do realizacji tego programu przez zdobycie i sprawowanie władzy państwowej lub przez wpływanie na działalność ośrodków aktualnie dysponujących władzą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6388">
<skos:prefLabel>władza polityczna</skos:prefLabel>
<skos:definition>prawo rządzenia państwem; rządzenie dotyczące narodu, regionu itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6392">
<skos:prefLabel>pyłek</skos:prefLabel>
<skos:definition>u roślin nasiennych: zbiór męskich elementów rozrodczych tworzących się w pylniku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6395">
<skos:prefLabel>zanieczyszczenia</skos:prefLabel>
<skos:definition>każda substancja, zwykle stanowiąca pozostałość po działalności ludzkiej, wykazująca niekorzystny wpływ na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6396">
<skos:prefLabel>absorpcja zanieczyszczeń</skos:prefLabel>
<skos:definition>proces, w wyniku którego zanieczyszczenie jest fizycznie wchłonięte przez inną substancję lub ciało</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6397">
<skos:prefLabel>akumulacja zanieczyszczeń</skos:prefLabel>
<skos:definition>proces, który jest odpowiedzialny za stały wzrost zanieczyszczeń w tkankach żywych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6399">
<skos:prefLabel>analiza zanieczyszczeń</skos:prefLabel>
<skos:definition>wyznaczenie składu substancji powodującej zanieczyszczenie z wykorzystaniem technik laboratoryjnych i innych metod chemii analitycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6400">
<skos:prefLabel>ocena zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6402">
<skos:prefLabel>zachowanie się zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6404">
<skos:prefLabel>koncentracja zanieczyszczeń</skos:prefLabel>
<skos:definition>miara ilości substancji zanieczyszczającej w jednostce wody, gleby, powietrza, żywności lub innego środka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6405">
<skos:prefLabel>degradacja zanieczyszczeń</skos:prefLabel>
<skos:definition>fizyczny, chemiczny lub biologiczny rozkład zanieczyszczenia do prostszych składników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6406">
<skos:prefLabel>opad zanieczyszczeń</skos:prefLabel>
<skos:definition>proces, w wyniku którego czynniki zanieczyszczające osiadają lub akumulują się w ekosystemach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6407">
<skos:prefLabel>rozprzestrzenianie się zanieczyszczeń</skos:prefLabel>
<skos:definition>rozprzestrzenianie się zanieczyszczeń z miejsca uwolnienia do powietrza, wody i gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6409">
<skos:prefLabel>rozkład przestrzenny zanieczyszczeń</skos:prefLabel>
<skos:definition>model opisujący zachowanie się zanieczyszczeń w regionie geograficznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6412">
<skos:prefLabel>eliminacja zanieczyszczeń</skos:prefLabel>
<skos:definition>proces całkowitego usuwania zanieczyszczeń oraz ich źródeł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6413">
<skos:prefLabel>emisja zanieczyszczeń</skos:prefLabel>
<skos:definition>uwalnianie substancji zanieczyszczających do powietrza, wody i gleby z określonego źródła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6414">
<skos:prefLabel>narażenie na zanieczyszczenia</skos:prefLabel>
<skos:definition>stan zagrożenia ludzkiego zdrowia i mienia oraz środowiska spowodowany zanieczyszczeniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6415">
<skos:prefLabel>tworzenie się zanieczyszczeń</skos:prefLabel>
<skos:definition>proces tworzenia się zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6417">
<skos:prefLabel>imisja zanieczyszczeń</skos:prefLabel>
<skos:definition>przenoszenie stałych, ciekłych lub gazowych zanieczyszczeń w powietrzu, wodzie i glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6418">
<skos:prefLabel>unieruchomienie zanieczyszczeń</skos:prefLabel>
<skos:definition>procesy obróbki wykorzystywane do ograniczenia rozpuszczalności zanieczyszczeń w celu zminimalizowania ich migracji i wymywania lub w celu przygotowania do ich usuwania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6421">
<skos:prefLabel>poziom zanieczyszczeń</skos:prefLabel>
<skos:definition>wartość określająca stężenie czynnika zanieczyszczającego w obrębie określonej powierzchni, na ogół określana metodą pomiarów oraz przy pomocy urządzenia rejestrującego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6422">
<skos:prefLabel>ładunek zanieczyszczeń</skos:prefLabel>
<skos:definition>masa zanieczyszczeń przenoszona przez czynnik taki jak strumień, lodowiec lub wiatr w danej jednostce czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6423">
<skos:prefLabel>uruchomienie zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6425">
<skos:prefLabel>monitoring zanieczyszczeń</skos:prefLabel>
<skos:definition>okresowe lub stałe określanie ilości substancji zanieczyszcających obecnych w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6428">
<skos:prefLabel>drogi przemieszczanie się zanieczyszczeń</skos:prefLabel>
<skos:definition>identyfikowalna trasa przemieszczania się zanieczyszczeń, od źródła, poprzez interakcje ze środowiskiem, aż do ostatecznych skutków w ekosystemie lub organizmach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6429">
<skos:prefLabel>redukcja zanieczyszczeń</skos:prefLabel>
<skos:definition>podejmowanie środków nakierowanych na redukcję zanieczyszczeń, często poprzez stosowanie fizycznego lub chenicznego usuwania substancji trujących lub potencjalnie trujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6430">
<skos:prefLabel>wtórne uruchamianie zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6438">
<skos:prefLabel>wykrywanie źródeł zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6441">
<skos:prefLabel>substancja zanieczyszczona</skos:prefLabel>
<skos:definition>ciało stałe, ciecz lub gaz, które zostały zanieczyszczone lub są niebezpieczne w użytkowaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6443">
<skos:prefLabel>zasada &quot;zanieczyszczający płaci&quot;</skos:prefLabel>
<skos:definition>zasada, zgodnie z którą ten, kto powoduje zanieczyszczenie, powinien pokryć również koszty związane z usuwaniem szkód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6445">
<skos:prefLabel>zanieczyszczenie</skos:prefLabel>
<skos:definition>pośrednie lub bezpośrednie zakłócenie biologicznych, termicznych, fizycznych i radioaktywnych właściwości jakiegokolwiek medium w taki sposób, że powstaje niebezpieczeństwo dla zdrowia ludzi, bezpieczeństwa lub pomyślności żyjących gatunków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6446">
<skos:prefLabel>redukcja zanieczyszczeń</skos:prefLabel>
<skos:definition>zmniejszanie stopnia lub intensywności zanieczyszczeń w glebie, rzekach, jeziorach, morzach, atmosferze itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6447">
<skos:prefLabel>sprzęt i urządzenia do redukcji zanieczyszczeń</skos:prefLabel>
<skos:definition>sprzęt i urządzenia służące do zmniejszania stopnia lub intensywności zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6451">
<skos:prefLabel>kontrola zanieczyszczeń</skos:prefLabel>
<skos:definition>chemiczne i fizyczne metody zmniejszania ładunków większości zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6452">
<skos:prefLabel>sprzęt i urządzenia do kontroli zanieczyszczeń</skos:prefLabel>
<skos:definition>sprzęt i urządzenia służące do zmniejszania lub usuwania ze środowiska tych emisji, które potencjalnie mogą spowodować zanieczyszczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6453">
<skos:prefLabel>przepisy dotyczące ochrony przed zanieczyszczeniem</skos:prefLabel>
<skos:definition>akty prawne rządu, administracji lub organizacji międzynarodowej lub traktat określające granice emisji substancji, które zagrażają lub negatywnie oddziaływują na środowisko i ludzkie zdrowie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6454">
<skos:prefLabel>technologia redukcji zanieczyszczeń</skos:prefLabel>
<skos:definition>metody stosowane do zmniejszania ilości substancji zanieczyszczających emitowanych ze źródeł zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6458">
<skos:prefLabel>kryterium zanieczyszczenia</skos:prefLabel>
<skos:definition>norma ustanowiona dla pewnych zanieczyszczeń, która ogranicza ich dopuszczalne stężenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6459">
<skos:prefLabel>efekt oddziaływania zanieczyszczeń</skos:prefLabel>
<skos:definition>zasadniczy skutek (skutki) zanieczyszczenia oddziałującego na ludzkie zdrowie oraz różne aspekty związane ze środowiskiem (powietrze, woda, gleba), np. zmiany klimatu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6462">
<skos:prefLabel>organizm wskaźnikowy</skos:prefLabel>
<skos:definition>organizmy, głównie rośliny, które są najbardziej czułe na zmiany czynników środowiskowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6464">
<skos:prefLabel>odpowiedzialność za zanieczyszczenie</skos:prefLabel>
<skos:definition>odpowiedzialność za szkody wynikłe z powodu zanieszczenia substancjami niebezpiecznymi, zanieczyszczającymi itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6465">
<skos:prefLabel>ładunek zanieczyszczeń</skos:prefLabel>
<skos:definition>ilość czynnika stresu wprowadzonego do ekosystemu jako zanieczyszczenie natury fizycznej lub chemicznej, uwalniane ze źródeł naturalnych i antropogenicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6467">
<skos:prefLabel>pomiar zanieczyszczeń</skos:prefLabel>
<skos:definition>ocena koncentracji substancji zanieczyszczającej w jednostce czasu w określonym miejscu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6469">
<skos:prefLabel>monitoring zanieczyszczeń</skos:prefLabel>
<skos:definition>ilościowe lub jakościowe określenie obecności, skutków lub poziomu jakiegoś zanieczyszczenia w powietrzu, wodzie lub w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6470">
<skos:prefLabel>norma zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6472">
<skos:prefLabel>ryzyko zanieczyszczenia</skos:prefLabel>
<skos:definition>prawdopodobieństwo powstania szkody dla zdrowia ludzi, własności lub środowiska spowodowane przez wprowadzenie do ekosystemu niepożądanych substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6473">
<skos:prefLabel>odbiornik zanieczyszczeń</skos:prefLabel>
<skos:definition>urządzenie do usuwania chemikaliów lub gazów z systemu atmosfera-biosfera-ocean, w którym substancja jest absorbowana w stałym lub pół-stałym odbiorniku składującym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6474">
<skos:prefLabel>źródło zanieczyszczeń</skos:prefLabel>
<skos:definition>miejsca lub obszary skąd subtancje zanieczyszczające są uwalniane do atmosfery lub wody, albo miejsce skąd rozlega się hałas; źródła zanieczyszczeń mogą być punktowe, liniowe i powierzchniowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6481">
<skos:prefLabel>difenyle polibromowane</skos:prefLabel>
<skos:definition>substancja chemiczna będąca bromowanymi difenylami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6485">
<skos:prefLabel>dwufenyle polichlorowane</skos:prefLabel>
<skos:definition>PCB-sy są rodziną związków chemicznych nie istniejących w naturze. Mieszanina handlowa jest przezroczystą jasno-żółtą cieczą, wytwarzaną przez zastąpienie atomów wodoru w cząsteczce difenylu atomami chloru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6486">
<skos:prefLabel>polichlorodibenzo-p-dioksyna</skos:prefLabel>
<skos:definition>PCDD powstają (wraz z furanami), gdy związki zawierające chlor są spalane w niskiej temperaturze w niewłaściwie eksploatowanych i zaprojektowanych spalarniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6487">
<skos:prefLabel>dwubenzofurany polichlorowane</skos:prefLabel>
<skos:definition>rodzina 135 bezbarwnych związków znanych jako kongenery, różniących się szkodliwością dla zdrowia ludzi i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6488">
<skos:prefLabel>terfenyle polichlorowane</skos:prefLabel>
<skos:definition>związki składające się z trzech pierścieni benzenowych połączonych jeden do drugiego w pozycji orto, meta lub para i podstawionych atomami chloru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6492">
<skos:prefLabel>wielopierścieniowe węglowodory aromatyczne</skos:prefLabel>
<skos:definition>węglowodory zawierające dwa lub więcej pierścieni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6493">
<skos:prefLabel>związki węglowodorowe</skos:prefLabel>
<skos:definition>węglowodory o cząsteczkach zawierających dwa lub więcej pierścieni, np.naftalen o dwóch pierścieniach benzenowych lub difenyl o dwóch powiązanych pierścieniach benzenowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6497">
<skos:prefLabel>politereftalan etylenowy</skos:prefLabel>
<skos:definition>1) termoplastyczna żywica poliestrowa będąca produktem reakcji glikolu etylenowego z kwasem tereftalowym używana do wytwarzania folmów i błon; 2) rodzaj tworzywa sztucznego wykorzystywanego di wytwarzania butelek, które mogą być poddawane recyklingowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6498">
<skos:prefLabel>polimeryzacja</skos:prefLabel>
<skos:definition>reakcja chemiczna, podczas której wiele cząsteczek tego samego związku łączy się w jedną cząsteczkę o wielokrotnym ciężarze cząsteczkowym i odmiennych właściwościach chemicznych i fizycznych, przy czym nie powstają produkty uboczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6499">
<skos:prefLabel>polimery</skos:prefLabel>
<skos:definition>substancje wielkocząsteczkowe otrzymywane z monomerów w wyniku polireakcji, zwłaszcza polimeryzacji, stanowiące podstawowy, niekiedy jedyny składnik tworzyw sztucznych, włókien syntetycznych, kauczuków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6505">
<skos:prefLabel>polichlorek winylu</skos:prefLabel>
<skos:definition>polimer chlorku winylu, bez smaku, zapachu, nierozpuszczalny w większości rozpuszczalników organicznych, należy do grupy żywic winylowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6507">
<skos:prefLabel>jezioro</skos:prefLabel>
<skos:definition>naturalne zagłębienie lądu tworzące zbiornik wodny, nie mający połączenia z morzem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6508">
<skos:prefLabel>staw osadowy</skos:prefLabel>
<skos:definition>zbiornik, w którym następuje wydzielenie substancji zawieszonych w postaci osadu, najczęściej  na skutek zwolnienia prędkości przepływu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6510">
<skos:prefLabel>sadzawka</skos:prefLabel>
<skos:definition>niewielki zbiornik wodny w naturalnym lub wykopanym zagłębieniu ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6517">
<skos:prefLabel>rozmieszczenie ludności</skos:prefLabel>
<skos:definition>gęstość, rozproszenie i rozmieszczenie całkowitej liczby ludzi na określonej powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6518">
<skos:prefLabel>dynamika ludności</skos:prefLabel>
<skos:definition>proces zmian liczbowych i strukturalnych dotyczących ludności, wynikających z urodzin, zgonów i migracji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6519">
<skos:prefLabel>populacja</skos:prefLabel>
<skos:definition>grupa krzyżujących się osobników jednego gatunku żyjących na określonym obszarze i izolowanych rozrodczo od innych grup tego gatunku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6520">
<skos:prefLabel>ekologia populacji</skos:prefLabel>
<skos:definition>badanie związków danej populacji z jej środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6523">
<skos:prefLabel>przyrost liczby ludności</skos:prefLabel>
<skos:definition>wzrost całkowitej liczby mieszkańców kraju, miasta, okręgu lub obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6525">
<skos:prefLabel>migracje ludności</skos:prefLabel>
<skos:definition>ruchy lub migracje statystycznie istotnej liczby mieszkańców kraju, miasta, okręgu lub obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6527">
<skos:prefLabel>struktura ludnościowa</skos:prefLabel>
<skos:definition>organizacja i wzajemne związki pomiędzy mieszkańcami danego kraju, miasta, okręgu lub obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6528">
<skos:prefLabel>tendencje ludnościowe</skos:prefLabel>
<skos:definition>kierunek zmian całkowitej liczby ludności zamieszkującej dany kraj, okręg lub obszar</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6536">
<skos:prefLabel>uzdatnianie wody</skos:prefLabel>
<skos:definition>ponowne oczyszczanie wody już wstępnie oczyszczonej lub zrzucanej, w celu poprawy jej jakości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6540">
<skos:prefLabel>techniczny węglan potasowy</skos:prefLabel>
<skos:definition>każdy z grupy związków zawierających potas, w szczególności rozpuszczalnych w wodzie, takich jak tlenek potasu, chlorek potasu, siarczan, używane głównie jako składniki nawozów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6541">
<skos:prefLabel>górnictwo soli kamiennej</skos:prefLabel>
<skos:definition>podziemne pozyskiwanie soli kamiennej w kopalniach, gdzie są drążone chodniki wydobywcze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6548">
<skos:prefLabel>drób</skos:prefLabel>
<skos:definition>ptactwo domowe, jak kury, gęsi, kaczki itp., hodowane ze względu na mięso, pierze i jaja</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6549">
<skos:prefLabel>hodowla drobiu</skos:prefLabel>
<skos:definition>jedno z najpopularniejszych zajęć rolniczych, polegające na utrzymaniu drobiu na mięso i jajka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6551">
<skos:prefLabel>ubóstwo</skos:prefLabel>
<skos:definition>brak dostatecznych środków do życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6552">
<skos:prefLabel>przedsiębiorstwo energetyczne</skos:prefLabel>
<skos:definition>przedsiębiorstwo odpowiadające za dostawy energii elektrycznej na danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6558">
<skos:prefLabel>wydajność ciepła</skos:prefLabel>
<skos:definition>stosunek pracy wykonanej przez silnik do ilości dostarczonego ciepła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6561">
<skos:prefLabel>elektrownia</skos:prefLabel>
<skos:definition>zakład energetyczny przetwarzający różne rodzaje energii na prąd elektryczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6570">
<skos:prefLabel>strącanie (chemiczne)</skos:prefLabel>
<skos:definition>proces, w wyniku którego powstaje oddzielna faza stała w fazie ciekłej (roztworze)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6571">
<skos:prefLabel>zwiększanie ilości opadów atmosferycznych</skos:prefLabel>
<skos:definition>zwiększanie opadów atmosferycznych poprzez zmiany w koloidalnej stabilności chmur</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6577">
<skos:prefLabel>drapieżnik</skos:prefLabel>
<skos:definition>zwierzę drapieżne, ptak drapieżny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6579">
<skos:prefLabel>budynek prefabrykowany</skos:prefLabel>
<skos:definition>budynek, którego elementy wytwarza się w wyspecjalizowanych fabrykach a następnie przewozi i montuje na placu budowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6583">
<skos:prefLabel>sądowe postępowanie wstępne</skos:prefLabel>
<skos:definition>postępowanie wstępne, poprzedzające proces sądowy, mające określić zakres dalszego zaangażowania sądu lub wskazania innych rozwiązań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6584">
<skos:prefLabel>składka ubezpieczeniowa</skos:prefLabel>
<skos:definition>kwota, którą należy zapłacić za ubezpeiczenie lub polisę na życie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6586">
<skos:prefLabel>ochrona dowodów</skos:prefLabel>
<skos:definition>zachowanie w bezpiecznym stanie wszelkich dowodów legalnie przedstawionych w procesie sądowym (świadkowie, zapisy, nagrania, dokumenty, przedmioty itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6587">
<skos:prefLabel>środek konserwujący</skos:prefLabel>
<skos:definition>substancja chemiczna dodawana do żywności w celu zapobiegania utlenianiu, fermentacji lub innym formom psucia się, najczęściej związanym z rozwojem bakterii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6588">
<skos:prefLabel>konserwa</skos:prefLabel>
<skos:definition>produkty spożywcze w szczelnym opakowaniu, przeznaczone do dłuższego przechowywania, często gotowe do spożycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6591">
<skos:prefLabel>prasa</skos:prefLabel>
<skos:definition>ogół dzienników i czasopism, szczególnie wychodzących w danym czasie w danym kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6592">
<skos:prefLabel>wytłaczanie</skos:prefLabel>
<skos:definition>wywoływanie nacisku w celu wyciśnięcia soku lub miąższu z owoców, nasion itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6593">
<skos:prefLabel>ciśnienie</skos:prefLabel>
<skos:definition>rodzaj nacisku, który jest równomierny we wszystkich kierunkach, mierzony jako siła wywierana na jednostkę powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6594">
<skos:prefLabel>grupa nacisku</skos:prefLabel>
<skos:definition>grupa aktywna politycznie, mająca wspólnie określony zespół wartości dotyczący sposobów korzystania z zasobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6600">
<skos:prefLabel>zdrowotne środki zapobiegawcze</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6602">
<skos:prefLabel>cena</skos:prefLabel>
<skos:definition>wartość przedmiotu lub usługi wyrażona w pieniądzach jako środku zamiennym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6607">
<skos:prefLabel>polityka wyceny zasobów</skos:prefLabel>
<skos:definition>filozofia podejmowania decyzji dotyczących wyceny krajowych lub regionalnych zasobów naturalnych, ludzkich i kapitałowych oraz wytworzonych dóbr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6610">
<skos:prefLabel>nauczanie podstawowe</skos:prefLabel>
<skos:definition>pierwsze pięć lub sześć lat nauczania w szkole podstawowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6614">
<skos:prefLabel>zużycie energii pierwotnej</skos:prefLabel>
<skos:definition>konsumpcja energii w postaci nieprzetworzonej, występującej w formie naturalnej, np. surowej ropy naftowej, węgla, gazu ziemnego, zanim zostanie z nich wytworzona energia elektryczna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6619">
<skos:prefLabel>sektor surowcowy i rolny</skos:prefLabel>
<skos:definition>część gospodarki kraju lub regionu bezpośrednio korzystająca z zasobów naturalnych, w tym rolnictwo, leśnictwo, rybołówstwo oraz górnictwo węgla, rud itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6621">
<skos:prefLabel>naczelne</skos:prefLabel>
<skos:definition>primates, rząd ssaków o silnie rozwiniętym mózgu i układzie nerwowym oraz o jednej przynajmniej parze kończyn chwytnych; obejmuje: małpiatki, małpy i człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6622">
<skos:prefLabel>puszcza pierwotna</skos:prefLabel>
<skos:definition>las, który porastał dany obszar przed zmianami wprowadzonymi przez osadnictwo i gospodarkę człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6623">
<skos:prefLabel>zasada przezorności</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6624">
<skos:prefLabel>zasada zrównoważnego rozwoju</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6627">
<skos:prefLabel>przemysł poligraficzny</skos:prefLabel>
<skos:definition>dział gospodarki, obejmujący przedsiębiorstwa zajmujące się publikowaniem tekstu i ilustracji w wielu kopiach, w postaci książek, czasopism, gazet, dzienników itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6630">
<skos:prefLabel>poligrafia</skos:prefLabel>
<skos:definition>dział przemysłu obejmujący wszystkie rodzaje techniki drukarskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6631">
<skos:prefLabel>zezwolenie po uprzedzeniu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6632">
<skos:prefLabel>zezwolenie pod warunkiem uprzedniego zawiadomienia o przewozie odpadów niebezpiecznych i substancji chemicznych</skos:prefLabel>
<skos:definition>formalne powiadomienie (często również podanie o zezwolenie) właściwych władz o zamiarze przewozu przez granicę ładunków potencjalnie niebezpiecznych, odpadów niebezpiecznych i substancji chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6634">
<skos:prefLabel>samochód prywatny</skos:prefLabel>
<skos:definition>pojazd samochodowy należący do osoby prywatnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6636">
<skos:prefLabel>dom prywatny</skos:prefLabel>
<skos:definition>dom, pomieszczenie mieszkalne gdzie wspólnie zamieszkuje rodzina, grupa ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6637">
<skos:prefLabel>prawo cywilne</skos:prefLabel>
<skos:definition>ogół przepisów normujących stosunki osobowe i majątkowe jednostki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6640">
<skos:prefLabel>sektor prywatny</skos:prefLabel>
<skos:definition>część gospodarki narodowej traktowana odrębnie ze względu na prywatną, a nie państwową własność środków produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6641">
<skos:prefLabel>transport prywatny</skos:prefLabel>
<skos:definition>transport odbywający się prywatnymi środkami transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6643">
<skos:prefLabel>prywatyzacja</skos:prefLabel>
<skos:definition>przejmowanie mienia państwowego (społecznego) przez prywatnych właścicieli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6645">
<skos:prefLabel>słoniowate</skos:prefLabel>
<skos:definition>rząd roślinożernych ssaków łożyskowych wyposażonych w trąbę, siekacze rozrośnięte w postaci kłów oraz kolumnoksztaltne nogi o 5 złączonych palcach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6646">
<skos:prefLabel>organizm prokariotyczny</skos:prefLabel>
<skos:definition>organizmy bezjądrowe, ich materiał genetyczny (nici DNA) nie jest otoczony błoną jądrowa, nie mają mitochondriów ani plastydów; należą tu bakterie i sinice jako jedyne prokariota</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6647">
<skos:prefLabel>prawo procesowe</skos:prefLabel>
<skos:definition>ogół przepisów dotyczących postępowania sądowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6650">
<skos:prefLabel>przetwarzanie</skos:prefLabel>
<skos:definition>przetwarzanie materiałów z jednej formy w inną pożądaną formę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6654">
<skos:prefLabel>technologia procesowa</skos:prefLabel>
<skos:definition>każda strategia technologiczna, metody i narzędzia wykorzytywane do koncepcji, projektu, rozwinięcia i wdrożenia jakiegokolwiek systemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6655">
<skos:prefLabel>woda technologiczna</skos:prefLabel>
<skos:definition>woda używana w procesach produkcji lub przetwarzania lub podczas wytwarzania konkretnego wyrobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6658">
<skos:prefLabel>odpowiedzialność wytwórcy</skos:prefLabel>
<skos:definition>odpowiedzialność i obowiązki spooczywające na wytwórcy produktu lub wykonawcy usługi, w związku z wyrządzeniem szkody lub krzywdy konsumentowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6660">
<skos:prefLabel>produkt</skos:prefLabel>
<skos:definition>dobro powstałe w wyniku procesu produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6661">
<skos:prefLabel>porównywanie produktów</skos:prefLabel>
<skos:definition>porównywanie produktów lub procesów, w celu zidentyfikowania tych, które mają zmiejszone (negatywne) oddziaływania na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6662">
<skos:prefLabel>ocena produktu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6663">
<skos:prefLabel>identyfikacja produktów</skos:prefLabel>
<skos:definition>załączona do produktu informacja dotycząca zawartości, właściwego wykorzystania oraz ostrzeżenia o niebezpieczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6664">
<skos:prefLabel>informacja o produktach</skos:prefLabel>
<skos:definition>wiedza o różnych towarach i usługach, ich jakości oraz producentach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6670">
<skos:prefLabel>produkcyjność</skos:prefLabel>
<skos:definition>zdolność wytwarzania, wydawania plonów; wydajność w produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6672">
<skos:prefLabel>tendencja dotyczące produkcji</skos:prefLabel>
<skos:definition>ogólny kierunek lub trend w określaniu produkcji dóbr i usług, mających wartość wymienną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6675">
<skos:prefLabel>odpowiedzialność producenta</skos:prefLabel>
<skos:definition>odpowiedzialność prawna wytwórcy i sprzedawcy do pokrycia nabywcy, użytkownikowi a nawet osobom postronnym, szkód wyrządzonych przez zakupione wadliwe towary</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6676">
<skos:prefLabel>norma produkcyjna</skos:prefLabel>
<skos:definition>standard określający apekty fizyczne i skład chemiczny produktów, które mogą mieć negatywny wpływ na środowisko, a także standard dotyczący opakowań i obchodzenia się z produktem, szczególnie jeśli jest toksyczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6678">
<skos:prefLabel>zysk</skos:prefLabel>
<skos:definition>nadwyżka wpływów nad wydatkami w działalności gospodarczej; potocznie: dochód, zarobek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6681">
<skos:prefLabel>dane prognostyczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6705">
<skos:prefLabel>program</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6708">
<skos:prefLabel>wykres</skos:prefLabel>
<skos:definition>rysunek przedstawiający przebieg jakiegoś zjawiska, zmiany liczbowe czegoś, zależności między określonymi wielkościami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6709">
<skos:prefLabel>prohibicja</skos:prefLabel>
<skos:definition>zakaz prowadzenia działalności społecznej niepożądanej lub szkodliwej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6710">
<skos:prefLabel>projekt</skos:prefLabel>
<skos:definition>zamierzony plan działania, postępowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6718">
<skos:prefLabel>proces rozchodzenia się</skos:prefLabel>
<skos:definition>proces, w czasie którego następuje przekazanie pewnych zaburzeń z jednego miejsca do innego odległego miejsca bez przemieszczania się samego ośrodka, w którym odbywa się ten proces, np. rozchodzenie się fal elektromagnetycznych, dźwiękowych, wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6720">
<skos:prefLabel>napędzany aerozolem</skos:prefLabel>
<skos:definition>gaz stosowany do preparatów aerozolowych w celu rozpylania cieczy w atomizerach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6724">
<skos:prefLabel>właściwość materiałów</skos:prefLabel>
<skos:definition>fizyczna i chemiczna charakterystyka substancji lub części, z której wykonano daną rzecz lub obiekt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6728">
<skos:prefLabel>ochrona własności</skos:prefLabel>
<skos:definition>korzyść lub ochrona, jaką państwo zapewnia obywatelom w celu zabezpieczenia ich indywidulanych lub firmowych praw do wyłącznego dysponowania własnością, pieniędzmi lub innymi rzeczami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6736">
<skos:prefLabel>technika napędowa</skos:prefLabel>
<skos:definition>technika powodująca poruszanie się ciała poprzez wywieranie na nie jakiejś siły</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6737">
<skos:prefLabel>ściganie sądowe</skos:prefLabel>
<skos:definition>postępowanie według procedury prawnej, szczególnie w sprawach kryminalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6739">
<skos:prefLabel>dobrobyt</skos:prefLabel>
<skos:definition>stan dobrobytu, zamożności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6740">
<skos:prefLabel>obszar chroniony</skos:prefLabel>
<skos:definition>określony obszar chroniony na mocy szczególnych przepisów, ze względu na walory przyrodnicze, krajobrazowe i kulturowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6743">
<skos:prefLabel>krajobraz chroniony</skos:prefLabel>
<skos:definition>krajobraz o wartościowych cechach naturalnych, przyrodniczych lub kulturowych, który jest chroniony dla celów naukowych, dydaktycznych, zachowania walorów przyrodniczych i rekreacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6746">
<skos:prefLabel>gatunek chroniony</skos:prefLabel>
<skos:definition>gatunek zagrożony lub wrażliwy, który jest chroniony poprzez podjęcie działań zapobiegawczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6747">
<skos:prefLabel>strefa ochrony wód</skos:prefLabel>
<skos:definition>obszar otaczający stacje uzdatniania wód lub inne tego typu obiekty, na którym pewne formy użytkowania ziemi są ograniczone lub zabronione w celu ochrony zasobów wód podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6749">
<skos:prefLabel>ochrona przed sąsiadami</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6750">
<skos:prefLabel>ochrona ptaków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6752">
<skos:prefLabel>ochrona gatunkowa</skos:prefLabel>
<skos:definition>środki przyjęte w celu zapewnienia istnienia gatunków, ich ekosystemów i ich różnorodności biologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6753">
<skos:prefLabel>system ochrony</skos:prefLabel>
<skos:definition>zespół procedur i urządzeń przeznaczonych dla ochrony ludzi, ich własności lub środowiska przed niebezpieczeństwem i szkodami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6755">
<skos:prefLabel>proteiny</skos:prefLabel>
<skos:definition>białka proste zbudowane tylko z aminokwasów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6758">
<skos:prefLabel>protokół</skos:prefLabel>
<skos:definition>pisemne sprawozdanie z obrad, posiedzenia, sesji, zebrania zawierające streszczenie przemówień, wniosków i uchwał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6759">
<skos:prefLabel>pierwotniaki</skos:prefLabel>
<skos:definition>Protozoa, podkrólestwo zwierząt obejmujące około 30 tysięcy gatunków jednokomórkowych organizmów, zwykle mikroskopijnej wielkości, charakteryzujących się wyodrębnionym jądrem komórkowym lub wieloma jądrami, poruszających się za pomocą nibynóżek, wici albo rzęsek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6760">
<skos:prefLabel>prowincja</skos:prefLabel>
<skos:definition>jednostka administracyjno-terytorialna w niektórych państwach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6766">
<skos:prefLabel>władze regionalne</skos:prefLabel>
<skos:definition>władza odnosząca się do administrowania pewnym regionem i stosowania praw, obowiązujących w całym kraju (w dwóch lub więcej regionach, stanach itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6773">
<skos:prefLabel>efekt psychiczny</skos:prefLabel>
<skos:definition>rezultat lub konsekwencja procesów mentalnych, które tworzą lub wpływają na ludzką świadomość i emocje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6774">
<skos:prefLabel>efekt psychologiczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6775">
<skos:prefLabel>stres psychologiczny</skos:prefLabel>
<skos:definition>stan mobilizacji sił organizmu będący reakcją na negatywne bodźce fizyczne i psychiczne (stresory), mogący doprowadzić do zaburzeń czynnościowych, a nawet do schorzeń organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6776">
<skos:prefLabel>psychologia</skos:prefLabel>
<skos:definition>nauka zajmująca się powstawaniem i przebiegiem procesów psychicznych, cechami psychicznymi człowieka i regulacją jego stosunków z otoczeniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6777">
<skos:prefLabel>efekt psychosomatyczny</skos:prefLabel>
<skos:definition>rezultat dotyczący wzajemnych stosunków między psychiką a ciałem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6778">
<skos:prefLabel>choroba psychosomatyczna</skos:prefLabel>
<skos:definition>choroba wynikająca z (pogarszających się) relacji między psychiką a ciałem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6784">
<skos:prefLabel>publiczna dostępność terenów</skos:prefLabel>
<skos:definition>prawo powszechnego wstępu na dany obszar geograficzny, należący do państwa, np. park narodowy, krajobrazowy lub miejsce historyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6785">
<skos:prefLabel>akcje publiczne</skos:prefLabel>
<skos:definition>działania podejmowane w imieniu i za zgodą ogółu społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6787">
<skos:prefLabel>łaźnia publiczna</skos:prefLabel>
<skos:definition>miejsce przeznaczone do kąpieli w łaźni dostępnej dla społeczństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6788">
<skos:prefLabel>budynek publiczny</skos:prefLabel>
<skos:definition>budynek dostępny publicznie, przeznaczony do użytku społecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6790">
<skos:prefLabel>domena publiczna</skos:prefLabel>
<skos:definition>dostępność, użytkowanie lub współkorzystanie przez ogół społeczeństwa bez żadnych ograniczeń lub opłat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6791">
<skos:prefLabel>granice bezpieczeństwa publicznego</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6793">
<skos:prefLabel>finanse publiczne</skos:prefLabel>
<skos:definition>teoria i praktyka w zakresie gospodarowania finansami publicznymi wraz z podatkami, dochodami i wydatkami, itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6794">
<skos:prefLabel>finasowanie publiczne</skos:prefLabel>
<skos:definition>zapewnienie finansowania jakiegoś programu, kupna lub przedsięwzięcia, zwykle przez przyznanie środków z budżetu państwa pochodzących z podatków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6798">
<skos:prefLabel>zdrowie społeczeństwa</skos:prefLabel>
<skos:definition>dyscyplina nauki dotycząca zdrowia na poziomie ogólnospołecznym, nakierowana na zapobieganie chorobom, krzewienie zasad higieny, promowanie zdrowszego życia i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6799">
<skos:prefLabel>rozprawa publiczna</skos:prefLabel>
<skos:definition>prawo do obserwacji i uczestniczenia, a także wysłuchiwania świadków, których zeznania są prezentowane przez stronę przeciwną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6801">
<skos:prefLabel>informacja powszechnie dostępna</skos:prefLabel>
<skos:definition>wiedza i usługa oferowana przez wyznaczoną instytucję, udostępniana całemu społeczeństwu bez żadnych ograniczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6802">
<skos:prefLabel>prawo powszechne</skos:prefLabel>
<skos:definition>ogólny podział prawa na konstytucyjne, administracyjne, kryminalne i międzynarodowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6804">
<skos:prefLabel>opinia publiczna</skos:prefLabel>
<skos:definition>ogólny pogląd społeczeństwa na jakąś sprawę lub problem, zwykle formułowany poprzez wybraną grupę i często używany jako wskazówka do działania lub podejmowania decyzji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6805">
<skos:prefLabel>badanie opinii publicznej</skos:prefLabel>
<skos:definition>badanie reprezantatywnej próby dużej grupy ludzi odnośnie jakiejś kwestii w celu określenia ogólnej opinii tej grupy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6806">
<skos:prefLabel>park publiczny</skos:prefLabel>
<skos:definition>park z drzewami, krzewami i  innymi roślinami ozdobnymi, z alejami spacerowymi, często ozdobiony fontannami i rzeźbami, umiejscowiony w mieście i bezpłatnie udostępniony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6808">
<skos:prefLabel>udział we władzy samorządowej</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6809">
<skos:prefLabel>przedsiębiorstwo społeczno-prywatne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6810">
<skos:prefLabel>dostawy publiczne</skos:prefLabel>
<skos:definition>rządowe zakupy dóbr, sprzętu i usług lub zawieranie kontraktów na dostawę dóbr, sprzętu i usług, które są często zlecane sektorowi prywatnemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6812">
<skos:prefLabel>biuro oskarżyciela publicznego</skos:prefLabel>
<skos:definition>organ państwowy powołany do strzeżenia praworządności, szczególnie do ścigania przestępstw oraz do nadzoru nad przestrzeganiem prawa przez organy administracji państwowej, przedsiębiorstwa i organizacje zawodowe, samorządowe, spółdzielcze i społeczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6815">
<skos:prefLabel>budowanie dróg</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6816">
<skos:prefLabel>sektor publiczny</skos:prefLabel>
<skos:definition>część gospodarki pozostająca w gestii państwa, w tym władz państwowych, administracji lokalnej, firm będących własnością skarbu państwa lub quasi-autonomicznych organizacji pozarządowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6817">
<skos:prefLabel>służby użyteczności publicznej</skos:prefLabel>
<skos:definition>zakłady użyteczności publicznej odpowiadające za dostawy społeczeństwu podstawowych środków takich jak energia elektryczna lub woda</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6818">
<skos:prefLabel>transport publiczny</skos:prefLabel>
<skos:definition>przewóz pasażerów środkami komunikacji publicznej, w przeciwieństwie do transportu prywatnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6821">
<skos:prefLabel>pojazd transportu publicznego</skos:prefLabel>
<skos:definition>pojazd przeznaczony do przewożenia znacznej liczby pasażerów, za opłatą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6822">
<skos:prefLabel>zakłady użyteczności publicznej</skos:prefLabel>
<skos:definition>zakłady użyteczności publicznej odpowiadające za dostawy społeczeństwu podstawowych środków takich jak energia elektryczna lub woda</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6824">
<skos:prefLabel>obiekty publiczne</skos:prefLabel>
<skos:definition>infrastruktura, taka jak drogi, zapory lub urzędy pocztowe, wykonane ze środków państwowych i przeznaczone na cele publiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6827">
<skos:prefLabel>choroba płuc</skos:prefLabel>
<skos:definition>choroba atakująca płuca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6828">
<skos:prefLabel>pulpa</skos:prefLabel>
<skos:definition>masa celulozowa bez dodatku ścieru drzewnego, z której otrzymuje się papier bezdrzewny, celulozowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6830">
<skos:prefLabel>przemysł celulozowy</skos:prefLabel>
<skos:definition>część gospodarki, obejmująca przedsiębiorstwa zajmujące się produkcją i sprzedażą masy celulozowej używanej do wyrobu papieru itp. produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6831">
<skos:prefLabel>pompa</skos:prefLabel>
<skos:definition>maszyna do podnoszenia cieczy z poziomu niższego na wyższy lub do przetłaczania cieczy z obszaru o ciśnieniu niższym do obszaru o ciśnieniu wyższym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6833">
<skos:prefLabel>pompowanie</skos:prefLabel>
<skos:definition>czynność przesuwania cieczy (lub gazu) za pomocą pompy i rur</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6835">
<skos:prefLabel>nabywanie</skos:prefLabel>
<skos:definition>umowa, na podstawie której kupujący zobowiązuje się odebrać rzecz od sprzedawcy i zapłacić mu ustaloną cenę w zamian za przeniesienie na siebie prawa własności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6838">
<skos:prefLabel>oczyszczanie</skos:prefLabel>
<skos:definition>usuwanie z substancji niepożądanych składników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6841">
<skos:prefLabel>sprzęt do oczyszczania</skos:prefLabel>
<skos:definition>sprzęt do usuwania zanieczyszczeń i innych zbędnych cząstek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6842">
<skos:prefLabel>oczyszczalnia</skos:prefLabel>
<skos:definition>zakład, pomieszczenie, gdzie coś podlega oczyszczaniu przez działanie mechaniczne, fizykochemiczne lub biologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6846">
<skos:prefLabel>puryny</skos:prefLabel>
<skos:definition>każda z wielu zasad azotowych, takich jak guanina i adenina, które są pochodnymi puryny i składają się z kwasów nukleinowych i określonych koenzymów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6855">
<skos:prefLabel>piroliza</skos:prefLabel>
<skos:definition>termiczny rozkład cząsteczek złożonych do prostych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6859">
<skos:prefLabel>gwarancja jakości</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6860">
<skos:prefLabel>kontrola jakości</skos:prefLabel>
<skos:definition>sprawdzanie, analiza i inne odpowiednie czynności podejmowane w celu kontrolowania wykonania czegoś, wyprodukowania itp., aby zapewnić i utrzymać odpowiedni poziom jakości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6861">
<skos:prefLabel>jakość życia</skos:prefLabel>
<skos:definition>indywidualnie odczuwana i postrzegana jakość, związana z wizją społecznego dobrobytu, zwykle odnosząca się do takich elementów jak społeczny podział dóbr, ochrona zdrowia, opieka społeczna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6867">
<skos:prefLabel>kamieniołomy</skos:prefLabel>
<skos:definition>kopalnia odkrywkowa litych surowców skalnych dająca kamień drogowy, budowlany, surowce skalne dla przemysłu hutniczego, chemicznego oraz materiały wiążące (wapień, gips) dla przemysłu budowlanego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6870">
<skos:prefLabel>górnictwo w kamieniołomach</skos:prefLabel>
<skos:definition>eksploatacja kopalni odkrywkowych litych surowców skalnych dających kamień drogowy, budowlany, surowce skalne dla przemysłu hutniczego, chemicznego oraz materiały wiążące (wapień, gips) dla przemysłu budowlanego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6882">
<skos:prefLabel>związki międzyrasowe</skos:prefLabel>
<skos:definition>relacje, napięcia lub harmonia pomiędzy dwoma lub więcej grupami ludzi o odmiennej historii, kulturze, religii lub wyglądzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6883">
<skos:prefLabel>radar</skos:prefLabel>
<skos:definition>urządzenie radiolokacyjne do wykrywania i określania położenia lub parametrów ruchu (prędkości, kierunku) obiektów, wyzyskujące zdolność odbijania się od nich wysyłanych przez nadajnik fal elektromagnetycznych (zwykle radiowych); stosowane w nawigacji morskiej, lotniczej, meteorologii itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6884">
<skos:prefLabel>promieniowanie</skos:prefLabel>
<skos:definition>strumień cząstek lub fal elektromagnetycznych emitowanych przez ciała</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6887">
<skos:prefLabel>uszkodzenie wywołane promieniowaniem</skos:prefLabel>
<skos:definition>uszkodzenia somatyczne lub genetyczne wywołane wystawieniem na działanie promieniowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6888">
<skos:prefLabel>dawka napromieniowania</skos:prefLabel>
<skos:definition>całkowita dawka napromieniowania przyjęta przez materiał lub tkankę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6889">
<skos:prefLabel>efekt napromieniowania</skos:prefLabel>
<skos:definition>przedłużone wystawienie na promieniowanie z różnych źródeł (wybuchów jądrowych lub awarii elektrowni jądrowych); może stanowić zagrożenie dla ludzi i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6890">
<skos:prefLabel>narażenie na promieniowanie</skos:prefLabel>
<skos:definition>wystawienie na oddziaływanie energii elektromagnetycznej wystarczająco silne do zjonizowania atomów i przez to zagrażające ludzkiemu życiu lub środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6892">
<skos:prefLabel>monitoring radiologiczny</skos:prefLabel>
<skos:definition>okresowe lub stałe badanie poziomu promieniowania na pewnym obszarze, w celu określenia czy nie zostały przekroczone określone normy lub czy zachowano właściwe standardy bezpieczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6894">
<skos:prefLabel>radiologia</skos:prefLabel>
<skos:definition>badanie działania promieniowania jonizującego i jego wpływu na otoczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6896">
<skos:prefLabel>ochrona radiologiczna</skos:prefLabel>
<skos:definition>działania ochronne, środki lub sprzęt, stosowane do ochrony ludzi, mienia lub zasobów naturalnych przed szkodliwym działaniem promieniowania jonizującego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6897">
<skos:prefLabel>prawo o ochronie radiologicznej</skos:prefLabel>
<skos:definition>regulacje prawne dotyczące ochrony radiologicznej w celu określenia środków zabezpieczających ludzi i zasoby naturalne przed szkodliwym działaniem promieniowania jonizującego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6899">
<skos:prefLabel>choroba popromienna</skos:prefLabel>
<skos:definition>zespół objawów charakteryzowany jako obrażenia popromienne, doznane w wyniku nadmiernej ekspozycji ciała lub jego części na działanie promieniowania jonizującego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6901">
<skos:prefLabel>radio</skos:prefLabel>
<skos:definition>urządzenie nadające i odbierające fale elektromagnetyczne w celu przekazywania sygnałów, dźwięków itp. rodzajów informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6904">
<skos:prefLabel>skażenie radioaktywne</skos:prefLabel>
<skos:definition>skażenie substancji, organizmu żywego lub miejsca spowodowane przez materiał radioaktywny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6908">
<skos:prefLabel>usuwanie skażeń radioaktywnych</skos:prefLabel>
<skos:definition>usuwanie skażeń promieniotwórczych z powierzchni lub miejsca pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6909">
<skos:prefLabel>usuwanie odpadów radioaktywnych</skos:prefLabel>
<skos:definition>usuwanie odpadów, charakteryzujcych się emisją promieniowania elektromagnetycznego lub korpuskularnego wynikającego z rozpadu jąder niestabilnych pierwiastów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6911">
<skos:prefLabel>emisja radioaktywna</skos:prefLabel>
<skos:definition>uwolnienie substancji radioaktywnych do środowiska z instalacji, z kopalni, oczyszczania i wzbogacania paliwa jądrowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6912">
<skos:prefLabel>skażenie radioaktywne</skos:prefLabel>
<skos:definition>opad radioaktywny występujący w wyniku eksplozji jądrowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6915">
<skos:prefLabel>zanieczyszczenia radioaktywne</skos:prefLabel>
<skos:definition>substancja ulegająca procesowi rozpadu jąder atomowych i oddająca energię jądrową w formie cząsteczek lub fal, często związana z wybuchami jądrowymi lub awarią elektrowni jądrowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6918">
<skos:prefLabel>substancja radioaktywna</skos:prefLabel>
<skos:definition>substancja zawierająca jeden lub więcej radionuklidów, których radioaktywność lub stężenie nie może być lekceważona ze względów bezpieczeństwa radiologicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6919">
<skos:prefLabel>wskaźnik promieniotwórczy</skos:prefLabel>
<skos:definition>izotop radioaktywny, który po wprowadzeniu do systemu biologicznego lub fizycznego może być śledzony przez aparaturę wykrywającą promieniowanie, co pozwala na określenie umiejscowienia substancji, do której jest dołączony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6920">
<skos:prefLabel>metoda z użyciem znacznika izotopowego</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6921">
<skos:prefLabel>odpady promieniotwórcze</skos:prefLabel>
<skos:definition>wszelkie odpady, które emitują promieniowanie powyżej poziomu tła, w tym toksyczne produkty uboczne przemysłu jądrowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6923">
<skos:prefLabel>gospodarka odpadami promieniotwórczymi</skos:prefLabel>
<skos:definition>całkowity nadzór nad produkcją, składowaniem i transportem materiałów, które zawierają substancje radioaktywne, a których ponowne wykorzystanie lub odzysk są niepraktyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6925">
<skos:prefLabel>radioaktywność</skos:prefLabel>
<skos:definition>właściwość niektórych pierwiastków chemicznych, polegająca na emisji promieni alfa, beta, gamma, powstających na skutek samorzutnej przemiany jąder atomowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6928">
<skos:prefLabel>radionuklid</skos:prefLabel>
<skos:definition>nuklid ulegający samorzutnym przemianom jądrowym i emitujący promieniowanie jądrowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6935">
<skos:prefLabel>radon</skos:prefLabel>
<skos:definition>(Rn) promieniotwórczy pierwiastek chemiczny z grupy gazów szlachetnych, o liczbie atomowej 86, stosowany w leczeniu nowotworów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6937">
<skos:prefLabel>szmaty</skos:prefLabel>
<skos:definition>odpady tekstylne, pokonsumpcyjne albo przemysłowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6939">
<skos:prefLabel>pojazd szynowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6940">
<skos:prefLabel>ruch kolejowy</skos:prefLabel>
<skos:definition>ruch pojazdów szynowych transportujących ludzi i towary, poruszających się po sieci kolejowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6941">
<skos:prefLabel>transport kolejowy</skos:prefLabel>
<skos:definition>transport ludzi i towarów koleją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6942">
<skos:prefLabel>kolej</skos:prefLabel>
<skos:definition>sieć składająca się z ułożonych na stałe torów, zbudowanych z równoległych szyn żelaznych na podkładach, służąca do transportu ludzi i towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6947">
<skos:prefLabel>deszcz</skos:prefLabel>
<skos:definition>opad atmosferyczny w postaci kropel wody spadających z chmury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6949">
<skos:prefLabel>lasy deszczowe</skos:prefLabel>
<skos:definition>wiecznie zielone lasy liściaste, występujące na obszarach o dużej wilgotności, głównie w strefach klimatu tropikalnego i subtropikalnego, miejscami w strefie klimatu umiarkowanego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6950">
<skos:prefLabel>wymywanie z atmosfery</skos:prefLabel>
<skos:definition>proces usuwania zanieczyszczeń z atmosfery, które tworzą jądra kondensacji dla tworzących się kropel wody i spadających w postaci deszczu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6951">
<skos:prefLabel>woda deszczowa</skos:prefLabel>
<skos:definition>woda spadająca z chmur w postaci deszczu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6953">
<skos:prefLabel>podniesienie terenu</skos:prefLabel>
<skos:definition>podwyższenie poziomu gruntu poprzez nasypanie materiału, np. skalnego, żwirowego itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6959">
<skos:prefLabel>test losowy</skos:prefLabel>
<skos:definition>test, którego wynik nie zawsze jest taki sam przy powtarzaniu takich samych warunków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6962">
<skos:prefLabel>rzepak</skos:prefLabel>
<skos:definition>Brassica napus var. oleifera, roślina z rodziny krzyżowych, o jasnożółtych kwiatach, mająca formy jare i ozime, uprawiana ze względu na nasiona zawierające olej, a także na paszę zieloną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6963">
<skos:prefLabel>szybki pociąg tranzytowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6965">
<skos:prefLabel>rzadki gatunek</skos:prefLabel>
<skos:definition>gatunek o bardzo ograniczonej liczebności na świecie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6967">
<skos:prefLabel>surowiec</skos:prefLabel>
<skos:definition>surowy, nieprzetworzony lub przetworzony częściowo materiał wykorzytywany jako materiał wsadowy w procesie technologicznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6968">
<skos:prefLabel>zużycie surowców</skos:prefLabel>
<skos:definition>zużycie surowców przez gospodarkę i społeczeństwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6970">
<skos:prefLabel>zapewnienie surowców</skos:prefLabel>
<skos:definition>środki podejmowane w celu zabezpieczenia dostępu i dostaw surowców, używanych do przetwórstwa i wyrobu produktów gotowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6975">
<skos:prefLabel>kinetyka reakcji</skos:prefLabel>
<skos:definition>dział chemii fizycznej zajmujący się mechanizmami i szybkościami reakcji chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6977">
<skos:prefLabel>reaktor</skos:prefLabel>
<skos:definition>urządzenie do wyzwalania energii jądrowej w wyniku kontrolowanej łańcuchowej reakcji rozszczepiania jąder atomowych substancji rozszczepialnej, np. uranu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6978">
<skos:prefLabel>bezpieczeństwo reaktorów</skos:prefLabel>
<skos:definition>badania i działania mające na celu minimalizację ryzyka związanego z wystąpieniem awarii reaktorów jądrowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6979">
<skos:prefLabel>zalesianie</skos:prefLabel>
<skos:definition>sadzenie drzew na obszarze, na którym wycięto las</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6982">
<skos:prefLabel>rozsądne rozumowanie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6988">
<skos:prefLabel>technologia rekombinacji DNA</skos:prefLabel>
<skos:definition>techniki i zastosowania praktyczne związane z rekombinacją DNA (wprowadzanie kwasu dezoksyrybonukleinowego do komórki, co prowadzi do zmian jej genotypu i fenotypu; kwas dezoksyrybonukleinowy ulega replikacji razem z naturalnym DNA)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6989">
<skos:prefLabel>rekomendacje</skos:prefLabel>
<skos:definition>zalecenia i rady odnoszące się do określonych działań, nie mające charakteru wiążącego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6992">
<skos:prefLabel>spis subtancji</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6997">
<skos:prefLabel>restytucja krajobrazu</skos:prefLabel>
<skos:definition>środki podejmowane w celu przywracania walorów krajobrazowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/6999">
<skos:prefLabel>restytucja fauny</skos:prefLabel>
<skos:definition>proces przywracania naturalnych ekosystemów i siedlisk niezbędnych dla życia dzikiej fauny, do ich  warunków pierwotnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7000">
<skos:prefLabel>plan restytucji</skos:prefLabel>
<skos:definition>określona metoda odnowy zasobów naturalnych lub ponownego wykorzystania materiałów i obiektów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7001">
<skos:prefLabel>wypoczynek</skos:prefLabel>
<skos:definition>pozbywanie się zmęczenia po pracy, w przerwie w pracy, nabieranie sił</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7003">
<skos:prefLabel>miejsce wypoczynku</skos:prefLabel>
<skos:definition>określony fragment terenu publicznego, szczególnie w miastach, przeznaczony do uprawiania sportów i gier</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7009">
<skos:prefLabel>nadający się do ponownego przetworzenia</skos:prefLabel>
<skos:definition>cechy materiału, który po pierwotnym użyciu nadal zachowuje swoje własności fizyczne i chemiczne i nadaje się do powtórnego wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7010">
<skos:prefLabel>tworzywo sztuczne nadające się do ponownego przetworzenia</skos:prefLabel>
<skos:definition>odpady z tworzyw sztucznych nadające się do powtórnego wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7012">
<skos:prefLabel>surowce wtórne</skos:prefLabel>
<skos:definition>odpady produkcyjne nadające się do dalszego przerobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7014">
<skos:prefLabel>papier nadający się do ponownego przetworzenia</skos:prefLabel>
<skos:definition>papier oddzielony od innych odpadów i jako surowiec przeznaczony do przerobu i produkcji nowych produktów; nie każdy papier nadaje się do powtórnego przerobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7015">
<skos:prefLabel>recykling</skos:prefLabel>
<skos:definition>wykorzystanie odpadów technologicznych i surowców wtórnych w przemyśle do wyprodukowania takich samych lub podobnych produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7018">
<skos:prefLabel>sterowanie cyklem życia produktu</skos:prefLabel>
<skos:definition>sterowanie wszystkimi fazami życia produktu, takimi jak pozyskanie surowców, produkcja, dystrybucja, sprzedaż, użytkowanie (pierwotne i wtórne), utrzymanie, recykling i gospodarowanie odpadami, w celu wytwarzania produktów mniej szkodliwych dla środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7019">
<skos:prefLabel>regulacje prawne dotyczące recyklingu i odpadów</skos:prefLabel>
<skos:definition>regulacje prawne dotyczące recyklingu i odpadów zmierzające do zmniejszenia ilości odpadów, których nie można wykorzystać do ponownego przerobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7021">
<skos:prefLabel>potencjał recyklingu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7023">
<skos:prefLabel>współczynnik recyklingu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7028">
<skos:prefLabel>rekompensata</skos:prefLabel>
<skos:definition>środek administracyjny lub prawny mający zadośćuczynić danej osobie stratę lub krzywdę, z zerwaniem kontraktu włącznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7029">
<skos:prefLabel>czerwony zakwit</skos:prefLabel>
<skos:definition>masowy rozród (zakwit) czerwonawych glonów na powierzchni mórz lub gęsty nalot jednokomórkowych bruzdnic</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7030">
<skos:prefLabel>redukcja chemiczna</skos:prefLabel>
<skos:definition>reakcja chemiczna, w której maleje wartościowość pierwiastka chemicznego, wskutek przyłączenia elektronów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7032">
<skos:prefLabel>trzcina</skos:prefLabel>
<skos:definition>roślina z rodziny traw, o wysokiej łodydze, z kwiatami w postaci wiech; najbardziej znanym gatunkiem tej rośliny jest trzcina pospolita rosnąca gromadnie przy brzegach wód i na mokradłach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7034">
<skos:prefLabel>rafa</skos:prefLabel>
<skos:definition>skała podwodna leżąca na małej głębokości lub nieco wystająca nad wodę, powstająca zwykle z nagromadzonych wapiennych szkieletów organizmów morskich, głównie koralowców, mszywiołów i glonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7035">
<skos:prefLabel>rafineria</skos:prefLabel>
<skos:definition>zakład przemysłowy, w którym poddaje się oczyszczeniu pewne produkty, substancje (np. cukier, spirytus, oleje roślinne) oraz przerabia (np. ropę naftową) na produkty użytkowe, handlowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7036">
<skos:prefLabel>rafinacja</skos:prefLabel>
<skos:definition>proces oczyszczania i uszlachetniania substancji naturalnych lub produktów przemysłowych w celu nadania im odpowiedniej czystości, barwy, zapachu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7037">
<skos:prefLabel>odbicie</skos:prefLabel>
<skos:definition>światło odbite od powierzchni czegoś</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7038">
<skos:prefLabel>reflektometria</skos:prefLabel>
<skos:definition>badania odbicia światła lub innej energii promienistej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7042">
<skos:prefLabel>środek chłodzący</skos:prefLabel>
<skos:definition>substancja, która przechodząc w różne stany skupienia (z ciekłego w gazowy, z gazowego w ciekły) uwalnia lub pochłania znaczne ilości ciepła w stosunku do swojej objętości, w związku z czym wywołuje istotny efekt schładzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7043">
<skos:prefLabel>przemysł chłodniczy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7044">
<skos:prefLabel>chłodzenie</skos:prefLabel>
<skos:definition>obniżanie temperatury substancji, najczęściej żywności, poniżej temperatury otoczenia, w celu przełużenia przydatności do spożycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7045">
<skos:prefLabel>chłodnia</skos:prefLabel>
<skos:definition>pomieszczenie (także mebel lub pojazd) z urządzeniem chłodniczym, przeznaczone do przechowywania w niskiej temperaturze substancji nietrwałych, np. żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7046">
<skos:prefLabel>ostoja</skos:prefLabel>
<skos:definition>izolowany obszar, na którym zwierzęta i rośliny przetrwały okres niesprzyjających zmian klimatu w skali kontynentalnej, np. podczas zlodowacenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7047">
<skos:prefLabel>uchodźca</skos:prefLabel>
<skos:definition>osoba w obcym kraju, która opuszcza własny kraj z przyczyn ekonomicznych, politycznych lub religijnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7050">
<skos:prefLabel>samochód-śmieciarka</skos:prefLabel>
<skos:definition>samochód przystosowany do zbierania i przewozu śmieci na wysypisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7051">
<skos:prefLabel>paliwo z odpadów komunalnych</skos:prefLabel>
<skos:definition>paliwa produkowane z odpadów bytowych, po usunięciu z nich szkła i metalu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7056">
<skos:prefLabel>kompost z osadów ściekowych</skos:prefLabel>
<skos:definition>kompost otrzymywany z organicznych składników odpadów stałych i osadów ściekowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7057">
<skos:prefLabel>regeneracja</skos:prefLabel>
<skos:definition>odnawianie materiałów, substancji lub przyrządów poprzez przywracanie im pierwotnych właściwości w celu powtórnego użytkowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7058">
<skos:prefLabel>doposażenie starych fabryk</skos:prefLabel>
<skos:definition>zmiany w starych zakładach przemysłowych polegające na wprowadzaniu nowoczesnych instalacji ograniczających emisję gazów, ścieków i odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7059">
<skos:prefLabel>region</skos:prefLabel>
<skos:definition>wydzielony, stosunkowo jednorodny obszar geograficzny (często jednostka administracyjna, w tym część miasta) odróżniający się od terenów przyległych określonymi cechami naturalnymi lub nabytymi (kryteria przyrodnicze, demograficzne, ekonomiczne, polityczne)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7063">
<skos:prefLabel>umowa międzynarodowa o zasięgu regionalnym</skos:prefLabel>
<skos:definition>porozumienie zawarte pomiędzy państwami, partiami politycznymi, organizacjami dotyczące specyficznych zagadnień danego regionu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7066">
<skos:prefLabel>rozwój regionalny</skos:prefLabel>
<skos:definition>postęp, szczególnie ekonomiczny, dotyczący regionu, rozumianego jako częśc kraju lub większego obszaru geograficznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7070">
<skos:prefLabel>prawo regionalne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7073">
<skos:prefLabel>plan regionalny</skos:prefLabel>
<skos:definition>plan dla regionu opracowany zgodnie z kryteriami fizjograficznymi, biologicznymi, politycznymi, administracyjnymi, ekonomicznymi, demograficznymi i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7075">
<skos:prefLabel>planowanie regionalne</skos:prefLabel>
<skos:definition>metody i procesy prowadzące do określenia, przeanalizowania i wypracowania możliwych kierunków rozwoju regionu, zmierzających do realizacji obecnych i przyszłych celów oraz zaspokojenia wiążących się z nimi potrzeb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7077">
<skos:prefLabel>prawo regionalne</skos:prefLabel>
<skos:definition>regulacje prawne ustanowione przez władze państwowe, instytucje lub organizacje międzynarodowe lub określone konwencją, obowiązujące na terytorium jednego lub kilku państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7079">
<skos:prefLabel>statystyka regionalna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7080">
<skos:prefLabel>struktura regionalna</skos:prefLabel>
<skos:definition>organizacja większego obszaru geograficznego lub części kraju określona według kryteriów administracyjnych, biologicznych, politycznych, ekonomicznych, demograficznych i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7082">
<skos:prefLabel>rejestracja</skos:prefLabel>
<skos:definition>oficjalny wykaz, spis zawierający dane dotyczące ludności, mienia lub spraw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7083">
<skos:prefLabel>rejestrowanie</skos:prefLabel>
<skos:definition>formalne wciąganie danych dotyczących osób, firm lub organizacji do rejestru prowadzonego przez władze, m.in. w celu nabycia pewnych praw, np. znaku handlowego, praw autorskich, zezwolenia i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7084">
<skos:prefLabel>surowiec odnawialny</skos:prefLabel>
<skos:definition>zasoby cechujące się naturalnie stałą dostępnością oraz dostarczające stałych użytków, konsumpcja których nie zagraża przyszłym możliwościom użytkowania, o ile nie zostanie przekroczony próg ich zdolności naturalnego odnawiania się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7086">
<skos:prefLabel>ustawodawstwo określające poziomy dopuszczalne</skos:prefLabel>
<skos:definition>zespół aktów i praw ustanowionych przez rząd lub organizację międzynarodową lub konwencję ustalających dopuszczalne poziomy substancji niebezpiecznych w środowisku lub w substancjach połkniętych, powyżej których może dojść do zagrożenia zdrowia ludzkiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7087">
<skos:prefLabel>rozporządzenie</skos:prefLabel>
<skos:definition>zarządzenie ustanowione przez władzę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7089">
<skos:prefLabel>regulacje prawne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7090">
<skos:prefLabel>kontrola przestrzegania przepisów prawa</skos:prefLabel>
<skos:definition>nadzór władz państwowych w zakresie przestrzegania przepisów prawa i wywiązywania się przez podmioty z obowiązku dostarczania społeczeństwu usług, zgodnie ze statutowymi zobowiązaniami tych podmiotów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7091">
<skos:prefLabel>rehabilitacja</skos:prefLabel>
<skos:definition>podejmowanie działań ochronnych mających na celu poprawę skutków minionego złego użytkowania, naruszającego produkcyjność zasobów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7092">
<skos:prefLabel>przesiedlenie</skos:prefLabel>
<skos:definition>zapewnienie nowego lub zastępczego domu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7093">
<skos:prefLabel>reintrodukcja</skos:prefLabel>
<skos:definition>wprowadzenie na określony teren gatunku(-ów), które wcześniej tam występowały, a z różnych przyczyn wyginęły</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7094">
<skos:prefLabel>związek przyczynowo-skutkowy</skos:prefLabel>
<skos:definition>związek pomiędzy czynnikiem wywołującym (przyczyna) oraz wynikiem, rezultatem lub konsekwencją (skutkiem) wystąpienia tego czynnika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7097">
<skos:prefLabel>uwalnianie organizmów</skos:prefLabel>
<skos:definition>uwalnianie organizmów w środowisku, które mogą wykazać nieznane poprzednio własności patogeniczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7099">
<skos:prefLabel>religia</skos:prefLabel>
<skos:definition>zespół wierzeń dotyczących genezy, struktury i celu istnienia człowieka, ludzkości i świata, wiara w Boga, bóstwa i w nieśmiertelność duszy; związane z nią zachowania oraz formy organizacyjne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7102">
<skos:prefLabel>teledetekcja</skos:prefLabel>
<skos:definition>technika pozyskiwania danych, które są przestrzennie odniesione do powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7103">
<skos:prefLabel>usuwanie</skos:prefLabel>
<skos:definition>eliminacja pewnych substancji z jakiegoś miejsca lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7104">
<skos:prefLabel>renaturyzacja</skos:prefLabel>
<skos:definition>proces przywracania naturalnym ekosystemom lub siedliskom ich pierwotnej struktury i składu gatunkowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7107">
<skos:prefLabel>źródło energii odnawialnej</skos:prefLabel>
<skos:definition>źródło energii nie ulegającej wyczerpaniu jak np. energia wodna, słoneczna, wiatrowa itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7108">
<skos:prefLabel>zasoby odnawialne</skos:prefLabel>
<skos:definition>zasoby mające zdolność stałego odnawiania się lub zastępowania w takich procesach jak np. reprodukcja; wykorzystywane w rolnictwie, leśnictwie, rybołówstwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7110">
<skos:prefLabel>mieszkanie czynszowe</skos:prefLabel>
<skos:definition>mieszkanie zajęte przez najemcę dokonującego regularnych opłat na rzecz właściciela tytułem użytkowania lokalu w celach mieszkalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7112">
<skos:prefLabel>warsztat naprawczy</skos:prefLabel>
<skos:definition>zakład, w którym dokonuje się naprawy całości lub części jakiegoś sprzętu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7114">
<skos:prefLabel>podstawienie</skos:prefLabel>
<skos:definition>zastąpienie jakiegoś atomu lub grupy atomów przez inne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7115">
<skos:prefLabel>koszt zastąpienia</skos:prefLabel>
<skos:definition>kwota potrzebna do zastąpienia jakiegoś elementu przez inny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7116">
<skos:prefLabel>proces sprawozdawczy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7118">
<skos:prefLabel>reprezentowanie</skos:prefLabel>
<skos:definition>działanie podjęte w imieniu jakiejś osoby, grupy, przedsiębiorstwa lub rządu, często pochodzące z wyboru lub nadania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7119">
<skos:prefLabel>regeneracja paliwa jądrowego</skos:prefLabel>
<skos:definition>przywracanie zanieczyszczonego paliwa radiaktywnego do stanu nadającego się do powtórnego zastosowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7120">
<skos:prefLabel>reprodukcja (biologia)</skos:prefLabel>
<skos:definition>procesy związane z rozmnażaniem roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7122">
<skos:prefLabel>ingerencja w proces reprodukcji</skos:prefLabel>
<skos:definition>technika polegająca na wprowadzaniu zmian do genetycznej budowy organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7124">
<skos:prefLabel>gady</skos:prefLabel>
<skos:definition>Reptilia, gromada lądowych kręgowców zmiennocieplnych, oddychających płucami, przeważnie jajorodnych, o skórze pokrytej łuskami, tarczkami lub płytkami; należą tu: hatterie, żółwie, jaszczurki, węże i krokodyle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7126">
<skos:prefLabel>pogotowie ratunkowe</skos:prefLabel>
<skos:definition>służba zorganizowana w celu udzielania natychmiastowej pomocy osobom poszkodowanym i znajdującym się w stanie zgrożenia życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7127">
<skos:prefLabel>badania naukowe</skos:prefLabel>
<skos:definition>badania naukowe mające na celu odkrywanie i stosowanie nowej wiedzy, techniki i praw przyrody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7129">
<skos:prefLabel>ośrodek naukowy</skos:prefLabel>
<skos:definition>miejsce, gdzie odbywają się systematyczne badania związane z działalnością poznawczą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7135">
<skos:prefLabel>projekt naukowy</skos:prefLabel>
<skos:definition>propozycja, plan lub projekt zawierający niezbędne informacje i dane do przeprowadzenia określonego badania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7137">
<skos:prefLabel>rezerwat przyrody</skos:prefLabel>
<skos:definition>obszar lądowy i/lub wodny, przeznaczony do ochrony w celu zachowania pewnych cech przyrodniczych, biologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7138">
<skos:prefLabel>zbiornik zaporowy</skos:prefLabel>
<skos:definition>sztuczny lub nauralny zbiornik wodny, np. jezioro lub staw, wykorzystywany w celach dostawy wody, nawadniania lub kontroli przeciwpowodziowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7143">
<skos:prefLabel>teren mieszkaniowy</skos:prefLabel>
<skos:definition>obszar, na którym znajdują się wyłącznie budynki mieszkalne (prywatne), bez biur i fabryk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7144">
<skos:prefLabel>budynek mieszkalny</skos:prefLabel>
<skos:definition>budynek przeznaczony na cele mieszkalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7146">
<skos:prefLabel>strefa mieszkalna o spowolnionym ruchu</skos:prefLabel>
<skos:definition>obszar mieszkalny, w obrębie którego ulice mają wbudowane progi spowalniające, wymuszające ograniczenie szybkości ruchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7149">
<skos:prefLabel>stan wody gwarantujacy przepływ nienaruszalny</skos:prefLabel>
<skos:definition>ilość wody pozostawiona w cieku po opuszczeniu hydroelektrowni, pozwalająca na utrzymanie wystarczającego poziomu w czasie niżówek, niezbędnego dla przetrwania zespołów organizmów żywych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7155">
<skos:prefLabel>ryzyko dodatkowe</skos:prefLabel>
<skos:definition>potencjalne ryzyko dodatkowego zagrożenia dla człowieka i środowiska po uwzględnieniu wszystkich zabiegów mających na celu ograniczenie zagrożeń przewidywalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7156">
<skos:prefLabel>odpady technologiczne po spalaniu, parowaniu lub innych procesach</skos:prefLabel>
<skos:definition>odpady pochodzące z różnych procesów przetwarzania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7159">
<skos:prefLabel>analiza pozostałości</skos:prefLabel>
<skos:definition>analiza pozostałości środków agrochemicznych stosowanych do ochrony roślin w żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7160">
<skos:prefLabel>recykling pozostałości</skos:prefLabel>
<skos:definition>przywracanie do obiegu materii lub energii odpadowej marnotrawionej w procesach przemysłowych lub podczas innych działań gospodarczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7161">
<skos:prefLabel>żywica</skos:prefLabel>
<skos:definition>produkt organiczny, pochodzenia naturalnego lub syntetycznego, stały lub półstały, bez określonego punktu topnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7163">
<skos:prefLabel>odporność</skos:prefLabel>
<skos:definition>zdolność rośliny do pokonania lub zapobieżenia infekcji lub kolonizacji patogenów, pasożytów itp.; również zdolność owadów, grzybów, chwastów i innych szkodników do przetrwania po przyjęciu śmiertelnej dawki środka owadobójczego, grzybobójczego lub chwastobójczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7165">
<skos:prefLabel>rezolucja</skos:prefLabel>
<skos:definition>zbiorowa uchwała, deklaracja powzięta przez jakieś zgromadzenie, komitet, zjazd, kongres w wyniku obrad</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7167">
<skos:prefLabel>resorpcja</skos:prefLabel>
<skos:definition>proces przenikania substancji przez powierzchnię skóry, błon śluzowych i komórkowych, np. tlenu w płucach, substancji odżywczych w przewodzie pokarmowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7168">
<skos:prefLabel>zasoby</skos:prefLabel>
<skos:definition>komponenty środowiska, kóre mogą być użytkowane przez organizmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7169">
<skos:prefLabel>szacowanie zasobów</skos:prefLabel>
<skos:definition>ocena ilości zasobów na danych obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7170">
<skos:prefLabel>ochrona zasobów</skos:prefLabel>
<skos:definition>redukcja ogólnej konsumpcji i użytkowania zasobów w celu uniknięcia strat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7172">
<skos:prefLabel>eksploatacja zasobów</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7173">
<skos:prefLabel>rezerwa zasobów</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7175">
<skos:prefLabel>gospodarka zasobami</skos:prefLabel>
<skos:definition>zarządzanie zasobami naturalnymi i kulturowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7176">
<skos:prefLabel>oddychanie</skos:prefLabel>
<skos:definition>proces, w czasie którego żywe organizmy pobierają z otoczenia tlen i wydalają dwutlenek węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7178">
<skos:prefLabel>powietrze oddechowe</skos:prefLabel>
<skos:definition>ilość powietrza wdychanego i wydychanego z płuc</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7179">
<skos:prefLabel>choroba układu oddechowego</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7180">
<skos:prefLabel>urządzenia ochrony dróg oddechowych</skos:prefLabel>
<skos:definition>urządzenia służące do ochrony dróg oddechowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7181">
<skos:prefLabel>układ oddechowy</skos:prefLabel>
<skos:definition>zespół narządów człowieka i zwierząt umożliwiający wymianę gazów między organizmem a środowiskiem, np. u kręgowców lądowych - drogi oddechowe i płuca, u zwierząt wodnych - skrzela</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7182">
<skos:prefLabel>drogi oddechowe</skos:prefLabel>
<skos:definition>u człowieka i ssaków: narządy doprowadzające powietrze do płuc - jama nosowa, gardło, krtań, tchawica, oskrzela</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7184">
<skos:prefLabel>odpowiedzialność</skos:prefLabel>
<skos:definition>obowiązek odpowiadania za swoje czyny i ponoszenia za nie konsekwencji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7185">
<skos:prefLabel>przeciwdziałanie negatywnym wpływom</skos:prefLabel>
<skos:definition>przeciwdziałanie lub odwracanie negatywnych skutków w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7187">
<skos:prefLabel>forma spoczynkowa</skos:prefLabel>
<skos:definition>odporna struktura pozwalająca organizmowi na przetrwanie niekorzystnych warunkow środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7188">
<skos:prefLabel>restytucja</skos:prefLabel>
<skos:definition>przywrócenie dawnego, pierwotnego stanu rzeczy, odtworzenie, odnowa (najczęściej dóbr kultury)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7189">
<skos:prefLabel>proces restytucji</skos:prefLabel>
<skos:definition>proces przywracania dawnego, pierwotnego stanu (także równowagi ekologicznej)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7191">
<skos:prefLabel>ograniczenie produkcji</skos:prefLabel>
<skos:definition>decyzja, działanie lub polityka które ograniczają lub utrudniają wytwarzanie towarów lub usług na sprzedaż</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7192">
<skos:prefLabel>ograniczenie użytkowania</skos:prefLabel>
<skos:definition>ograniczenie dotyczące użytkowania ziemi lub jakiejś innej własności, często zapisane w dokumentach dzierżawy lub akcie notarialnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7193">
<skos:prefLabel>handel detaliczny</skos:prefLabel>
<skos:definition>sprzedaż dóbr ostatecznemu konsumentowi, zwykle w małych ilościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7194">
<skos:prefLabel>zbiornik retencyjny</skos:prefLabel>
<skos:definition>sztuczne jezioro utworzone przez spiętrzenie wód rzeki, potoku za pomocą zapory, przechwytujące fale powodziowe, wykorzystywane często do celów energetycznych, żeglugowych, przeciwpowodziowych, melioracyjnych, rekreacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7195">
<skos:prefLabel>modernizacja</skos:prefLabel>
<skos:definition>modernizacja poprzez wprowadzanie nowych elementów, zwłaszcza aparatury pomiaru zanieczyszczeń w pracujących urządzeniach jakiejś fabryki, bez innych większych zmian; również - wprowadzanie nowych (nowocześniejszych) elementów do urządzeń, pojazdów, fabryk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7197">
<skos:prefLabel>powrót do natury</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7198">
<skos:prefLabel>pojemnik wielokrotnego użytku</skos:prefLabel>
<skos:definition>pojemnik zaprojektowany i przeznaczony do wielokrotnego napełniania i użytku w tym samym celu podczas swojej przydatności eksploatacyjnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7201">
<skos:prefLabel>ponowne użytkowanie materiałów</skos:prefLabel>
<skos:definition>ponowne użytkowanie materiałów, przedmiotów i komponentów, w ich oryginalnej formie, np. opakowania szklane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7202">
<skos:prefLabel>wprowadzanie roślinności</skos:prefLabel>
<skos:definition>sadzenie nowych drzew, ze szczególnym uwzględnieniem gatunków rodzimych na obszarach o zdewastowanej roślinności i zaburzonym składzie gatunkowym, w celu stabilizacji gruntu, zagrożonego erozją i wywiewaniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7204">
<skos:prefLabel>osmoza odwrócona</skos:prefLabel>
<skos:definition>metoda otrzymywania czystej wody z wód zasolonych, np. desalinizacja; z zastosowaniem półprzepuszczalnej membrany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7214">
<skos:prefLabel>ryż</skos:prefLabel>
<skos:definition>Oryza, roślina zbożowa z rodziny traw, występująca w wielu gatunkach na obszarach tropikalnych i subtropikalnych; najbardziej znany jest ryż siewny, uprawiany dla celów konsumpcyjnych, służący także do wyrobu alkoholu; słoma używana jako pasza oraz jako surowiec do wyplatania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7217">
<skos:prefLabel>prawo do zgłaszania petycji</skos:prefLabel>
<skos:definition>prawo do zgłaszania odwołań skierowane do władz, zwykle w imieniu zbiorowości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7218">
<skos:prefLabel>prawo własności</skos:prefLabel>
<skos:definition>prawo do rozporządzania rzeczą i korzystania z niej z wyłączeniem innych osób, w granicach określonych przez ustawy i zasady współżycia społecznego; w szczególności prawo do rozporządzania własnością ziemską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7221">
<skos:prefLabel>prawo do odszkodowania</skos:prefLabel>
<skos:definition>prawo do otrzymania kwoty rekompensującej poniesione straty albo doznane krzywdy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7222">
<skos:prefLabel>prawo do informacji</skos:prefLabel>
<skos:definition>prawo jednostki do informacji o publicznie dostępnych bankach danych i innych zasobach informacyjnych, prawo obywateli do otrzymania informacji, które są w posiadaniu odpowiednich władz publicznych lub instytucji, prawo do otrzymania kopii publicznie dostępnych informacji, prawo obywateli do aktualnej wiedzy i danych w zakresie problematyki powszechnie dostępnej; prawo dostępu do informacji o środowisku gwarantuje &quot;Konwencja o dostępie do informacji, udziale społeczeństwa w podejmowaniu decyzji oraz dostępie do sprawiedliwości w sprawach dotyczących środowiska&quot;</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7223">
<skos:prefLabel>obrączkowanie (ptaków)</skos:prefLabel>
<skos:definition>zakładanie ptakom obrączek na nogę z numerami identyfikacyjnymi, w celu śledzenia ich migracji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7227">
<skos:prefLabel>spłukiwanie</skos:prefLabel>
<skos:definition>usuwanie cienkich warstw gleby z powierzchni stoków przez spływające wody opadowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7230">
<skos:prefLabel>strefa nadrzeczna</skos:prefLabel>
<skos:definition>obszar lądowy na brzegu rzeki, którego roślinność i warunki mikroklimatyczne są pod ciągłym oddziaływaniem wód, a gleby wykazują stałe uwilgotnienie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7231">
<skos:prefLabel>wznoszenie (geologiczne ruchy wznoszące)</skos:prefLabel>
<skos:definition>pionowa powolna niestabilność skorupy ziemskiej, polegająca na ruchach wznoszących (i opadających)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7232">
<skos:prefLabel>podnoszenie się poziomu mórz</skos:prefLabel>
<skos:definition>podnoszenie się poziomu mórz jako konsekwencja ocieplenia globalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7233">
<skos:prefLabel>ryzyko</skos:prefLabel>
<skos:definition>prawdopodobieństwo wystąpienia szkód i strat, związanych z jakimś wydarzeniem, np. kataklizmem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7235">
<skos:prefLabel>analiza ryzyka</skos:prefLabel>
<skos:definition>określenie prawdopodobieństwa wystąpienia zjawisk niebezpiecznych, np. wybuchu toksycznego gazu i ich następstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7236">
<skos:prefLabel>ocena ryzyka</skos:prefLabel>
<skos:definition>jakościowa i ilościowa ocena określająca ryzyko, np. wystąpienia zanieczyszczenia, na jakie jest wystawione ludzkie zdrowie oraz środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7237">
<skos:prefLabel>analiza ryzyka i korzyści</skos:prefLabel>
<skos:definition>systematyczny proces określania niebezpieczeństwa w stosunku do możliwości osiągnięcia korzyści finansowawych lub zysku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7238">
<skos:prefLabel>wymiana informacji o zagrożeniu</skos:prefLabel>
<skos:definition>wymiana informacji o zagrożeniach zdrowia lub środowiska, pomiędzy ekspertami oceniającymi ryzyko a administracją, społeczeństwem, mediami, grupami interesu itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7240">
<skos:prefLabel>percepcja ryzyka</skos:prefLabel>
<skos:definition>subiektywne odczucie ryzyka, zwykle bez związku ze statystycznym prawodpodobieństwem wystąpienia szkody lub nieszczęścia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7241">
<skos:prefLabel>zmniejszanie ryzyka</skos:prefLabel>
<skos:definition>działanie lub proces obniżające prowadopodobieństwo wystąpienia ryzyka na danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7244">
<skos:prefLabel>rzeka</skos:prefLabel>
<skos:definition>masa wody wypływającej ze źródeł lub biorącej swój początek ze strumieni górskich, potoków itp., płynąca w sposób ciągły lub okresowy w wyraźnym łożysku (korycie), opadającym w określonym kierunku do ujścia (morza, jeziora lub innej rzeki)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7251">
<skos:prefLabel>zagospodarowanie zlewni rzeki</skos:prefLabel>
<skos:definition>zmiany spowodowane zagospodarowaniem obszarów odwadnianych przez daną rzekę i jej dopływy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7257">
<skos:prefLabel>regulowanie rzek</skos:prefLabel>
<skos:definition>zmiany naturalnego biegu rzeki poprzez pogłębianie, umacnienie brzegów itp. w celu przyśpieszenia przepływu wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7266">
<skos:prefLabel>zanieczyszczenie rzek</skos:prefLabel>
<skos:definition>bezpośrednie lub pośrednie zaburzanie naturalnych własności (fizycznych i chemicznych) wód rzecznych lub ekosystemów rzecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7269">
<skos:prefLabel>woda rzeczna</skos:prefLabel>
<skos:definition>woda płynąca w korycie rzeki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7272">
<skos:prefLabel>droga</skos:prefLabel>
<skos:definition>wydzielony, zwykle utwardzony pas ziemi łączący poszczególne miejscowości lub punkty terenu, przystosowany do komunikacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7276">
<skos:prefLabel>budowa dróg</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7279">
<skos:prefLabel>utrzymanie dróg</skos:prefLabel>
<skos:definition>konserwacja ulic, dróg i autostrad wraz z poprawą nawierzchni i oznakowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7282">
<skos:prefLabel>bezpieczeństwo ruchu drogowego</skos:prefLabel>
<skos:definition>działania mające na celu redukcję wystąpienia ryzyka związanego z kolizjami pojazdów poruszających się po drogach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7283">
<skos:prefLabel>sól do posypywania dróg</skos:prefLabel>
<skos:definition>sól służąca do posypywania dróg w celu uniknięcia oblodzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7293">
<skos:prefLabel>ruch drogowy</skos:prefLabel>
<skos:definition>poruszanie się pojazdów i osób po drogach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7295">
<skos:prefLabel>inżynieria ruchu drogowego</skos:prefLabel>
<skos:definition>dyscyplina obejmująca projektowanie dróg i ciągów pieszych wraz z badaniem ruchu drogowego i aspektów środowiskowych związanych z transportem towarów i osób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7296">
<skos:prefLabel>transport drogowy</skos:prefLabel>
<skos:definition>transport towarów i osób pojazdami poruszającymi się po drogach kołowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7298">
<skos:prefLabel>skała</skos:prefLabel>
<skos:definition>naturalny zespół minerałów tworzący w skorupie ziemskiej większe masy, powstały w wyniku określonych procesów geologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7302">
<skos:prefLabel>wełna mineralna</skos:prefLabel>
<skos:definition>ogólny termin oznaczający włókna wytwarzane ze stopionych skał lub szkła, stosowane jako materiał izolacyjny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7306">
<skos:prefLabel>gryzonie</skos:prefLabel>
<skos:definition>drobne ssaki łożyskowe należące do rzędu Rodentia, charakteryzujące się stałym wzrostem siekaczy, zębów przystosowanych do zgryzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7312">
<skos:prefLabel>korzeń</skos:prefLabel>
<skos:definition>bezlistny organ roślin naczyniowych, rosnący zwykle pod powierzchnią ziemi; służy roślinie do pobierania wody i soli mineralnych oraz umocowuje ją w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7314">
<skos:prefLabel>piec rotacyjny</skos:prefLabel>
<skos:definition>piec do obróbki cieplnej, w którym przedmiot obrabiany obracany jest wokół osi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7319">
<skos:prefLabel>droga</skos:prefLabel>
<skos:definition>wyznaczony szlak komunikacyjny, służący do przejazdu i podróży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7320">
<skos:prefLabel>planowanie sieci dróg</skos:prefLabel>
<skos:definition>projektowanie przebiegu i organizowanie budowy dróg, autostrad i ulic</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7324">
<skos:prefLabel>guma</skos:prefLabel>
<skos:definition>produkt wulkanizacji kauczuku naturalnego lub syntetycznego, tworzywo odznaczające się dużą elastycznością o właściwościach zależnych od ilości i rodzaju składników; stosowana m.in. do wyrobu opon, sprężystego łączenia elementów maszyn, impregnacji tkanin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7326">
<skos:prefLabel>przetwórstwo gumy</skos:prefLabel>
<skos:definition>zespół działań polegających na przerabianiu substancji pochodzącej z drzew kauczukowych, utwardzaniu i obróbce chemicznej w celu nadania elastyczności, wytrzymałości i innych pożądanych cech</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7327">
<skos:prefLabel>przemysł gumowy</skos:prefLabel>
<skos:definition>gałąź gospodarki skupiająca przedsiębiorstwa zajmujące się produkcją i sprzedażą wyrobów gumowych z gumy naturalnej i syntetycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7328">
<skos:prefLabel>odpady gumowe</skos:prefLabel>
<skos:definition>odpady sztucznej lub naturalnej gumy, często również produkty uboczne przetwórstwa gumy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7340">
<skos:prefLabel>odpływ powierzchniowy</skos:prefLabel>
<skos:definition>tempo spływania wody z powierzchni gleby, uzależnione od takich czynników jak tekstura gleby, nachylenie stoku, warunki klimatyczne, a także pokrycie terenu, np. trawa, las, nawierzchnia utwardzona, odkryta gleba</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7345">
<skos:prefLabel>tereny wiejskie</skos:prefLabel>
<skos:definition>obszar poza granicami miasta, osiedla, wsi lub innego terenu zabudowanego: mieszkalnego, handlowego, przemysłowego, usługowego itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7348">
<skos:prefLabel>środowisko wiejskie</skos:prefLabel>
<skos:definition>środowisko charakteryzujące obszary wiejskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7352">
<skos:prefLabel>ludność mieszkająca na wsi</skos:prefLabel>
<skos:definition>całkowita liczba ludności zamieszkującej obszary wiejskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7353">
<skos:prefLabel>osadnictwo wiejskie</skos:prefLabel>
<skos:definition>zabudowa mieszkalna na terenach wiejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7354">
<skos:prefLabel>agroturystyka</skos:prefLabel>
<skos:definition>wypoczynek w gospodarstwach wiejskich, gdzie zwykle posiłki są przygotowywane z naturalnych miejscowych produktów a goście spędzają czas poznając czynności gospodarskie, miejscowe wyroby oraz aktywnie wypoczywają</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7355">
<skos:prefLabel>zaopatrzenie wsi w wodę</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7362">
<skos:prefLabel>bezpieczeństwo</skos:prefLabel>
<skos:definition>poczucie braku zagrożenia, spokoju, pewności; często jest wynikiem zaplanowanych przygotowań i podjętych środków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7363">
<skos:prefLabel>analiza bepieczeństwa</skos:prefLabel>
<skos:definition>proces badania potrzeb lub skutecznośći działań, procedur lub urządzeń w celu zmiejszenia wystąpienia ryzyka lub zagrożenia osób, mienia, środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7364">
<skos:prefLabel>środki bezpieczeństwa</skos:prefLabel>
<skos:definition>działania, procedury lub urządzenia projektowane w celu zmniejszenia wystąpienia ryzyka lub zagrożenia osób, mienia, środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7366">
<skos:prefLabel>zasada bezpieczeństwa</skos:prefLabel>
<skos:definition>zasada lub przepis regulujący działania, procedury lub urządzenia zaprojektowane w celu zmniejszenia  ryzyka wystąpienia uszczerbku, straty lub zagrożenia osób, mienia lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7367">
<skos:prefLabel>norma bezpieczeństwa</skos:prefLabel>
<skos:definition>norma lub zasada stosowana w postępowaniu prawnym w celu zmniejszenia ryzyka niebezpieczeństwa dla ludzi, mienia, środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7369">
<skos:prefLabel>norma bezpieczeństwa dla budynków</skos:prefLabel>
<skos:definition>zbiór zasad i zarządzeń przyjętych przez władze, dotyczących standardów mechanicznych i strukturalnych w zakresie bezpieczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7370">
<skos:prefLabel>studium bezpieczeństwa</skos:prefLabel>
<skos:definition>badanie i szczegółowe sprawdzanie oraz (zwykle) pisemny raport dotyczący potrzeb lub skutecznośći działań, procedur lub urządzeń w celu zmiejszenia wystąpienia lub ryzyka pojawienia się urazu, straty lub zagrożenia osób, mienia lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7373">
<skos:prefLabel>płazy ogoniaste</skos:prefLabel>
<skos:definition>rząd płazów (Urodela) mających wydłużone ciało, krótkie nogi i charakterystyczny spłaszczony lub walcowaty ogon, jak np. salamandra, występujaca w Europie Środkowej i Południowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7376">
<skos:prefLabel>zasolenie</skos:prefLabel>
<skos:definition>nagromadzenie rozpuszczalnych soli powstałe w wyniku odparowania wód w glebach stref suchych i słabo odwadnianych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7384">
<skos:prefLabel>salmonella</skos:prefLabel>
<skos:definition>pałeczkowate bakterie, przeważnie orzęsione, żyjące w przewodzie pokarmowym zwierząt i ludzi; liczne gatunki wywołują choroby zwierząt i człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7390">
<skos:prefLabel>zawartość soli</skos:prefLabel>
<skos:definition>ilość soli zawarta w roztworze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7395">
<skos:prefLabel>ładunek soli</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7396">
<skos:prefLabel>słone bagno</skos:prefLabel>
<skos:definition>obszar słonej, płytkiej wody, zwykle występujący w strefach wybrzeży i w deltach, a w klimacie zwrotnikowym - również z dala od mórz; jako ekosystem odznacza się dużą wrażliwością i podatnością na zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7397">
<skos:prefLabel>słona łąka</skos:prefLabel>
<skos:definition>łąka zalewana przez słone wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7400">
<skos:prefLabel>wysad solny</skos:prefLabel>
<skos:definition>masa soli wciśnięta w nadległe warstwy skał osadowych w postaci diapiru, czyli wielkiego stwardniałego czopa, powstałego w wyniku wzajemnego nacisku plastycznych materiałów skalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7401">
<skos:prefLabel>sól</skos:prefLabel>
<skos:definition>produkt reakcji, w wyniku której wodór w kwasie jest zastępowany przez metal</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7402">
<skos:prefLabel>woda słona</skos:prefLabel>
<skos:definition>woda morska charakteryzująca się wysokim zasoleniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7404">
<skos:prefLabel>ratownictwo morskie</skos:prefLabel>
<skos:definition>działanie, proces lub przedsiębiorstwo zajmujące się ratownictwem statków lub ich ładunków z katastrof morskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7407">
<skos:prefLabel>pobór próbek</skos:prefLabel>
<skos:definition>pobieranie do analizy niewielkiej reprezentatywnej ilości danego materiału</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7410">
<skos:prefLabel>metoda poboru prób</skos:prefLabel>
<skos:definition>metoda losowego doboru pewnych obiektów z całości w ten sposób, aby były one reprezentatywne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7411">
<skos:prefLabel>sankcje</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7413">
<skos:prefLabel>piasek</skos:prefLabel>
<skos:definition>luźny materiał składający się z małych ziaren mineralnych i skalnych, rozróżnialnych gołym okiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7419">
<skos:prefLabel>utrwalanie wydm</skos:prefLabel>
<skos:definition>utrwalanie wydm poprzez sadzenie traw, takich jak piaskownica zwyczajna (Ammophila arenaria), której długie korzenie przerastają warstwy piasku i w ten sposób przeciwdziałąją ich przemieszczaniu przez wiatr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7420">
<skos:prefLabel>wydma</skos:prefLabel>
<skos:definition>piaszczyste wzniesienie o asymetrycznych stokach, usypane przez wiatr; występująca zwykle w strefie wybrzeży, również nad jeziorami i rzekami lub w regionach pustynnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7423">
<skos:prefLabel>eksploatacja piasku</skos:prefLabel>
<skos:definition>wydobycie piasku na potrzeby budownictwa oraz w celu pozyskania pewnych minerałów, takich jak rutyl i cyrkon</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7425">
<skos:prefLabel>kopalnia piasku</skos:prefLabel>
<skos:definition>miejsce wydobywania piasku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7436">
<skos:prefLabel>sanitariat</skos:prefLabel>
<skos:definition>zestaw urządzeń przeznaczonych do higieny osobistej i usuwania odpadów organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7438">
<skos:prefLabel>składowisko zabezpieczone sanitarnie</skos:prefLabel>
<skos:definition>inżynieryjna metoda usuwania odpadów stałych na lądzie w taki sposób, żeby nie zagrażać środowisku, poprzez rozpryskiwanie cienkich warstw, zgniatanie do możliwie najmniejszych objętości i przykrywanie ubitą glebą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7443">
<skos:prefLabel>sanitacja</skos:prefLabel>
<skos:definition>badania i działania praktyczne w zakresie ochrony zdrowia publicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7444">
<skos:prefLabel>plan sanitacji</skos:prefLabel>
<skos:definition>plan dotyczący kontroli czynników fizycznych w środowisku człowieka, które mogą zaburzyć rozwój, zdrowie lub przetrwanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7449">
<skos:prefLabel>wskaźnik saprobowości</skos:prefLabel>
<skos:definition>wskaźnik lub miara poziomu zanieczyszczeń organicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7450">
<skos:prefLabel>organizm saprobowy</skos:prefLabel>
<skos:definition>odnoszący się do klasyfikacji organizmów pod względem ich tolerancji na zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7452">
<skos:prefLabel>satelita</skos:prefLabel>
<skos:definition>obiekt, który krąży wokół innego większego obiektu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7457">
<skos:prefLabel>oszczędności</skos:prefLabel>
<skos:definition>część bieżących przychodów, która nie jest przeznaczona na utrzymanie lub konsumpcję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7458">
<skos:prefLabel>trociny</skos:prefLabel>
<skos:definition>drobiny drewna powstałe w wyniku piłowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7464">
<skos:prefLabel>schistosomiasis</skos:prefLabel>
<skos:definition>choroba ludzi powodowana przez przywry (Schistosoma mansonii, S.haematobium i S.japonicum) pasożytujące w naczyniach krwionośnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7465">
<skos:prefLabel>szkoła</skos:prefLabel>
<skos:definition>instytucja lub budynek, w którym uczą się dzieci i młodzież</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7470">
<skos:prefLabel>nauczanie szkolne</skos:prefLabel>
<skos:definition>proces edukacji odbywający się w placówkach szkolnych, dotyczący szczególnie osób w wieku przedmaturalnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7472">
<skos:prefLabel>nauka</skos:prefLabel>
<skos:definition>badanie świata materialnego za pomocą powtarzalnych obserwacji, pomiarów i eksperymentów w celu ustalenia, zweryfikowania i zmodyfikowania powszechnie obowiązujących praw wyjaśniających jego działanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7477">
<skos:prefLabel>informacja techniczna</skos:prefLabel>
<skos:definition>przekazywana wiedza dotycząca systematycznego badania świata materialnego lub mechaniki i przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7478">
<skos:prefLabel>współpraca naukowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7480">
<skos:prefLabel>polityka dotycząca badań naukowych</skos:prefLabel>
<skos:definition>szereg działań związanych ze wspieraniem lub określaniem kierunków badawczych i eksperymentalnych podejmowanych przez państwo, przemysł lub inne organizacje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7482">
<skos:prefLabel>procedura ustalania zakresu</skos:prefLabel>
<skos:definition>zalecany w procedurze ocen oddziaływania na środowisko sposób postępowania, obejmujący publiczną dyskusję na temat zakresu badań i analiz, które powinny zostać przeprowadzone, wariantów, które powinny być wzięte pod uwagę oraz innych zagadnień istotnych z punktu widzenia ochrony środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7483">
<skos:prefLabel>złom</skos:prefLabel>
<skos:definition>materiał nadający się do powtórnego przetwarzania, pochodzący z procesu produkcyjnego lub odpadów z produktów gotowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7484">
<skos:prefLabel>handel złomem</skos:prefLabel>
<skos:definition>handel materiałami złomowanymi lub porzuconymi, które mogą być jeszcze w jakikolwiek sposób użyte</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7485">
<skos:prefLabel>cena złomu</skos:prefLabel>
<skos:definition>suma pieniędzy lub kwota za jaką materiały złomowane mogą być nabywane lub zbywane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7486">
<skos:prefLabel>złom metalowy</skos:prefLabel>
<skos:definition>metal nadający się do powtórnego przetwarzania, odrzucony w czasie procesu produkcyjnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7489">
<skos:prefLabel>pojazd przeznaczony na złom</skos:prefLabel>
<skos:definition>samochód zepsuty lub porzucony z powodu zużycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7491">
<skos:prefLabel>ekranowanie</skos:prefLabel>
<skos:definition>zabezpieczenie urządzenia elektrycznego (zwłaszcza telekomunikacyjnego, elektronicznego, pomiarowego) przed oddziaływaniem postronnych pól elektrycznych lub magnetycznych za pomocą metalowych osłon — ekranów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7495">
<skos:prefLabel>morze</skos:prefLabel>
<skos:definition>1) przeciwieństwo lądów lub całość wód oceanicznych; 2) wielki zamknięty obszar wodny, zbiornik słonej wody, mniejszy niż ocean</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7497">
<skos:prefLabel>dno morskie</skos:prefLabel>
<skos:definition>prawie pozioma powierzchnia podłoża mórz i oceanów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7498">
<skos:prefLabel>eksploatacja dna morskiego</skos:prefLabel>
<skos:definition>wydobycie zasobów minerałów z dna mórz, np. spod szelfu kontynentalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7500">
<skos:prefLabel>wydobycie z dna morza</skos:prefLabel>
<skos:definition>eksploatacja złóż minerałów z powierchni lub spod powierzchni dna morza lub oceanu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7502">
<skos:prefLabel>krążenie wód w oceanach</skos:prefLabel>
<skos:definition>wielkoskalowe ruchy poziome mas wodnych w oceanach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7513">
<skos:prefLabel>poziom morza</skos:prefLabel>
<skos:definition>poziom powierzchni wód w morzach i oceanach, określany jako średnia pomiędzy poziomami przypływu i odpływu, używana w odniesieniu do wysokości lub głębokości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7514">
<skos:prefLabel>podniesienie poziomu mórz</skos:prefLabel>
<skos:definition>podniesienie poziomu mórz jako prawdopodobna konsekwencja globalnego ocieplenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7516">
<skos:prefLabel>uszczelnianie</skos:prefLabel>
<skos:definition>kitowanie, zaklejanie, uszczelnianie przed wodą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7519">
<skos:prefLabel>uszczelnienie</skos:prefLabel>
<skos:definition>przyrząd lub system tworzący szczelne połączenie pomiędzy dwoma elementami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7520">
<skos:prefLabel>odprowadzanie ścieków do morza</skos:prefLabel>
<skos:definition>punkt, miejsce lub struktura, gdzie ścieki odprowadzane są do morza lub oceanu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7525">
<skos:prefLabel>zasoby mórz</skos:prefLabel>
<skos:definition>zasoby mórz, takie jak żywność, energia i minerały</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7528">
<skos:prefLabel>wybrzeże morskie</skos:prefLabel>
<skos:definition>strefa nieustabilizowanego obszaru, rozciągająca się pomiędzy linią odpływu a strefą umocnionego brzegu, tj. granicą odmiennej formy fizjograficznej lub linią stałej roślinności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7529">
<skos:prefLabel>pora roku</skos:prefLabel>
<skos:definition>okresy klimatyczne związane z obiegowym ruchem Ziemi i nachyleniem jej osi ku Słońcu: wiosna, lato, jesień, zima</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7531">
<skos:prefLabel>migracje sezonowe</skos:prefLabel>
<skos:definition>okresowe ruchy ludności z jednego regionu lub strefy klimatycznej to innego, zgodnie z rocznym cyklem zmian pogodowych i temperaturowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7535">
<skos:prefLabel>woda morska</skos:prefLabel>
<skos:definition>wodny roztwór soli w mniej więcej stałych proporcjach, uzależnionych m.in. od żyjących organizmów, osadów lądowych i zachodzących reakcji chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7536">
<skos:prefLabel>odsalanie wody morskiej</skos:prefLabel>
<skos:definition>usuwanie soli z wody morskiej lub zasolonej wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7538">
<skos:prefLabel>ochrona wód morskich</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7541">
<skos:prefLabel>biotop zastępczy</skos:prefLabel>
<skos:definition>biotop, który można tworzyć w przypadku zniszczenia biotopu pierwotnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7542">
<skos:prefLabel>edukacja na poziomie średnim</skos:prefLabel>
<skos:definition>lata nauki w okresie pomiędzy szkołą podstawową a studiami wyższymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7548">
<skos:prefLabel>sektor przemysłowy</skos:prefLabel>
<skos:definition>część gospodarki narodowej lub regionalnej, która wytwarza produkty bez większego bezpośredniego korzystania z zasobow naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7551">
<skos:prefLabel>rzecz używana</skos:prefLabel>
<skos:definition>dobra lub produkty, które były już pierwotnie używane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7554">
<skos:prefLabel>sektor gospodarki</skos:prefLabel>
<skos:definition>sektor gospodarki określany według źródła powstania (handel, przemysł, finanse) lub według rodzaju wytwarzanego produktu (rolnictwo, wytwórczość, usługi)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7557">
<skos:prefLabel>bezpieczeństwo instalacji</skos:prefLabel>
<skos:definition>stosowane środki, techniki lub rozwiązania chroniące przed niebezpieczeństwem lub ograniczające dostęp do aparatury, urządzeń lub konstrukcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7560">
<skos:prefLabel>basen sedymentacyjny</skos:prefLabel>
<skos:definition>zagłębiona forma ukształtowania ziemi, taka jak dna oceaniczne czy doliny ryftowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7562">
<skos:prefLabel>osadzanie się</skos:prefLabel>
<skos:definition>oddzielanie się nierozpuszczalnych stałych cząstek od cieczy, w której są zawieszone, w wyniku oddziaływania grawitacji lub odwirowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7563">
<skos:prefLabel>sedymentacja</skos:prefLabel>
<skos:definition>proces formowania się lub nagromadzania się osadów w formie warstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7565">
<skos:prefLabel>osad</skos:prefLabel>
<skos:definition>materiał transportowany przez wodę, który ostatecznie opada na dno, gdy woda traci siłę transportową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7566">
<skos:prefLabel>przenoszenie osadów</skos:prefLabel>
<skos:definition>ruch i wynoszenie osadów przez naturalne czynniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7567">
<skos:prefLabel>nasienie (biologia)</skos:prefLabel>
<skos:definition>dojrzała, zapłodniona roślinna komórka jajowa z zarodkiem i otoczającymi go substancjami zapasowymi, okryta łupiną nasienną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7568">
<skos:prefLabel>zaprawy nasion</skos:prefLabel>
<skos:definition>substancja chemiczna stosowana przed wysiewem na nasiona i sadzonki, chroniąca przed chorobami i insektami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7571">
<skos:prefLabel>przesiąkanie wody</skos:prefLabel>
<skos:definition>powolny ruch wody poprzez niewielkie puste przestrzenie w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7574">
<skos:prefLabel>woda przesiąkająca</skos:prefLabel>
<skos:definition>woda powolnie przedostająca się poprzez niewielkie przestrzenie porowatego materiału, takiego jak gleba</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7575">
<skos:prefLabel>aktywność sejsmiczna</skos:prefLabel>
<skos:definition>zjawisko ruchów skorupy ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7578">
<skos:prefLabel>monitoring sejsmograficzny</skos:prefLabel>
<skos:definition>gromadzenie danych sejsmogarficznych dotyczących danego regionu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7579">
<skos:prefLabel>morskie fale sejsmiczne</skos:prefLabel>
<skos:definition>wielkie fale (nawet do 15 m) wywołane przez zjawiska sejsmiczne, mogące wyrządzic znaczne szkody w regionach nadmorskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7581">
<skos:prefLabel>zajęcie własności</skos:prefLabel>
<skos:definition>oficjalne lub prawne przejęcie własności lub posiadłości, często z pogwałceniem prawa lub w wyniku egzekucji wyroku sądowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7582">
<skos:prefLabel>zajęcie zysków</skos:prefLabel>
<skos:definition>oficjalne lub prawne odebranie pieniędzy lub zysków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7584">
<skos:prefLabel>wybór technologii</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7586">
<skos:prefLabel>selektywna hodowla zwierząt</skos:prefLabel>
<skos:definition>hodowla zwierząt posiadających pożądane cechy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7588">
<skos:prefLabel>selektywna hodowla roślin</skos:prefLabel>
<skos:definition>hodowla roślin posiadających pożądane cechy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7590">
<skos:prefLabel>selen</skos:prefLabel>
<skos:definition>bardzo toksyczny pierwiastek niemetaliczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7592">
<skos:prefLabel>program samopomocy</skos:prefLabel>
<skos:definition>system usług lub działań ukierunkowanych na pomaganie osobom indywidualnym i ich samokształcenie, tak aby były niezależne od pomocy innych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7593">
<skos:prefLabel>samokontrola</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7594">
<skos:prefLabel>samooczyszczanie</skos:prefLabel>
<skos:definition>naturalny proces degradacji materii organicznej, w wyniku którego powstają składniki odżywcze potrzebne organizmom samożywnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7595">
<skos:prefLabel>ekosystem stepowy</skos:prefLabel>
<skos:definition>ekosystem charakterystyczny dla obszarów o znikomych opadach, które wystarczają do wytwożenia się roślinności trawiastej i zakrzewień, ale nie lasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7597">
<skos:prefLabel>półprzewodnik</skos:prefLabel>
<skos:definition>niemetaliczne przewodniki elektronowe będące ciałami stałymi, których opór właściwy jest znacznie większy niż przewodników, a znacznie mniejszy niż nieprzewodników; mają zastosowanie w produkcji różnego rodzaju przyrządów, np. tranzystorów, układów scalonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7599">
<skos:prefLabel>półprodukt</skos:prefLabel>
<skos:definition>produkt przejściowy otrzymany w wyniku procesu produkcyjnego i przeznaczony do przerobu w innych procesach produkcyjnych; produkt, który przeszedł częściową obróbkę i jest wykorzystywany jako surowiec w następnej fazie produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7600">
<skos:prefLabel>półmetal</skos:prefLabel>
<skos:definition>pierwiastki o cechach częściowo metali, a częściowo niemetali, wykazujące w reakcjach chemicznych tendencję zarówno do oddawania, jak i do pobierania elektronów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7603">
<skos:prefLabel>obszar wrażliwy</skos:prefLabel>
<skos:definition>obszary, kóre wymagają specjalnej ochrony swoich naturalnych siedlisk; charakteryzują się dużą wrażliwością na degradację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7604">
<skos:prefLabel>analiza wrażliwości</skos:prefLabel>
<skos:definition>sformalizowana procedura identyfikowania zagrożeń w związku ze zmianami komponentów modelu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7609">
<skos:prefLabel>selektywna zbiórka</skos:prefLabel>
<skos:definition>zbieranie pojedynczych sztuk odpadów stałych, pochodzących z różnych źródeł, zwykle oddzielanych do osobnych pojemników, w celu ponownego przerobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7611">
<skos:prefLabel>segregacja u źródła</skos:prefLabel>
<skos:definition>segregacja różnych odpadów w miejscu ich wytwarzania, w celu ułatwiania efetywnego recyklingu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7612">
<skos:prefLabel>separator</skos:prefLabel>
<skos:definition>urządzenie oddzielające materiały o różnej gęstości własnej za pomocą wody lub powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7613">
<skos:prefLabel>szambo</skos:prefLabel>
<skos:definition>zbiornik, zwykle podziemny, do którego wpływają zanieczyszczenia i są częściowo lub całkowicie rozkładane w warunkach beztlenowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7616">
<skos:prefLabel>zajęcie mienia</skos:prefLabel>
<skos:definition>określenie prawne, odnoszące się do przejęcia i zamknięcia przez sąd pewnej własności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7617">
<skos:prefLabel>wąż</skos:prefLabel>
<skos:definition>gady z podrzędu węże (Ophidia) o pokrytym łuskami, walcowatym ciele, bez kończyn posiadające zrośnięte powieki i zmodyfikowane szczęki umożliwiające połykanie dużych ofiar; m.in. jadowite formy, takie jak kobry i grzechotniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7618">
<skos:prefLabel>strefa usług</skos:prefLabel>
<skos:definition>obszar przeznaczony do użytku publicznego, związany np. ze szkołą, biblioteką, parkiem itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7621">
<skos:prefLabel>usługi</skos:prefLabel>
<skos:definition>wykonywnie pracy, na którą jest stałe zapotrzebowanie społeczne, poprzez zaangażowanie siły roboczej i narzędzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7622">
<skos:prefLabel>koncentracja osadnictwa</skos:prefLabel>
<skos:definition>ogół skupionych na pewnym obszarze miejscowości, wsi i domów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7625">
<skos:prefLabel>niekontrolowany rozwój miast</skos:prefLabel>
<skos:definition>rozrastanie się wielkich miast poprzez ekspansję osadnictwa na okoliczne tereny wiejskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7627">
<skos:prefLabel>osadnik</skos:prefLabel>
<skos:definition>zbiornik osadów, do którego wprowadza się dwufazową mieszaninę, a cząstki stałe osadzają się grawitacyjnie w czasie składowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7628">
<skos:prefLabel>ścieki</skos:prefLabel>
<skos:definition>odpady ciekłe, zawierające w roztworze lub zawiesinie materiał zwierzęcy, roślinny, glebowy lub chemiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7629">
<skos:prefLabel>odprowadzanie ścieków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7630">
<skos:prefLabel>terenowa oczyszczalnia ścieków</skos:prefLabel>
<skos:definition>obszar, na którym ścieki lub inna woda odpadowa jest rozlewana w celu oczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7632">
<skos:prefLabel>osad ściekowy</skos:prefLabel>
<skos:definition>półpłynny odpad z zawartością powyżej 2500 cząstek na milion, uzyskiwany przy oczyszczaniu miejskich ścieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7636">
<skos:prefLabel>system oczyszczania ścieków</skos:prefLabel>
<skos:definition>dwustopniowe oczyszczanie ścieków; oddzielanie części stałych od wody, która podlega dalszemu oczyszczaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7641">
<skos:prefLabel>system kanalizacyjny</skos:prefLabel>
<skos:definition>system rur, zwykle podziemnych, odprowadzających z domów i budynków zużytą wodę i ścieki bytowe do miejsca, gdzie można się ich bezpiecznie pozbyć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7646">
<skos:prefLabel>skorupowce</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7651">
<skos:prefLabel>osłona</skos:prefLabel>
<skos:definition>bariera przeznaczona do odgrodzenia ludzi od substancji niebezpiecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7652">
<skos:prefLabel>rolnictwo z odłogowaniem</skos:prefLabel>
<skos:definition>praktyka rolnicza polegająca na zmianie pól a nie upraw, kiedy to krótsze okresy uprawne są przedzielone długimi okresami odłogowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7654">
<skos:prefLabel>statek</skos:prefLabel>
<skos:definition>jednostka pływająca o napędzie spalinowym, parowym, elektrycznym, atomowym, mogąca się poruszać na wodzie, pod wodą lub ponad powierzchnią wody (poduszkowiec), przeznaczona do komunikacji wodnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7655">
<skos:prefLabel>budownictwo okrętowe</skos:prefLabel>
<skos:definition>sztuka lub przemysł projektowania i budowania statków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7659">
<skos:prefLabel>wypadek morski</skos:prefLabel>
<skos:definition>nieoczekiwany wypadek lub strata dotycząca statku lub jego ładuku w czasie transportu, który stwarza potencjalne zagrożenie dla ludzi, mienia lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7662">
<skos:prefLabel>odpady ze statku</skos:prefLabel>
<skos:definition>odpady bytowe lub funkcjonalne, zrzucane ze statku w sposób ciągły lub okresowy, wytwarzane w czasie zwykłego funkcjonowania statku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7663">
<skos:prefLabel>zrzut odpadów ze statków do morza</skos:prefLabel>
<skos:definition>zrzucanie odpadów ze statku do wód morskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7668">
<skos:prefLabel>strzelnica</skos:prefLabel>
<skos:definition>wydzielony teren z odpowiednimi urządzeniami, przeznaczony do ćwiczeń w strzelaniu z broni palnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7669">
<skos:prefLabel>sklep</skos:prefLabel>
<skos:definition>lokal, pomieszczenie, w którym prowadzi się sprzedaż detaliczną towarów i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7670">
<skos:prefLabel>centrum handlowe</skos:prefLabel>
<skos:definition>zamknięty obszar, w obrębie którego znajdują się różne sklepy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7679">
<skos:prefLabel>pokaz</skos:prefLabel>
<skos:definition>występ, program lub wystawa oferujące rozrywkę grupie ludzi; oglądane na żywo, np. w teatrze lub muzeum, albo poprzez media, np. radio lub telewizja</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7680">
<skos:prefLabel>rozdrabniacz</skos:prefLabel>
<skos:definition>maszyna rozdrabniająca, która rozrywa lub miele materiał na drobniejsze i bardziej jednorodne cząstki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7681">
<skos:prefLabel>rozdrabniacz odpadów</skos:prefLabel>
<skos:definition>maszyna stosowana do rozdrabniania odpadów na mniejsze kawałki poprzez rozrywanie lub zgniatanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7683">
<skos:prefLabel>krzew</skos:prefLabel>
<skos:definition>roślina drzewiasta nie tworząca wyraźnego pnia ani korony, lecz rozgałęziająca się na wiele równorzędnych pędów przeważnie przy ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7684">
<skos:prefLabel>plac manewrowy</skos:prefLabel>
<skos:definition>obszar, na którym samochód lub pociąg może być przetaczany, obracany lub przemieszczany z jednego toru na drugi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7686">
<skos:prefLabel>zespół objawów chorobowych w wysokich budynkach</skos:prefLabel>
<skos:definition>zespół objawów, takich jak zmęczenie, ból głowy, podrażnienie oczu i zawroty głowy, dolegające pracownikom w nowoczesnych, wysokich biurowcach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7688">
<skos:prefLabel>skutek uboczny</skos:prefLabel>
<skos:definition>dodatkowe, zwykle niepożądane, szkodliwe działanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7689">
<skos:prefLabel>skutek uboczny leków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7690">
<skos:prefLabel>przesiewanie</skos:prefLabel>
<skos:definition>rozdzielenie wg rozmiaru stałych cząstek na zestawie sit o malejącym rozmiarze, wyrażone stosunkiem wagowym (%)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7692">
<skos:prefLabel>tłumik</skos:prefLabel>
<skos:definition>urządzenie przeznaczone do redukowania hałasu, zwłaszcza w rurach wydechowych pojazdów mechanicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7694">
<skos:prefLabel>krzem</skos:prefLabel>
<skos:definition>(Si) pierwiastek chemiczny z grupy węglowców, liczba atomowa 14, substancja krystaliczna (półprzewodnik) lub bezpostaciowa, mająca zastosowanie w przyrządach półprzewodnikowych oraz używana do produkcji stopów o dużej wytrzymałości mechanicznej i odporności na korozjękruchy pierwiastek metaliczny występujący w dwóch formach allotropowych;  w piasku, kwarcu, granicie, skaleniu i glinie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7697">
<skos:prefLabel>silos</skos:prefLabel>
<skos:definition>wielka okrągła wieża służąca w gospodarstwie do przechowywania ziarna lub paszy na zimę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7698">
<skos:prefLabel>muł</skos:prefLabel>
<skos:definition>bardzo drobny materiał mineralny, powstały w wyniku erozji skał i osadzony przez rzeki i jeziora</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7699">
<skos:prefLabel>srebro</skos:prefLabel>
<skos:definition>ciągliwy kowalny połyskliwy szarawobiały pierwiastek mający wśród metali najwyższe własności przewodnictwa elektrycznego i cieplnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7702">
<skos:prefLabel>symulacja</skos:prefLabel>
<skos:definition>przedstawienie problemu lub sytuacji we wzorach matematycznych, szczególnie za pomocą komputera</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7707">
<skos:prefLabel>spiekanie</skos:prefLabel>
<skos:definition>ogrzanie rozdrobnionej, sproszkowanej substancji, np. sproszkowanego metalu prawie do temperatury topnienia, bez doprowadzania jej do stanu ciekłego, celem sklejenia poszczególnych ziaren w porowatą masę; stosowane najczęściej w metalurgii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7709">
<skos:prefLabel>wybór miejsca</skos:prefLabel>
<skos:definition>proces wybierania miejsca lub lokalizacji dla jakiegoś określonego celu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7712">
<skos:prefLabel>wymiarowanie</skos:prefLabel>
<skos:definition>ustalanie wymiarów przekrojów elementów konstrukcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7715">
<skos:prefLabel>narciarstwo</skos:prefLabel>
<skos:definition>jazda po śniegu na nartach, przede wszystkim dla sportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7716">
<skos:prefLabel>skóra</skos:prefLabel>
<skos:definition>zewnętrzna powłoka ciała kręgowców, pełniąca funkcję ochronną, biorąca udział w termoregulacji, oddychaniu, wydalaniu (pot) i gospodarce wodnej; zawiera receptory wrażliwe na dotyk, ucisk, ciepło, zimno, ból oraz barwnik melaninę, który decyduje o jej zabarwieniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7718">
<skos:prefLabel>zakłócenie linii horyzontu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7720">
<skos:prefLabel>żużel</skos:prefLabel>
<skos:definition>produkt uboczny otrzymywany w przebiegających w wysokich temperaturach procesach metalurgicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7724">
<skos:prefLabel>rzeźnia</skos:prefLabel>
<skos:definition>zakład produkcyjny, w którym dokonuje się uboju zwierząt rzeźnych, patroszenia ich, dzielenia tuszy mięsnej itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7725">
<skos:prefLabel>odpady z rzeźni</skos:prefLabel>
<skos:definition>odcięte części, pozostale po rozbiorze tusz zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7726">
<skos:prefLabel>ubój zwierząt</skos:prefLabel>
<skos:definition>zabijanie zwierząt rzeźnych dla celów konsumpcyjnych w sposób uregulowany przez przepisy prawne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7729">
<skos:prefLabel>sen</skos:prefLabel>
<skos:definition>okresowy stan odpoczynku polegający na zwolnieniu czynności fizjologicznych organizmu oraz czasowym zaniku świadomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7730">
<skos:prefLabel>zaburzenia snu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7731">
<skos:prefLabel>ściana wykopu</skos:prefLabel>
<skos:definition>opadająca powierzchnia wykopu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7733">
<skos:prefLabel>osady ściekowe</skos:prefLabel>
<skos:definition>miękki, mazisty, mulisty osad denny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7737">
<skos:prefLabel>fermentacja osadów ściekowych</skos:prefLabel>
<skos:definition>przetwarzanie stabilizujące osady ściekowe w procesach tlenowych lub beztlenowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7738">
<skos:prefLabel>spopielanie osadów ściekowych</skos:prefLabel>
<skos:definition>metoda mająca na celu wysuszenie i zmniejszenie masy osadów ściekowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7739">
<skos:prefLabel>odstojnik</skos:prefLabel>
<skos:definition>zbiornik do usuwania strąconych części stałych ze ścieków, przez który przepuszcza się ścieki w oczyszczalniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7740">
<skos:prefLabel>oczyszczanie osadów ściekowych</skos:prefLabel>
<skos:definition>przetwarzanie osadów ściekowych w celu ich unieszkodliwienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7742">
<skos:prefLabel>śluza</skos:prefLabel>
<skos:definition>pionowa, opuszczana brama lub zastawka regulująca przepływ wody w kanale lub śluzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7747">
<skos:prefLabel>mały i średni przemysł</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7752">
<skos:prefLabel>mała wyspa (geografia polityczna)</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7755">
<skos:prefLabel>piecyk małoskalowy</skos:prefLabel>
<skos:definition>niewielkie zamknięte urządzenie posiadające źródło ciepła, najczęściej używane do intensywnego ogrzewania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7756">
<skos:prefLabel>induktor małoskalowy</skos:prefLabel>
<skos:definition>domowe źródło wprowadzające niewielką ilość zanieczyszczeń do publicznej sieci kanalizacyjnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7759">
<skos:prefLabel>smog</skos:prefLabel>
<skos:definition>gęsta mgła unosząca cząsteczki dymu i spalin, charakterystyczna dla wielkich miast</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7760">
<skos:prefLabel>ostrzeżenie przed smogiem</skos:prefLabel>
<skos:definition>działanie, urządzenie lub ogłoszenie ostrzegające lub wykrywające poziom zanieczyszczeń w powietrzu na pewnym obszarze, głównie utleniaczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7761">
<skos:prefLabel>dym</skos:prefLabel>
<skos:definition>lotne produkty spalania unoszące się w powietrzu; aerozol składający się z widocznych cząstek i gazów, wytworzony z niecałkowitego spalania materiałów zawierających węgiel, takich jak drewno i paliwa kopalane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7763">
<skos:prefLabel>zapobieganie powstawaniu dymu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7765">
<skos:prefLabel>palenie</skos:prefLabel>
<skos:definition>wdychanie i wydychanie rakotwórczych dymów z palących się materiałów roślinnych, głównie tytoniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7768">
<skos:prefLabel>węże z rodziny Colubridae</skos:prefLabel>
<skos:definition>gady lądowe, do których należy wiele niegroźnych węży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7769">
<skos:prefLabel>śnieg</skos:prefLabel>
<skos:definition>najpopularniejsza forma zamarzniętego opadu, najczęściej w postaci płatków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7771">
<skos:prefLabel>lawina śnieżna</skos:prefLabel>
<skos:definition>wielka masa śniegu, kamieni lub lodu staczająca się gwałtownie ze zboczy górskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7774">
<skos:prefLabel>nasiąkanie</skos:prefLabel>
<skos:definition>absorpcja cieczy przez ciało stałe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7776">
<skos:prefLabel>mydło</skos:prefLabel>
<skos:definition>środek czyszczący, wytwarzany w kostkach, granulkach, płatkach albo w płynie, produkowany z mieszniny soli sodowych różnych kwasów tłuszczowych naturalnych olejów i tłuszczów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7781">
<skos:prefLabel>zachowania społeczne</skos:prefLabel>
<skos:definition>typowy sposób zachowania i działania jednostek i społeczeństwa w stosunku do innych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7784">
<skos:prefLabel>warunki społeczne</skos:prefLabel>
<skos:definition>istniejące warunki, sytuacja lub stan wpływające na życie, dobrobyt i relacje ludzi w społeczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7787">
<skos:prefLabel>koszt społeczny</skos:prefLabel>
<skos:definition>zapłacona cena lub poniesiona strata w grupie, w danej społeczności lub ogólnie w społeczeństwie, związana ze zdobyciem, wypracowaniem lub utrzymaniem jakiegoś celu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7788">
<skos:prefLabel>internalizacja kosztów zewnętrznych</skos:prefLabel>
<skos:definition>proces uznawania możliwych negatywnych skutków w środowisku jako istotnych uwarunkowań przyszłych decyzji ekonomicznych, podejmowanych przez producentów dóbr i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7790">
<skos:prefLabel>rozwój społeczny</skos:prefLabel>
<skos:definition>wieloetapowy proces przemian społecznych, przebiegających w określonym kierunku, których kolejność nie jest dowolna, lecz wykazuje prawidłowość wynikającą z wewnętrznych praw procesu historycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7791">
<skos:prefLabel>zróżnicowanie społeczne</skos:prefLabel>
<skos:definition>koncepcja związana z ewolucyjną teorią historii i funkcjonalizmem strukturalnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7793">
<skos:prefLabel>dynamika społeczna</skos:prefLabel>
<skos:definition>wzory, zmiany, bodźce oraz rozwój grupy ludzi, społeczności lub społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7795">
<skos:prefLabel>infrastruktura społeczna</skos:prefLabel>
<skos:definition>struktura zaprojektowana, zbudowana lub zainstalowana w celu zapewnienia przestrzeni do życia lub kontaktów w społeczństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7796">
<skos:prefLabel>grupa społeczna</skos:prefLabel>
<skos:definition>zbiór osób, które oddziałują wzajemnie na siebie i podzielają pewne poczucie jedności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7798">
<skos:prefLabel>wskaźniki społeczne</skos:prefLabel>
<skos:definition>łatwe do zidentyfikowania, zmienne w czasie cechy społeczeństwa, które można zmierzyć, traktowane jako wskaźniki głębszych aspektów rzeczywistości społecznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7803">
<skos:prefLabel>medycyna społeczna</skos:prefLabel>
<skos:definition>medycyna stosowana dotyczaca leczenia chorób pojawiających się w pewnych grupach społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7804">
<skos:prefLabel>zachowania pro-społeczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7805">
<skos:prefLabel>ruchy społeczne</skos:prefLabel>
<skos:definition>zorganizowany wysiłek znacznej grupy ludzi w celu zmiany lub przeciwdziałania zmianom istotnych zjawisk społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7807">
<skos:prefLabel>udział społeczny</skos:prefLabel>
<skos:definition>wspólne działanie będące udziałem znacznej liczby członków społeczności lub ogólnie społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7809">
<skos:prefLabel>polityka społeczna</skos:prefLabel>
<skos:definition>zespół działań podejmowanych przez rząd, przedsiębiorstwo lub organizację, które zmierzają do zapewnienia wszystkim ludziom właściwych warunków pracy i życia poprzez takie instrumenty jak ubezpieczenia społeczne, opieka medyczna itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7810">
<skos:prefLabel>problem społeczny</skos:prefLabel>
<skos:definition>ogólny termin stosowany dla określenia zespołu warunków i zachowań, które uważa się za manifestowanie rozprzężenia dyscypliny społecznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7811">
<skos:prefLabel>proces społeczny</skos:prefLabel>
<skos:definition>stałe działanie, operacja lub seria zmian zachodzących w określony sposób i dotyczący życia, dobrobytu i relacji w społeczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7813">
<skos:prefLabel>pomoc społeczna</skos:prefLabel>
<skos:definition>wsparcie publiczne, zwłaszcza w formie finansowej dla osób znajdujących się w szczególnej potrzebie lub trudnościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7815">
<skos:prefLabel>bezpieczeństwo społeczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7816">
<skos:prefLabel>służba społeczna</skos:prefLabel>
<skos:definition>działalność opieki społecznej zorganizowana przez państwo lub władze lokalne i realizowana przez wykwalifikowany personel</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7817">
<skos:prefLabel>struktura społeczna</skos:prefLabel>
<skos:definition>określenie stosowane ogólnie w odniesieniu do wzorca zachowań społecznych lub wzajemnych relacji pomiędzy różnymi elementami systemu społecznego lub społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7818">
<skos:prefLabel>badania społeczne</skos:prefLabel>
<skos:definition>zbieranie danych poprzez wywiady i ankiety w celu zgromadzenia określonego zbioru danych, które następnie podlegają analizie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7819">
<skos:prefLabel>system społeczny</skos:prefLabel>
<skos:definition>system odzwierciedlający typowe relacje zachodzące pomiędzy jego elementami, czyli w poszczególnych częściach społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7821">
<skos:prefLabel>wskaźniki społeczne</skos:prefLabel>
<skos:definition>odnośnie wskaźników społecznych, często rozróżnia się wartości, które są silne, prawie stałe i głęboko zakorzenione oraz nastawienia, które są najczęściej płytkimi, słabo zakorzenionymi i bardzo zmiennymi poglądami i opiniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7822">
<skos:prefLabel>dobrobyt społeczny</skos:prefLabel>
<skos:definition>wysoki poziom, dostatek lub komfort społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7823">
<skos:prefLabel>społeczeństwo</skos:prefLabel>
<skos:definition>znaczna grupa ludzi, zróżnicowana, związana wspólnymi interesami i charakteryzująca się pewnymi hierarchicznymi relacjami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7825">
<skos:prefLabel>społeczno-gospodarcze aspekt osadnictwa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7826">
<skos:prefLabel>czynnik społeczno-gospodarczy</skos:prefLabel>
<skos:definition>podstawowy element w strukturze społecznej, organizacji lub zachowaniu, który łączy wymiar finansowy z dynamiką interpersonalną lub międzygrupową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7827">
<skos:prefLabel>społeczno-gospodarcze oddziaływanie biotechnologii</skos:prefLabel>
<skos:definition>biotechnologia, jako zastosowanie rozwiązań biologicznych i technologicznych, często odnosi się do przemysłowego użytkowania mikroorganizmów (być może genetycznie zmienionych) w celu przeprowadzenia przetwórczych procesów chemicznych, którym poddawane są n</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7830">
<skos:prefLabel>badanie socjologiczne</skos:prefLabel>
<skos:definition>badanie odnoszące się do zagadnień i problemów społecznych, szczególnie w odniesieniu do kultury i środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7831">
<skos:prefLabel>socjologia</skos:prefLabel>
<skos:definition>nauka o społeczeństwie, o strukturze i prawach rozwoju społeczeństwa, o formach i przejawach życia oraz współżycia grup społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7832">
<skos:prefLabel>aspekt socjopolityczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7839">
<skos:prefLabel>zmiękczanie</skos:prefLabel>
<skos:definition>zmniejszanie twardości wody poprzez usuwanie jonów wpływających na jej twardość (głównie wapń i magnez)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7840">
<skos:prefLabel>środek zmiękczający</skos:prefLabel>
<skos:definition>1)substancja dodawana do innej substancji w celu zwiększenia jej miękkości i plastyczności; 2)substancja taka jak zeolit dodawana dla zmiękczenia wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7842">
<skos:prefLabel>oprogramowanie</skos:prefLabel>
<skos:definition>zbiór programów umożliwiających regularną eksploatację użytkową komputerów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7843">
<skos:prefLabel>gleba</skos:prefLabel>
<skos:definition>biologicznie czynna powierzchniowa warstwa skorupy ziemskiej, powstała ze skały macierzystej w procesie glebotwórczym, żyzna (dzięki zawartości próchnicy, mineralnych składników pokarmowych roślin i innych); stanowi podłoże życia roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7844">
<skos:prefLabel>zakwaszanie gleb</skos:prefLabel>
<skos:definition>naturalny proces występujący w klimacie wilgotnym; długotrwałe badania wykazują związek z kwaśnymi opadami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7845">
<skos:prefLabel>powietrze glebowe</skos:prefLabel>
<skos:definition>powietrze i inne gazy znajdujące się w przestrzeni międzyglebowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7846">
<skos:prefLabel>analiza glebowa</skos:prefLabel>
<skos:definition>stosowanie szybkich metod analizy chemicznej w celu określenia żyzności gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7847">
<skos:prefLabel>biologia gleb</skos:prefLabel>
<skos:definition>badanie żywych organizmów, głównie mikroorganizmów i mikroskopijnych bezkręgowców, które żyją w glebie i są w dużej mierze odpowiedzialne za procesy rozkładu niezbędne dla żyzności gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7848">
<skos:prefLabel>żyzność gleb</skos:prefLabel>
<skos:definition>przydatność gleby do różnych celów użytkowych, takich jak uprawa roślin żywieniowych i pastewnych, zależna od głębokości gleby, tekstury, zasobności w minerały, zasolenia, zakwaszenia itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7849">
<skos:prefLabel>chemizm gleb</skos:prefLabel>
<skos:definition>badanie nieorganicznych i organicznych komponentów gleby i jej procesów życiowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7850">
<skos:prefLabel>zagęszczanie gleby</skos:prefLabel>
<skos:definition>wzrost gęstości (masy na jednostkę objętości) oraz zmniejszenie się porowatości gleby w wyniku oddziaływania ciężaru, wibracji lub nacisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7851">
<skos:prefLabel>warunki glebowe</skos:prefLabel>
<skos:definition>opis charakteru powierzchni gruntu w danym czasie obserwacji, szczególnie w odniesieniu do oddziaływania deszczu i śniegu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7852">
<skos:prefLabel>ochrona gleb</skos:prefLabel>
<skos:definition>zabiegi mające na celu przeciwdziałanie lub zmniejszenie erozji glebowej, powodowanej przez wodę i wiatr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7853">
<skos:prefLabel>prawodawstwo dotyczące ochrony gleb</skos:prefLabel>
<skos:definition>proces powstawania aktów prawodawczych dotyczących ochrony i przeciwdziałania utracie wierzchniej warstwy rozdrobnionych skał i materii organicznej, cennej ze względu na swoje wartości odżywcze i zdolność podtrzymania życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7855">
<skos:prefLabel>uszkodzenia gleb</skos:prefLabel>
<skos:definition>gleba uszkodzona w wyniku działalności człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7856">
<skos:prefLabel>odkażanie gleb</skos:prefLabel>
<skos:definition>techniki stosowane przy usuwaniu polichlorowanych bifenyli i wielkopierścieniowych węglowodorów aromatycznych (WWA), pestycydów i ogólnie części organicznych poprzez zabiegi fizyczne, chemiczne i biologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7857">
<skos:prefLabel>degradacja gleb</skos:prefLabel>
<skos:definition>wyniszczenie gleby, które może być wynikiem fizycznego usunięcia cząstek glebowych z konkretnego miejsca lub zubożenie elementów rozpuszczalnych w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7858">
<skos:prefLabel>erozja gleb</skos:prefLabel>
<skos:definition>odrywanie się i ruch powierzchniowej warstwy gleby lub materiału glebowego z górnej warstwy profilu, w wyniku działania wiatru lub płynącej wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7860">
<skos:prefLabel>żyzność gleb</skos:prefLabel>
<skos:definition>określenie gleby w odniesieniu do zasobności i dostępności dla roślin elementów niezbędnych do ich wzrostu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7861">
<skos:prefLabel>nawożenie gleb</skos:prefLabel>
<skos:definition>stosowanie do gleby materiałów organiczncyh lub nieorganicznych pochodzenia naturalnego lub syntetycznego w celu wzbogacenia jej w jeden lub wiele elementów niezbędnch do wzrostu roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7862">
<skos:prefLabel>proces glebotwórczy</skos:prefLabel>
<skos:definition>kombinacja procesów naturalnych tworzących glebę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7863">
<skos:prefLabel>poprawa jakości gleby</skos:prefLabel>
<skos:definition>proces ochrony gleby przed nadmierną erozją i użyźnianie gleby, wraz ze wzrostem jej produkcyjności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7866">
<skos:prefLabel>warstwa glebowa</skos:prefLabel>
<skos:definition>odróżnialna kolejna warstwa gleby, wytworzona w czasie procesów przemieszczania się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7867">
<skos:prefLabel>mechaniczne obciążenie gleby</skos:prefLabel>
<skos:definition>termin stosowany do określenia wzrastającej masy obciążającej powierzchnię gruntu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7868">
<skos:prefLabel>mapa glebowa</skos:prefLabel>
<skos:definition>dwuwymiarowe zobrazowanie zasięgu lub rozmieszczenia gleb w stosunku do innych cech powierzchni ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7871">
<skos:prefLabel>mechanika gleb</skos:prefLabel>
<skos:definition>badanie fizycznych właściwości gleb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7873">
<skos:prefLabel>mineralogia gleb</skos:prefLabel>
<skos:definition>badanie tworzenia się, występowania, właściwości, składu i klasyfikacji minerałów występujących w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7874">
<skos:prefLabel>wilgotność glebowa</skos:prefLabel>
<skos:definition>woda zawarta w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7875">
<skos:prefLabel>glebowa gospodarka wodna</skos:prefLabel>
<skos:definition>reżim wodny w glebie, uzależniony od jej fizycznych własności i sposobu ułożenia cząstek glebowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7876">
<skos:prefLabel>organizm glebowy</skos:prefLabel>
<skos:definition>organizm żyjący w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7878">
<skos:prefLabel>zanieczyszczenia glebowe</skos:prefLabel>
<skos:definition>substancje stałe, ciekłe i gazowe, które w sposób szkodliwy zabużają naturalny stan gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7880">
<skos:prefLabel>zanieczyszczenie gleb</skos:prefLabel>
<skos:definition>zmiana cech glebowych lub, bardziej ogólnie, jej chemicznej i biologicznej równowagi, spowodowana oddziaływaniem substancji zanieczyszczających</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7881">
<skos:prefLabel>proces glebowy</skos:prefLabel>
<skos:definition>najważniejszymi procesami zachodzącymi w glebie są pozyskiwanie, straty i przemieszczanie się materii organicznej, soli rozpuszczalnych, węglanów itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7882">
<skos:prefLabel>profil glebowy</skos:prefLabel>
<skos:definition>pionowy przekrój gleby, odsłaniający poszczególne warstwy (horyzony) i skałę macieżystą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7884">
<skos:prefLabel>jakość gleb</skos:prefLabel>
<skos:definition>całość pozytywnych i negatywnych właściwości gleby w odniesieniu do przydatności gleby i jej funkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7886">
<skos:prefLabel>zasoby gleb</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7889">
<skos:prefLabel>zasolenie gleb</skos:prefLabel>
<skos:definition>nagromadzenie rozpuszczalnych soli mineralnych blisko powierzchni gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7890">
<skos:prefLabel>gleboznawstwo</skos:prefLabel>
<skos:definition>badanie właściwości, występowania i gospodarowania glebami jako zasobami naturalnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7892">
<skos:prefLabel>osiadanie gleb</skos:prefLabel>
<skos:definition>ścisłe upakowanie poszczególnych ziaren poprzez eliminację porów - przestrzeni pomiędy ziarnami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7894">
<skos:prefLabel>stabilizacja gleb</skos:prefLabel>
<skos:definition>oddziaływanie chemiczne lub fizyczne zwiększające lub utrzymujące stabilność gleb lub poprawiające ich właściwości techniczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7895">
<skos:prefLabel>struktura gleb</skos:prefLabel>
<skos:definition>kombinacja lub agregacja pierwotnych cząstek glebowych w grupki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7896">
<skos:prefLabel>zapadanie się gleb</skos:prefLabel>
<skos:definition>zapadanie się części skorupy ziemskiej, głównie w wyniku prowadzenia robót podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7897">
<skos:prefLabel>uszczelnienie powierzchni gruntu</skos:prefLabel>
<skos:definition>działalność lub proces, w wyniku którego powierzchnia gruntu jest upakowana lub uszczelniona w celu przeciwdziałania przesączaniu lub przepuszczaniu cieczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7898">
<skos:prefLabel>uziarnienie gleby</skos:prefLabel>
<skos:definition>określa wzajemne proporcje różnych frakcji (piasku, iłu i gliny) w całkowitej masie gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7899">
<skos:prefLabel>typ gleby</skos:prefLabel>
<skos:definition>faza lub wydzielenie serii glebowej w oparciu o uziarnienie prób powierzchniowej warstwy gleb do głębokości przynajmniej połowy głębokości warstwy ornej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7900">
<skos:prefLabel>użytkowanie ziemi</skos:prefLabel>
<skos:definition>funkcjonalne wykorzystanie gleb w rolnictwie, przemyśle lub budownictwie mieszkalnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7902">
<skos:prefLabel>woda glebowa</skos:prefLabel>
<skos:definition>woda zawarta w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7904">
<skos:prefLabel>bateria słoneczna</skos:prefLabel>
<skos:definition>urządzenie przetwarzające światło słoneczne na energię elektryczną poprzez stosowanie półprzewodników czułych na zjawisko fotowoltaiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7905">
<skos:prefLabel>kolektor słoneczny</skos:prefLabel>
<skos:definition>urządzenie przetwarzające światło słoneczne na energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7906">
<skos:prefLabel>energia słoneczna</skos:prefLabel>
<skos:definition>energia przesyłana ze słońca w formie promieniowania elektromagnetycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7907">
<skos:prefLabel>technologia wykorzystania energii słonecznej</skos:prefLabel>
<skos:definition>wykorzystywanie energii słonecznej poprzez przetwarzanie promieniowania słońca na ciepło</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7908">
<skos:prefLabel>ogrzewanie energią słoneczną</skos:prefLabel>
<skos:definition>mieszkaniowy lub przemysłowy system grzewczy korzystający bezpośrednio z energii słonecznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7910">
<skos:prefLabel>elektrownia słoneczna</skos:prefLabel>
<skos:definition>elektrownia, w której promieniowanie słoneczne wykorzystuje się do produkcji energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7912">
<skos:prefLabel>promieniowanie słoneczne</skos:prefLabel>
<skos:definition>promieniowanie elektromagnetyczne i cząsteczki wysyłane przez słońce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7918">
<skos:prefLabel>ciało stałe</skos:prefLabel>
<skos:definition>substancja krystaliczna, tj. taka, w której atomy usytuowane są w trójwymiarowej sieci, okresowo w trzech niezależnych kierunkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7919">
<skos:prefLabel>stan stały</skos:prefLabel>
<skos:definition>fizyczny stan materii, w którym cząsteczki, atomy i jony nie wykazują ruchu postępowego, choć wibrują w pozycjach statycznych zajmowanych przez nie w sieci krystalicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7920">
<skos:prefLabel>odpady stałe</skos:prefLabel>
<skos:definition>stałe materiały odpadowe, które mogą pochodzić z rolnictwa, przemysłu, górnictwa, mieszkalnictwa itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7921">
<skos:prefLabel>usuwanie odpadów stałych</skos:prefLabel>
<skos:definition>uporządkowane usuwanie, zbieranie i przetwarzanie niepotrzebnych materiałów i odpadów stałych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7923">
<skos:prefLabel>rozpuszczalność</skos:prefLabel>
<skos:definition>zdolność substancji do tworzenia roztworu z inną substancją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7925">
<skos:prefLabel>rozpuszczalnik</skos:prefLabel>
<skos:definition>ciekła substancja zdolna do rozpuszczania innej substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7926">
<skos:prefLabel>odzyskiwanie rozpuszczalników</skos:prefLabel>
<skos:definition>odzyskiwanie rozpuszczalników jako forma recyklingu, w celu ich oczyszczenia i ponownego wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7927">
<skos:prefLabel>ptak śpiewający</skos:prefLabel>
<skos:definition>ptaki wróblowe z podrzędu Oscines, posiadające wysoce rozwinięte organy wokalne i charakterystyczne melodyjne głosy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7928">
<skos:prefLabel>hałas powstający przy przekraczaniu bariery dźwięku</skos:prefLabel>
<skos:definition>hałas spowodowany falą uderzeniową generowaną przez samolot lub inny obiekt lecący z szybkością dźwięku lub większą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7929">
<skos:prefLabel>sadza</skos:prefLabel>
<skos:definition>zanieczyszczony węgiel łącznie ze związkami oleistymi powstający przy niecałkowitym spalaniu substancji żywicznych, olejów, drewna lub węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7930">
<skos:prefLabel>sorpcja</skos:prefLabel>
<skos:definition>wchłanianie, zwykle gazu lub cieczy przez inną substancję określaną jako absorbent; np. przy pomocy absorpcji na odpowiednim rozpuszczalniku można usunąć zanieczyszczenie powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7933">
<skos:prefLabel>dźwięk</skos:prefLabel>
<skos:definition>wrażenie słuchowe wytworzone przez drgania, nacisk, ciśnienie, przemieszczenie cząsteczek i prędkość cząsteczek w ośrodku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7936">
<skos:prefLabel>emisja dźwięku</skos:prefLabel>
<skos:definition>rozproszenie w środowisku dźwięku wysyłanego z pewnego źródła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7937">
<skos:prefLabel>imisja dźwięku</skos:prefLabel>
<skos:definition>wprowadzenie do środowiska hałasu pochodzącego z różnych źródeł (np. komunkacji, przemysłu itp.)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7938">
<skos:prefLabel>poziom dźwięku</skos:prefLabel>
<skos:definition>poziom ciśnienia dźwięku (w decybelach) w określonym punkcie pola dźwiękowego, uśredniony po słyszalnym zakresie częstotliwości w określonym przedziale czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7939">
<skos:prefLabel>pomiary dźwięku</skos:prefLabel>
<skos:definition>pomiary logarytmiczne stosowane do określania natężenia dźwięku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7941">
<skos:prefLabel>izolacja akustyczna</skos:prefLabel>
<skos:definition>zmniejszanie lub eliminacja pogłosu w pomieszczeniu poprzez stosowanie dźwiękochłonnych wykładzin ściennych i sufitowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7942">
<skos:prefLabel>rozchodzenie się dźwięku</skos:prefLabel>
<skos:definition>przemieszczanie się fal akustycznych w atmosferze z prędkością niezależną od ich amplitudy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7945">
<skos:prefLabel>transmisja dźwięku</skos:prefLabel>
<skos:definition>przechodzenie fal dźwiękowych przez ośrodek lub kilka ośrodków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7947">
<skos:prefLabel>Ameryka Południowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7948">
<skos:prefLabel>Ocean Atlantycki Południowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7949">
<skos:prefLabel>Azja Południowo-Wschodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7950">
<skos:prefLabel>Afryka Południowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7951">
<skos:prefLabel>Azja Południowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7952">
<skos:prefLabel>Ocean Spokojny Południowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7962">
<skos:prefLabel>przestrzeń (międzyplanetarna)</skos:prefLabel>
<skos:definition>przestrzeń rozciągająca się pomiędzy Słońcem a innymi planetami układu słonecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7969">
<skos:prefLabel>transport kosmiczny</skos:prefLabel>
<skos:definition>transport za pomocą pojazdów przeznaczonych do działania w przestrzeni kosmicznej poza atmosferą ziemską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7970">
<skos:prefLabel>podróż kosmiczna</skos:prefLabel>
<skos:definition>podróż w przestrzeni poza atmosferą ziemską odbywana w celach naukowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7972">
<skos:prefLabel>odpady kosmiczne</skos:prefLabel>
<skos:definition>bezużyteczne pozostałości bytowania ludzi pozostawione na licznych orbitach okołoziemskich w wyniku badania i korzystania ze środowiska poza atmosferą ziemską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7973">
<skos:prefLabel>spazmatyczny kaszel</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7974">
<skos:prefLabel>mobilność przestrzenna</skos:prefLabel>
<skos:definition>współczynnik przemieszczania się lub migracji obserwowanych w danej populacji w danym czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7975">
<skos:prefLabel>zezwolenie specjalne</skos:prefLabel>
<skos:definition>wyjątkowe upoważnienie lub pozwolenie, albo akt prawny zezwalający na wydatkowanie pieniędzy na programy rządowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7976">
<skos:prefLabel>specjalizacja</skos:prefLabel>
<skos:definition>ewolucyjne przystosowanie się do szczególnego sposobu życia lub siedliska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7978">
<skos:prefLabel>ustawodawstwo specjalne</skos:prefLabel>
<skos:definition>prawo ograniczone co do zakresu działania, obowiązywania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7981">
<skos:prefLabel>odpady specjalne</skos:prefLabel>
<skos:definition>odpady, kóre należy traktować w specjalny sposób i do których odnoszą się specjalne przepisy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7982">
<skos:prefLabel>gatunek</skos:prefLabel>
<skos:definition>kategoria taksonomiczna poniżej rodzaju, obejmująca osobniki blisko spokrewnione, morfologicznie podobne, dające płodne potomstwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7983">
<skos:prefLabel>ochrona gatunków</skos:prefLabel>
<skos:definition>kontrolowane użytkowanie, ochrona lub rozwój wybranych klas roślin i zwierząt ze względu na ich bogactwo, różnorodność biologiczną i korzyści dla ludzkości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7984">
<skos:prefLabel>progam ochrony gatunków</skos:prefLabel>
<skos:definition>zorganizowane działania i procedury, zwykle prowadzone przez agencje rządowe lub organizacje pozarządowe, w celu zachowania i ochrony organizmów żywych, które są zagrożone wyginięciem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7985">
<skos:prefLabel>zubożenie gatunkowe</skos:prefLabel>
<skos:definition>utrata gatunków z powodu takich czynników, jak zmiany klimatyczne lub przypadki losowe, np. długotrwałe susze, katastrofy przyrodnicze, pojawienie się nowych drapieżników lub mutacje genowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7986">
<skos:prefLabel>reintrodukcja gatunków</skos:prefLabel>
<skos:definition>wprowadzanie gatunków dzikich roślin i zwierząt do ich naturalnego siedliska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7992">
<skos:prefLabel>spektroskopia</skos:prefLabel>
<skos:definition>dział fizyki zajmujący się badaniem budowy i właściwości cząsteczek, atomów i jąder atomowych oraz oddziaływań wzajemnych atomów i cząsteczek, na podstawie emitowanego lub pochłanianego przez nie promieniowania elektromagnetycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7994">
<skos:prefLabel>prędkość</skos:prefLabel>
<skos:definition>miara ruchu ciała, wyrażona odległością pokonaną w jednostce czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7996">
<skos:prefLabel>ograniczenie prędkości</skos:prefLabel>
<skos:definition>maksymalna dozwolona prędkość z jaką pojazdy mogą poruszać się na określonej drodze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/7999">
<skos:prefLabel>pająk</skos:prefLabel>
<skos:definition>Araneus, pajęczak z rzędu o tej samej nazwie mający na odwłoku kądziołki przędne, wydzielające pajęczynę, w szczękoczułkach znajduje się gruczoł jadowy; znanych jest kilkadziesiąt tysięcy gatunków; żyje głównie na lądzie, drapieżny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8000">
<skos:prefLabel>wyciek</skos:prefLabel>
<skos:definition>niekontrolowany wypływ lub przeciekanie cieczy lub substancji ciekłych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8004">
<skos:prefLabel>zwałowisko</skos:prefLabel>
<skos:definition>miejsce składowania odpadów i minerałów odpadowych wydobytych z kopalni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8006">
<skos:prefLabel>gąbki</skos:prefLabel>
<skos:definition>Porifera (Spongiaria), typ prymitywnych osiadłych zwierząt wielokomórkowych, nie mających wyodrębnionych tkanek i narządów, obejmujący około pięciu tysięcy gatunków różnej wielkości i ubarwienia, żyjących najczęściej w koloniach na dnie mórz i mniej licznie w wodach słodkich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8008">
<skos:prefLabel>sport</skos:prefLabel>
<skos:definition>ćwiczenia i gry służące podnoszeniu i nabywaniu sprawności fizycznej i wyrabianiu pewnych cech charakteru, jak wytrwałość, lojalność, nawyk przestrzegania reguł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8014">
<skos:prefLabel>urządzenia sportowe</skos:prefLabel>
<skos:definition>budynki, konstrukcje, instalacje, urządzone miejsca i sprzęt do uprawiania sportów na wolnym powietrzu i w hali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8019">
<skos:prefLabel>puszka z aerozolem</skos:prefLabel>
<skos:definition>puszka z aerozolem do rozpylania farby, dezodorantu itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8020">
<skos:prefLabel>aerozol azbestu</skos:prefLabel>
<skos:definition>azbest emitowany do atmosfery podczas rozpylania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8025">
<skos:prefLabel>źródło (hydrologiczne)</skos:prefLabel>
<skos:definition>miejsce, z którego wody podziemne wypływają ze skał lub gleby na powierzchnię lub do zbiornika wodnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8028">
<skos:prefLabel>woda źródlana</skos:prefLabel>
<skos:definition>woda uzyskiwana z formacji podziemnych, z których woda wypływa naturalnie na powierzchnię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8029">
<skos:prefLabel>oprysk</skos:prefLabel>
<skos:definition>opryskiwanie roślin roztworem pestycydów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8031">
<skos:prefLabel>plac</skos:prefLabel>
<skos:definition>otwarte miejsce w mieście, często otoczone budynkami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8032">
<skos:prefLabel>nielegalne budownictwo</skos:prefLabel>
<skos:definition>budynek zbudowany bezprawnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8036">
<skos:prefLabel>zbiornik stabilizujący</skos:prefLabel>
<skos:definition>zbiornik, w którym ścieki są pozostawione na dłuższy czas do dekompozycji, samoistnego napowietrzenia się i działania proimeni słonecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8037">
<skos:prefLabel>stajnia</skos:prefLabel>
<skos:definition>budynek, pomieszczenie dla koni, niekiedy też dla innych zwierząt gospodarskich (bydła, owiec)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8039">
<skos:prefLabel>komin</skos:prefLabel>
<skos:definition>część przewodu kominowego z wylotem na dachu (wystająca ponad dach)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8047">
<skos:prefLabel>norma</skos:prefLabel>
<skos:definition>ustalona, ogólnie przyjęta zasada, reguła, przepis, wzór stanowiące punkt odniesienia dla porównań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8048">
<skos:prefLabel>normalizacja</skos:prefLabel>
<skos:definition>opracowanie i wprowadzenie jednolitych norm</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8051">
<skos:prefLabel>norma budowlana</skos:prefLabel>
<skos:definition>norma lub zasada stosowana w postępowaniu prawnym przez firmy budowlane lub remontowe w zakresie budownictwa mieszkaniowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8056">
<skos:prefLabel>podstawowe artykuły spożywcze</skos:prefLabel>
<skos:definition>najbardziej podstawowe pożywienie, stanowiące w danym kraju lub społeczności podstawę egzystencji, będąc głównym źródłem kalorii, zwłaszcza uboższej ludności i w okresach niedoboru żywności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8057">
<skos:prefLabel>skrobia</skos:prefLabel>
<skos:definition>związek organiczny z grupy węglowodanów, wielocukier powstający w komórkach roślin podczas fotosynezy; stanowi podstawowy składnik pokarmu człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8062">
<skos:prefLabel>stan</skos:prefLabel>
<skos:definition>zorganizowana politycznie społeczność zamieszkująca określone granicami terytorium, mająca niepodległą formę rządów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8067">
<skos:prefLabel>najnowsza technologia</skos:prefLabel>
<skos:definition>aktualny stan rozwoju jakiejś dziedziny wiedzy i umiejętności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8068">
<skos:prefLabel>dane o stanie środowiska</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8069">
<skos:prefLabel>raport o stanie środowiska</skos:prefLabel>
<skos:definition>pisemne opracowanie opisujące warunki ekosystemu i zasobów naturalnych danego regionu, zwykle przedstawiane przez oficjalną osobę lub jednostkę upoważnioną do ochrony ludzkiego zdrowia i środowiska w danym regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8070">
<skos:prefLabel>stan odpadów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8072">
<skos:prefLabel>stan skupienia</skos:prefLabel>
<skos:definition>jeden z trzech podstawowych stanów materii: stan stały, ciekły i gazowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8073">
<skos:prefLabel>stacja</skos:prefLabel>
<skos:definition>miejsce, budynki i usługi przy trasie lub linii, w którym autobus, pociąg i in. zatrzymują się, gdzie pasażerowie mogą wsiąść i wysiąć, gdzie możliwe jest nabranie paliwa lub załadunek i wyładunek towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8074">
<skos:prefLabel>analiza statystyczna</skos:prefLabel>
<skos:definition>techniki i metody stosowane we wnioskowaniu statystycznym dotyczącym ludności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8075">
<skos:prefLabel>dane statystyczne</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8076">
<skos:prefLabel>statystyka</skos:prefLabel>
<skos:definition>dział matematyki zajmujący się zbieraniem, analizą, interpretacją i prezentacją masowych danych liczbowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8077">
<skos:prefLabel>statystyka odpadów</skos:prefLabel>
<skos:definition>określenie ilości i rodzaju odpadów wyrzuconych przez daną społeczność, poprzez zastosowanie metody próbek punktowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8078">
<skos:prefLabel>stan rozwoju</skos:prefLabel>
<skos:definition>zakres w jakim społeczństwo promuje pomyślność we wszystkich możliwych aspektach poprzez wyrabianie ludzkich zdolności, rozszerzanie możliwości wyboru i zwiększanie szans</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8082">
<skos:prefLabel>termin ustalony prawnie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8083">
<skos:prefLabel>statutowy organ publiczny</skos:prefLabel>
<skos:definition>stowarzyszenie finansowane w całości lub częściowo z budżetu państwa, powoływane do wprowadzania i obsługiwania programów, projektów dla społeczeństwa, często z niewielką ingerencją rządu w codzienną działalność</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8084">
<skos:prefLabel>hałas ciągły</skos:prefLabel>
<skos:definition>długotrwały, nieprzerwany hałas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8086">
<skos:prefLabel>generator pary</skos:prefLabel>
<skos:definition>urządzenie ciśnieniowe zamieniające wodę w parę wodną pod wpływem wysokiej temperatury,  uzyskiwanej przy spalaniu paliw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8088">
<skos:prefLabel>stal</skos:prefLabel>
<skos:definition>stop żelaza zawierający węgiel (ok. 0,1-0,7%) oraz małe ilości innych pierwiastków (fosfor, siarka, magnez, chrom, nikiel), charakteryzujący się wytrzymałością, podatnością na przetwarzanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8089">
<skos:prefLabel>przemysł stalowy</skos:prefLabel>
<skos:definition>przemysł przetwórstwa żelaza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8095">
<skos:prefLabel>wyjaławianie</skos:prefLabel>
<skos:definition>proces niszczenia wszelkich form życia mikrobiologicznego wewnątrz i na powierzchni obiektu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8096">
<skos:prefLabel>steroid</skos:prefLabel>
<skos:definition>związek zbudowany z szeregu grup stanowiących połączone razem 4 pierścienie węglowe tworząc jednostkę strukturalną zwaną cyclopentanoperthy drophenanthrene</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8098">
<skos:prefLabel>fundusz wspierania inicjatyw</skos:prefLabel>
<skos:definition>środki pieniężne lub fundusze, mające na celu stymulowanie, wspieranie produkcyjności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8099">
<skos:prefLabel>pogłowie</skos:prefLabel>
<skos:definition>grupa osobników jednego gatunku w obrębie wyznaczonego obszaru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8100">
<skos:prefLabel>gospodarowanie zapasami</skos:prefLabel>
<skos:definition>zarządzanie, kontrolowanie zgromadzonymi, zmagazynowanymi materiałami lub dobrami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8102">
<skos:prefLabel>inwentaryzacja</skos:prefLabel>
<skos:definition>przeliczanie materiałów lub dóbr w magazynie, składzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8104">
<skos:prefLabel>kamień</skos:prefLabel>
<skos:definition>ogólne określenie dla skał stosowanych w konstrukcjach, kruszonych lub formowanych w bloki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8110">
<skos:prefLabel>tama</skos:prefLabel>
<skos:definition>betonowa, usypana z ziemi, itp. zapora, bariera zbudowana w poprzek rzeki, powodująca spiętrzanie się wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8115">
<skos:prefLabel>burza</skos:prefLabel>
<skos:definition>zkłócenia atmosferyczne obejmujące zakłócenia dominującego ciśnienia i wiatrów w skali od tornad do cyklonów tropikalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8117">
<skos:prefLabel>szkody wywołane przez burze</skos:prefLabel>
<skos:definition>burze mogą powodować m.in. zalewanie, niszczenie upraw; wyłamywanie, wyrywanie drzew z korzeniami; niszczenie dachów i kominów; wybijanie szyb w oknach; wywracanie wagonów kolejowych; wpływać na transport, komunikację i dostawy prądu; opóźniać budowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8122">
<skos:prefLabel>piec</skos:prefLabel>
<skos:definition>komora, w której spalane są mieszanki paliwowe; wytworzone ciepło wykorzystywane do  ogrzewania otoczenia, podgrzewania płynów technologicznych, w piecach hutniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8125">
<skos:prefLabel>układ warstw</skos:prefLabel>
<skos:definition>układ warstw w zbiorniku wodnym, np. jeziorze, polegający na istnieniu dwóch lub więcej poziomych, zróżnicowanych (szczególnie pod względem ciężaru właściwego) warstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8128">
<skos:prefLabel>stratosfera</skos:prefLabel>
<skos:definition>warstwa atmosfery, znajdująca się pomiędzy troposferą a mezosferą, pochłaniająca 3% promieniowania słonecznego - w tym promieniowanie nadfioletowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8129">
<skos:prefLabel>niszczenie ozonu stratosferycznego</skos:prefLabel>
<skos:definition>uszkadzanie warstwy ozonowej przez związki chemiczne stosowane w lodówkach, zamrażarkach, aerozolach i jako środki czyszczące w przemyśle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8133">
<skos:prefLabel>pomiar przepływu</skos:prefLabel>
<skos:definition>określenie ilościowe tempa, ilości przepływu lub wycieku z naturalnego cieku wodnego takiego jak mała rzeka lub strumień</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8136">
<skos:prefLabel>oczyszczanie ulic</skos:prefLabel>
<skos:definition>proces usuwania nieczystości, śmieci z ulic miejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8140">
<skos:prefLabel>wytrzymałość materiałów</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8141">
<skos:prefLabel>stres</skos:prefLabel>
<skos:definition>bodziec lub seria bodźców tak znaczących,  że powodujących zachwianie w homeostazie organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8143">
<skos:prefLabel>górnictwo odkrywkowe</skos:prefLabel>
<skos:definition>sposób powierzchniowego wydobywania węgla, licznych pierwiastków niemetalicznych, rud metali (żelaza, miedzi) poprzez usuwanie nadkładu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8144">
<skos:prefLabel>stront</skos:prefLabel>
<skos:definition>pierwiastek chemiczny, o barwie srebrzysto-białej, należący do grupy metali alkaicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8145">
<skos:prefLabel>program adaptacji strukturalnej</skos:prefLabel>
<skos:definition>program reform ekonomicznych mający na celu postęp czy liberalizację gospodarki, ekonomii; zalecany i narzucany przez Bank Światowy i Międzynarodowy Fundusz Monetarny krajom rozwijającym się w zamian za nowe pożyczki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8148">
<skos:prefLabel>zależność strukturalno-funkcjonalna</skos:prefLabel>
<skos:definition>związek pomiędzy strukturą chemiczną a rakotwórczością</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8149">
<skos:prefLabel>przenoszenie hałasu przez konstrukcje</skos:prefLabel>
<skos:definition>dźwięk, który przenosi się przynajmiej częściowo drogą wibracji jednolitej, masywnej konstrukcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8151">
<skos:prefLabel>plan strukturalny, plan zagospodarowania przestrzennego</skos:prefLabel>
<skos:definition>plan struktury i zagospodarowania terenów miejskich, zdeterminowany zarysem podziału obszarów ze względu na ich użytkowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8158">
<skos:prefLabel>geomorfologia podmorska</skos:prefLabel>
<skos:definition>część oceanografii geologicznej zajmująca się cechami rzeźby dna oceanicznego i zmieniającymi je siłami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8159">
<skos:prefLabel>łódź podwodna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8162">
<skos:prefLabel>kolejność</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8163">
<skos:prefLabel>osiadanie</skos:prefLabel>
<skos:definition>1) opadanie cześci skorupy ziemskiej, związane z podziemnymi pracami wydobywczymi;
2) gwałtowne opadanie lub stopniowe osiadanie powierzchni Ziemi z małymi (lub całkowicie bez) ruchami poziomymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8165">
<skos:prefLabel>dotacja</skos:prefLabel>
<skos:definition>grant przekazany przez rząd prywatnemu przedsiębiorstwu lub organizacji charytatywnej; dotacja adresowana do klientów lub producentów, mająca na celu obniżenie cen usług lub produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8167">
<skos:prefLabel>podglebie</skos:prefLabel>
<skos:definition>warstwy gleby leżące pod warstwą orną gleby, poza zasięgiem korzeni roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8168">
<skos:prefLabel>odpływ podskórny</skos:prefLabel>
<skos:definition>usuwanie nadwyżki wody z warstw podglebia, za pomocą naturalnych bądź sztucznych środków, takich jak umiejscowienie drenów w taki sposób, aby poziom zwierciadła wody znajdował się poniżej strefy korzeniowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8171">
<skos:prefLabel>podstawialność (chemiczna)</skos:prefLabel>
<skos:definition>zdolność substancji do bycia zastępowanymi przez inne, np. słodziki używane w zastępstwie cukru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8173">
<skos:prefLabel>podstawianie składników halogenowych</skos:prefLabel>
<skos:definition>związki chlorowcopochodne, z powodu ich toksycznych właściwości oraz dużej trwałości, powinny zostać zastąpione przez przyjazne środowisku substytuty; jako przykład można wymienić zastosowanie wodnych rozpuszczalników do odtłuszczania w przemyśle metalowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8174">
<skos:prefLabel>zastępowanie fosforanów</skos:prefLabel>
<skos:definition>zastępowanie fosforanów w środkach myjących substancjami bezpieczniejszymi dla środowiska, takimi jak zeolity; substytut nie ma właściwości odżywczych a tym samym nie spowoduje eutrofizacji i w rezultacie przyspieszenia wzrostu roślin i mikroorganizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8175">
<skos:prefLabel>uprawa podłoża</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8179">
<skos:prefLabel>ekosystem podzwrotnikowy</skos:prefLabel>
<skos:definition>interakcyjny, sprzężony system świata biologicznego i jego nieożywionego, środowiskowego otoczenia w regionach graniczących ze strefą tropikalną oraz umiarkowaną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8182">
<skos:prefLabel>przedmieście</skos:prefLabel>
<skos:definition>dzielnica mieszkaniowa usytuowana na peryferiach miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8185">
<skos:prefLabel>cukier</skos:prefLabel>
<skos:definition>słodka, skrystalizowana lub sproszkowana substancja, biała w stanie czystym, składająca się z sacharozy uzyskiwanej głównie z trzciny cukrowej lub buraka cukrowego, używana w wielu środkach spożywczych, napojach, lekarstwach w celu poprawienia ich smaku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8188">
<skos:prefLabel>przemysł cukrowniczy</skos:prefLabel>
<skos:definition>przemysł zajmujący się przetwarzaniem surowców takich jak trzcina cukrowa, buraki cukrowe lub skrobia na wyroby gotowe nadające się do celów konsumpcyjnych lub produkcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8190">
<skos:prefLabel>siarczan</skos:prefLabel>
<skos:definition>pochodne nieorganiczne (sole) lub organiczne (estry) kwasu siarkowego; sole tego kwasu rozpowszechnione w przyrodzie jako minerały (np. gips, baryt, kainit), mają szerokie zastosowanie w przemyśle chemicznym, papierniczym, szklarskim, w farbiarstwie, galwanotechnice, lecznictwie oraz jako nawozy sztuczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8192">
<skos:prefLabel>siarczek</skos:prefLabel>
<skos:definition>pochodne kwasu siarkowodorowego, sole obojętne lub kwaśne, rozpowszechnione w przyrodzie w postaci minerałów (np. antymonitu, galeny, pirytu), stanowiące surowiec do otrzymywania metali oraz dwutlenku siarki; stosowane też w ceramice, garbarstwie, w produkcji barwników, celulozy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8198">
<skos:prefLabel>siarka</skos:prefLabel>
<skos:definition>pierwiastek niemetaliczny występujący w formie krystalicznej lub amorficznej posiadający cztery izotopy trwałe, będący półproduktem w przemyśle chemicznym, wykorzystywany w środkach grzybobójczych oraz przy wulkanizacji gumy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8200">
<skos:prefLabel>zawartość siarki</skos:prefLabel>
<skos:definition>zawartość siarki w roztworze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8202">
<skos:prefLabel>dwutlenek siarki</skos:prefLabel>
<skos:definition>gaz emitowany podczas procesu spalania paliw kopalnych w elektrowniach lub kotłowniach;powstawanie dwutlenku siarki związane jest z obecnoscią siarki w węglu i paliwach ropopochodnych; podczas spalania paliwa rozgrzana siarka reaguje z tlenem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8203">
<skos:prefLabel>kwas siarkowy</skos:prefLabel>
<skos:definition>toksyczny, korozyjny, silnie kwaśny bezbarwny płyn mieszający się z wodą, rozpuszcza większość metali, topi się w temperaturze 10 st C; wykorzystywany w przemyśle produkcji chemikaliów, nawozów sztucznych, materiałów wybuchowych i rafinacji ropy naftowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8204">
<skos:prefLabel>tlenek siarki</skos:prefLabel>
<skos:definition>tlenek podobnie jak dwutlenek i trójtlenek siarki, powstający podczas spalania paliw kopalnych, stanowiący podstawowe substancje zanieczyszczające powietrze, powodujący uszkodzenia układu oddechowego oraz straty w roślinności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8212">
<skos:prefLabel>nadzór budowlany</skos:prefLabel>
<skos:definition>nadzór, kierownictwo nad konstruowaniem i konserwacją domów, biur i innych budowli oraz ich wyposażenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8213">
<skos:prefLabel>kontrola instalacji</skos:prefLabel>
<skos:definition>nadzór, kierownictwo nad procesem zakładania, przystosowywania, przygotowywania do budowy lub nadzór nad systemem mechanicznym, elektrycznym lub aparaturą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8214">
<skos:prefLabel>organ kontroli</skos:prefLabel>
<skos:definition>mianowana lub urzędowa, oficjalna grupa, odpowiedzialna za nadzór lub zarządzanie pracą, operacjami, poszczególnymi projektami lub innymi działaniami organizacji lub agencji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8219">
<skos:prefLabel>podaż</skos:prefLabel>
<skos:definition>skłonność i gotowość do sprzedania danej ilości towarów w danym zakresie cenowym oraz czasowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8222">
<skos:prefLabel>środek powierzchniowo czynny</skos:prefLabel>
<skos:definition>substancja, która użyta w niewielkich ilościach modyfikuje właściwości powierzchni płynów i ciał stałych redukując napięcie powierzchniowe w cieczy lub napięcie międzyfazowe pomiędzy dwoma niemieszającymi się cieczami, takimi jak olej i woda</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8225">
<skos:prefLabel>spływ powierzchniowy</skos:prefLabel>
<skos:definition>woda przemieszczająca się po powierzchni gleby do najbliższego strumienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8226">
<skos:prefLabel>napięcie powierzchniowe</skos:prefLabel>
<skos:definition>siła działająca na powierzchni cieczy, zmierzająca do zmniejszenia obszaru powierzchni; ilościowo - siła działająca poprzecznie na jednostkę długości linii na powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8227">
<skos:prefLabel>obróbka powierzchniowa</skos:prefLabel>
<skos:definition>jakakolwiek metoda obróbki tworzywa (metal, polimer, drewno) mająca na celu zmianę powierzchni, czyniącą ją: chłonną - dla atramentu, farb, lakierów, lepiszczy, spoiw czy innych środków stosowanych do obróbki lub odporną - na wodę czy szkodliwe działanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8229">
<skos:prefLabel>woda powierzchniowa</skos:prefLabel>
<skos:definition>wszystkie wody na powierzchni Ziemi przybierające postać strumieni, rzek, bagien, moczarów, obszarów podmokłych oraz lodu i śniegu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8236">
<skos:prefLabel>odpady chirurgiczne</skos:prefLabel>
<skos:definition>tkanka, krew lub śluz usunięte podczas operacji, zabiegu chirurgicznego lub sekcji zwłok, poplamione ubiory chirurgiczne lub inne materiały, tworzywa wymagające specjalnej procedury usuwania, niszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8237">
<skos:prefLabel>nadmiar</skos:prefLabel>
<skos:definition>różnica pomiędzy dochodami a wydatkami, również zysk pozostały po opłaceniu wszelkich świadczeń i zobowiązań, takich jak koszty, podatki, odsetki, ubezpieczenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8239">
<skos:prefLabel>nadzór</skos:prefLabel>
<skos:definition>system umożliwiający ciągłą obserwację, pomiary i ocenę rozwoju procesu lub zjawiska, mający na celu pobranie pomiarów korygujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8240">
<skos:prefLabel>badanie</skos:prefLabel>
<skos:definition>krytyczne badanie faktów lub warunków dostarczające informacji dotyczących sytuacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8247">
<skos:prefLabel>zrównoważony rozwój</skos:prefLabel>
<skos:definition>rozwój zapewniający ekonomiczne, socjologiczne i środowiskowe korzyści na długi okres, uwzględniający potrzeby żyjących i przyszłych pokoleń; zdefiniowany przez Światową Komisję Środowiska i Rozwoju w 1987</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8249">
<skos:prefLabel>wskaźnik zrównoważonego rozwoju</skos:prefLabel>
<skos:definition>wskaźniki statystyczne używane do pomiaru zrównoważonego rozwoju, które mogą być wybrane spośród szerokiego zakresu tematów, np. wydajność środowiska i jakość życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8250">
<skos:prefLabel>zrównoważone użytkowanie</skos:prefLabel>
<skos:definition>użytkowanie środowiska i jego żywych zasobów w stopniu, który nie przekracza jego wydajności i możliwości odnowienia w taki sposób, który zapewni jego dostępność przyszłym pokoleniom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8252">
<skos:prefLabel>bagno</skos:prefLabel>
<skos:definition>okresowo zatapiany obszar o glebach podmokłych, pokrych krzewami i drzewami, z/lub bez wykształconymi torfowiskami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8255">
<skos:prefLabel>słodzik</skos:prefLabel>
<skos:definition>środek słodzący, nie zawiera cukru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8259">
<skos:prefLabel>symbioza</skos:prefLabel>
<skos:definition>bliski i obustronnie korzystny związek organizmów dwóch różnych gatunków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8262">
<skos:prefLabel>synekologia</skos:prefLabel>
<skos:definition>nauka o ekologii organizmów, populacji, społeczności lub systemów, układów społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8263">
<skos:prefLabel>współoddziaływanie</skos:prefLabel>
<skos:definition>współpraca ekologiczna, w której procesy fizjologiczne zachowań osobników są wzmagane przez obecność innych organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8265">
<skos:prefLabel>efekt synergiczny działania substancji toksycznych</skos:prefLabel>
<skos:definition>1) stan, w którym efekt połączenia dwóch lub więcej substancji jest większy niż suma pojedynczych zjawisk; 2) zjawisko, w którym dwie połączone substancje toksyczne mają większą siłę działania niż przewidywano</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8268">
<skos:prefLabel>syntetyczny środek piorący</skos:prefLabel>
<skos:definition>sztucznie wyprodukowane substancje (w postaci ciała stałego lub płynu) czyszczące, o działaniu podobnym do mydła ale silniejszym, zdolne rozpuszczać tworzywa oleiste oraz rozpraszać je w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8269">
<skos:prefLabel>przemysł włókien syntetycznych</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8272">
<skos:prefLabel>wyrób syntetyczny</skos:prefLabel>
<skos:definition>materiał, tworzywo wyprodukowane sztucznie, za pomocą reakcji chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8273">
<skos:prefLabel>przemysł wyrobów syntetycznych</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8275">
<skos:prefLabel>włókno syntetyczne</skos:prefLabel>
<skos:definition>sztucznie wyprodukowane włókno lub nić, używana do produkcji ubrań lub innych wyrobów wymagających tkania, dziania, filcowania, np. polyester, nylon, sztuczny jedwab itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8278">
<skos:prefLabel>analiza systemów</skos:prefLabel>
<skos:definition>sposób organizowania elementów w ujednoliconą, analityczną i/lub wskazującą sposób podejmownia decyzji procedurę, mającą na celu uzyskanie jak najlepszych rezultatów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8279">
<skos:prefLabel>porównywanie systemów</skos:prefLabel>
<skos:definition>analiza lub oszacowania biorące po uwagę podobieństwa i różnice w działaniach firm lub organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8280">
<skos:prefLabel>teoria systemów</skos:prefLabel>
<skos:definition>nauka związana z ogólnym badaniem struktur i funkcjonowaniem systemów, które mogłyby być zastosowane w innych gałęziach nauki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8283">
<skos:prefLabel>przeprowadzanie dowodów</skos:prefLabel>
<skos:definition>środki prawne, które służą do ustalenia okoliczności faktycznych, niezbędnych do rozstrzygnięcia sprawy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8284">
<skos:prefLabel>samochód-cysterna</skos:prefLabel>
<skos:definition>pojazd umożliwiający przewóz dużej ilości płynów lub gazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8285">
<skos:prefLabel>tankowiec</skos:prefLabel>
<skos:definition>statek umożliwiający przewóz dużej ilości płynów lub gazu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8286">
<skos:prefLabel>park cystern</skos:prefLabel>
<skos:definition>miejsce magazynowania (parkowania) zbiorników (cystern) z płynami lub gazem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8289">
<skos:prefLabel>tanina</skos:prefLabel>
<skos:definition>jedna z grup organicznych, złożonych związków chemicznych powszechnie występujących w liściach, w niedojrzałych owocach i w korze drzew</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8296">
<skos:prefLabel>smoła</skos:prefLabel>
<skos:definition>lepkie tworzywo zbudowane ze złożonych, wielkocząsteczkowych związków pochodzących z destylacji ropy naftowej lub z suchej destylacji drewna lub węgla</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8299">
<skos:prefLabel>docelowa grupa społeczna</skos:prefLabel>
<skos:definition>wybrana grupa ludzi, traktowana przez jakąś jednostkę jako grupa służąca do różnych celów, związanych z pomocą, naborem do pracy, przekazywaniem informacji, marketingiem i badaniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8301">
<skos:prefLabel>taryfa</skos:prefLabel>
<skos:definition>sklasyfikowana lista lub tabela opłat tworzona w każdej prywatnej lub publicznej firmie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8303">
<skos:prefLabel>produkcja smoły</skos:prefLabel>
<skos:definition>produkcja ciemnej, ciężkiej, lepkiej substancji lub pozostałości, uzyskiwanej przz destylację tworzyw organicznych takich jak węgiel, drewno i ropa naftowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8304">
<skos:prefLabel>piasek smolisty</skos:prefLabel>
<skos:definition>piaskowiec, w którym zostały uwięzione węglowodory, a lżejsze związki wyparowały pozostawiając resztę asfaltu w porach skały</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8305">
<skos:prefLabel>użytkowanie smoły</skos:prefLabel>
<skos:definition>zastosowanie, wykorzystanie ciemnej, ciężkiej, lepkiej substancji lub pozostałości pochodzących z destylacji tworzyw organicznych często do produkcji benzyny, mydła, farb, kosmetyków i innych produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8307">
<skos:prefLabel>podatek</skos:prefLabel>
<skos:definition>kwota pieniędzy wymagana przez rząd przeznaczona na wsparcie jego prac, na infrastrukturę czy służby, najczęściej pobierany od przychodu, własności lub sprzedaży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8308">
<skos:prefLabel>opodatkowanie</skos:prefLabel>
<skos:definition>proces lub rezultat rządowego wymogu płacenia pewnych kwot pieniędzy, przeznaczanych na wsparcie prac rządu i pracę służb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8309">
<skos:prefLabel>zróżnicowanie podatkowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8312">
<skos:prefLabel>prawo podatkowe</skos:prefLabel>
<skos:definition>prawo do bezzwrotnego, przymusowego świadczenia pieniężnego nałożonego na obywatela oraz na podmioty prowadzące działalność gospodarczą przez państwo lub organ samorządu terytorialnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8313">
<skos:prefLabel>taksonomia</skos:prefLabel>
<skos:definition>część nauk biologicznych skupiająca się na klasyfikacji organizmów w grupy oparte na podobieństwach budowy, pochodzenia itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8319">
<skos:prefLabel>nauczanie</skos:prefLabel>
<skos:definition>proces przyswajania wiedzy lub umiejętności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8321">
<skos:prefLabel>metoda nauczania</skos:prefLabel>
<skos:definition>procedura, technika lub system ze zdefiniowanymi, wyraźnymi planami szkolenia, dzielenia się wiedzą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8326">
<skos:prefLabel>zasada postępowania z substancjami niebezpiecznymi</skos:prefLabel>
<skos:definition>zasada postępowania z substancjami niebezpiecznymi; techniczne, fachowe zasady obchodzenia się z niebezpiecznymi materiałami, tworzywami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8330">
<skos:prefLabel>zmiana technologiczna</skos:prefLabel>
<skos:definition>zmiana metod przemysłowych przez wprowadzenie nowej technologii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8333">
<skos:prefLabel>rozwój technologiczny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8334">
<skos:prefLabel>niebezpieczeństwo technologiczne</skos:prefLabel>
<skos:definition>zastosowanie wiedzy praktycznej lub mechaniki w przemyśle lub handlu mogące powodować uszczerbek dla ludzi, nieruchomości lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8337">
<skos:prefLabel>proces technologiczny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8339">
<skos:prefLabel>technologia</skos:prefLabel>
<skos:definition>usysytematyzowana wiedza na temat procesów przemysłowych oraz jej zastosowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8340">
<skos:prefLabel>aprobata technologiczna</skos:prefLabel>
<skos:definition>zgoda, przychylne przyjęcie i stałe, ciągłe używanie nowo wprowadzonych urządzeń i systemów, przeważnie będących wynikiem ostatnich osiągnięć, postępu w naukach inżynieryjnych lub przemyśle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8341">
<skos:prefLabel>ocena technologiczna</skos:prefLabel>
<skos:definition>systematyczna analiza, badanie przewidywanego wpływu danej technologii, szacujące jej bezpieczeństwo, wydajność oraz jej socialne, polityczne, ekonomiczne czy etniczne konsekwencje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8342">
<skos:prefLabel>transfer technologii</skos:prefLabel>
<skos:definition>przekazanie nowych technologii i prac projektowych: a) z przedsiębiorstwa kontrolującego do kontrolowanego, np. w innym państwie, b) z jednego kraju do innego jako formę pomocy w celu promowania rozwoju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8344">
<skos:prefLabel>tektonika</skos:prefLabel>
<skos:definition>część geologii zajmująca się ogólną budową zewnętrznej części Ziemi, np. regionalny zespół cech strukturalnych i deformacyjnych, badanie ich wzajemnych relacji, ich pochodzenia i ewolucji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8347">
<skos:prefLabel>telekomunikacja</skos:prefLabel>
<skos:definition>przekazywanie obrazów i dźwięków, najczęściej na duże odległości, poprzez urządzenia technologiczne takie jak telewizja, telegraf, telefon lub radio</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8350">
<skos:prefLabel>telematyka</skos:prefLabel>
<skos:definition>nowa dyscyplina zajmująca się przekazem cyfrowej informacji multimedialnej (audio, video, grafika, dane) za pośrednictwem sieci teleinformatycznych oraz przygotowaniem, gromadzeniem i udostępnianiem tego rodzaju informacji w formie usług teleinformatycznych; termin w obecnym znaczeniu wylansowany przez Komisję Europejską w latach 1994-1998, gdy w ramach IV Programu Ramowego realizowano program zastosowań telematyki w różnych dziedzinach życia publicznego, gospodarki i nauki jako Telematics Applications Programme; znaczącym obszarem zastosowań telematyki jest sektor środowisko; w krajach zachodnich funkcjonuje termin enviromatics, który należałoby przetłumaczyć jako ekomatyka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8351">
<skos:prefLabel>telemetria</skos:prefLabel>
<skos:definition>dział telekomunikacji dotyczący zdalnego mierzenia wielkości fizycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8353">
<skos:prefLabel>telewizja</skos:prefLabel>
<skos:definition>dział telekomunikacji obejmujący całokształt zagadnień związanych z przekazywaniem na odległość ruchomych obrazów, zwykle z towarzyszącym im dźwiękiem, za pomocą urządzeń elektronicznych i torów telekomunikacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8360">
<skos:prefLabel>ekosystem strefy umiarkowanej</skos:prefLabel>
<skos:definition>układ wzajemnie oddziałujących na siebie: środowiska biologicznego i jego nieożywionego otoczenia, związanych z klimatem umiarkowanym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8361">
<skos:prefLabel>lasy strefy umiarkowanej</skos:prefLabel>
<skos:definition>lasy mieszane, złożone z drzew iglastych i drzew liściastych - zrzucających liście na zimę lub złożone z drzew iglastych i liściastych - zimozielonych, lub całkowicie z drzew liściastych - zrzucających liście na zimę, lub całkowicie z drzew liściastych zimozielonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8362">
<skos:prefLabel>obszar leśny strefy umiarkowanej</skos:prefLabel>
<skos:definition>lasy zdominowane przez drzewa liściaste, występujące na dużych obszarach strefy klimatu umiarkowanego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8366">
<skos:prefLabel>temperatura</skos:prefLabel>
<skos:definition>własność determinująca kierunek przepływu ciepła, kiedy obiekt wchodzi w kontakt termiczny z innymi obiektami: ciepło przepływa z obszarów o wyższej temperaturze do tych o niższej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8367">
<skos:prefLabel>mieszkanie tymczasowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8369">
<skos:prefLabel>schronienie tymczasowe</skos:prefLabel>
<skos:definition>prosto urządzone schronienie lub tymczasowe zakwaterowanie pojedynczych osób lub grup ludzi w krytycznych sytuacjach, w nagłych wypadkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8371">
<skos:prefLabel>przechowywanie tymczasowe</skos:prefLabel>
<skos:definition>chwilowy, krótkotrwały depozyt lub przechowalnie przedmiotów, materiałów lub zgub w przeznaczonym do tego miejscu (przechowalnia), w kontenerze, zbiorniku lub w jakimkolwiek innym miejscu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8376">
<skos:prefLabel>teratogeneza</skos:prefLabel>
<skos:definition>proces powodujący nieprawidłowości w rozwoju, budowie potomstwa; przeważnie jest wynikiem uszkodzenia budowy embrionu podczas pierwszego trymestru ciąży, powoduje deformację płodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8377">
<skos:prefLabel>badanie teratogenezy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8378">
<skos:prefLabel>teratogeniczność</skos:prefLabel>
<skos:definition>zdolność lub skłonność do tworzenia się nieprawidłowości w budowie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8379">
<skos:prefLabel>teratogen</skos:prefLabel>
<skos:definition>substancje powodujące wrodzone nieprawidłowości lub duże zniekształcenia embrionu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8380">
<skos:prefLabel>terminologia</skos:prefLabel>
<skos:definition>ogół terminów, słownictwo danej dziedziny wiedzy, techniki; zbiór, zasób wyspecjalizowanych słów odnoszących się do konkretnego tematu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8381">
<skos:prefLabel>termity</skos:prefLabel>
<skos:definition>owad o miękkim ciele z rzędu Isoptera, żywiący się celulozą, żyjący w koloniach, zorganizowanych w system kast, składających się z trzech typów osobników, spełniających funkcje: bezpłodnych robotników, żołnierzy i reproduktorów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8386">
<skos:prefLabel>obszar lądowy</skos:prefLabel>
<skos:definition>lądowa część kontynentów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8387">
<skos:prefLabel>lądowe zasoby biologiczne</skos:prefLabel>
<skos:definition>źródło zaopatrzenia czerpane z roślin, zwierząt lub innej formy życia na lądzie lub w gruncie, wykorzystywane przez ludzi jako pożywienie, ubranie lub zaspokajające inne potrzeby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8388">
<skos:prefLabel>ekosystem lądowy</skos:prefLabel>
<skos:definition>środowisko ziemskie, lądowe - od małego do dużego, w obrębie którego rośliny i zwierzęta oddziałują wzajemnie na siebie oraz na chemiczne i fizyczne elementy środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8398">
<skos:prefLabel>polityka terytorialna</skos:prefLabel>
<skos:definition>kierunek działania przyjmowany i stosowany przez rząd, przedsiębiorstwa lub organizacje, determinujący teraźniejsze i przyszłe wykorzystywanie działki, gruntu lub terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8401">
<skos:prefLabel>terytorium</skos:prefLabel>
<skos:definition>strefa broniona przez zwierzę lub grupę zwierząt, głównie przed osobnikami tego samego gatunku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8403">
<skos:prefLabel>sektor handlu i usług</skos:prefLabel>
<skos:definition>część gospodarki kraju lub regionu wytwarzająca usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8405">
<skos:prefLabel>doświadczenie</skos:prefLabel>
<skos:definition>wykonywanie badania (substancji, materiału lub systemu, organizmu) przez zastosowanie chemicznej lub fizycznej procedury, metody badań stworzonej do wykrywania obecności substancji lub właściwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8407">
<skos:prefLabel>zwierzę doświadczalne</skos:prefLabel>
<skos:definition>zwierzęta, na których przeprowadzane są eksperymenty, aby dostarczyć dowodów za lub przeciw naukowej hipotezie, lub udowodnić skuteczność lekarstw, lub zbadać reakcję na pewne produkty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8411">
<skos:prefLabel>wytyczne do badań</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8412">
<skos:prefLabel>metoda badań</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8415">
<skos:prefLabel>organizm doświadczalny</skos:prefLabel>
<skos:definition>jakikolwiek organizm zwierzęcy wykorzystywany do badań naukowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8421">
<skos:prefLabel>przemysł odzieżowy</skos:prefLabel>
<skos:definition>przemysł wytwarzający tkaniny, materiały</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8422">
<skos:prefLabel>tekstylia</skos:prefLabel>
<skos:definition>materiał wytworzony z włókna naturalnego lub sztucznego, używany do produkcji ubrań, obić na meble</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8428">
<skos:prefLabel>tal</skos:prefLabel>
<skos:definition>niebieskawo - biały metal stosowany w stopach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8433">
<skos:prefLabel>teoria pieniądza</skos:prefLabel>
<skos:definition>zespół spójnych założeń dotyczących popytu i podaży pieniądza, oprocentowania kredytu, przepływu pieniądza i jego wpływu na całość gospodarki lub polityki, którą powinny przyjąć instytucje kontrolujące podaż pieniądza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8434">
<skos:prefLabel>teoria państwa dobrobytu</skos:prefLabel>
<skos:definition>polityczna koncepcja rządu w gospodarce kapitalistycznej, w której państwo jest odpowiedzialne za zapewnienie, żeby wszyscy członkowie społeczeństwa osiągali minimalny poziom życia poprzez rozdzielanie zasobów, progresywne opodatkowanie i powszechny program socjalny obejmujący ochronę zdrowia i edukację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8435">
<skos:prefLabel>terapia</skos:prefLabel>
<skos:definition>leczenie fizycznych, psychicznych lub społecznych chorób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8440">
<skos:prefLabel>izolacja termiczna</skos:prefLabel>
<skos:definition>proces zapobiegania przenikaniu ciepła do/lub z ciała, przez otaczanie go nieprzewodzącym materiałem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8442">
<skos:prefLabel>zanieczyszczenia termiczne</skos:prefLabel>
<skos:definition>nadmierne wzrastanie lub obniżanie temperatury wody powyżej lub poniżej prawidłowego, okresowego zakresu w strumieniach, jeziorach lub oceanach, co jest wynikiem wpuszczania gorących lub zimnych ścieków do tych wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8443">
<skos:prefLabel>elektrownia cieplna</skos:prefLabel>
<skos:definition>zakład wytwarzający energię elektryczną, pochodzącą ze spalania paliw kopalnych lub jądrowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8446">
<skos:prefLabel>energia cieplna mórz</skos:prefLabel>
<skos:definition>koncepcja wykorzystania różnic temperatur 20°C lub więcej, która pojawia się pomiędzy powierzchnią oceanu a jego głębinami, do uzyskania ciągłej dostawy energii - owa różnica temperatur występuje w tropikalnych regionach świata</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8447">
<skos:prefLabel>obróbka cieplna</skos:prefLabel>
<skos:definition>1) ogrzewanie i chłodzenie metalu lub stopu mające na celu otrzymanie pożądanych właściwości lub warunków;
2) sposób obrabiania niebezpiecznych odpadów w urządzeniu, w którym wykorzystuje się podwyższone temperatury jako środek zmieniający chemiczne, fizyczne lub biologiczne właściwości i skład</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8448">
<skos:prefLabel>woda termalna</skos:prefLabel>
<skos:definition>woda, głównie występujaca w postaci źródła lub gejzeru, której temperatura jest znacznie powyżej średniej, rocznej, lokalnej temperatury powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8450">
<skos:prefLabel>termodynamika</skos:prefLabel>
<skos:definition>dział fizyki zajmujący się zagadnieniami przemian energetycznych oraz analizą zmian stanów równowagi, ze szczególnym uwzględnieniem stanów równowagi cieplnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8452">
<skos:prefLabel>termiczny rozkład odpadów</skos:prefLabel>
<skos:definition>nazwa handlowa technologii procesu przetwarzania termicznego; gazyfikacja może być alternatywą dla spalania w procesie obróbki i odzyskiwaniu energii z odpadów, śmieci miejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8454">
<skos:prefLabel>tezaurus</skos:prefLabel>
<skos:definition>dynamiczny centralnie koordynowany słownik, zawierający słownictwo w wybranym języku naturalnym dla określonych dziedzin tematycznych ze wskazaniem relacji semantycznych między jego terminami; pomocny do opracowania, gromadzenia i wyszukiwania informacji; dla wielkich międzynarodowych systemów informacyjnych opracowuje się tezaurusy wielojęzyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8470">
<skos:prefLabel>wartość progowa</skos:prefLabel>
<skos:definition>maksymalna koncentracja określonej substancji, na której wpływ może być narażony pracownik przez dany okres</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8475">
<skos:prefLabel>energia pływów</skos:prefLabel>
<skos:definition>energia mechaniczna, która może być przetworzona na energię elektryczną, tworzona przez wznoszenie się i opadanie fal oceanicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8478">
<skos:prefLabel>woda pływowa</skos:prefLabel>
<skos:definition>woda w jakiejkolwiek postaci, której poziom zmienia się cyklicznie, w zależności od ruchów pływowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8480">
<skos:prefLabel>pływ</skos:prefLabel>
<skos:definition>cykliczne unoszenie się i opadanie wody będące wnikiem wzajemnego, grawitacyjnego oddziaływania pomiedzy Słońcem, Księżycem i Ziemią; w każdym dniu księżycowym, w czasie 24 godzin i 49 minut odbywają się dwa wysokie i dwa niskie pływy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8486">
<skos:prefLabel>przemysł drzewny</skos:prefLabel>
<skos:definition>przemysł związny z pozyskiwaniem i przetwarzaniem drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8487">
<skos:prefLabel>czas</skos:prefLabel>
<skos:definition>wymiar wszechświata materialnego, który w danym miejscu, porządkuje kolejność wydarzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8490">
<skos:prefLabel>cyna</skos:prefLabel>
<skos:definition>(Sn) pierwiastek chemiczny z grupy węglowców, liczba atomowa 50, metal barwy srebrzystej, miękki, kowalny, łatwo topliwy, odporny na wpływy atmosferyczne, używany w metalurgii do pokrywania żelaza i miedzi w celu ochrony ich przed korozją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8494">
<skos:prefLabel>tkanka</skos:prefLabel>
<skos:definition>część organizmu składająca się z dużej ilości komórek, mających podobną budowę i funkcję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8495">
<skos:prefLabel>tytan</skos:prefLabel>
<skos:definition>(Ti) pierwiastek chemiczny z podgrupy tytanowców, liczba atomowa 22, metal srebrzystoszary, lekki, twardy, trudno topliwy, o dużej wytrzymałości mechanicznej, odporny na korozję, stosowany głównie w postaci stopów jako materiał konstrukcyjny w produkcji samolotów, okrętów, aparatury chemicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8496">
<skos:prefLabel>dwutlenek tytanu</skos:prefLabel>
<skos:definition>biały, nierozpuszczalny w wodzie proszek, topiący się w 1560°C, produkowany z minerałów dwutlenku tytanu: ilmenitu i rutylu, stosowany w farbach i kosmetykach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8499">
<skos:prefLabel>ropucha</skos:prefLabel>
<skos:definition>Bufo, płaz bezogonowy lądowy z rodziny o tej samej nazwie, charakteryzujący się krępą budową ciała, chropowatą skórą z licznymi gruczołami śluzowymi i jadowymi; występuje w ciepłej i umiarkowanej strefie wszystkich kontynentów, w Polsce w trzech gatunkach będących pod ochroną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8501">
<skos:prefLabel>tytoń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8503">
<skos:prefLabel>dym tytoniowy</skos:prefLabel>
<skos:definition>szara, brązowa lub prawie czarna mieszanina gazów i zawieszonych cząsteczek węgla otrzymywanych ze spalania tytoniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8516">
<skos:prefLabel>tornado</skos:prefLabel>
<skos:definition>szybko wirujący słup powietrza, wytwarzający się wokół ośrodka o skrajnie niskim ciśnieniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8519">
<skos:prefLabel>żółw</skos:prefLabel>
<skos:definition>roślinożerny gad lądowy z rodziny Testudinidae, występujący w wielu ciepłych regionach Ziemi, mający ciężką, kopulastą skorupę i odnóża zakończone pazurami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8521">
<skos:prefLabel>parametr całkowity</skos:prefLabel>
<skos:definition>suma parametrów, które muszą być brane pod uwagę podczas szacowania jakości wody (współczynniki organoleptyczne, fizyczno-chemiczne, substancje toksyczne, parametry mikrobiologiczne)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8522">
<skos:prefLabel>turystyka</skos:prefLabel>
<skos:definition>zorganizowane zbiorowe lub indywidualne wyjazdy poza miejsce stałego zamieszkania, wędrówki po obcym terenie, mające cele krajoznawcze lub będące formą czynnego wypoczynku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8525">
<skos:prefLabel>infrastruktura turystyczna</skos:prefLabel>
<skos:definition>wszystkie usługi związane z turystyką, szczególnie jeśli traktowane są jako przemysł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8527">
<skos:prefLabel>strefa turystyczna</skos:prefLabel>
<skos:definition>część regionu atrakcyjnego dla turystów, przyciągająca ich uwagę, często z powodu krajobrazu, interesujących obiektów czy możliwości rekreacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8537">
<skos:prefLabel>skutek toksyczny</skos:prefLabel>
<skos:definition>skutek spowodowany spożyciem lub kontaktem z susbstancją trującą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8538">
<skos:prefLabel>toksyczność</skos:prefLabel>
<skos:definition>stopień zagrożenia, jakie stanowi dana substancja dla roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8539">
<skos:prefLabel>toksyczność pestycydów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8540">
<skos:prefLabel>metal toksyczny</skos:prefLabel>
<skos:definition>metale (najczęściej metale ciężkie) ingerujące w oddychanie, metabolizm lub wzrost organizmów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8541">
<skos:prefLabel>ocena toksykologiczna</skos:prefLabel>
<skos:definition>charakterystyka i ocena toksyczności właściwej substancji chemicznej, trucizny, itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8542">
<skos:prefLabel>badanie toksykologiczne</skos:prefLabel>
<skos:definition>test na określenie toksyczności właściwej chemikaliów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8543">
<skos:prefLabel>toksykologia</skos:prefLabel>
<skos:definition>nauka zajmująca się truciznami, ich działaniem, wykrywaniem i leczeniem skutków, które powodują</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8545">
<skos:prefLabel>produkt toksyczny</skos:prefLabel>
<skos:definition>produkt, który może spowodować gwałtowne lub chroniczne uszkodzenie ludzkiego ciała lub jest podejrzany o powodowanie choroby lub uszkodzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8546">
<skos:prefLabel>substancja toksyczna</skos:prefLabel>
<skos:definition>substancje chemiczne, mieszaniny które mogą powodować narażenie lub uszkodzenie zdrowia lub środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8547">
<skos:prefLabel>odpady toksyczne</skos:prefLabel>
<skos:definition>odpady stanowiące znaczne zagrożenie dla środowiska lub ludzkiego zdrowia jeśli są niewłaściwie zagospodarowane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8549">
<skos:prefLabel>toksyna</skos:prefLabel>
<skos:definition>substancja trująca głównie pochodzenia roślinego lub zwierzęcego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8554">
<skos:prefLabel>pierwiastek śladowy</skos:prefLabel>
<skos:definition>pierwiastek chemiczny występujący w bardzo małych ilościach w organizmach, niezbędny dla wielu fizjologicznych i biochemicznych procesów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8556">
<skos:prefLabel>substancja śladowa</skos:prefLabel>
<skos:definition>metale i niemetale, niezbędne dla procesów metabolicznych glonów, bezkręgowców i kręgowców; organizmy, które czerpią całą energię z procesu fotosyntezy są szczególnie uzależnione od rozpuszczalnych substancji śladowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8559">
<skos:prefLabel>roślina naczyniowa</skos:prefLabel>
<skos:definition>wielka grupa roślin charakteryzujaca się obecnością wyspecjalizowanych tkanek przewodzących (ksylem i łyko) w korzeniach, łodygach i liściach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8562">
<skos:prefLabel>metoda badania problemów ekologicznych</skos:prefLabel>
<skos:definition>sformułowana lub usystematyzowana metoda śledzenia zagadnień i problemów związanych ze środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8563">
<skos:prefLabel>handel</skos:prefLabel>
<skos:definition>czynność lub proces kupowania, sprzedawania, wymieniania towarów i usług w sprzedaży hurtowej i detalicznej, wewnątrz kraju lub pomiędzy państwami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8564">
<skos:prefLabel>handel pozwoleniami</skos:prefLabel>
<skos:definition>pozwolenia na emisję podlegające handlowi są używane w środowiskowym planie regulacyjnym, w którym źródła zanieczyszczeń (najczęściej zanieczyszczenia powietrza) podlegają normom, wydawane są im pozwolenia na wytwarzanie sprecyzowanej ilości ton substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8566">
<skos:prefLabel>handel i konsumpcja</skos:prefLabel>
<skos:definition>czynność lub proces kupowania, sprzedawania lub wymieniania artykułów oraz użytkowanie towarów i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8567">
<skos:prefLabel>bariera w zakresie handlu</skos:prefLabel>
<skos:definition>sztuczne ograniczenie na wolnym rynku wymiany towarów i usług pomiędzy państwami, takie jak systemy ceł, kontyngenty i kontrola wymiany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8569">
<skos:prefLabel>handel</skos:prefLabel>
<skos:definition>zorganizowana wymiana dóbr, obrót towarów polegający na kupnie-sprzedaży, prowadzony na obszarze celnym danego państwa lub w relacji z innymi państwami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8570">
<skos:prefLabel>oddziaływanie handlu na środowisko</skos:prefLabel>
<skos:definition>oddziaływanie handlu na środowisko może być bezpośrednie, np. handel zagrożonymi gatunkami, bogactwami, surowcami naturalnymi np. drewnem, itp. oraz pośredni np. wycinanie lasów, niszczenie siedlisk, zanieczyszczenia pochodzące z górnictwa, z produkcji energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8571">
<skos:prefLabel>polityka handlowa</skos:prefLabel>
<skos:definition>zamierzenia i działalność rządu, przedsiębiorstwa lub innej instytucji w dziedzinie handlu wewnętrznego lub zagranicznego dotycząca promocji i wymiany towarów i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8572">
<skos:prefLabel>handel</skos:prefLabel>
<skos:definition>zorganizowana wymiana dóbr, obrót towarów polegający na kupnie-sprzedaży, prowadzony na obszarze celnym danego państwa lub w relacji z innymi państwami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8573">
<skos:prefLabel>relacje handlowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8574">
<skos:prefLabel>restrykcje handlowe</skos:prefLabel>
<skos:definition>dyskryminacja handlowa dotycząca eksportu z pewnych krajów, jednocześnie nie stosowana wobec podobnych towarów z innych krajów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8576">
<skos:prefLabel>związek zawodowy</skos:prefLabel>
<skos:definition>organizacja, której członkowie w całości lub w większości to pracownicy i której głównymi celami jest regulacja relacji pomiędzy pracownikami a pracodawcami albo związkami pracodawców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8579">
<skos:prefLabel>kultura tradycyjna</skos:prefLabel>
<skos:definition>wyuczone, nieprzypadkowe, regularne zachowania i wiedza przekazywane przez pokolenia, szczególnie zwyczaje i wierzenia wyprzedzające współczesną naukę i technologię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8580">
<skos:prefLabel>tradycyjna opieka zdrowotna</skos:prefLabel>
<skos:definition>metoda leczenia chorób, dolegliwości oparta na wierzeniach i praktykach kulturowych, przekazywanych z pokolenia na pokolenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8582">
<skos:prefLabel>ruch</skos:prefLabel>
<skos:definition>1) ruch pojazdów, statków, samolotów, osób w przestrzeni lub na drodze
2) pojazdy, osoby poruszające się w przestrzeni lub na drodze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8583">
<skos:prefLabel>wypadek</skos:prefLabel>
<skos:definition>niespodziewany wypadek powodujący możliwość zranienia, wynikający z ruchu lub kolizji statków, pojazdów lub osób na drodze, na powierzchni wody, w powietrzu lub przestrzeni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8585">
<skos:prefLabel>inżynieria ruchu drogowego</skos:prefLabel>
<skos:definition>organizacja bardziej efektywnego, sprawnego ruchu drogowego w obrębie systemu dróg, uzyskiwana przez zmianę natężenia ruchu, regulację skrzyżowań, wyznaczenie pór i miejsc parkowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8586">
<skos:prefLabel>procedura kontroli ruchu</skos:prefLabel>
<skos:definition>procedura kontroli liczby i prędkości pojazdów mechanicznych korzystających z dróg</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8588">
<skos:prefLabel>emisja komunikacyjna</skos:prefLabel>
<skos:definition>gazy i opary wydalane przez pojazdy mechaniczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8589">
<skos:prefLabel>inżynieria ruchu drogowego</skos:prefLabel>
<skos:definition>wyznaczenie odpowiedniej przepustowości i rozplanowanie infrastruktury autostrad i ulic, które zapewnią bezpieczną i ekonomiczną obsługę ruchu pojazdów pomiędzy wyznaczonymi punktami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8590">
<skos:prefLabel>infrastruktura ruchu drogowego</skos:prefLabel>
<skos:definition>podstawowe urządzenia i systemy urządzeń wykorzystywane w ruchu pojazdów, często tworzone i utrzymywane z funduszów publicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8592">
<skos:prefLabel>korek komunikacyjny</skos:prefLabel>
<skos:definition>duża ilość pojazdów, których ruch został tak utrudniony, że ledwo mogą się poruszać</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8595">
<skos:prefLabel>monitoring ruchu drogowego</skos:prefLabel>
<skos:definition>czasowy lub ciągły nadzór lub analiza ruchu osób, obiektów, pojazdów i innych środków transportu w obrębie strefy przejazdu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8596">
<skos:prefLabel>hałas drogowy</skos:prefLabel>
<skos:definition>hałas emitowany przez pojazdy (samochody osobowe, ciężarowe, motocykle, kontakt opon z drogą)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8597">
<skos:prefLabel>kontrola hałasu drogowego</skos:prefLabel>
<skos:definition>hałas drogowy może być kontrolowany przez redukcję źródeł hałasu, instalację cicho pracujących silników pojazdów, instalację osłon dźwiękowych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8598">
<skos:prefLabel>ruch na drogach wodnych</skos:prefLabel>
<skos:definition>ruch statków, okrętów w obrębie dróg lub obszarów wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8599">
<skos:prefLabel>droga</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8606">
<skos:prefLabel>pociąg</skos:prefLabel>
<skos:definition>zespół połączonych ze sobą wagonów i lokomotywy albo wagonu silnikowego, odbywający jazdę po wytyczonej trasie, po szynach, lub przygotowany do drogi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8607">
<skos:prefLabel>szkolenie</skos:prefLabel>
<skos:definition>zorganizowane nauczanie grupy osób w celu uzupełnienia ich wykształcenia, wiadomości z jakiejś dziedziny; systematyczne ćwiczenia mające na celu uzyskanie maksymalnej sprawności, biegłości, wprawy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8608">
<skos:prefLabel>ośrodek szkoleniowy</skos:prefLabel>
<skos:definition>miejsce, w którym ludzie przygotowywani są do określonych zadań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8610">
<skos:prefLabel>tor</skos:prefLabel>
<skos:definition>trasa, droga, po których porusza się jakieś ciało; droga, którą zakreśla w przestrzeni poruszający się obiekt, np. pocisk, pojazd kosmiczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8621">
<skos:prefLabel>zanieczyszczenie transgraniczne</skos:prefLabel>
<skos:definition>zanieczyszczone powietrze lub woda czy skażone odpady wytworzone w jednym państwie i przeniesione do innych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8626">
<skos:prefLabel>postanowienie przejściowe</skos:prefLabel>
<skos:definition>zasady, wytyczne lub porozumienie w procesie zmian administracji czy struktury rządu lub organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8627">
<skos:prefLabel>osiedle przejściowe</skos:prefLabel>
<skos:definition>mała wioska, społeczność lub grupa domów i innych schronień, zlokalizowana na terenie o małej populacji, istniejąca tam tymczasowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8629">
<skos:prefLabel>pierwiastek przejściowy</skos:prefLabel>
<skos:definition>pierwiastki przejściowe; metale rodzin pobocznych - charakteryzują się innymi cechami niż metale rodzin głównych; różnią się rozmieszczeniem elektronów wartościowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8630">
<skos:prefLabel>transpiracja</skos:prefLabel>
<skos:definition>wydzielanie wody w postaci pary wodnej przez rośliny lądowe; odbywa się głównie przez aparaty szparkowe, rzadziej przez skórkę i kutykulę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8633">
<skos:prefLabel>przedsiębiorstwo transportowe</skos:prefLabel>
<skos:definition>przedsiębiorstwo handlowe zajmujące się transportem towarów lub ludzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8634">
<skos:prefLabel>transport rurociągami</skos:prefLabel>
<skos:definition>transport gazów, płynów, nieczystości czy ropy naftowej systemem stalowych lub plastikowych rur</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8639">
<skos:prefLabel>polityka transportowa</skos:prefLabel>
<skos:definition>zwięzłe określenie celów, które lokalne władze transportowe zamierzają osiągnąć</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8641">
<skos:prefLabel>transport</skos:prefLabel>
<skos:definition>czynność lub środki przemieszczania się obiektów (osób lub towarów) z miejsca na miejsce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8648">
<skos:prefLabel>planowanie transportu</skos:prefLabel>
<skos:definition>program działania zapewniający w teraźniejszości i przyszłości zapotrzebowanie na przemieszczanie się ludzi lub towarów; opracowywany jest na podstawie badań nad transportem, powinien uwzględniać różne rodzaje transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8650">
<skos:prefLabel>transport (fizyka)</skos:prefLabel>
<skos:definition>przenoszenie masy, pędu lub energii w systemie jako wynik ruchu molekularnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8653">
<skos:prefLabel>system transportu</skos:prefLabel>
<skos:definition>system ruchu składający się z linii komunikacyjnych takich jak drogi, tory kolejowe, szlaki wodne i korytarze powietrzne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8655">
<skos:prefLabel>chwytanie do pułapek</skos:prefLabel>
<skos:definition>łapanie zwierząt w pułapki, wnyki, sidła itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8658">
<skos:prefLabel>podróż</skos:prefLabel>
<skos:definition>przemieszczanie się z jednego do drugiego odległego miejsca z wykorzystaniem środka transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8659">
<skos:prefLabel>koszt podróży</skos:prefLabel>
<skos:definition>wydatek pewnej kwoty pieniędzy ponoszony z tytułu podróży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8663">
<skos:prefLabel>traktat</skos:prefLabel>
<skos:definition>międzynarodowe porozumienie spisane pomiędzy dwoma lub większą liczbą państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8664">
<skos:prefLabel>drzewo</skos:prefLabel>
<skos:definition>każda roślina wieloletnia dużych rozmiarów o wyraźnie wykształconym pniu, który w pewnej wysokości nad ziemią rozgałęzia się w koronę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8667">
<skos:prefLabel>szkółka</skos:prefLabel>
<skos:definition>dział ogrodnictwa i leśnictwa obejmujący hodowlę siewek i sadzonek w szkółkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8671">
<skos:prefLabel>kierunek</skos:prefLabel>
<skos:definition>kierunek, tendencja a w statystyce również zwrot w wielkości danych statystycznych dotyczących pewnego okresu lub odnoszących się do innych danych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8673">
<skos:prefLabel>triazyna</skos:prefLabel>
<skos:definition>azyna zawierająca trzy atomy azotu w cząsteczce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8675">
<skos:prefLabel>nawadnianie kroplowe</skos:prefLabel>
<skos:definition>metoda nawadniania polegająca na dostarczaniu wody dziurkowanymi rurkami lub emiterami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8679">
<skos:prefLabel>trzy główne zasady ochrony środowiska</skos:prefLabel>
<skos:definition>trzy główne zasady ochrony środowiska: zasada przezorności, zasada płacenia przez zatruwającego i zasada współpracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8680">
<skos:prefLabel>tryt</skos:prefLabel>
<skos:definition>izotop wodoru mający masę atomową 3; jedna z form ciężkiego wodoru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8683">
<skos:prefLabel>poziom troficzny</skos:prefLabel>
<skos:definition>poziom odżywczy, przez który następuje porces krążenia energii w ekosystemie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8685">
<skos:prefLabel>ekosystem tropikalny</skos:prefLabel>
<skos:definition>system życia biologicznego wraz z jego nieożywionym otoczeniem ziemi i wody w regionach równikowych pomiędzy zwrotnikami Raka i Koziorożca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8686">
<skos:prefLabel>ekosystem lasów tropiklanych</skos:prefLabel>
<skos:definition>system życia biologicznego wraz z jego nieożywionym otoczeniem w lasach regionów tropikalnych okołorównikowych, charakteryzujących się gorącym i wilgotnym klimatem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8687">
<skos:prefLabel>las tropikalny</skos:prefLabel>
<skos:definition>pokrywa roślinna na obszarach tropikalnych, składająca się z wysokich zwartych drzew o kolumowych pniach, mających rozłorzyste i często spłaszczone korony, poniżej których nie ma zazwyczaj gałęzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8688">
<skos:prefLabel>tropikalny las deszczowy</skos:prefLabel>
<skos:definition>najcenniejszy i najbogatszy ekosystem na Ziemi, występujący na obszarach tropikalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8693">
<skos:prefLabel>tropiki</skos:prefLabel>
<skos:definition>region kuli ziemskiej, rozciągający się pomiędzy zwrotnikami, czyli szerokością geograficzną północną 23°27&apos; (Zwrotnik Raka) i szerokością geograficną południową 23°27&apos; (Zwrotnik Koziorożca)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8695">
<skos:prefLabel>troposfera</skos:prefLabel>
<skos:definition>najniższa z koncentrycznie układających się warstw atmosfery, leżąca pomiędzy powierzchnią Ziemi a tropopauzą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8696">
<skos:prefLabel>ozon troposferyczny</skos:prefLabel>
<skos:definition>ozon troposferyczny jest zanieczyszczeniem wtórnym; powstaje w wyniku emisji tlenków azotu, lotnych związków organicznych poza metanem i tlenku węgla; ozon uszkadza tkankę płuc, powoduje odczucie kłucia w oczach i swędzenie gardła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8701">
<skos:prefLabel>droga główna</skos:prefLabel>
<skos:definition>droga główna, nadająca się do ciężkiego transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8703">
<skos:prefLabel>guz nowotworowy</skos:prefLabel>
<skos:definition>tkanka wywodząca się z prawidłowych tkanek organizmu, lecz wskutek utrwalonych cech patologicznych rozrastająca się w sposób nie podporządkowany harmonii ustrojowej i czynnikom regulującym wzrost, dojrzewanie i czynności komórek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8704">
<skos:prefLabel>tundra</skos:prefLabel>
<skos:definition>bezleśny obszar w północnej strefie podbiegunowej, pokryty roślinnością złożoną z mchów, porostów, nielicznych roślin zielnych (głównie traw), krzewinek oraz karłowatych brzóz i wierzb; również górskie piętro roślinne znajdujące się poniżej wiecznych śniegów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8707">
<skos:prefLabel>tunel</skos:prefLabel>
<skos:definition>podziemny przejazd, najczęściej dla pociągów i samochodów, prowadzący poprzez góry, pod rzekami lub gęsto zabudowanymi obszarami miejskimi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8711">
<skos:prefLabel>mętność</skos:prefLabel>
<skos:definition>zmętnienie zawiesiną cieczy, która w naturalnym stanie jest klarowna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8713">
<skos:prefLabel>turbina</skos:prefLabel>
<skos:definition>maszyna napędzna energią przepływającej cieczy, którą zamienia w energię obrotową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8719">
<skos:prefLabel>zbiornikowiec międzypokładowy</skos:prefLabel>
<skos:definition>statek pełnomorski posiadający wolną przestrzeń pomiędzy dwoma poziomami pokładów, która służy do przewożenia towarów masowych, takich jak ciecze i gazy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8720">
<skos:prefLabel>zbiornikowiec z podwójnym kadłubem</skos:prefLabel>
<skos:definition>zbiornikowiec do przewozu ropy, który zgodnie z wymogami bezpieczeństwa posiada podwójny kadłub</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8721">
<skos:prefLabel>silnik dwusuwowy</skos:prefLabel>
<skos:definition>silnik spalinowy tłokowy, w którym pełny obieg odbywa się w czasie dwu kolejnych suwów tłoka, co odpowiada jednemu pełnemu obrotowi wału korbowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8722">
<skos:prefLabel>rodzaj działalności gospodarczej</skos:prefLabel>
<skos:definition>rodzaj lub kategoria przedsiębiorstwa lub instytucji prowadzącej działalność gospodarczą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8723">
<skos:prefLabel>rodzaj roszczeń</skos:prefLabel>
<skos:definition>rodzaj korzyści lub odszkodowań uznanych przez prawo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8724">
<skos:prefLabel>sposób zarządzania</skos:prefLabel>
<skos:definition>różne specyficzne metody administrowania przedsiębiorstwem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8727">
<skos:prefLabel>tajfun</skos:prefLabel>
<skos:definition>groźny cyklon tropikalny na zachodnim Pacyfiku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8731">
<skos:prefLabel>opona</skos:prefLabel>
<skos:definition>sprężysta powłoka z pokrytych gumą i złączonych ze sobą warstw tkaniny, stanowiąca zewnętrzną, stykającą się z nawierzchnią drogi część ogumienia pojazdów w celu zapewnienia lepszej trakcji i zmniejszenia wstrząsów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8736">
<skos:prefLabel>ultrafiltracja</skos:prefLabel>
<skos:definition>oddzielanie bardzo drobnych cząstek poprzez filtrację w mikroporach lub półprzepuszczalnych ośrodkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8739">
<skos:prefLabel>ultradźwięk</skos:prefLabel>
<skos:definition>fale dźwiękowe o częstotliwości 20 000 Hz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8742">
<skos:prefLabel>promieniowanie ultrafioletowe</skos:prefLabel>
<skos:definition>część spektrum elektromagnetycznego o długości fal krótszych od światła i dłuższych od promieniowania rentgenowskiego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8745">
<skos:prefLabel>Konferencja Narodów Zjednoczonych &quot;Środowisko i Rozwój&quot;</skos:prefLabel>
<skos:definition>konferencja Narodów Zjednoczonych &quot;Środowisko i Rozwój&quot;, Rio de Janeiro, Brazylia, 1992</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8747">
<skos:prefLabel>nielegalne składowisko odpadów</skos:prefLabel>
<skos:definition>porzucanie odpadów w miejscach niedozwolonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8755">
<skos:prefLabel>uszczelnianie podłoża składowisk odpadów</skos:prefLabel>
<skos:definition>uszczelnianie podłoża składowisk odpadów względnie nieprzepuszczalną barierą w celu zatrzymywania odcieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8758">
<skos:prefLabel>zbiornik podziemny</skos:prefLabel>
<skos:definition>zbiornik usytuowany pod ziemią, przeznaczony do przechowywania benzyny i innych produktów naftowych lub chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8760">
<skos:prefLabel>pociąg podziemny</skos:prefLabel>
<skos:definition>kolej przeznaczoną do transportu pasażerskiego, zazwyczaj usytuowana pod ziemią w celu zmniejszenia ruchu ulicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8761">
<skos:prefLabel>grupa społecznie i ekonomicznie upośledzona</skos:prefLabel>
<skos:definition>grupa społeczeństwa, nie mająca dostępu do praw lub korzyści zagwarantowanych dla reszty społeczństwa, często z powodu niskiego statusu ekonomicznego lub społecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8762">
<skos:prefLabel>usługi pogrzebowe</skos:prefLabel>
<skos:definition>działalność gospodarcza związana z obsługą zmarłych, pochówkiem i urządzaniem pogrzebów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8766">
<skos:prefLabel>wylot podwodny</skos:prefLabel>
<skos:definition>miejsce wypływu wody poniżej poziomu morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8768">
<skos:prefLabel>bezrobocie</skos:prefLabel>
<skos:definition>zjawisko braku pracy zarobkowej dla osób zdolnych do pracy i poszukujących jej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8769">
<skos:prefLabel>kopytne</skos:prefLabel>
<skos:definition>Ungulata, ssaki, których końcowe człony palców zaopatrzone są w kopyta; należą do nich rzędy: góralków, nieparzystokopytnych i parzystokopytnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8776">
<skos:prefLabel>Narody Zjednoczone</skos:prefLabel>
<skos:definition>organizacja zrzeszająca ponad 180 państw świata, której celem jest utrzymanie międzynarodowego pokoju i bezpieczeństwa; rozwiazywanie w drodze międzynarodowej współpracy zagadnień o charakterze gospodarczym, społecznym, kulturalnym i humanitarnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8780">
<skos:prefLabel>benzyna bezołowiowa</skos:prefLabel>
<skos:definition>benzyna o niskim poziomie oktanowym, bez dodatku ołowiu, powodująca mniejsze zanieczyszczenie ołowiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8782">
<skos:prefLabel>obszar czystego powietrza</skos:prefLabel>
<skos:definition>obszary, na których osiągnięto istotną redukcję zanieczyszczeń ozonotwórczych dzięki inicjatywom przemysłu polegającym na kontroli i/lub przeciwdziałaniu zanieczyszczeniom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8784">
<skos:prefLabel>woda surowa</skos:prefLabel>
<skos:definition>woda, która nie została oczyszczana</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8785">
<skos:prefLabel>wychowanie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8789">
<skos:prefLabel>senat</skos:prefLabel>
<skos:definition>izba wyższa dwuizbowego parlamentu; ciało w dwuizbowym systemie legislacyjnym, składające się z przedstawicieli krajów (stanów) członkowskich federacji lub wybranej liczby osób z pewnych uprzywilejowanych stanów lub klas społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8791">
<skos:prefLabel>uran</skos:prefLabel>
<skos:definition>(U) promieniotwórczy pierwiastek chemiczny, o liczbie atomowej 92, srebrzystobiały metal ciężki z podgrupy aktynowców, wysoce toksyczny i aktywny chemicznie, nie mający trwałych izotopów, stosowany jako paliwo jądrowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8794">
<skos:prefLabel>program działań ekologiczno-urbanistycznych</skos:prefLabel>
<skos:definition>zaplanowane, skoordynowane działania lub usługi mające na celu poprawę warunków życia i zdrowia w centrach miast, wydajniejszy transport i komunikację, odpowiednią infrastukturę publiczną i estetykę otoczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8797">
<skos:prefLabel>teren miejski</skos:prefLabel>
<skos:definition>obszar określony granicami administracyjnymi miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8801">
<skos:prefLabel>rozpad miast</skos:prefLabel>
<skos:definition>stan, w którym część miasta ulega zaniedbaniu, zanieczyszczeniu i ruinie, w związku opuszczeniem tej części przez zamożniejszych mieszkańców, przedsiębiorstwa handlowe i usługowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8803">
<skos:prefLabel>planowanie miast</skos:prefLabel>
<skos:definition>plan, zarys lub wstępny szkic rozwoju miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8804">
<skos:prefLabel>urbanizacja</skos:prefLabel>
<skos:definition>rozbudowa przestrzenna miast lub zmiany użytkowania w obszarze metropolitalnym, często polegające na strefowaniu i intensyfikacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8805">
<skos:prefLabel>prawodawstwo dotyczące rozwoju miast</skos:prefLabel>
<skos:definition>zespół aktów prawnych ustanowionych przez rząd, regulujących zasady planowania przestrzennego, rozwoju i rozbudowy usług publicznych, mieszkalnictwa, handlu i przemysłu, na terenach miejskich o umiarkowanej lub dużej gęstości zaludnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8807">
<skos:prefLabel>ekosystem miejski</skos:prefLabel>
<skos:definition>miasta postrzegane jako ekosystemy z charakterystycznym dopływem materii i energii, recyklingiem wewnątrz systemu oraz odpływem materii i energii do otoczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8811">
<skos:prefLabel>usługi komunalne</skos:prefLabel>
<skos:definition>zapewnienie dostawy podstawowych usług dla ludności, jak energia elektryczna, woda i gaz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8812">
<skos:prefLabel>przepływ zasobów w miastach</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8815">
<skos:prefLabel>zieleń miejska</skos:prefLabel>
<skos:definition>zespół prywatnych i publicznych ogrodów na obszarze miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8820">
<skos:prefLabel>urbanizacja</skos:prefLabel>
<skos:definition>proces społeczny i kulturowy wyrażający się w rozwoju miast i zwiększaniu się społeczności o cechach miejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8824">
<skos:prefLabel>krajobraz miejski</skos:prefLabel>
<skos:definition>cechy, formy i struktury geograficznego obszaru miasta, wraz z jego biologiczną kompozycją, środowiskiem nieożywionym i formami społecznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8826">
<skos:prefLabel>zarządzanie miastami</skos:prefLabel>
<skos:definition>administrowanie, zarządzanie i planowanie miast, zwłaszcza w zakresie przekształcania obszarów rolniczych lub niedoinwestowanych w tereny biurowe, handlowe, mieszkalne i in. związane z rozwojem miast</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8829">
<skos:prefLabel>planowanie miejskie</skos:prefLabel>
<skos:definition>projektowanie, organizowanie i przygotowanie przyszłego wyglądu miasta i jego kondycji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8832">
<skos:prefLabel>polityka miejska</skos:prefLabel>
<skos:definition>plan działań przyjęty i wykonywany przez rząd, przedsiębiorstwo lub inną organizację, który ma na celu poprawę lub rozwój miast poprzez planowanie przestrzenne, gospodarkę wodną, rozwój centrów miast, przestrzeganie prawa, ładu i porządku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8833">
<skos:prefLabel>zanieczyszczenia miast</skos:prefLabel>
<skos:definition>zanieczyszczenia gęsto zaludnionych obszarów, spowodowane przez pojazdy mechaniczne, obiekty przemysłowe, ciepłownie, spalarnie itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8834">
<skos:prefLabel>ludność miejska</skos:prefLabel>
<skos:definition>ludność, która zamieszkuje na obszarach miejskich lub w jednostkach osadniczych, których liczba mieszkańców klasyfikuje ją jako miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8839">
<skos:prefLabel>rewaloryzacja miast</skos:prefLabel>
<skos:definition>proces przebudowy obszarów miejskich poprzez odnowę i przywracanie funkcjonalności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8840">
<skos:prefLabel>sanitacja miast</skos:prefLabel>
<skos:definition>renowacja lub remont zdewastowanych obszarów miast poprzez wyburzanie lub odnawianie istniejących budynków oraz ogólna poprawa warunków środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8841">
<skos:prefLabel>osiedle miejskie</skos:prefLabel>
<skos:definition>zespół zabudowy mieszkalnej w obrębie miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8843">
<skos:prefLabel>stres spowodowany życiem w mieście</skos:prefLabel>
<skos:definition>stan napięcia fizycznego lub psychicznego wywołany życiem w mieście, lub czynniki fizyczne, chemiczne i emocjonalne powodujące to napięcie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8844">
<skos:prefLabel>struktura miejska</skos:prefLabel>
<skos:definition>zabudowa, system ulic i infrastruktura skłądające się na jednostkę miejską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8845">
<skos:prefLabel>urbanistyka</skos:prefLabel>
<skos:definition>badania i teoria zabudowy oraz innych zgadanień planowania przestrzennego w miastach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8847">
<skos:prefLabel>ruch uliczny</skos:prefLabel>
<skos:definition>ruch pojazdów i osób w mieście</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8850">
<skos:prefLabel>woda miejska</skos:prefLabel>
<skos:definition>woda przeznaczona do prywatnego i publicznego użytku w mieście</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8852">
<skos:prefLabel>zaopatrzenie miasta w wodę</skos:prefLabel>
<skos:definition>dostarczanie wody wraz z jej poborem, oczyszczaniem i magazynowaniem na potrzeby komunalne i przemysłowe obszarów miejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8857">
<skos:prefLabel>niedźwiedź</skos:prefLabel>
<skos:definition>rodzina ssaków z rzędu Carnivora, obejmująca niedźwiedzie i ich najbliższych krewniaków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8866">
<skos:prefLabel>zużyta opona</skos:prefLabel>
<skos:definition>materiał nadający się do recyklingu, pochodzący z zużytych opon pojazdów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8870">
<skos:prefLabel>wypoczynek</skos:prefLabel>
<skos:definition>wykorzystanie czasu wolnego na rekreację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8872">
<skos:prefLabel>przewaga użytkownika</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8875">
<skos:prefLabel>wykorzystanie kaloryczności</skos:prefLabel>
<skos:definition>wykorzystanie wartości opałowej, czyli ilości ciepła pochodzącej z całkowitego spalenia jednostki masy danej substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8876">
<skos:prefLabel>użytkowanie surowców miejscowych</skos:prefLabel>
<skos:definition>użytkowanie surowców pochodzących z najbliższej okolicy lub regionu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8877">
<skos:prefLabel>stosowanie pestycydów</skos:prefLabel>
<skos:definition>stosowanie chemicznych lub biologicznych substancji do celowego wyniszczenia niepożądanych roślin lub zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8879">
<skos:prefLabel>użytkowanie surowców</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8886">
<skos:prefLabel>nieokreślone warunki umowy</skos:prefLabel>
<skos:definition>warunek lub okres w umowie rządowej lub porozumieniu, któremu brak precyzji, określającej cechy lub ścisły zakres</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8887">
<skos:prefLabel>dolina</skos:prefLabel>
<skos:definition>obniżenie powierzchni ziemi, otoczone wyżej położonymi obszarami, stosunkowo duże i najczęsciej wydłużone, z ciekiem lub jego korytem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8892">
<skos:prefLabel>oceniany składnik ekosystemu</skos:prefLabel>
<skos:definition>wartościowany, oceniony lub oszacowany element lub składnik środowiska biologicznego i jego nieożywionego otoczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8893">
<skos:prefLabel>wanad</skos:prefLabel>
<skos:definition>srebrzystoszary, ciągliwy metal, odporny na korozję, używany w stopach stali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8894">
<skos:prefLabel>wandalizm</skos:prefLabel>
<skos:definition>specjalne, złośliwe lub bezmyślne niszczenie własności prywatnej lub publicznej, dokonywane przez wandali</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8895">
<skos:prefLabel>gatunek wymarły</skos:prefLabel>
<skos:definition>gatunek, który zniknął z jakiegoś obszaru w wyniku niekorzystnych warunków środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8898">
<skos:prefLabel>ciśnienie pary</skos:prefLabel>
<skos:definition>ciśnienie pary wodnej w atmosferze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8899">
<skos:prefLabel>lakier</skos:prefLabel>
<skos:definition>przezroczysta płynna powłoka z rozpuszczonych substancji żywicznych, wysychająca i twardniejąca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8906">
<skos:prefLabel>nosiciel chorób ludzkich</skos:prefLabel>
<skos:definition>czynnik lub organizm, który przenosi lub przekazuje choroby ludzkie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8908">
<skos:prefLabel>pokrywa roślinna</skos:prefLabel>
<skos:definition>rośliny rosnące na pewnym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8909">
<skos:prefLabel>uprawa roślin</skos:prefLabel>
<skos:definition>uprawa roślin w celach użytkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8910">
<skos:prefLabel>ekologia roślin</skos:prefLabel>
<skos:definition>badanie związków pomiędzy roślinami i ich środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8912">
<skos:prefLabel>dobór roślin</skos:prefLabel>
<skos:definition>dokonywany przez człowieka wybór poszczególnych genotypów z populacji roślin ze względu na ich fenotypowe cechy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8913">
<skos:prefLabel>olej roślinny</skos:prefLabel>
<skos:definition>jadalny mieszany olej otrzymywany z owoców, liści i nasion roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8918">
<skos:prefLabel>rozmnażanie się roślin</skos:prefLabel>
<skos:definition>procesy płciowe i bezpłciowe, w wyniku których roślina produkuje jedną lub wiele podobnych do siebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8919">
<skos:prefLabel>zasoby roślinne</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8920">
<skos:prefLabel>warzywo</skos:prefLabel>
<skos:definition>roślina zielna, której jadalne części są spożywane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8921">
<skos:prefLabel>odpady warzywne</skos:prefLabel>
<skos:definition>odpady składające się głównie z części warzywnych, ulegających rozkładowi przez mikroorganizmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8922">
<skos:prefLabel>roślinność</skos:prefLabel>
<skos:definition>1) roślinność określonego obszaru, w ujęciu ogólnym lub jako zbiorowiska roślin, wyłączając ujęcie taksonomiczne, tj. nie poszczególne gatunki roślin; całość pokrywy roślinnej określonego obszaru lub kuli ziemskiej; 2) wszystkie formy roślinne na danym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8925">
<skos:prefLabel>typ roślinności</skos:prefLabel>
<skos:definition>zespół roślinny o pewnych odróżnialnych cechach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8926">
<skos:prefLabel>pojazd</skos:prefLabel>
<skos:definition>środek transportu, którym przewożeni są ludzie lub towary</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8929">
<skos:prefLabel>spaliny pojazdów</skos:prefLabel>
<skos:definition>smuga bardzo drobnych cząstek, zawieszonych w powietrzu, powstałych w procesie pracy silników spalaninowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8931">
<skos:prefLabel>kontrola pojazdów</skos:prefLabel>
<skos:definition>oficjalne okresowe sprawdzanie samochodów, ciężarówek, samolotów i innych środków transportu w celu określenia zgodności z obowiązującymi normami bezpieczeństwa lub emisji zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8934">
<skos:prefLabel>przemysł środków transportu</skos:prefLabel>
<skos:definition>gałąź gospodarki, która obejmuje przedsiębiorstwa produkujące i sprzedające środki transportu osobowego i towarowego, poruszające się po lądzie, po wodzie i w powietrzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8940">
<skos:prefLabel>klimatyzacja</skos:prefLabel>
<skos:definition>utrzymywanie w pomieszczeniach zamkniętych odpowiedniego ciśnienia, temperatury, wilgotności, składu powietrza itp. niezależnie od warunków zewnętrznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8944">
<skos:prefLabel>robactwo</skos:prefLabel>
<skos:definition>drobne zwierzęta bezkręgowe, owady zwłaszcza insekty lub pasoży, które mogą być niebezpieczne i trudne do opanowania, jeśli pojawiają się w dużej liczbie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8946">
<skos:prefLabel>kręgowce</skos:prefLabel>
<skos:definition>kręgowce z podtypu Vertebrata, charakteryzujące się kostnym lub chrzęstnym szkieletem i dobrze rozwiniętym mózgiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8951">
<skos:prefLabel>weterynaria</skos:prefLabel>
<skos:definition>częśc medycyny zajmująca się chorobami zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8955">
<skos:prefLabel>wiadukt</skos:prefLabel>
<skos:definition>rodzaj mostu zbudowanego nad przeszkodą terenową, inną niż woda, np. nad wąwozem, doliną, drogą komunikacyjną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8959">
<skos:prefLabel>wibracje</skos:prefLabel>
<skos:definition>okresowe drganie o małej amplitudzie i wysokiej częstotliwości, charakterystyczne dla elastycznych ciał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8964">
<skos:prefLabel>magnetowid</skos:prefLabel>
<skos:definition>urządzenie telewizyjne do zapisywania obrazu i dźwięku na taśmie magnetycznej w celu ich późniejszego odtworzenia na ekranie odbiornika telewizyjnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8967">
<skos:prefLabel>wieś</skos:prefLabel>
<skos:definition>osiedle z zabudowaniami mieszkalnymi i innymi budynkami, takimi jak kościół, szkoła czy sklepy, mniejsze od miasteczka, zwykle na terenach wiejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8969">
<skos:prefLabel>wywar melasowy</skos:prefLabel>
<skos:definition>ciekła pozostałość z destylacji alkoholu, najczęściej pochodzącego z fermentacji melasy z buraków cukrowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8974">
<skos:prefLabel>wirusologia</skos:prefLabel>
<skos:definition>badanie submikroskopowych organizmów znanych jako wirusy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8976">
<skos:prefLabel>wirus</skos:prefLabel>
<skos:definition>submikroskopowy organizm, pasożyt rozmnażający się tylko w żywych komórkach organizmów właściwych; wywołuje choroby u ludzi, zwierząt i roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8977">
<skos:prefLabel>lepkość</skos:prefLabel>
<skos:definition>tarcie wewnętrzne, opór stawiany przez ośrodek płynny podczas ruchu jednych jego warstw względem drugich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8986">
<skos:prefLabel>witamina</skos:prefLabel>
<skos:definition>związki organiczne niezbędne dla podtrzymania funkcji życiowych organizmu, obecne w zmiennych i niewielkich ilościach w żywności naturalnej, produkowane także przez przemysł farmaceutyczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8987">
<skos:prefLabel>uprawa winorośli</skos:prefLabel>
<skos:definition>działalność rolnicza polegajaca na uprawie winorośli, badaniu odmian winogron, metod uprawy oraz zwalczaniu chorób i owadów pasożytniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8990">
<skos:prefLabel>słownik</skos:prefLabel>
<skos:definition>zbiór wyrazów ułożonych i opracowanych według pewnej zasady, zwykle alfabetycznie, najczęściej objaśnianych pod względem znaczeniowym i ilustrowanych przykładami użycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8992">
<skos:prefLabel>szkolenie zawodowe</skos:prefLabel>
<skos:definition>szkolenie specjalistyczne dla uzupełnienia wykształcenia zawodowego odpowiednich osób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8993">
<skos:prefLabel>lotny związek organiczny</skos:prefLabel>
<skos:definition>związek organiczny, który latwo paruje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8995">
<skos:prefLabel>lotność</skos:prefLabel>
<skos:definition>właściwość substancji, polegająca na przechodzeniu w stan pary lub gaz bez zmiany chemicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8997">
<skos:prefLabel>obszar wulkaniczny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/8999">
<skos:prefLabel>erupcja wulkaniczna</skos:prefLabel>
<skos:definition>wyrzucenie z wulkanu materiałów stałych, ciekłych i gazowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9003">
<skos:prefLabel>wulkanizm</skos:prefLabel>
<skos:definition>ogół zjawisk związanych z działaniem wulkanów; procesy zachodzące w skorupie ziemskiej, związane z przemieszczaniem się lawy i towarzyszących jej gazów, aż do jej wydostania się na zewnątrz do atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9004">
<skos:prefLabel>wulkan</skos:prefLabel>
<skos:definition>góra w kształcie stożka z kraterem na szczycie stanowiącym wylot komina wulkanicznego, przez który wydobywają się z głębi ziemi lawa, gazy i popioły wulkaniczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9008">
<skos:prefLabel>ogólna ilość odpadów</skos:prefLabel>
<skos:definition>całkowita ilość odpadów lub bezużytecznych materiałów wchodzących do procesu lub systemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9010">
<skos:prefLabel>praca woluntariuszy</skos:prefLabel>
<skos:definition>niepłatna działaność obywateli, często stowarzyszonych w organizacjach obsługujących innych, np. starszych, ubogich, niepełnosprawnych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9013">
<skos:prefLabel>wulkanizacja</skos:prefLabel>
<skos:definition>proces technologiczny polegający na działaniu siarką lub jej niektórymi związkami na kauczuk naturalny lub syntetyczny w celu otrzymania tworzyw o cennych właściwościach chemicznych i mechanicznych, np. gumy, ebonitu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9015">
<skos:prefLabel>gatunki podatne na zagrożenie</skos:prefLabel>
<skos:definition>gatunki, które mogą stać się zagrożonymi, jeśli nie zostaną podjęte działania ochronne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9019">
<skos:prefLabel>Wadden zee</skos:prefLabel>
<skos:definition>płytkie morze u wybrzeży Holandii, Niemiec i Danii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9022">
<skos:prefLabel>system płac</skos:prefLabel>
<skos:definition>system wynagradzania pracowników przez wypłacanie ustalonej kwoty za jednostkę, godzinę, dzień lub inny okres, pokrywający wszelkie świadczenia wraz z pensją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9023">
<skos:prefLabel>ściana</skos:prefLabel>
<skos:definition>pionowa konstrukcja z kamienia, cegły, drewna itp., o długości i wysokości znacznie większej niż grubość, stosowana do zamykania jakiegoś miejsca, dzielenia lub wspierania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9028">
<skos:prefLabel>wojna</skos:prefLabel>
<skos:definition>konflikt lub stan wrogości pomiędzy dwoma lub więcej stronami, narodami lub państwami, w którym są używane siły zbrojne i operacje militarne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9032">
<skos:prefLabel>zwierzę stałocieplne</skos:prefLabel>
<skos:definition>zwierzęta o prawie stałej temperaturze ciała, niezależnej od temperatury otoczenia (ssaki, ptaki)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9034">
<skos:prefLabel>plan ostrzegania</skos:prefLabel>
<skos:definition>z góry opracowany schemat lub metoda działania mająca na celu jak najszybsze ostrzeganie zagrożonej ludności przed nagłym niebezpieczeństwem i innymi niespodziewanymi zdarzeniami wymagającymi natychmaistowej reakcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9035">
<skos:prefLabel>system ostrzegania</skos:prefLabel>
<skos:definition>zestaw procedur i urządzeń przeznaczonych do wykrywania nagłego lub potencjalnego zagrożenia osób, mienia lub środowiska, często z zastosowaniem techniki radarowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9040">
<skos:prefLabel>marnotrawstwo</skos:prefLabel>
<skos:definition>lekkomyślne, bezużyteczne, nieoszczędne gospodarowanie czymś</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9041">
<skos:prefLabel>odpady</skos:prefLabel>
<skos:definition>materiały nie nadające się do użycia, pozostałe po procesie produkcji przemysłowej, rolniczej lub innej działalności człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9042">
<skos:prefLabel>wyziewy</skos:prefLabel>
<skos:definition>wyziewy lub gazy wypuszczane podczas procesów przemysłowych, produkcyjnych lub chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9043">
<skos:prefLabel>oczyszczanie wyziewów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9044">
<skos:prefLabel>analiza odpadów</skos:prefLabel>
<skos:definition>badania mające na celu ustalenie najbardziej odpowiedniej metody traktowania różnych rodzajów odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9045">
<skos:prefLabel>zdolność środowiska do asymilacji odpadów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9048">
<skos:prefLabel>bilans odpadów</skos:prefLabel>
<skos:definition>inwentaryzacja wszystkich odpadów wyprodukowanych lub odzyskanych w określonym czasie, pogrupowanych wg rodzajów i ilości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9049">
<skos:prefLabel>pojemnik na odpady</skos:prefLabel>
<skos:definition>kontener na śmieci, odpady itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9051">
<skos:prefLabel>rodzaj odpadów</skos:prefLabel>
<skos:definition>wydzielenie rodzaju nieużytecznego materiału pozostawionego po jakimś procesie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9052">
<skos:prefLabel>opłata za odpady</skos:prefLabel>
<skos:definition>ustalona opłata, wydatek lub koszt związany z zagospodarowaniem odpadów lub niepotrzebnych materiałów pozostałych po procesie produkcyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9053">
<skos:prefLabel>klasyfikacja odpadów</skos:prefLabel>
<skos:definition>organizacja podziału zbędnych materiałów odpadowych powstałych w procesie produkcyjnym lub odpadów komunalnych lub zwierzęcych na kategorie oparte na ich cechach chemicznych, mikrobiologicznych lub innych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9054">
<skos:prefLabel>zbiórka odpadów</skos:prefLabel>
<skos:definition>okresowe usuwanie odpadów stałych lub wywożenie ich na żądanie, z miejsca ich powstania, poprzez stosowanie odpowiednich pojazdów odwożących je na wysypisko lub inne wyznaczone miejsce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9055">
<skos:prefLabel>zbiórka odpadów u źródła</skos:prefLabel>
<skos:definition>zbieranie odpadów w miejscu ich powstawania i wywożenie ich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9057">
<skos:prefLabel>skład odpadów</skos:prefLabel>
<skos:definition>skład materiałów odpadowych określony procentowo lub wagowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9058">
<skos:prefLabel>techniki przeróbki odpadów</skos:prefLabel>
<skos:definition>wyspecjalizowana procedura lub metoda stosowana do przetworzenia odpadów z jednej formy, stanu lub składu chemicznego w inną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9059">
<skos:prefLabel>degazyfikacja odpadów</skos:prefLabel>
<skos:definition>usuwanie gazowych składników z odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9062">
<skos:prefLabel>usuwanie odpadów</skos:prefLabel>
<skos:definition>zorganizowany proces usuwania zbędnych lub niepotrzebnych materiałów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9063">
<skos:prefLabel>ustawodawstwo dotyczące gospodarki odpadami</skos:prefLabel>
<skos:definition>przepisy i regulacje prawne dotyczące składowania, recyklingu i przetwarzania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9065">
<skos:prefLabel>koszt gospodarki odpadami</skos:prefLabel>
<skos:definition>koszty poniesione w związku z usuwaniem lub wywożeniem odpadów lub zbędnych materiałów pozostałych po procesie produkcyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9067">
<skos:prefLabel>składowanie odpadów w terenie</skos:prefLabel>
<skos:definition>zaplanowane wysypywanie, składowanie lub zakopywnie odpadów lub innych nieużytecznych materiałów wypełniających wgłębienia na powierzchni ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9068">
<skos:prefLabel>opłata za składowanie odpadów</skos:prefLabel>
<skos:definition>ustanowiona opłata, wydatek lub koszt związany z usuwaniem lub wywożeniem odpadów lub niepotrzebnych materiałów pozostałych po procesie produkcyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9069">
<skos:prefLabel>wysypisko odpadów</skos:prefLabel>
<skos:definition>miejsce składowania i zasypywania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9071">
<skos:prefLabel>wyrzucanie odpadów</skos:prefLabel>
<skos:definition>niekontrolowane wyrzucanie odpadów stałych w terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9072">
<skos:prefLabel>wymiana odpadów</skos:prefLabel>
<skos:definition>wymiana części odpadów nadających się do recyklingu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9073">
<skos:prefLabel>eksport odpadów</skos:prefLabel>
<skos:definition>wywóz zbędnych materiałów wraz z odpadami pozostałymi po procesie produkcyjnym i śmieciami do innych krajów lub obszarów w ramach handlu zagranicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9074">
<skos:prefLabel>elektrociepłownia opalana odpadami</skos:prefLabel>
<skos:definition>elektrociepłownia, która spala śmieci jako paliwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9075">
<skos:prefLabel>ciepłownia opalana odpadami</skos:prefLabel>
<skos:definition>ciepłownia, która spala śmieci jako paliwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9076">
<skos:prefLabel>elektrownia opalana odpadami</skos:prefLabel>
<skos:definition>elektrownia wykorzystująca paliwo pochodzące z odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9077">
<skos:prefLabel>gazy odpadowe</skos:prefLabel>
<skos:definition>zbędne gazy lub zawieszone w powietrzu drobiny powstałe podczas procesu produkcyjnego lub spalania na ogrodzonym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9078">
<skos:prefLabel>emisja gazów odpadowych</skos:prefLabel>
<skos:definition>bezpośrednie lub pośrednie wydalanie gazów wydechowych do atmosfery</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9079">
<skos:prefLabel>badanie gazów odpadowych</skos:prefLabel>
<skos:definition>analiza ilościowa i jakościowa gazów wydechowych emitowanych przez pojazdy, przemysł itp. w celu określenia ich składu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9080">
<skos:prefLabel>redukcja gazów odpadowych</skos:prefLabel>
<skos:definition>zmniejszanie ilości emisji gazowych do atmosfery, pochodzących z pojazdów mechanicznych, przemysłu i ciepłowni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9082">
<skos:prefLabel>odpady szklane</skos:prefLabel>
<skos:definition>materiał odpadowy pochodzący z procesu produkcji szkła lub ze zużytych wyrobów szklanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9083">
<skos:prefLabel>gips z odsiarczania spalin</skos:prefLabel>
<skos:definition>produkt uboczny mokrej metody odsiarczania spalin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9085">
<skos:prefLabel>ciepło odpadowe</skos:prefLabel>
<skos:definition>ciepło pochodzące z procesu chłodzenia w elektrowniach, które może powodować niebezpieczne podgrzanie wody, sprzyjające zakwitom glonów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9086">
<skos:prefLabel>ulot ciepła odpadowego</skos:prefLabel>
<skos:definition>uwalnianie się ciepła jako produktu ubocznego w procesach produkcji przemysłowej lub energetycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9087">
<skos:prefLabel>użytkowanie ciepła odpadowego</skos:prefLabel>
<skos:definition>użytkowanie ciepła odpadowego obejmuje ogrzewanie i chłodzenie w miastach, odmrażanie zalodzonych dróg wodnych, stymulację wzrostu roślin w rolnictwie i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9088">
<skos:prefLabel>pozwolenie na import odpadów</skos:prefLabel>
<skos:definition>pozwolenie, licencja lub inny dokument wystawiony przez agencję rządową, zatwierdzające przywóz z zagranicy śmieci lub odpadów powstałych w procesie produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9090">
<skos:prefLabel>spalarnia odpadów</skos:prefLabel>
<skos:definition>urządzenie do spalania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9091">
<skos:prefLabel>prawodawstwo dotyczące odpadów</skos:prefLabel>
<skos:definition>proces powstawania aktów prawodawczych regulujących składowanie odpadów pozostałych po procesie produkcyjnym lub odpady komunalne, bytowe lub zwierzęce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9092">
<skos:prefLabel>zagospodarowanie odpadów</skos:prefLabel>
<skos:definition>całkowity nadzór nad wytwarzaniem odpadów, przechowywaniem i transportem z miejsca pochodzenia do ostateczengo i akceptowalnego miejsca składowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9095">
<skos:prefLabel>procedura minimalizacji ilości odpadów</skos:prefLabel>
<skos:definition>metody i technologie, wraz z planami i dyrektywami, które zmniejszają ilość wytwarzanych odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9096">
<skos:prefLabel>potencjał redukcji odpadów</skos:prefLabel>
<skos:definition>potencjalna zdolność metod i technologii do zmniejszania ilości wytwarzanych odpadów, szczególnie podczas procesu produkcji przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9098">
<skos:prefLabel>oleje odpadowe</skos:prefLabel>
<skos:definition>odpady olejowe przemysłowe, energetyczne i motoryzacyjne, które powinny być odpowiednio składowane lub właściwie przerabiane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9099">
<skos:prefLabel>papiery odpadowe</skos:prefLabel>
<skos:definition>gazety, magazyny, kartony i inny papier oddzielony od pozostałych odpadów stałych, w celu ponownego przerobu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9103">
<skos:prefLabel>przemysł przetwarzania odpadów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9105">
<skos:prefLabel>odzysk surowców odpadowych</skos:prefLabel>
<skos:definition>proces zbierania i segregowania odpadów w celu przygotowania ich do ponownego użycia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9106">
<skos:prefLabel>odzyskiwanie surowców</skos:prefLabel>
<skos:definition>proces uzyskiwania materiałów lub energii z odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9107">
<skos:prefLabel>recykling odpadów</skos:prefLabel>
<skos:definition>metoda odzyskiwania odpadów jako zasobów, obejmująca ich zbiórkę i przerób w celu zastąpnienia całości lub części surowców w produkcji przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9108">
<skos:prefLabel>redukcja odpadów</skos:prefLabel>
<skos:definition>praktyka, która zmniejsza ilość odpadów wytwarzanych przez konkretne źródło poprzez przeprojektowanie produktów lub wzrorców produkcji lub konsumpcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9110">
<skos:prefLabel>przemysł usuwania odpadów</skos:prefLabel>
<skos:definition>całość firm komercyjnych zajmujących się głównie likwidacją lub usuwaniem odpadów z obszarów zamieszkiwanych przez ludzi lub zwierzęta lub materiałów odpadowych pozostalych po procesie produkcyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9116">
<skos:prefLabel>segregowanie odpadów</skos:prefLabel>
<skos:definition>oddzielanie odpadów od siebie, poprzez segregację szkła, metalu, papieru, plastiku itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9118">
<skos:prefLabel>przechowywanie odpadów</skos:prefLabel>
<skos:definition>czasowe przechowywwanie odpadów oczekujących na przerób lub składowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9119">
<skos:prefLabel>transport odpadów</skos:prefLabel>
<skos:definition>przewożenie odpadów za pomocą specjalnych pojazdów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9121">
<skos:prefLabel>przerób odpadów</skos:prefLabel>
<skos:definition>proces lub zespół procesów, które zmieniają skład i właściwości chemiczne, fizyczne lub biologiczne odpadów lub zmniejszają bądź likwidują ich niebezpieczne cechy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9123">
<skos:prefLabel>zakład utylizacji odpadów</skos:prefLabel>
<skos:definition>miejsce, gdzie materiały odpadowe są obrabiane w celu uzdatnienia ich do ponownego przerobu lub do bezpiecznego składowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9125">
<skos:prefLabel>zużycie odpadów</skos:prefLabel>
<skos:definition>włączenie odpadów do naturalnego lub sztucznego obiegu, głównie w celu odzyskania surowców wtórnych lub energii</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9126">
<skos:prefLabel>masa odpadów</skos:prefLabel>
<skos:definition>całkowita ilość odpadów lub bezużytecznych materiałów wytworzonych przez jakieś źródło</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9127">
<skos:prefLabel>ścieki</skos:prefLabel>
<skos:definition>zużyta lub zbędna woda, odprowadzana lub wyciekająca z kanałów, rowów i zbiorników, za które odpowiadają prawnie ich właściciele</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9128">
<skos:prefLabel>opłata za ścieki</skos:prefLabel>
<skos:definition>ustalona opłata, wydatek lub koszt związany z zagospodarowaniem zużytej wody, zawierającej substancje rozpuszczone lub zawiesiny pochodzące z gospodarstw domowych lub rolnych, ścieki komunalne lub przemysłowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9129">
<skos:prefLabel>zrzut ścieków</skos:prefLabel>
<skos:definition>odpływ oczyszczonych ścieków z procesu oczyszczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9130">
<skos:prefLabel>usuwanie ścieków</skos:prefLabel>
<skos:definition>zbiór i usuwanie ścieków pochodzących z przemysłu lub osiedli miejskich za pomocą systemu rur i oczyszczalni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9132">
<skos:prefLabel>zanieczyszczenie wód przez działalność handlową</skos:prefLabel>
<skos:definition>płynne lub zawarte w wodzie zanieczyszczenia pochodzące z działalności handlowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9133">
<skos:prefLabel>prawodawstwo dotyczące ścieków</skos:prefLabel>
<skos:definition>proces powstawania aktów prawodawczych regulujących odpływ i składowanie zużytej wody zawierajacej rozpuszczone substancje lub zawiesiny, pochodzące z gospodarstw domowych lub rolnych, ścieki komunalne lub przemysłowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9135">
<skos:prefLabel>ładunek ścieków</skos:prefLabel>
<skos:definition>ilość zużytej wody, często zawierającej substancje rozpuszczone lub zawiesiny, znajdujące się w ciekach lub innych wodach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9136">
<skos:prefLabel>oczyszczanie ścieków</skos:prefLabel>
<skos:definition>oczyszczanie ściekow w celu ponownego użycia wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9138">
<skos:prefLabel>jakość ścieków</skos:prefLabel>
<skos:definition>stan lub charakter zużytej wody, która zawiera substancje rozpuszczone lub zawiesiny, pochodzące z gospodarstw domowych lub rolnych, ścieki komunalne lub przemysłowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9139">
<skos:prefLabel>redukcja ścieków</skos:prefLabel>
<skos:definition>działanie lub proces zmniejszania ilości zużytej wody odprowadzanej z domów, przedsiębiorstw lub przemysłu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9140">
<skos:prefLabel>osady ściekowe</skos:prefLabel>
<skos:definition>usunięty materiał pochodzący z fizycznego, biologicznego i chemicznego oczyszczania ścieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9141">
<skos:prefLabel>statystyka ścieków</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9143">
<skos:prefLabel>oczyszczanie ścieków</skos:prefLabel>
<skos:definition>proces, któremu poddawane są ścieki w celu usunięcia lub zmniejszenia szkodliwego oddziaływania ścieków na ludzkie zdrowie i środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9144">
<skos:prefLabel>oczyszczalnia ścieków</skos:prefLabel>
<skos:definition>instalacja, która poprzez procesy fizyko-chemiczne i biologiczne usuwa ze ścieków bytowych, komunalnych i przemysłowych materię organiczną, bakterie, virusy i części stałe, zanim zostaną one odprowadzone do rzek, jezior i mórz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9147">
<skos:prefLabel>analiza wód</skos:prefLabel>
<skos:definition>badanie chemicznych, fizycznych i biologicznych właściwości wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9149">
<skos:prefLabel>biologiczny bilans wód</skos:prefLabel>
<skos:definition>ilości wody wchodzącej i wychodzącej z systemu, które są w założeniu równe w dłuższym okresie, co bilansuje zasoby wodne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9151">
<skos:prefLabel>wody</skos:prefLabel>
<skos:definition>masa wody mająca określone właściwości hydrologiczne, fizyczne, chemiczne i biologiczne, która może być użyta do jednego lub wielu celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9153">
<skos:prefLabel>dno wód</skos:prefLabel>
<skos:definition>dno, na którym spoczywają wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9157">
<skos:prefLabel>zbieranie wód</skos:prefLabel>
<skos:definition>zbieranie wód, szczególnie opadowych, w takim miejscu jak zbiornik lub basen</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9159">
<skos:prefLabel>ochrona wód</skos:prefLabel>
<skos:definition>ochrona, rozwój i efektywne zarządzanie zasobami wodnymi w celu korzystania z nich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9160">
<skos:prefLabel>użytkowanie wody</skos:prefLabel>
<skos:definition>sposoby użytkowania wody pobieranej na potrzeby życiowe i związane z gospodarką człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9161">
<skos:prefLabel>cieki wodne</skos:prefLabel>
<skos:definition>naturalne cieki w danej zlewni, które płyną w korycie o wyraźnie określonym zasięgu pomiędzy widocznymi brzegami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9164">
<skos:prefLabel>zapotrzebowanie na wodę</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9165">
<skos:prefLabel>system zaopatrzenia w wodę</skos:prefLabel>
<skos:definition>system rur doprowadzających wodę do osiedli i urządzeń przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9166">
<skos:prefLabel>zagrożenie wód</skos:prefLabel>
<skos:definition>zagrożenie wód pochodzące z różnych źródeł, takich jak np. intensywne rolnictwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9168">
<skos:prefLabel>erozja wodna</skos:prefLabel>
<skos:definition>rozdrabnianie skał na mniejsze cząstki i ich usuwanie przez wodę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9169">
<skos:prefLabel>eksploatacja wód</skos:prefLabel>
<skos:definition>eksploatacja wody wykorzystywanej do różnych celów, np. w gospodarstwach domowych, w rolnictwie, w przemyśle itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9170">
<skos:prefLabel>wodospad</skos:prefLabel>
<skos:definition>prostopadły lub bardzo stromy opad wód strumienia lub rzeki, występujący na skalnych progach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9171">
<skos:prefLabel>rozwielitka</skos:prefLabel>
<skos:definition>słodkowodne liścionogi, skorupiaki o charakterystycznej przezroczystej dwudzielnej skorupce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9172">
<skos:prefLabel>przepływ hydrologiczny</skos:prefLabel>
<skos:definition>całkowita ilość wody w zlewni wraz z charakterystyką reżimu wodnego i pomiarami opadu, retencją i odpływem powierzchniowym i podziemnym oraz ewapotranspiracją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9176">
<skos:prefLabel>woda wykorzystywana w rolnictwie</skos:prefLabel>
<skos:definition>woda wykorzystywana w rolnictwie do nawadniania i dla inwentarza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9178">
<skos:prefLabel>woda wykorzystywana do konsumpcji</skos:prefLabel>
<skos:definition>woda pobierana na potrzeby życiowe i związane z gospodarką człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9180">
<skos:prefLabel>woda przemyslowa</skos:prefLabel>
<skos:definition>woda wykorzystywana w przemyśle do celów produkcyjnych oraz czyszczenia i chłodzenia, pochodząca ze źródeł publicznych lub własnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9183">
<skos:prefLabel>ptactwo wodne</skos:prefLabel>
<skos:definition>ptactwo wodne należące do rzędu Anseriformes, obejmujące m.in. łabędzie, kaczki, gęsi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9184">
<skos:prefLabel>twardość wody</skos:prefLabel>
<skos:definition>ilość rozpuszczonych w wodzie soli wapnia i magnezu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9186">
<skos:prefLabel>hiacynt wodny</skos:prefLabel>
<skos:definition>pływająca wodna roślina, Eichornia crassipes, z rodziny Pontederiaceae, pochodząca z tropikalnych regionów Ameryki, gdzie tworzy gęste pokrywy na rzekach i zbiornikach wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9190">
<skos:prefLabel>poziom wód</skos:prefLabel>
<skos:definition>poziom, na którym kształtuje się lustro wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9193">
<skos:prefLabel>grunt nawodniony</skos:prefLabel>
<skos:definition>nasycenie gruntu wodą w wyniku nawodnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9195">
<skos:prefLabel>gospodarka wodna</skos:prefLabel>
<skos:definition>środki podejmowane w celu zapewnienia właściwego zaopatrzenie w wodę i odpowiedzialne korzystanie z zasobów wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9196">
<skos:prefLabel>monitoring wód</skos:prefLabel>
<skos:definition>badania prowadzone w celu określenia ilości i jakości zanieczyszczeń, składników pokarmowych i substancji zawieszonych w wodach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9199">
<skos:prefLabel>zanieczyszczenia wód</skos:prefLabel>
<skos:definition>czynnik chemiczny lub fizyczny, znajdujący się w wodzie, który może istotnie zaburzyć jej naturalne właściwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9202">
<skos:prefLabel>zanieczyszczenie wód</skos:prefLabel>
<skos:definition>zaburzenia właściwości chemicznych, fizycznych, biologicznych i radiologiczncyh wody spowodowane działalnośćią człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9203">
<skos:prefLabel>przeciwdziałanie zanieczyszczaniu wód</skos:prefLabel>
<skos:definition>środki ostrożności, działania lub instalacje stosowane w celu odwrócenia lub powstrzymania zanieczyszczania wód spowodowanego działalnością człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9206">
<skos:prefLabel>ochrona wód</skos:prefLabel>
<skos:definition>środki podejmowane w celu ochrony wód powierzchniowych i podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9207">
<skos:prefLabel>dyrektywa dotycząca ochrony wód</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9208">
<skos:prefLabel>ustawodawstwo dotyczące ochrony wód</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9209">
<skos:prefLabel>pompa wodna</skos:prefLabel>
<skos:definition>maszyna stosowana do wyciągania wody, zwykle ze studni lub z odwiertu, napędzana ręcznie, mechanicznie, siłą wiatru lub inną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9210">
<skos:prefLabel>oczyszczanie wód</skos:prefLabel>
<skos:definition>procesy usuwania i neutralizowania niepożądanych zanieczyszczeń wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9211">
<skos:prefLabel>oczyszczalnia wód</skos:prefLabel>
<skos:definition>instalacja, która poprzez procesy fizyczne i chemiczne oczyszcza wodę na potrzeby konsumpcyjne ludnośći i inne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9214">
<skos:prefLabel>jakość wody</skos:prefLabel>
<skos:definition>podzielone na klasy zawartości w wodzie poszczególnych komponentów (organicznych i nieorganicznych, chemicznych lub fizycznych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9215">
<skos:prefLabel>dyrektywa dotycząca jakości wody</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9216">
<skos:prefLabel>zarządzanie jakością wód</skos:prefLabel>
<skos:definition>zarządzanie dotyczy użytkowania wód dla zapewnienia zasobów wody pitnej dla ludności, dla przemysłu, rolnictwa, a także jako środowiska bytowania ryb i innych zwierząt oraz celów rekreacyjnych; ochrona zasobów wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9217">
<skos:prefLabel>zarządzanie ilością wód</skos:prefLabel>
<skos:definition>administrowanie lub utrzymywanie ilości zasobów wody pitnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9220">
<skos:prefLabel>administracja zasobów wodnych</skos:prefLabel>
<skos:definition>kompetencje agendy rządowej lub jej administratorów do administrowania i realizowania zarządzeń, aktów prawnych i polityki rządowej odnoszącej się do ochrony zasobów wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9221">
<skos:prefLabel>zasoby wodne</skos:prefLabel>
<skos:definition>woda w jakiejkolwiek postaci, gdziekolwiek istniejąca - w atmosferze, na lądzie czy w głębi ziemi - mająca wartość dla człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9223">
<skos:prefLabel>ochrona zasobów wodnych</skos:prefLabel>
<skos:definition>kontrolowane użytkowanie lub ochrona zasobów wodnych potencjalnie przydatnych do jakiegoś celu, np. ekonomicznego, rekreacyjnego, podtrzymującego życie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9225">
<skos:prefLabel>zagospodarowanie zasobów wodnych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9227">
<skos:prefLabel>ponowne wykorzystanie wody</skos:prefLabel>
<skos:definition>wykorzystanie oczyszczonych ścieków w innym procesie wytwórczym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9229">
<skos:prefLabel>zasolenie wód</skos:prefLabel>
<skos:definition>proces podwyższania zawartości soli w wodzie, zwłaszcza w krajach gorących, gdzie stosuje się nawadnianie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9230">
<skos:prefLabel>oszczędzanie zasobów wodnych</skos:prefLabel>
<skos:definition>gospodarowanie zasobami wodnymi w celu zapewnienia ciągłej dostępności wody na potrzeby człowieka i ekosystemów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9231">
<skos:prefLabel>nauka o wodzie</skos:prefLabel>
<skos:definition>nauka o wodach występujących w przyrodzie, badająca zjawiska i procesy zachodzące w hydrosferze takie jak występowanie, krążenie, rozmieszczenie i właściwości wód na Ziemi oraz ich związki ze środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9232">
<skos:prefLabel>wody (geografia)</skos:prefLabel>
<skos:definition>woda tworząca cieki, jeziora i morza oraz wypływająca z ziemi w postaci źródeł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9234">
<skos:prefLabel>gospodarka na wododziałach</skos:prefLabel>
<skos:definition>użytkowanie, regulacja i gospodarownie zasobami wody i ziemi na obszarach wododziałowych, ze względu na określony cel</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9235">
<skos:prefLabel>wododział</skos:prefLabel>
<skos:definition>linia rozdzielająca dorzecza dwóch rzek, mórz lub oceanów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9236">
<skos:prefLabel>rozwój brzegów wodnych</skos:prefLabel>
<skos:definition>rozbudowa lub zmiany w użytkowaniu obszarów przylegających do wód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9241">
<skos:prefLabel>statystyka wodna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9242">
<skos:prefLabel>woda (substancja)</skos:prefLabel>
<skos:definition>tlenek wodoru, ciecz bezbarwna, bez zapachu i smaku, rozpowszechniona w przyrodzie w postaci deszczu, rzek, morza itp.;niezbędny składnik organizmów żywych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9244">
<skos:prefLabel>zaopatrzenie w wodę</skos:prefLabel>
<skos:definition>źródło lub ilość wody dostępna do użytkowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9245">
<skos:prefLabel>transport wodny</skos:prefLabel>
<skos:definition>transport towarowy lub pasażerski statkami pływającymi po morzach lub wodach śródlądowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9246">
<skos:prefLabel>uzdatnianie wody</skos:prefLabel>
<skos:definition>oczyszczanie wody w celu jej uzdatnienia do picia lub innego celu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9251">
<skos:prefLabel>wykorzystanie wody</skos:prefLabel>
<skos:definition>użytkowanie wody do określonych celów, związanych z zaopatrzeniem ludności i gospodarką człowieka, np. w przemyśle i rolnictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9254">
<skos:prefLabel>droga wodna</skos:prefLabel>
<skos:definition>rzeka, kanał lub inny żeglowny korytarz wodny wykorzystywany jako szlak komunikacyjny lub środek transportu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9255">
<skos:prefLabel>studnia wodna</skos:prefLabel>
<skos:definition>otwór wykopany lub wywiercony w ziemi i obudowany, z odpowiednim urządzeniem do wydobywania wody ze strefy wodonośnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9257">
<skos:prefLabel>stacja filtrów</skos:prefLabel>
<skos:definition>instalacja oczyszczająca wodę przed jej wprowadzeniem do sieci wodociągowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9259">
<skos:prefLabel>energia fal</skos:prefLabel>
<skos:definition>energia związana z ruchem fal morskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9262">
<skos:prefLabel>fale morskie</skos:prefLabel>
<skos:definition>wznoszące się i opadające masy wody blisko powierzchni morza o charakterystycznym, oscylacyjnym ruchu, wywoływanym głównie przez działanie wiatru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9267">
<skos:prefLabel>broń</skos:prefLabel>
<skos:definition>narzędzie walki, każda rzecz służąca do obrony własnej lub do rażenia nieprzyjaciela</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9269">
<skos:prefLabel>pogoda</skos:prefLabel>
<skos:definition>chwilowy stan atmosfery, charakteryzowany przez warunki meteorologiczne danego miejsca, takie jak temperatura, zachmurzenie i opady</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9270">
<skos:prefLabel>stan pogody</skos:prefLabel>
<skos:definition>zespół cech meteorologicznych w danym regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9274">
<skos:prefLabel>modyfikacje pogody</skos:prefLabel>
<skos:definition>zmiany naturalnych cech pogodowych przez środki techniczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9276">
<skos:prefLabel>monitoring meteorologiczny</skos:prefLabel>
<skos:definition>okresowe lub ciągłe nadzorowanie lub analiza stanu atmosfery i klimatu, poprzez pomiary temperatury, wilgotności, prędkości wiatru, ciśnienia atmosferycznego i in.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9278">
<skos:prefLabel>przewidywanie pogody</skos:prefLabel>
<skos:definition>akt lub proces przewidywania i określania warunków meteorologicznych, jakich należy się spodziewać w określonym czasie na pewnym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9280">
<skos:prefLabel>chwast</skos:prefLabel>
<skos:definition>bujna, dziko rosnąca roślina, szczególnie wśród roślin uprawnych, którym zabiera przestrzeń i pożywienie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9281">
<skos:prefLabel>odchwaszczanie</skos:prefLabel>
<skos:definition>uwalnianie ziemi uprawnej od chwastów poprzez takie środki jak herbicydy, orka, wypalanie, koszenie, dobór upraw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9286">
<skos:prefLabel>ciężar</skos:prefLabel>
<skos:definition>siła grawitacyjna z jaką Ziemia przyciąga dane ciało</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9289">
<skos:prefLabel>spawanie</skos:prefLabel>
<skos:definition>sposób łączenia dwóch metali polegający na ich stopieniu i łączeniu, przy użyciu dodatkowego metalu jako spoiwa lub bez niego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9290">
<skos:prefLabel>studnia</skos:prefLabel>
<skos:definition>otwór, odwiert lub szyb w ziemi sięgający zasobów wody, ropy, gazu, solanki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9293">
<skos:prefLabel>Afryka Zachodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9294">
<skos:prefLabel>Azja Zachodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9295">
<skos:prefLabel>Europa Zachodnia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9297">
<skos:prefLabel>obszar podmokły</skos:prefLabel>
<skos:definition>obszar zalewany wodami powierzchniowymi lub podziemnymi, wystarczająco często, aby panowała tam roślinność bagienna i związana z nią fauna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9298">
<skos:prefLabel>ekosystem bagienny</skos:prefLabel>
<skos:definition>ekosystem znacznych obszarów zalewanych, pośród których znajdują się nieco wyżej położone miejsca, dające schronienie zwierzętom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9299">
<skos:prefLabel>mokry proces</skos:prefLabel>
<skos:definition>proces stosowany do usuwania cząstek zanieczyszczeń stałych lub gazowych przy użyciu strumienia wody lub mułu płuczkowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9300">
<skos:prefLabel>płuczka mokra</skos:prefLabel>
<skos:definition>urządzenie oczyszczające powietrze, które wymywa pył</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9301">
<skos:prefLabel>odpady mokre</skos:prefLabel>
<skos:definition>wilgotne odpady organiczne lub zawierający wilgoć materiał pozostały po procesie produkcyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9302">
<skos:prefLabel>walenie</skos:prefLabel>
<skos:definition>Cetacea, rząd wielkich ssaków morskich przystosowanych do życia wodnego, niezdolnych do przebywania na lądzie; obejmuje kilkadziesiąt gatunków zwierząt o krępym, wrzecionowatym, bezwłosym ciele, w którym kończyny przednie w kształcie płetw tworzą narząd sterowy, tylne są w zaniku, a głównym narządem ruchu jest pozioma płetwa ogonowa; żyją w morzach całego świata, należą do nich m.in. delfiny, kaszaloty, wale</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9305">
<skos:prefLabel>połowy wielorybów</skos:prefLabel>
<skos:definition>połowy wielorybów w celu zdobycia pożywienia, oleju itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9308">
<skos:prefLabel>handel hurtowy</skos:prefLabel>
<skos:definition>kupowanie u producentów znacznych ilości towarów i sprzedawanie hurtowych (ale mniejszych niż kupowane u producenta) ilości towarów odbiorcom, którzy sprzedają konsumentom ilości detaliczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9310">
<skos:prefLabel>dzikie zwierzę</skos:prefLabel>
<skos:definition>zwierzę nieudomowione, dziko żyjące, niezależne od człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9313">
<skos:prefLabel>dzika przyroda</skos:prefLabel>
<skos:definition>zwierzęta i rośliny dziko żyjące niezażne od człowieka, zwykle w naturalnych warunkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9314">
<skos:prefLabel>ochrona dzikiej przyrody</skos:prefLabel>
<skos:definition>zespół środków wymaganych do utrzymania lub przywrócenia naturalnych siedlisk i populacji gatunków dzikiej fauny i flory do odpowiedniego stanu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9315">
<skos:prefLabel>siedlisko dzikiej przyrody</skos:prefLabel>
<skos:definition>siedlisko odpowiednie dla dzikiej przyrody, np.obszary górskie lub wodnobłotne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9318">
<skos:prefLabel>statystyka populacji dzikich zwierząt</skos:prefLabel>
<skos:definition>dane liczbowe zdobywane różnymi metodami, takimi jak badania terenowe, które prezentują lub pozwalają oszacować liczbę dzikich zwierząt danego gatunku w celu określenia np. trendu populacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9321">
<skos:prefLabel>ostoja dzikiej przyrody</skos:prefLabel>
<skos:definition>obszar zachowany w stanie naturalnym, zarezerwowany okresowo lub całorocznie dla pewnych gatunków zwierząt, przez instytucję rządową lub prywatną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9322">
<skos:prefLabel>dziko żyć</skos:prefLabel>
<skos:definition>stan przyrody lub cecha bytowania w stanie dzikim, nieudomowionym, nieoswojonym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9323">
<skos:prefLabel>dzika roślina</skos:prefLabel>
<skos:definition>roślina dziko żyjąca (nie uprawiana)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9324">
<skos:prefLabel>gotowość do ponoszenia wydatków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9325">
<skos:prefLabel>analiza gotowości ponoszenia wydatków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9327">
<skos:prefLabel>wiatr</skos:prefLabel>
<skos:definition>poziomy ruch powietrza powstający wskutek nierównomiernego rozkładu ciśnienia atmosferycznego na powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9330">
<skos:prefLabel>erozja wietrzna</skos:prefLabel>
<skos:definition>rozpadanie się skał na drobne części i wywiewanie ich przez wiatr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9331">
<skos:prefLabel>wiatrak</skos:prefLabel>
<skos:definition>urządzenie do mielenia lub pompowania napędzane siłą wiatru za pośrednictwem skrzydeł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9335">
<skos:prefLabel>siła wiatrów</skos:prefLabel>
<skos:definition>energia uzyskiwana dzięki sile wiatru, tradycyjnie wykorzystywana do przemiału zbóż w wiatrakach i coraz częściej w turbinach wytwarzających prąd elektryczny lub pompujących wodę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9336">
<skos:prefLabel>elektrownia wiatrowa</skos:prefLabel>
<skos:definition>elektrownia wykorzystująca siłę wiatru do napędzania turbiny wytwarzającej prąd elektryczny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9344">
<skos:prefLabel>kobieta</skos:prefLabel>
<skos:definition>osoba dorosła płci żeńskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9347">
<skos:prefLabel>status kobiety</skos:prefLabel>
<skos:definition>pozycja społeczna, ranga lub relatywne znaczenie kobiety w społeczeństwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9348">
<skos:prefLabel>drewno</skos:prefLabel>
<skos:definition>materiał jako surowiec używany w budownictwie, stolarstwie, meblarstwie itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9349">
<skos:prefLabel>las</skos:prefLabel>
<skos:definition>obszar gęsto porośnięty drzewami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9352">
<skos:prefLabel>zrąb leśny</skos:prefLabel>
<skos:definition>trwałe usunięcie drzew z obszaru leśnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9353">
<skos:prefLabel>ekosystem obszarów zalesionych</skos:prefLabel>
<skos:definition>układ współzależnych zbiorowisk roślin i zwierząt wraz z ich nieożywionym środowiskiem na terenach leśnych lub obszar z okrywą drzewiasto-krzewiastą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9356">
<skos:prefLabel>konserwacja drewna</skos:prefLabel>
<skos:definition>stosowanie środków chemicznych chroniących lub opóźniających gnicie drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9358">
<skos:prefLabel>wyrób drewniany</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9362">
<skos:prefLabel>odpady drzewne</skos:prefLabel>
<skos:definition>odpady powstałe przy obróbce surowego drewna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9363">
<skos:prefLabel>wełna</skos:prefLabel>
<skos:definition>włókno otrzymywane z wełny jako surowca, charakteryzujące się chłonnością, sprężystością i izolacyjnością</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9366">
<skos:prefLabel>wypadek przy pracy</skos:prefLabel>
<skos:definition>wypadek podczas wykonywania pracy, mający związek z czynnościami zawodowymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9367">
<skos:prefLabel>wyeksploatowana kopalnia</skos:prefLabel>
<skos:definition>kopalnia, z której cały użyteczny materiał został wybrany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9369">
<skos:prefLabel>warunki pracy</skos:prefLabel>
<skos:definition>wszelkie istniejące warunki dotyczące pracy w miejscu jej wykonywania, wraz z takimi zagadnieniami jak godziny pracy, aspekty fizyczne, prawne oraz zakres obowiązków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9371">
<skos:prefLabel>godziny pracy</skos:prefLabel>
<skos:definition>czas przeznaczony na zatrudnienie lub działania związane z pracą, zwykle określane jako liczba godzin dziennie lub tygodniowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9373">
<skos:prefLabel>uprawa gleb</skos:prefLabel>
<skos:definition>uprawa gleb w celach rolniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9375">
<skos:prefLabel>miejsce pracy</skos:prefLabel>
<skos:definition>miejsce lub środowisko zatrudnienia pracowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9380">
<skos:prefLabel>świat</skos:prefLabel>
<skos:definition>Ziemia jako miejsce bytowania człowieka wraz ze wszystkim, co na niej istnieje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9382">
<skos:prefLabel>obiekt światowego dziedzictwa</skos:prefLabel>
<skos:definition>obiekt o najwyższej wartości kulturowej i obszar geograficzny o nieprzeciętnych światowych wartościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9389">
<skos:prefLabel>spisanie na straty</skos:prefLabel>
<skos:definition>procedura księgowa stosowana wtedy, gdy aktywa są nieściągalne i w związku z tym są spisane na straty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9391">
<skos:prefLabel>bezprawie</skos:prefLabel>
<skos:definition>działanie przeciwne prawu naturalnemu lub stanowionemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9392">
<skos:prefLabel>bezprawne działanie władz</skos:prefLabel>
<skos:definition>działanie urzędnika państwowego lub przedstawiciela policji, władzy ustawodawczej, administracyjnej lub sądowniczej, które narusza prawa innych i powoduje szkody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9395">
<skos:prefLabel>substancja ksenobiotyczna</skos:prefLabel>
<skos:definition>substancja, która w warunkach naturalnych nie występuje w określonym środowisku, zazwyczaj substancja toksyczna całkowicie obca warunkom naturalnym, np.chlorowany związek organiczny lub organiczny związek siarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9398">
<skos:prefLabel>promienie rentgenowskie</skos:prefLabel>
<skos:definition>promieniowanie elektromagnetyczne odznaczające się zdolnością przenikania przez ciała nieprzezroczyste</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9403">
<skos:prefLabel>drożdże</skos:prefLabel>
<skos:definition>Saccharomyces, grzyby saprofityczne, jednokomórkowe z klasy workowców, rozmnażające się przez pączkowanie; żyją na podłożu zawierającym cukier, wywołując w warunkach beztlenowych fermentację alkoholową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9404">
<skos:prefLabel>dochód</skos:prefLabel>
<skos:definition>dochód lub zysk wytworzony poprzez inwestowanie lub działalność gospodarczą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9406">
<skos:prefLabel>niemowlę</skos:prefLabel>
<skos:definition>dziecko w pierwszym roku życia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9407">
<skos:prefLabel>młodzież</skos:prefLabel>
<skos:definition>stan pomiędzy dzieciństwem a dorosłością</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9409">
<skos:prefLabel>praca niepełnoletnich</skos:prefLabel>
<skos:definition>oferta pracy i zatrudnianie młodocianych, zarówno w celach zarobkowych jak i edukacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9412">
<skos:prefLabel>cynk</skos:prefLabel>
<skos:definition>(Zn) pierwiastek chemiczny należący do cynkowców, liczba atomowa 30, metal niebieskawobiały, występujący w przyrodzie tylko w związkach, otrzymywany z rud, używany do cynkowania przedmiotów żelaznych dla ochrony przed rdzewieniem oraz jako składnik stopów, np. mosiądzu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9417">
<skos:prefLabel>strefowanie</skos:prefLabel>
<skos:definition>procedura planistyczna, stosowana w planach zagospodarowania przestrzennego, polegająca na określaniu tego co i gdzie może być budowane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9422">
<skos:prefLabel>ogród zoologiczny</skos:prefLabel>
<skos:definition>placówka naukowo-dydaktyczna eksponująca zwierzęta pochodzące z różnych rejonów geograficznych, zwykle na terenie o charakterze parkowym, udostępnianym zwiedzającym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9425">
<skos:prefLabel>zoologia</skos:prefLabel>
<skos:definition>nauka o zwierzętach obejmująca całokształt wiedzy o zwierzętach, ich klasyfikację, strukturę, fizjologię i historię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/9426">
<skos:prefLabel>choroba odzwierzęca</skos:prefLabel>
<skos:definition>choroby biologicznie przystosowane i normalnie występujące u zwierząt, które w pewnych okolicznościach mogą być zakaźne dla człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10002">
<skos:prefLabel>księgowość</skos:prefLabel>
<skos:definition>prowadzenie ksiąg, w które wpisuje się wszystkie transakcje wpływające na sytuację finansową przedsiębiorstwa lub organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10003">
<skos:prefLabel>życie zwierząt</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10008">
<skos:prefLabel>produkt konsumpcyjny</skos:prefLabel>
<skos:definition>produkt, który bezpośrednio zaspokaja ludzkie potrzeby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10012">
<skos:prefLabel>organ administracji odpowiedzialny za ochronę środowiska</skos:prefLabel>
<skos:definition>organ centralnej administracji państwowej, która ma władzę w zakresie ochrony środowiska i jest odpowiedzialna za ochronę zasobów naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10017">
<skos:prefLabel>wpływ hałasu na zdrowie</skos:prefLabel>
<skos:definition>szkodliwe konsekwencje hałasu, obejmujące utratę słuchu i skutki psychologiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10018">
<skos:prefLabel>ciało ludzkie</skos:prefLabel>
<skos:definition>fizyczna struktura czlowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10019">
<skos:prefLabel>nauki społeczne</skos:prefLabel>
<skos:definition>grupa nauk obejmująca socjologię, antropologię, psychologię, pedagogikę itp. pozostające poza grupą nauk humanistycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10024">
<skos:prefLabel>przekaz informacji</skos:prefLabel>
<skos:definition>komunikowanie się lub przekazywanie danych lub dokumentów pomiędzy osobami lub miejscami w celach informacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10026">
<skos:prefLabel>akta sądowe</skos:prefLabel>
<skos:definition>dokumenty urzędowe, prawne odnoszące się do jakiejś sprawy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10031">
<skos:prefLabel>badania meteorologiczne</skos:prefLabel>
<skos:definition>badanie takich elementów meterorologicznych jak prędkość i kierunek wiatru, temperatura i wilgotność powietrza, ciśnienie atmosferyczne, opady, parowanie, promieniowanie słoneczne, widoczność i zachmurzenie, potrzebne do prognozowania pogody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10032">
<skos:prefLabel>ochrona obszarów przyrodniczych</skos:prefLabel>
<skos:definition>aktywna ochrona obszarów cennych przyrodniczo w celu zapewnienia warunków życia dzikich zwierząt i utrzymania jakości środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10034">
<skos:prefLabel>zmniejszenie ryzyka przyrodniczego</skos:prefLabel>
<skos:definition>środki zapobiegawcze, działania i urządzenia stosowane w celu zmniejszenia ryzyka zagrożenia człowieka, a także uniknięcia szkód materialnych i szkód w zasobach przyrodniczych wskutek naturalnych wypadków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10039">
<skos:prefLabel>chemia fizyczna</skos:prefLabel>
<skos:definition>nauka zajmująca się skutkami wpływu zjawisk fizycznych na właściwości chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10040">
<skos:prefLabel>fizyczny pomiar zanieczyszczeń</skos:prefLabel>
<skos:definition>ilościowe oznaczenie obecności, zakresu lub rodzaju zanieczyszczeń w środowisku przy użyciu środków mechanicznych, np. technik optycznych, elektrycznych, akustycznych i termodynamicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10043">
<skos:prefLabel>życie roślin</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10044">
<skos:prefLabel>produkcja roślinna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10048">
<skos:prefLabel>rodzaj zanieczyszczeń</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10049">
<skos:prefLabel>zapobieganie zanieczyszczeniom</skos:prefLabel>
<skos:definition>ograniczenie wytwarzania odpadów niebezpieczncyh i gazów cieplarnianych u źródła, podczas procesu produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10053">
<skos:prefLabel>sterowanie ryzykiem</skos:prefLabel>
<skos:definition>proces oceny zagrożenia oraz wyboru alternatywnych środków działania, prawnych i innych, w celu przygotowania się na okoliczność przypadkowego wystąpienia szkód, uwzględniając rozmiar szkód oraz zagadnienia prawne, ekonomiczne i związane z zachowaniami społecznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10054">
<skos:prefLabel>system bezpieczeństwa</skos:prefLabel>
<skos:definition>ujednolicony, skoordynowany układ urządzeń lub plan procedur postępowania mający na celu ograniczenie wystąpienia ryzyka obrażeń, uszkodzeń, strat lub niebezpieczeństwa dla człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10055">
<skos:prefLabel>inżynieria sejsmiczna</skos:prefLabel>
<skos:definition>badanie zachowania się podłoża i struktur w związku z ruchami sejsmicznymi Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10057">
<skos:prefLabel>nauki społeczne</skos:prefLabel>
<skos:definition>nauki zajmujące się społeczeństwem: socjologia, ekonomia, demografia, etnografia, pedagogika, psychologia, prawo, badające z różnych punktów widzenia zjawiska i procesy społeczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10058">
<skos:prefLabel>gospodarowanie wodami powierzchniowymi</skos:prefLabel>
<skos:definition>administrowanie lub zarządzanie wodami otwartymi, takimi jak rzeki, jeziora, sztuczne zbiorniki, morza itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10064">
<skos:prefLabel>ochrona dzikiej przyrody</skos:prefLabel>
<skos:definition>środki ostrożności, procedury lub urządzenia stosowane w celu zapobiegania lub ograniczenia zagrożenia zwierząt, roślin i innych organizmów dziko żyjących w ich naturalnym środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10066">
<skos:prefLabel>centrum historyczne</skos:prefLabel>
<skos:definition>część miasta, w której mieszczą się główne budynki publiczne i historyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10067">
<skos:prefLabel>promocja handlu i przemysłu</skos:prefLabel>
<skos:definition>działalność, która zachęca lub wspiera kupowanie, sprzedawanie lub wymianę towarów i usług z innymi krajami, w którą może być zaangażowany marketing, stosowane naciski dyplomatyczne lub zachęty eksportowe, jak kredyty, gwarancje itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10068">
<skos:prefLabel>murarstwo</skos:prefLabel>
<skos:definition>budowanie z cegły, kamienia, betonu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10071">
<skos:prefLabel>równina piaszczysta</skos:prefLabel>
<skos:definition>piaszczysta równina odpływowa, pozbawiona roślinności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10073">
<skos:prefLabel>gatunki zwierząt</skos:prefLabel>
<skos:definition>gatunki należące do królestwa zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10074">
<skos:prefLabel>gatunki roslin</skos:prefLabel>
<skos:definition>gatunki należące do królestwa roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10076">
<skos:prefLabel>zajęcie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10078">
<skos:prefLabel>tradycje ludowe</skos:prefLabel>
<skos:definition>wspólne wierzenia, praktyki, zwyczaje i inne elemnty kulturowe charakterysytyczne dla grupy etnicznej lub społecznej, które są zakorzenione w przeszłości i wyrażąjące się w sztuce, rękodziele, pieśniach, muzyce, tańcach, opowieściach, potrawach itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10084">
<skos:prefLabel>dziedzina prawa</skos:prefLabel>
<skos:definition>sfera działania prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10085">
<skos:prefLabel>system sądowniczy</skos:prefLabel>
<skos:definition>system związany z organami wymiaru sprawiedliwości, z władzą przez nie sprawowaną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10087">
<skos:prefLabel>chemiczny pomiar zanieczyszczeń</skos:prefLabel>
<skos:definition>ilościowe oznaczanie substancji zanieczyszczających, ich rodzaju i rozprzestrzeniania w środowisku, przy zastosowaniu badania oddziaływania lub reakcji znanych związków chemicznych z zanieczyszczeniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10088">
<skos:prefLabel>urządzenia pomiarowe</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10089">
<skos:prefLabel>przekształcanie zanieczyszczeń</skos:prefLabel>
<skos:definition>procesy przemian zachodzące po wprowadzeniu substancji zanieczyszczającej do środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10092">
<skos:prefLabel>hałas miejski</skos:prefLabel>
<skos:definition>hałas emitowany z różnych źródeł w środowisku miejskim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10097">
<skos:prefLabel>czystość</skos:prefLabel>
<skos:definition>zespół warunków wpływających dodatnio na zdrowie ludzkie, zwłaszcza na czystość</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10099">
<skos:prefLabel>środowisko przemysłowe</skos:prefLabel>
<skos:definition>środowisko, w którym ma miejsce powielana działalność związana z produkcją dóbr i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10102">
<skos:prefLabel>ryzyko przyrodnicze</skos:prefLabel>
<skos:definition>prawdopodobieństwo zagrożenia ludzkiego zdrowia, mienia lub środowiska ze strony środowiska fizycznego a nie działaności człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10103">
<skos:prefLabel>analiza ryzyka przyrodniczego</skos:prefLabel>
<skos:definition>analiza prawodpodobieństwa wystąpienia potencjalnie niszczącego zjawiska przyrodniczego w określonym czasie i określonym miejscu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10104">
<skos:prefLabel>wysoki poziom ryzyka</skos:prefLabel>
<skos:definition>wysokie prawdopodobieństwo, że dane zagrożenie lub sytuacja wywoła znaczne straty istnień ludzkich, zranień, zniszczeń mienia itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10106">
<skos:prefLabel>system ratownictwa</skos:prefLabel>
<skos:definition>zespół procedur i urządzeń stosowanych przez wykwalifikowany personel w celu udzielania natychmiastowej pomocy rannym i zagrożonym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10107">
<skos:prefLabel>zarządzanie kryzysowe</skos:prefLabel>
<skos:definition>technika, praktyka lub nauka kierowania lub kontrolowania sytuacjami szczególnie trudnymi lub niebezpiecznymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10120">
<skos:prefLabel>system podatkowy</skos:prefLabel>
<skos:definition>koordynowany zespół metod lub plan procedur nakładania i ściągania obowiązkowych opłat na rzecz skarbu państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10126">
<skos:prefLabel>pojęcie środowiska</skos:prefLabel>
<skos:definition>ogólne pojęcie ekosystemu jako środowiska życia, na dowolnym szczeblu  hierarchicznym, uwzględniające jego znaczenie dla życia człowieka i potrzeby ochrony tego systemu jako całości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10128">
<skos:prefLabel>ekologia leśna</skos:prefLabel>
<skos:definition>nauka badająca współzależności między drzewami tworzącymi las a ich środowiskiem, m.in.w tym innymi roślinami oraz zwierzętami leśnymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10129">
<skos:prefLabel>ekologia</skos:prefLabel>
<skos:definition>dziedzina biologii badająca wzajemne stosunki między organizmami a otaczającym je środowiskiem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10130">
<skos:prefLabel>ekologia miejska</skos:prefLabel>
<skos:definition>pojęcie wywodzące się z biologii, miasto jest rozważane jako środowisko bytowania, system podtrzymujący życie wielu ludzi zamieszkałych w skupisku; ludzie ci organizują się między sobą i przystosowują do ciągle zmieniającego się środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10131">
<skos:prefLabel>strefy ekologiczne</skos:prefLabel>
<skos:definition>rozległe obszary geograficzne odznaczające się charakterystycznymi cechami klimatu, warunkami oceanicznymi, rodzajami krajobrazów, charakterystyką flory i fauny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10133">
<skos:prefLabel>inżynieria środowiska</skos:prefLabel>
<skos:definition>gałąź inżynierii związana z różnymi aspektami gospodarowania w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10135">
<skos:prefLabel>paleoekologia</skos:prefLabel>
<skos:definition>stosowanie wiedzy ekologicznej do badania skamieniałości i osadów w celu określenia związków powierzchni ziemi, atmosfery i biosfery w minionych epokach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10136">
<skos:prefLabel>dziedzictwo biologiczne</skos:prefLabel>
<skos:definition>dziedziczenie i zachowanie w równowadze funkcji naturalnego środowiska na Ziemi lub w określonym regionie, szczególnie zachowanie zasobów wodnych niezbędnych do podtrzymania życia ekosystemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10140">
<skos:prefLabel>wysokość nad poziom terenu</skos:prefLabel>
<skos:definition>określenie wysokości jakiegoś punktu ponad poziom odniesienia (np. poziom morza)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10142">
<skos:prefLabel>zatoczka</skos:prefLabel>
<skos:definition>głęboko wcięta zatoczka w stromym brzegu typu klifowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10143">
<skos:prefLabel>kanion</skos:prefLabel>
<skos:definition>długa, głęboka, stosunkowo wąska dolina o stromych ścianach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10144">
<skos:prefLabel>przylądek</skos:prefLabel>
<skos:definition>najbardziej w kierunku morza wysunięty fragment linii brzegowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10146">
<skos:prefLabel>cyrk lodowcowy</skos:prefLabel>
<skos:definition>głęboki półkolisty kocioł skalny, otoczony stromymi ścianami, wyrzeźbiony przez lodowiec w wysokich partiach gór</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10148">
<skos:prefLabel>kontynent</skos:prefLabel>
<skos:definition>wielki obszar lądu otoczony morzem i oceanem; fragment skorupy ziemskiej, znacznych rozmiarów, wzniesiony ponad poziom morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10149">
<skos:prefLabel>lido</skos:prefLabel>
<skos:definition>wynurzony ponad powierzchnię morza piaszczysty wał przybrzeżny, zamykający całkowicie lub częściowo zatokę morską, występujący głównie na adriatyckim wybrzeżu Włoch</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10152">
<skos:prefLabel>zatoczka</skos:prefLabel>
<skos:definition>część morza lub jeziora wcinająca się w ląd; niewielka wąska zatoka, zwłaszcza morska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10154">
<skos:prefLabel>uskok</skos:prefLabel>
<skos:definition>pionowe lub ukośne pęknięcie mas skalnych i przesunięcie się ich względem siebie, dokonujące się wskutek ruchów górotwórczych; szczelina powstała wskutek tego przesunięcia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10155">
<skos:prefLabel>klif</skos:prefLabel>
<skos:definition>urwisko brzegu morskiego powstałe na wysokich wybrzeżach wskutek niszczącej działalności fal morskich; stromo opadający urwisty brzeg</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10158">
<skos:prefLabel>otwarte morze</skos:prefLabel>
<skos:definition>obszar morski leżący poza wyłączną strefą ekonomiczną państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10160">
<skos:prefLabel>laguna rafy koralowej</skos:prefLabel>
<skos:definition>płytka słonowodna przestrzeń odcięta od otwartego morza rafą koralową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10161">
<skos:prefLabel>koryto rzeczne</skos:prefLabel>
<skos:definition>koryto, którym płynie lub płynęła rzeka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10166">
<skos:prefLabel>środowisko fizyczne</skos:prefLabel>
<skos:definition>otoczenie materialne systemu, procesu lub organizmu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10170">
<skos:prefLabel>równina aluwialna</skos:prefLabel>
<skos:definition>płaska lub lekko nachylona równina, zbudowana z obfitych materiałów osadowych, zwykle sąsiadująca z okresowo zalewającą je rzeką</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10175">
<skos:prefLabel>rafa barierowa</skos:prefLabel>
<skos:definition>wydłużona forma powstała z nagromadzenia koralowców, układająca się równolegle do linii brzegowej na poziomie odpływu i oddzielona od brzegu głęboką laguną lub cieśniną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10176">
<skos:prefLabel>rzeźba terenu</skos:prefLabel>
<skos:definition>fizyczny kształt, konfiguracja lub ogólnie wszelkie nierówności powierzchni ziemi, określane zmiennością wysokości i stoków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10189">
<skos:prefLabel>regulacje prawne dotyczące biotopów</skos:prefLabel>
<skos:definition>regulacje prawne dotyczące obszaru siedliska ekologicznego charakteryzującego się wysokim stopniem jednorodności warunków środowiska i rozmieszczenia roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10190">
<skos:prefLabel>obszar przyrodniczy wrażliwy na zakłócenia</skos:prefLabel>
<skos:definition>obszar lądowy lub wodny lub inny wrażliwy obszar naturalny o unikatowych lub cennych przyrodniczo wartościach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10191">
<skos:prefLabel>analiza środowiska</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10193">
<skos:prefLabel>środowisko abiotyczne</skos:prefLabel>
<skos:definition>nieożywione komponenty środowiska (skały, minerały, gleba, woda i klimat)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10194">
<skos:prefLabel>środowisko wodne</skos:prefLabel>
<skos:definition>wody, wraz z obszarami wodnobłotnymi, które są siedliskiem powiązanych ze sobą zespołów roślinnych i populacji zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10197">
<skos:prefLabel>środowisko wrażliwe</skos:prefLabel>
<skos:definition>działka ziemi prywatnej lub państwowej, która posiada lub dzięki działaniom ochronno-naprawczym mogłaby uzyskać pożądane cechy środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10198">
<skos:prefLabel>środowisko lądowe</skos:prefLabel>
<skos:definition>środowisko lądowe w odróżnieniu do morskiego i atmosferycznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10199">
<skos:prefLabel>park morski</skos:prefLabel>
<skos:definition>ustanowiony rezerwat obejmujący dno morskie w celu ochrony gatunków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10200">
<skos:prefLabel>park regionalny</skos:prefLabel>
<skos:definition>park administrowany w danym regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10201">
<skos:prefLabel>rezerwat</skos:prefLabel>
<skos:definition>teren o szczególnych wartościach przyrodniczych lub krajoznawczych, objęty ochroną (ścisłą albo częściową) w celu zachowania na nim całej przyrody w stanie pierwotnym lub utrzymania znajdujących się tam rzadkich zwierząt, roślin, osobliwych zjawisk geologicznych, krajobrazu itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10202">
<skos:prefLabel>państwowy rezerwat biologiczny</skos:prefLabel>
<skos:definition>wydzielony obszar lądu i/lub wody, mający określony status ochronny, powołany w celu ochrony pewnych biologicznych cech</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10203">
<skos:prefLabel>rezerwat leśny</skos:prefLabel>
<skos:definition>chroniony obszar leśny, zabezpieczony przed wycinaniem z powodu wrażliwości jego ekosystemu; jest siedliskiem bytowania setek gatunków roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10208">
<skos:prefLabel>prywatny rezerwat przyrody</skos:prefLabel>
<skos:definition>obszar mający wartości ogólnonarodowe, który jest chroniony przez prywatnego właściciela w celu utrzymania flory i fauny oraz wartości archeologicznych i geologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10210">
<skos:prefLabel>obszar centralny parków</skos:prefLabel>
<skos:definition>obszar znajdujący się we wnętrzu parku lub rezerwatu, podlegający ochronie ścisłej, bez żadnej ingerencji w naturalne ekosystemy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10212">
<skos:prefLabel>morska strefa chroniona</skos:prefLabel>
<skos:definition>obszar morski, chroniący dzikie gatunki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10214">
<skos:prefLabel>obszar peryferyjny parków</skos:prefLabel>
<skos:definition>obszar parku, na którym dozwolone są badania naukowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10215">
<skos:prefLabel>profil trasy komunikacyjnej</skos:prefLabel>
<skos:definition>szczegółowy projekt tras transportowych pod względem konstrukcji, funkcjonowania, usług, technologii i kryteriów ekonomicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10217">
<skos:prefLabel>krajobraz polno-leśny</skos:prefLabel>
<skos:definition>lesisty krajobraz poprzetykany nieregularnymi polami z zadrzewieniami i zakrzaczonymi miedzami, typowy dla północnej Francji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10225">
<skos:prefLabel>park francuski</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10228">
<skos:prefLabel>park angielski</skos:prefLabel>
<skos:definition>park lub ogród zaaranżowany według brytyjskiej tradycji i stylu, polegających na prowadzeniu kwiatów, krzewów i drzew w stanie naśladującym lub zbliżonym do naturalnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10229">
<skos:prefLabel>ogród tarasowaty</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10233">
<skos:prefLabel>krajobraz tarasowaty</skos:prefLabel>
<skos:definition>krajobraz ukształtowany pod wpływem formowania tarasów dla pól uprawnych na stokach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10236">
<skos:prefLabel>ochrona gór</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10237">
<skos:prefLabel>ochrona miejscowa</skos:prefLabel>
<skos:definition>środki zapobiegawcze, działania i urządzenia stosowane w celu przeciwdziałania lub zmniejszenia zagrożenia integralności środowiska danego obszaru lub miejsca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10238">
<skos:prefLabel>ochrona wybrzeży</skos:prefLabel>
<skos:definition>sposób gospodarowania środowiskiem, mający na celu osłabienie degradacji wybrzeży morskich spowodowanych przez procesy erozyjne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10240">
<skos:prefLabel>przywracanie walorów ekologicznych</skos:prefLabel>
<skos:definition>przywracanie walorów ekologicznych danego obszaru lub miejsca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10243">
<skos:prefLabel>obiekt i obszar prawnie chroniony</skos:prefLabel>
<skos:definition>obszar lub obiekt chroniony ze względu na cechy przyrodnicze, krajobrazowe, artystyczne i archeologiczne w celu zagwarantowania jego ochrony, utrzymania i odnowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10244">
<skos:prefLabel>obiekt i obszar zarejestrowany</skos:prefLabel>
<skos:definition>obszar lub obiekt oficjalnie zarejestrowany ze względu na swoje wyjątkowe cechy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10251">
<skos:prefLabel>mapa ekologii miejskiej</skos:prefLabel>
<skos:definition>graficzne przedstawienie obszaru miejskiego lub innego gęsto zaludnionego obszaru, obrazującego występowanie grup lub wybranych typów ludności w ich środowisku, za pomocą różnych technik kartograficznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10255">
<skos:prefLabel>obywatelstwo środowiskowe</skos:prefLabel>
<skos:definition>status, charakter lub sposób zachowania człowieka jako obywatela środowiska, z nieodłącznymi prawami i odpowiedzialnością, zwłaszcza zaś z odpowiedzialnością za zachowanie stanu środowiska i prawem do życia w czystym środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10256">
<skos:prefLabel>zagospodarowanie kompleksowe</skos:prefLabel>
<skos:definition>połączone i skoordynowane postępowanie w przypadku rozwiązywania problemów ekologicznych, w które włącza się zainteresowane organizacje i osoby oraz różne dycypliny wiedzy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10260">
<skos:prefLabel>sponsorowanie</skos:prefLabel>
<skos:definition>osoby, fimy, organizacje finansujące lub pośredniczące w zdobywaniu środków finansowych na realizację określonego przedsięwzięcia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10263">
<skos:prefLabel>miejski plan ochrony zasobów środowiska</skos:prefLabel>
<skos:definition>metody ochrony i zagospodarowania komunalnych zasobów przyrodniczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10274">
<skos:prefLabel>kampania na rzecz świadomości społecznej</skos:prefLabel>
<skos:definition>zorganizowany wysiłek z udziałem wielu mediów komunikacyjnych, na rzecz pobudzenia świadomości społecznej w określonym zakresie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10305">
<skos:prefLabel>ryby migrujące</skos:prefLabel>
<skos:definition>ryby migrujące gromadnie, najczęściej między miejscami rozrodu a obszarami łowisk zimowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10381">
<skos:prefLabel>kotowate</skos:prefLabel>
<skos:definition>ssaki drapieżne tj. koty, lwy, leopardy i inne, mające małą zaokrągloną głowę i ruchome, wysuwane pazury</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10421">
<skos:prefLabel>ssak morski</skos:prefLabel>
<skos:definition>ssaki przystosowane do życia w morzu, takie jak wieloryby, delfiny i morświny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10444">
<skos:prefLabel>owca</skos:prefLabel>
<skos:definition>rogaty ssak przeżuwający, występujący w kilku gatunkach, liczne rasy i typy użytkowe chowane i hodowane ze względu na skórę, wełnę, mięso i mleko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10495">
<skos:prefLabel>schronisko</skos:prefLabel>
<skos:definition>miejsce schronienia, np. przed deszczem lub niebezpieczeństwem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10496">
<skos:prefLabel>obszar gniazdowania</skos:prefLabel>
<skos:definition>miejsce, w którym ptaki gromadzą się na lęgi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10510">
<skos:prefLabel>korytarz dla zwierząt</skos:prefLabel>
<skos:definition>korytarze liniowe (drogi, ścieżki i żywopłoty) umożliwiające zwierzętom przemieszczanie się w obrębie siedlisk lub na znaczne odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10511">
<skos:prefLabel>szkody zwierzęce</skos:prefLabel>
<skos:definition>szkody wyrządzane przez zwierzęta, np. nadmierny wypas, wydeptywanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10512">
<skos:prefLabel>przemieszczanie się zwierząt</skos:prefLabel>
<skos:definition>zwyczaje wielu zwierząt polegające na przemieszczaniu się w obrębie siedlisk, migracji do innych biotopów, często na znaczne odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10521">
<skos:prefLabel>tarlisko</skos:prefLabel>
<skos:definition>obszar wodny, w którym ryby odbywają tarło i składają ikrę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10523">
<skos:prefLabel>siedlisko dla zwierząt</skos:prefLabel>
<skos:definition>miejsce, w którym zwierzę wzrasta i żyje; określony obszar geograficzny lub stanowisko, w którym spotyka się dane zwierzę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10535">
<skos:prefLabel>gniazdowanie</skos:prefLabel>
<skos:definition>budowa gniazd w celu złożenia jaj i wychowu piskląt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10539">
<skos:prefLabel>populacja zwierzęca</skos:prefLabel>
<skos:definition>grupa zwierząt zamieszkujących określony obszar</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10548">
<skos:prefLabel>reprodukcja zwierzęca</skos:prefLabel>
<skos:definition>procesy rozmnażania płciowego i bezpłciowego u zwierząt w celu wydawania potomstwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10554">
<skos:prefLabel>przetrwanie</skos:prefLabel>
<skos:definition>utrzymanie się przy życiu lub warunek przetrwania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10562">
<skos:prefLabel>gatunek endemiczny</skos:prefLabel>
<skos:definition>gatunki występujące naturalnie na określonym obszarze geograficznym i tylko z nim związane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10568">
<skos:prefLabel>drzewo liściaste</skos:prefLabel>
<skos:definition>drzewo o szerokich liściach, w przeciwieństwie do szpilek drzew iglastych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10574">
<skos:prefLabel>łąka podmorska</skos:prefLabel>
<skos:definition>zbiorowiska łąk podwodnych utworzone przez makroskopowe glony zielone, brązowe i czerwone oraz rośliny okrytonasienne, takie jak Posidonia oceanica i Zostera noltii, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10575">
<skos:prefLabel>makrofit</skos:prefLabel>
<skos:definition>duża makroskopowa roślina; pojęcie stosowane w odniesieniu szczególnie do podwodnych glonów takich jak brunatnice (kelp), które są źródłem jodu i potasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10593">
<skos:prefLabel>roślinność nadrzeczna</skos:prefLabel>
<skos:definition>roślinność zasiedlająca brzegi rzek i innych cieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10605">
<skos:prefLabel>kasztan jadalny</skos:prefLabel>
<skos:definition>drzewo z rodziny bukowatych pochodzące z północnej strefy umiarkowanej, z rodzaju Castanea, takie jak C.sativa (kasztan jadalny), który ma kotkowate kwiatostany i owoce w kolczastej łupinie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10612">
<skos:prefLabel>roślina trawiasta</skos:prefLabel>
<skos:definition>wielka rodzina roślin obejmująca m.in. zboża, takie jak pszenica, kukurydza itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10617">
<skos:prefLabel>Posidonia oceanica</skos:prefLabel>
<skos:definition>morska roślina o wydłużonych płaskich i zielonych liściach o długości do 1,5 m; owoce dojrzewają jesienią lecz nie corocznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10626">
<skos:prefLabel>piętro roślinne</skos:prefLabel>
<skos:definition>strefa roślinności w układzie pięter wysokościowych, charakterystyczna dla pewnej wysokości nad poziomem morza na określonej szerokości geograficznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10633">
<skos:prefLabel>populacja roślinna</skos:prefLabel>
<skos:definition>pewna liczba roślin występujących na określonym terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10640">
<skos:prefLabel>arboretum</skos:prefLabel>
<skos:definition>kolekcja drzew pochodzących z różnych części świata, zebrana w celach naukowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10644">
<skos:prefLabel>chorologia</skos:prefLabel>
<skos:definition>badania związków przyczynowych pomiędzy zjawiskami geograficznymi występującymi w określonym regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10646">
<skos:prefLabel>kolekcja odmian</skos:prefLabel>
<skos:definition>zgromadzenie roślin uprawnych odznaczających się swoistymi właściwościami morfologicznymi, fizjologicznymi, itp., mających znaczenie dla ogrodnictwa, rolnictwa lub leśnictwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10651">
<skos:prefLabel>rezerwat zwierzęcy</skos:prefLabel>
<skos:definition>obszary ochrony rzadkich lub zagrożonych gatunków zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10652">
<skos:prefLabel>rezerwat botaniczny</skos:prefLabel>
<skos:definition>ogród, w którym hoduje się rzadkie gatunki roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10660">
<skos:prefLabel>introdukcja gatunków zwierząt</skos:prefLabel>
<skos:definition>zwierzęta, wprowadzone przez człowieka do układów ekologicznych na lądzie i w wodach, przedtem tam nie spotykane, przynajmniej w czasach historycznych; przemieszczanie zwierząt niesie zawsze element ryzyka lub poważnego zagrożenia równowagi ekologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10661">
<skos:prefLabel>introdukcja gatunków roślin</skos:prefLabel>
<skos:definition>rośliny, wprowadzone przez człowieka do układów ekologicznych na lądzie i w wodach, przedtem tam nie spotykane, przynajmniej w czasach historycznych; przemieszczanie roślin niesie zawsze element ryzyka lub poważnego zagrożenia równowagi ekologicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10666">
<skos:prefLabel>dziedzictwo świata zwierząt</skos:prefLabel>
<skos:definition>zbiorowość wszystkich wielokomórkowych organizmów Ziemi z wyłączeniem roślin i człowieka jako spuścizna przekazana obecnym pokoleniom, szczególnie zaś gatunki zwierząt uważane za zagrożone wyginięciem i wymagające ochrony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10667">
<skos:prefLabel>dziedzictwo świata roślin</skos:prefLabel>
<skos:definition>zbiorowość wszystkich wielokomórkowych organizmów roślinnych Ziemi z wyłączeniem zwierząt i człowieka jako spuścizna przekazana obecnym pokoleniom, szczególnie zaś gatunki roślin uważane za zagrożone wyginięciem i wymagające ochrony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10670">
<skos:prefLabel>ochrona zwierząt</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10671">
<skos:prefLabel>reintrodukcja gatunków zwierząt</skos:prefLabel>
<skos:definition>usiłowania mające na celu przeciwdziałanie wymieraniu gatunków zagrożonych zwierząt i ich populacji przez reintrodukowanie tych gatunków na ich naturalne siedliska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10672">
<skos:prefLabel>reintrodukcja gatunków roślin</skos:prefLabel>
<skos:definition>usiłowania mające na celu przeciwdziałanie wymieraniu gatunków zagrożonych roślin przez reintrodukowanie tych gatunków na ich naturalne siedliska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10683">
<skos:prefLabel>dolesienie</skos:prefLabel>
<skos:definition>sadzenie drzew na określonym obszarze lub zagospodarowanie obszaru w sposób umożliwiający spontaniczną kolonizację przez gatunki drzewiaste tak, by powstał las</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10685">
<skos:prefLabel>wiatrołom</skos:prefLabel>
<skos:definition>1) wywracanie starych drzew w lesie przez wichurę; 2) obszar, na którym wystąpiły wiatrołomy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10691">
<skos:prefLabel>przycinanie gałęzi</skos:prefLabel>
<skos:definition>przycinanie i usuwanie martwych lub żywych gałęzi w celu poprawy kształtu lub wzrostu rośliny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10692">
<skos:prefLabel>eksploatacja lasów</skos:prefLabel>
<skos:definition>proces eksploatacji (wyręby) lasów w celu zdobycia surowca drzewnego i terenów pod uprawę, który  trwał od wieków i doprowadził do wycofywania się lasu jako formacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10693">
<skos:prefLabel>las górski</skos:prefLabel>
<skos:definition>rozległe obszary leśne naturalnie rosnące, spotykane na wysokości powyżej 600 m n.p.m.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10694">
<skos:prefLabel>las państwowy</skos:prefLabel>
<skos:definition>lasy będące własnością Skarbu Państwa i zarządzane przez państwo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10695">
<skos:prefLabel>las śródziemnomorski</skos:prefLabel>
<skos:definition>rodzaj lasu spotykany na obszarach śródziemnomorskich, tworzą go głównie drzewa sucholubne i wiecznie zielone</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10696">
<skos:prefLabel>las prywatny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10698">
<skos:prefLabel>las rębny</skos:prefLabel>
<skos:definition>las z dojrzałym drzewostanem, który osiągnął wiek reprodukcyjny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10699">
<skos:prefLabel>makia</skos:prefLabel>
<skos:definition>rodzaj niskiej formacji krzewiastej, spotykany na krzemianowych glebach obszarów śródziemnomorskich, gdzie cechami klimatu są opady deszczu zimą i letnie susze, tworzą ją głównie gatunki o dużej zawartości olejków aromatycznych, takie jak lawenda, mirt, oleander itd.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10703">
<skos:prefLabel>suchy trawnik</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10704">
<skos:prefLabel>szkółka</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10706">
<skos:prefLabel>ochrona lasów</skos:prefLabel>
<skos:definition>dziedzina leśnictwa zajmująca się zapobieganiem i ograniczaniem leśnych szkód powstających wskutek gospodarki człowieka, wypasu bydła, oddziaływania szkodników oraz wskutek czynników abiotycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10707">
<skos:prefLabel>naturalna regeneracja</skos:prefLabel>
<skos:definition>odnowa i zastąpienie tkanek lub organów, które zostały utracone lub poważnie uszkodzone</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10708">
<skos:prefLabel>roślina żywiczna</skos:prefLabel>
<skos:definition>rośliny żywicodajne lub produkujące żywicę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10711">
<skos:prefLabel>las odroślowy z pozostałością drzewostanu rębnego</skos:prefLabel>
<skos:definition>tradycyjny system odnowy lasu; odnowa odroślowa pod starszym bardzo rozluźnionym drzewostanem rębnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10721">
<skos:prefLabel>gra</skos:prefLabel>
<skos:definition>rozrywka, sposób spędzania czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10726">
<skos:prefLabel>gruba zwierzyna łowna</skos:prefLabel>
<skos:definition>duże zwierzęta, zwykle o ciężarze powyżej 13 kg w stadium dorosłego osobnika, przedmiot polowań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10744">
<skos:prefLabel>hodowla skorupiaków</skos:prefLabel>
<skos:definition>hodowla skorupiaków w celach handlowych; musi być kontrolowana z uwagi na możliwość zakażenia bakteryjnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10753">
<skos:prefLabel>hodowla ostryg</skos:prefLabel>
<skos:definition>dwa sposoby hodowli ostryg: hodowla zawieszona na powierzchni wody, lub hodowla na dnie zbiornika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10758">
<skos:prefLabel>rybołówstwo przemysłowe</skos:prefLabel>
<skos:definition>dzieli się na dwa rodzaje: połowy ryb żyjących przy dnie (demersalnych), takich jak dorsz, łupacz, flądra i sola, lub połowy ryb żyjących przy powierzchni (pelagicznych), takich jak sardela, tuńczyk i śledź</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10760">
<skos:prefLabel>państwowy rezerwat wędkarski</skos:prefLabel>
<skos:definition>wydzielony fragment zbiornika wodnego będącego własnością Skarbu Państwa, na którym dopuszcza się wędkowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10767">
<skos:prefLabel>działania edukacyjne w społeczństwie</skos:prefLabel>
<skos:definition>nauczanie lub działania oświatowe dostarczające wiedzy społecznościom pozbawionym dostępu do tradycyjnych instytucji edukacyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10773">
<skos:prefLabel>egzamin konkursowy</skos:prefLabel>
<skos:definition>egzamin sprawdzający umiejętności rozwiązywania problemów, opanowanie rzemiosła lub możliwości wykonywania zadań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10778">
<skos:prefLabel>ciągłe kształcenie</skos:prefLabel>
<skos:definition>nieformalne lub formalne metody i procesy kształcenia przeznaczone dla osób w różnych wieku i o różnym poziomie wykształcenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10780">
<skos:prefLabel>szkolenie wstępne</skos:prefLabel>
<skos:definition>działanie edukacyjne podejmowane na początku jakiejś działalności, w początkowym etapie formacji zawodowej lub cyklu kształcenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10785">
<skos:prefLabel>pedagogika</skos:prefLabel>
<skos:definition>zbiór zasad i wskazówek praktycznych dotyczących nauczania (istoty, celów, treści, metod, środków i form organizacyjnych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10818">
<skos:prefLabel>zawód związany z ochroną środowiska</skos:prefLabel>
<skos:definition>wykonywane w celach zarobkowych lub w ramach pracy  zawodowej działania związane z ekologią, w tym ochroną zasobów naturalnych i ekosystemów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10833">
<skos:prefLabel>służba publiczna</skos:prefLabel>
<skos:definition>działania podejmowane dla dobra społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10837">
<skos:prefLabel>organizacja pracy</skos:prefLabel>
<skos:definition>sposób koordynowania i zasady wykonywania pracy ludzkiej i procesów produkcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10847">
<skos:prefLabel>ośrodek sportu i rekreakcji</skos:prefLabel>
<skos:definition>budynek, w którym znajduje się basen, sala gimnastyczna lub inne pomieszczenia sportowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10851">
<skos:prefLabel>budynek użyteczności publicznej</skos:prefLabel>
<skos:definition>budynki, sprzęt i usługi przeznaczone dla społeczeństwa lub wspólnoty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10860">
<skos:prefLabel>ścieżka rowerowa</skos:prefLabel>
<skos:definition>wydzielona część jezdni lub specjalna droga służąca osobom poruszającym się na rowerach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10861">
<skos:prefLabel>trasa narciarska</skos:prefLabel>
<skos:definition>szlak, stok lub teren przeznaczony do jazdy na nartach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10884">
<skos:prefLabel>szlak turytyczny</skos:prefLabel>
<skos:definition>ustanowiona lub wybrana trasa, złożona najczęściej z mniej uczęszczanych dróg pozwalająca na poznanie miejsc ciekawych pod względem krajobrazu, kultury, historii, geologii lub przyrody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10890">
<skos:prefLabel>eko-museum</skos:prefLabel>
<skos:definition>prywatne, niedochodowe przedsięwzięcie służące pokazaniu roślin i zwierząt w ich naturalnym siedlisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10891">
<skos:prefLabel>folklor</skos:prefLabel>
<skos:definition>tradycyjne i powszechne wierzenia, praktyki i zwyczaje ludowe, przekazywane z pokolenia na pokolenie w powiązaniu ze sposobem życia, często w fomie ustnej jako baśnie, legendy, przysłowia i anegdoty oraz w innych formach (mity, rytuały, obrzędy, zwyczaje, taniec, muzyka, plastyka)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10892">
<skos:prefLabel>udział społeczeństwa</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10893">
<skos:prefLabel>ruch turystyczny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10895">
<skos:prefLabel>wiejska chata</skos:prefLabel>
<skos:definition>niewielki dom lub chata położona na w terenie wiejskim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10898">
<skos:prefLabel>zakwaterowanie</skos:prefLabel>
<skos:definition>zapewnienie miejsca pobytu lub przebywania w formie pokoju lub domu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10904">
<skos:prefLabel>społeczeństwo</skos:prefLabel>
<skos:definition>wspólnota, ludzie lub grupa osób, którą jednoczy wspólny interes lub zainteresowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10905">
<skos:prefLabel>ścieżka</skos:prefLabel>
<skos:definition>droga lub szlak łączący dwa miejsca</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10906">
<skos:prefLabel>ścieżka nadmorska</skos:prefLabel>
<skos:definition>droga lub szlak przebiegający wzdłuż wybrzeża</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10907">
<skos:prefLabel>ścieżka edukacyjna</skos:prefLabel>
<skos:definition>trasa wraz z objaśnieniami mającymi na celu przybliżenie (najczęściej młodzieży) rodzaju terenu, gleby, flory , fauny itp.;objaśnienia mogą być bierne (tablice, ulotki) lub czynne (przewodnik)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10909">
<skos:prefLabel>kurort nadmorski</skos:prefLabel>
<skos:definition>miejscowość nadmorska,w której  ludzie spędzają urlopy i odpoczywają</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10910">
<skos:prefLabel>kurort górski</skos:prefLabel>
<skos:definition>miejscowość górska, w której ludzie spędzają urlopy i odpoczywają</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10911">
<skos:prefLabel>ośrodek sportów zimowych</skos:prefLabel>
<skos:definition>ośrodki, w których  możliwe jest uprawianie sportów zimowych na wolnym powietrzu, w szczególności narciarstwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10914">
<skos:prefLabel>jednostka turystyczna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10915">
<skos:prefLabel>pojazd terenowy</skos:prefLabel>
<skos:definition>pojazd zbudowany tak, aby można go było używać na każdego rodzaju drodze oraz na bezdrożach w celu transportu towarów i ludzi, w tym rannych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10918">
<skos:prefLabel>gęstość zaludnienia</skos:prefLabel>
<skos:definition>proporcja liczby osób do powierzchni trenu, którym dysponują; liczba ludności zamieszkującej dany obszar przypadająca na jednostkę powierzchni terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10919">
<skos:prefLabel>młodzi</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10922">
<skos:prefLabel>ludność aktywna zawodowo</skos:prefLabel>
<skos:definition>liczba osób, które mogą wykonywać pracę w obrębie  przedsiębiorstwa, regionu lub kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10925">
<skos:prefLabel>przeznaczenie czasu</skos:prefLabel>
<skos:definition>przypisanie poszczególnych okresów dnia, tygodnia lub roku rodzajom działalności, w szczególności pracy i odpoczynkowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10927">
<skos:prefLabel>trwałe dobra</skos:prefLabel>
<skos:definition>dobra, które mogą być stosunkowo długo używane i nie są konsumowane w ramach użycia (np. lodówka)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10929">
<skos:prefLabel>nietrwałe dobra</skos:prefLabel>
<skos:definition>dobra nabywane przez konsumentów, które są zużywane w okresie krótszym niż rok (jedzenie, ubranie)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10930">
<skos:prefLabel>dobra</skos:prefLabel>
<skos:definition>termin wieloznaczny; obejmuje wszelkie przedmioty posiadania za wyjątkiem ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10931">
<skos:prefLabel>dobra i usługi</skos:prefLabel>
<skos:definition>suma działalności gospodarczej, obejmująca przedmioty materialne i działania ludzkie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10932">
<skos:prefLabel>bilans czasu</skos:prefLabel>
<skos:definition>określanie lub planowanie przeznaczenia czasu, wyrażone w godzinach, dniach, tygodniach itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10933">
<skos:prefLabel>środowisko człowieka</skos:prefLabel>
<skos:definition>zewnętrzne warunki lub otoczenie, w którym ludzie żyją i pracują</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10935">
<skos:prefLabel>cykl życiowy produktu</skos:prefLabel>
<skos:definition>cykl życiowy produktu obejmuje następujące etapy: pozyskiwanie surowców, produkcję, pakowanie, dystrybucję, używanie, recykling i unieszkodliwianie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10936">
<skos:prefLabel>ekoetykieta</skos:prefLabel>
<skos:definition>znak, pieczęć lub zaświadczenie umieszczone na produkcie lub dołączone do niego zawierające konkretną informację o oddziaływaniu na środowisko, która pozwala konsumentom na porównanie z innymi produktami lub zawiera wskazówki jak w sposób bezpieczny dla środowiska używać i właściwie unieszkodliwiać produkt i jego opakowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10938">
<skos:prefLabel>świadectwo jakości</skos:prefLabel>
<skos:definition>formalne pisemne potwierdzenie, że przedmiot lub usługa osiągnęło uznany i stosunkowo wysoki poziom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10939">
<skos:prefLabel>jakość życia</skos:prefLabel>
<skos:definition>miara poziomu rozwoju kraju lub społeczności, oparta na czynnikach takich jak dochód, wykształcenie, długość życia i zużycie zasobów naturalnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10940">
<skos:prefLabel>podaż i popyt</skos:prefLabel>
<skos:definition>stosunek pomiędzy ilością dobra dostępnego dla nabywców, a chęcią lub możliwością konsumentów do jego nabycia, który - teoretycznie - określa jego cenę w warunkach wolnorynkowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10959">
<skos:prefLabel>nierówność środowiskowa</skos:prefLabel>
<skos:definition>każda nierównośc lub nadmierna różnica pomiędzy mieszkańcami tego samego środowiska, która uznana jest za niewłaściwą, niesprawiedliwą lub szkodliwą dla spójności tego środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10960">
<skos:prefLabel>nierówność społeczna</skos:prefLabel>
<skos:definition>nierówne wynagrodzenie lub możliwości poszczególnych ludzi, grupy lub grup w obrębie społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10964">
<skos:prefLabel>mit</skos:prefLabel>
<skos:definition>baśniowa lub istniejąca w tradycji opowieść, w której występują stworzenia nadnaturalne, przodkowie, bohaterowie lub zdarzenia, nie bazująca na możliwych do zweryfikowania faktach, lecz służąca do uzasadnienia zwyczajów, rytuałów lub zjawisk przyrodniczych lub uzasadnienia istnienia instytucji społecznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10968">
<skos:prefLabel>postawa &quot;NIMBY&quot;</skos:prefLabel>
<skos:definition>&quot;nie na moim podwórku&quot; - określenie używane dla opisania postawy ludzi, którzy chętnie widzą powstawanie  inwestycji na terenach rolnych, pod warunkiem, że inwestycje te nie znajdą się w bezpośrednim sąsiedztwie ich miejsca zamieszkania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10971">
<skos:prefLabel>psychologia społeczna</skos:prefLabel>
<skos:definition>badanie oddziaływania struktur społecznych na poznawanie i zachowanie, w relacjach bezpośrednich oraz tworzeniu porządku społecznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10972">
<skos:prefLabel>społeczny system wartości</skos:prefLabel>
<skos:definition>system wartości i praktyki ustanowione aby stanowić punkt odniesienia jednostki w stosunku do społeczności i kultury oraz umożliwić nazywanie, klasyfikacje i porozumiewanie się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10976">
<skos:prefLabel>świadomość ekologiczna</skos:prefLabel>
<skos:definition>świadomość, wrażliwość lub sympatia do świata przyrody w niezmienionej postaci</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10977">
<skos:prefLabel>zagadnienia społeczno-gospodarcze</skos:prefLabel>
<skos:definition>gospodarcza i społeczna struktura społeczeństwa, zagadnienia podatków i rodzajów inwestycji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10980">
<skos:prefLabel>dostęp do dokumentów administarcyjnych</skos:prefLabel>
<skos:definition>zawarte w przepisach prawo dostępu do dokumentów administracyjnych  i zapoznania się z nimi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10981">
<skos:prefLabel>akta administracyjne</skos:prefLabel>
<skos:definition>wszelkie formalnie podjęte kroki lub postanowienia dotyczące polityki organizacji podjęte przez jej szefa lub osobę z grona zarządu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10985">
<skos:prefLabel>samorząd terytorialny</skos:prefLabel>
<skos:definition>jednostka administracyjna lub system w ramach którego podejmowane są decyzje polityczne dotyczące wskazanego terenu, części miasta, rejonu lub większej jednostki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10986">
<skos:prefLabel>wspomaganie decyzji</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10988">
<skos:prefLabel>kompetencje ministra</skos:prefLabel>
<skos:definition>wiedza, wykształcenie, kwalifikacje, zdolności oraz władza wiążące się ze stanowiskiem szefa administracji lub innym stanowiskiem kierowniczym będącym w gestii szefa rządu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10991">
<skos:prefLabel>obywatel</skos:prefLabel>
<skos:definition>członek społeczeństwa danego państwa, z racji urodzenia lub na skutek naturalizacji, mający określone uprawnienia i obowiązki wyznaczone przez prawo i konstytucję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10992">
<skos:prefLabel>lokalne władze</skos:prefLabel>
<skos:definition>jednostka administracyjna lub system, w ramach którego podejmowane są decyzje polityczne dotyczące miasta lub okręgu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10993">
<skos:prefLabel>lokalne społeczeństwo</skos:prefLabel>
<skos:definition>struktura społeczna, zespół ludzi, przebywający w jakimś obszarze, najczęściej pozamiejskim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/10999">
<skos:prefLabel>debata parlamentarna</skos:prefLabel>
<skos:definition>formalna dyskusja lub dysputa członków parlamentu dotycząca konkretnego zagadnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11007">
<skos:prefLabel>uzasadnienie decyzji administacyjnych</skos:prefLabel>
<skos:definition>poglądy lub czynniki wpływające na osoby podejmujące decyzję</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11016">
<skos:prefLabel>plan gospodarczy</skos:prefLabel>
<skos:definition>projekt lub plan dotyczący produkcji, dystrybucji i wykorzystania dóbr i dochodów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11018">
<skos:prefLabel>zasada podległości</skos:prefLabel>
<skos:definition>ogólna zasada stanowiąca, że decyzje polityczne powinny być podejmowane na szczeblu samorządowym, a rząd centralny podejmuje działania tylko wówczas,  jeśli mogą być bardziej skuteczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11030">
<skos:prefLabel>pomoc finansowa</skos:prefLabel>
<skos:definition>przekazywanie funduszy z krajów rozwiniętych do krajów mniej rozwiniętych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11031">
<skos:prefLabel>pomoc międzynarodowa</skos:prefLabel>
<skos:definition>gospodarcza, wojskowa, techniczna lub finansowa pomoc lub wsparcie dla krajów potrzebujących ze strony innych rządów lub organizacji międzynarodowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11032">
<skos:prefLabel>pomoc publiczna</skos:prefLabel>
<skos:definition>pomoc rządowa w formie pieniędzy lub bonów towarowych dla biednych, niepełnosprawnych, starych lub nieletnich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11034">
<skos:prefLabel>budżet państwa</skos:prefLabel>
<skos:definition>zorganizowana metoda dokumentowania transakcji wykonywanych w ramach gospodarki narodowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11035">
<skos:prefLabel>dodatkowe konto</skos:prefLabel>
<skos:definition>osobne konto lub oświadczenie dotyczące strony finansowej działania w określonej dzidzinie, które uzupełnia istniejące dane finansowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11038">
<skos:prefLabel>wydatki w gospodarstwie domowym</skos:prefLabel>
<skos:definition>wszelkie wydatki grupy ludzi mieszkających wspólnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11039">
<skos:prefLabel>wydatki publiczne</skos:prefLabel>
<skos:definition>wydatki rządu lub samorządu oraz organizacji rządowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11043">
<skos:prefLabel>fundusz interwencyjny</skos:prefLabel>
<skos:definition>pieniądze  lub zasoby finansowe przeznaczone na interwencję w określone działanie w celu wpłynięcia na jego wynik</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11044">
<skos:prefLabel>fundusz</skos:prefLabel>
<skos:definition>pieniądze przeznaczone na określony cel</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11045">
<skos:prefLabel>Europejski Fundusz Walutowy</skos:prefLabel>
<skos:definition>fundusz w ramach Europejskiego Sytemu Walutowego zasilany przez kraje członkowskie Wspólnoty Europejskiej w celu m.in.stabilizacji kursów walut</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11046">
<skos:prefLabel>Międzynarodowy Fundusz Walutowy</skos:prefLabel>
<skos:definition>organizacja międzynarodowa ustanowiona w 1944 r.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11047">
<skos:prefLabel>bodziec ekonomiczny</skos:prefLabel>
<skos:definition>nagrody lub kary proponowane przez rząd lub zarząd w celu skłaniania do spełniania założeń polityki krajowej lub firmowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11048">
<skos:prefLabel>zachęta podatkowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11050">
<skos:prefLabel>produkt krajowy brutto</skos:prefLabel>
<skos:definition>całość dóbr i usług wyprodukowanych przez gospodarkę w danym okresie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11051">
<skos:prefLabel>podatek celowy</skos:prefLabel>
<skos:definition>obowiązkowe obciążenie finansowe dla zgromadzenia funduszy na społecznie uzasadniony cel</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11055">
<skos:prefLabel>analiza procesowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11057">
<skos:prefLabel>audyt</skos:prefLabel>
<skos:definition>okresowa lub ciągła analiza ksiąg, własności i zobowiązań firmy lub innej organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11061">
<skos:prefLabel>ocena oddziaływania na dobra kultury</skos:prefLabel>
<skos:definition>inwentaryzacja zasobów naturalnych i krajobrazów w celu zapewniania ich ochrony i prawidłowego zarządzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11063">
<skos:prefLabel>cena wody</skos:prefLabel>
<skos:definition>ilość pieniędzy wydawana na zapewnienie dostaw wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11065">
<skos:prefLabel>ekomarketing</skos:prefLabel>
<skos:definition>kupowanie, sprzedawanie, reklamowanie, transportowanie i magazynowanie dóbr w zgodzie z zasadami ekrozwoju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11069">
<skos:prefLabel>patriarchalny system zarządzania</skos:prefLabel>
<skos:definition>sposób przewodzenia lub zarządzania, w którym usiłuje się zdobyć lojalność i wsparcie ze strony podwładnych poprzez nadopiekuńczośc i przesadną troskę o ich potrzeby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11070">
<skos:prefLabel>inwestycje ochrony środowiska</skos:prefLabel>
<skos:definition>inwestycje, z których zysk ma być przeznaczony na ograniczanie zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11071">
<skos:prefLabel>rynek ochrony środowiska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11073">
<skos:prefLabel>nagroda za działania na rzecz ochrony środowiska</skos:prefLabel>
<skos:definition>nagroda za działania podejmowane w celu ograniczenia zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11082">
<skos:prefLabel>cena wody</skos:prefLabel>
<skos:definition>cena, za którą woda może być sprzedana i kupowana</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11087">
<skos:prefLabel>teoria funkcjonowania środowiska</skos:prefLabel>
<skos:definition>spójny model lub opis funkcjonowania i współzależności w obrębie danego systemu oparty na oberwacji, doświadczeniu i logice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11089">
<skos:prefLabel>działalność handlowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11095">
<skos:prefLabel>lokalny rozwój</skos:prefLabel>
<skos:definition>wzrost lub postęp w obrębie małej społeczności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11097">
<skos:prefLabel>badania rynkowe</skos:prefLabel>
<skos:definition>zbieranie i analiza danych na temat popytu na towary lub usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11100">
<skos:prefLabel>wolny handel</skos:prefLabel>
<skos:definition>handel nie ograniczany cłami, limitami importowymi i eksportowymi itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11103">
<skos:prefLabel>kraje mniej rozwinięte</skos:prefLabel>
<skos:definition>kraje o niskim dochodzie narodowym, wysokim poziomie analfabetyzmu, niskich standardach zdrowotnych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11106">
<skos:prefLabel>relacje &quot;Północ-Południe&quot;</skos:prefLabel>
<skos:definition>związki pomiędzy krajami rozwiniętymi (Półkula Północna) i krajami trzeciego świata (Półkula Południowa)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11113">
<skos:prefLabel>obliczanie</skos:prefLabel>
<skos:definition>działanie, wynik lub proces liczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11116">
<skos:prefLabel>gęstość</skos:prefLabel>
<skos:definition>masa w jednostce objętości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11118">
<skos:prefLabel>rozkład przestrzenny</skos:prefLabel>
<skos:definition>rozmieszczenie w przestrzeni występowania pewnego zjawiska, zachowań lub charakterystyki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11119">
<skos:prefLabel>dane gospodarcze</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11124">
<skos:prefLabel>indeks</skos:prefLabel>
<skos:definition>lista uporządkowana mechanicznie według haseł</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11125">
<skos:prefLabel>spis ludności</skos:prefLabel>
<skos:definition>oficjalny okresowy spis ludności uwzględniający wiek, płeć, zawód itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11127">
<skos:prefLabel>szereg statystyczny</skos:prefLabel>
<skos:definition>uporządkowany ciąg danych wykorzystywany do kreślenia zmienności w czasie i przestrzeni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11128">
<skos:prefLabel>badanie opinii publicznej</skos:prefLabel>
<skos:definition>sondowanie reprezentatywnej grupy osób w celu oszacowania zdania większości na dany temat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11130">
<skos:prefLabel>tempo</skos:prefLabel>
<skos:definition>wielkość zmiany w czasie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11133">
<skos:prefLabel>zmiany sezonowe</skos:prefLabel>
<skos:definition>zmiany związane ze zmianą pór roku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11134">
<skos:prefLabel>komitet badawczy</skos:prefLabel>
<skos:definition>zorganizowana grupa osób wybrana lub mianowana dla przedyskutowania zagadnień naukowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11135">
<skos:prefLabel>dyskusja naukowa</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11146">
<skos:prefLabel>badania stosowane</skos:prefLabel>
<skos:definition>badania wykonywane w celu bezpośredniego wykorzystania wiedzy naukowej dla celów praktycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11148">
<skos:prefLabel>badania naukowe</skos:prefLabel>
<skos:definition>systematyczne badania mające na celu stwierdzenie faktów lub zasad</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11152">
<skos:prefLabel>agronomia</skos:prefLabel>
<skos:definition>zasady i procedury uprawy ziemi i produkcji roślinnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11153">
<skos:prefLabel>ekosystem rolny</skos:prefLabel>
<skos:definition>ekosystem z dominująca rolą ciągłej gospodarki rolnej prowadzonej przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11157">
<skos:prefLabel>klęska rolna</skos:prefLabel>
<skos:definition>gwałtowna, nagła i niszcząca zmiana w środowisku spowodowana przez/lub mająca wpływ na uprawę lub hodowlę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11158">
<skos:prefLabel>spadek wartości dóbr i usług rolnych</skos:prefLabel>
<skos:definition>spadek lub zaniżenie wartości dóbr i usług rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11159">
<skos:prefLabel>rozwój obszarów wiejskich</skos:prefLabel>
<skos:definition>wszelkie działania ukierunkowane na rozwój, modernizację i wzrost produkcji rolnej oraz zaspokojenie podstawowych potrzeb ludzi mieszkających na terenach wiejskich (woda, edukacja, opieka zdrowotna)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11165">
<skos:prefLabel>melioracje</skos:prefLabel>
<skos:definition>nauka i technika związana z zarządzaniem wodą, zapobieganiem erozji i usuwaniem nadmiaru wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11170">
<skos:prefLabel>agrotechnika</skos:prefLabel>
<skos:definition>działania, sztuka i technika związane z uprawą ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11177">
<skos:prefLabel>rośliny pastewne</skos:prefLabel>
<skos:definition>uprawy przeznaczone na pasze dla zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11178">
<skos:prefLabel>ogrodnictwo</skos:prefLabel>
<skos:definition>uprawa owoców i warzyw na skalę komercyjną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11184">
<skos:prefLabel>nasiona</skos:prefLabel>
<skos:definition>zarodek, z którego może powstać nowa roślina</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11186">
<skos:prefLabel>stosowanie pestycydów</skos:prefLabel>
<skos:definition>stosowanie substancji chemicznych aby ograniczyć straty w plonach powodowane przez owady lub chwasty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11187">
<skos:prefLabel>oczyszczanie roślinne</skos:prefLabel>
<skos:definition>usuwanie metali ciężkich z wody poprzez zastosowanie roślin lub sposobu oczyszczania, w którym działanie organizmów roślinnych powoduje rozkład niebezpiecznych zanieczyszczeń organicznych lub przekształacenie zanieczyszczeń nieorganicznych do bezpiecznych poziomów stężeń w glebie, ziemi, wodzie, osadach itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11192">
<skos:prefLabel>hodowla ptaków</skos:prefLabel>
<skos:definition>hodowanie, trzymanie i opieka nad ptakami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11198">
<skos:prefLabel>techniki hodowlane</skos:prefLabel>
<skos:definition>metody stosowane w hodowli zwierząt (intensywnej i ekstensywnej)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11199">
<skos:prefLabel>okresowy wypas zwierząt</skos:prefLabel>
<skos:definition>okresowe przemieszczanie się zwierząt na dogodne pastwiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11201">
<skos:prefLabel>substancje mineralne poprawiające stan gleby</skos:prefLabel>
<skos:definition>naturalne substancje nieorganiczne stosowane dla polepszenia jakości gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11204">
<skos:prefLabel>drenowanie</skos:prefLabel>
<skos:definition>sztuczne usuwanie wody z terenów podmokłych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11206">
<skos:prefLabel>gospodarka rolna oparta na karczowaniu i wypalaniu</skos:prefLabel>
<skos:definition>tradycyjna metoda uprawy stosowana w rejonie lasów tropikalnych i na sawannach w Afryce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11207">
<skos:prefLabel>kreda</skos:prefLabel>
<skos:definition>wapienna skała osadowa pochodzenia organicznego; biała, miękka, rozcieralna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11209">
<skos:prefLabel>nawóz azotowy</skos:prefLabel>
<skos:definition>naturalne lub sztuczne substancje zawierające formy azotu przyswajalne dla roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11211">
<skos:prefLabel>nawóz fosforowy</skos:prefLabel>
<skos:definition>substancje zawierające formy fosforu przyswajalne dla roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11212">
<skos:prefLabel>nawóz potasowy</skos:prefLabel>
<skos:definition>nawozy zawierające potas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11214">
<skos:prefLabel>samoooczyszczanie się gleby</skos:prefLabel>
<skos:definition>działanie lub proces, w ramach którego w naturalnym procesie z gleby usuwane są zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11224">
<skos:prefLabel>wymywanie z gleby</skos:prefLabel>
<skos:definition>usuwanie wody i substancji rozpuszczalnych z gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11226">
<skos:prefLabel>substancje mineralne</skos:prefLabel>
<skos:definition>materia nieorganiczna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11227">
<skos:prefLabel>substancja ograniczna</skos:prefLabel>
<skos:definition>pozostałości roślinne i zwierząece, które rozkładając się stają się częścią gleby</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11231">
<skos:prefLabel>dorzecze</skos:prefLabel>
<skos:definition>ciek powierzchniowy wraz z dopływami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11232">
<skos:prefLabel>system melioracycjny</skos:prefLabel>
<skos:definition>system stworzonych przez człowieka kanałów nawadniających, który umożliwia uprawę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11233">
<skos:prefLabel>zasolenie gleb</skos:prefLabel>
<skos:definition>badanie ilości soli mineralnych w glebie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11234">
<skos:prefLabel>wartość dóbr i usług rolnych</skos:prefLabel>
<skos:definition>materialna wartość, cena kupna lub sprzedaży, dóbr i usług rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11237">
<skos:prefLabel>zarządzanie terenami górskimi</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11243">
<skos:prefLabel>plan rozwoju terenów wiejskich</skos:prefLabel>
<skos:definition>działania i proces przygotowywania i kontrolowania przyszłych działań w obszarach wiejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11244">
<skos:prefLabel>zarządzanie turystyką</skos:prefLabel>
<skos:definition>administracja, promocja, organizacja i planowanie działań związanych z informacją, transportem, rozrywką, noclegami i innymi usługami dla podróżnych lub turystów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11246">
<skos:prefLabel>służba leśna</skos:prefLabel>
<skos:definition>jednostka lub grupa ludzi zaangażowana w tworzenie, zarządzanie i ochronę lasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11248">
<skos:prefLabel>plan zaopatrzenia</skos:prefLabel>
<skos:definition>systematyczna metoda dostarczania potrzebnych materiałów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11259">
<skos:prefLabel>dostęp</skos:prefLabel>
<skos:definition>sposób lub miejsce wejścia, dojazdu lub dostępu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11269">
<skos:prefLabel>dworzec kolejowy</skos:prefLabel>
<skos:definition>miejsce, w tym budynki, w którym pociągi zatrzymują się aby umożliwić pasażerom wsiadanie i wysiadanie oraz załadunek i rozładunek towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11270">
<skos:prefLabel>dworzec autobusowy</skos:prefLabel>
<skos:definition>miejsce, w tym budynki, w którym autobusy zatrzymują się aby umożliwić pasażerom wsiadanie i wysiadanie, tankowanie paliwa oraz załadowanie i rozładowanie towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11271">
<skos:prefLabel>materiał do budowy dróg</skos:prefLabel>
<skos:definition>komponenty stosowane do budowy ulic, autostrad i innych dróg np. piasek, żwir, kamienie, asfalt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11272">
<skos:prefLabel>prace inżynieryjne</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11276">
<skos:prefLabel>sieć kolejowa</skos:prefLabel>
<skos:definition>system torów kolejowych i ich połączeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11277">
<skos:prefLabel>sieć drogowa</skos:prefLabel>
<skos:definition>sieć drogowa w danym kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11289">
<skos:prefLabel>nawigacja</skos:prefLabel>
<skos:definition>nauka lub sztuka przeprowadzania samolotów lub statków z jednego miejsca na inne, w szczególności określania położenia, kursu i odległości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11294">
<skos:prefLabel>transport wodny</skos:prefLabel>
<skos:definition>transport osób i towarów statkami poruszającymi się po wyznaczonych szlakach wodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11296">
<skos:prefLabel>transport kombinowany</skos:prefLabel>
<skos:definition>transport, w którym stosuje się więcej niż jeden środek transportu (np. kolej i samochód)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11307">
<skos:prefLabel>społeczność miasta</skos:prefLabel>
<skos:definition>ludzie mieszkający w mieście</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11308">
<skos:prefLabel>plan rozwoju miasta</skos:prefLabel>
<skos:definition>tekst zawierający propozycje i procedury polepszenia warunków życia w mieście</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11309">
<skos:prefLabel>obszary podmiejskie</skos:prefLabel>
<skos:definition>tereny zlokalizowane w sąsiedztwie miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11312">
<skos:prefLabel>dom jednorodzinny</skos:prefLabel>
<skos:definition>samodzielny dom mieszkalny, który nie posiada wspólnych ścian z innym domem mieszkalnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11321">
<skos:prefLabel>plan zagospodarowania przestrzennego</skos:prefLabel>
<skos:definition>plan ustanawiający preferowane użytkowanie terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11322">
<skos:prefLabel>urbanistyka</skos:prefLabel>
<skos:definition>działanie lub proces przygotowywania przyszłych warunków życia w mieście, w szczególności w centrum miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11323">
<skos:prefLabel>sposób użytkowania terenu</skos:prefLabel>
<skos:definition>warunki ustalone pomiędzy właścicielem, a użytkowniekiem terenu, w szczególności w odniesieniu do terenów rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11332">
<skos:prefLabel>nowe miasto</skos:prefLabel>
<skos:definition>nowo utworzony, niezależny ośrodek miejski</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11333">
<skos:prefLabel>prawo pierwokupu ziemi</skos:prefLabel>
<skos:definition>tereny, w odniesieniu do których użytkownik terenu ma prawo pierwszeństwa zakupu ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11334">
<skos:prefLabel>obszar ograniczonego użytkowania</skos:prefLabel>
<skos:definition>każdy obszar, który wymaga szczególnej decyzji osoby lub agencji, aby ustalić jak może być użytkowany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11344">
<skos:prefLabel>architektura wiejska</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11345">
<skos:prefLabel>tradycyjna architektura</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11355">
<skos:prefLabel>burzenie budynków</skos:prefLabel>
<skos:definition>mechaniczne wyburzanie budynków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11359">
<skos:prefLabel>inżynieria miejska</skos:prefLabel>
<skos:definition>branża inżynierii zajmująca się formą i funkcją terenów miejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11375">
<skos:prefLabel>konserwacja budynków</skos:prefLabel>
<skos:definition>staranne odtwarzanie formy i detali budynków, najczęściej w postaci z określonej epoki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11427">
<skos:prefLabel>deklaracja statutowa</skos:prefLabel>
<skos:definition>deklaracja złożona w przepisany sposób, w obecności osoby uprawnionej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11429">
<skos:prefLabel>standard jakości</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11430">
<skos:prefLabel>norma europejska</skos:prefLabel>
<skos:definition>standard lub norma ustanowiona zgodnie z wymaganiami organizacji, z którymi Wspólnota Europejska zawarła stosowne porozumienie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11431">
<skos:prefLabel>norma ISO</skos:prefLabel>
<skos:definition>udokumentowane porozumienie dotyczące warunków technicznych lub innych ustaleń szczegółowych mających na celu zapewnienie właściwego standardu dóbr i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11433">
<skos:prefLabel>techniczne przepisy wykonawcze</skos:prefLabel>
<skos:definition>zatwierdzone przez rząd lub zarząd zasady lub wymogi odnoszące się do standardów, norm technicznych lub kodeksu praktyki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11466">
<skos:prefLabel>kodyfikacja</skos:prefLabel>
<skos:definition>proces zbierania i porządkowania praw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11468">
<skos:prefLabel>doktryna</skos:prefLabel>
<skos:definition>zasada, teoria lub prawo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11472">
<skos:prefLabel>prawo przewozowe</skos:prefLabel>
<skos:definition>zasady dotyczące przemieszczania dóbr lub osób</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11476">
<skos:prefLabel>przepisy dotyczące obszarów wiejskich</skos:prefLabel>
<skos:definition>wiążące zasady lub zestaw zasad ustanowionych przez rząd i dotyczących obszarów mniej zaludnionych, w szczególności wiejskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11477">
<skos:prefLabel>prawa zwierząt</skos:prefLabel>
<skos:definition>uzasadnione prawnie i moralnie prawa gatunków innych niż człowiek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11478">
<skos:prefLabel>prawa przyszłych pokoleń</skos:prefLabel>
<skos:definition>moralne, prawne i etyczne zobowiązania względem przyszłych pokoleń, które nie mają możliwości wpłynięcia na aktualnie podejmowane decyzje o znaczących w przyszłości skutkach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11479">
<skos:prefLabel>prawa obywatelskie</skos:prefLabel>
<skos:definition>uznane przez prawo i prawem chronione prawa obywateli danego państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11489">
<skos:prefLabel>ogłoszenie</skos:prefLabel>
<skos:definition>informacje, rady lub ostrzeżenia często przekazywane ze względu na prawny lub administracyjny wymóg</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11490">
<skos:prefLabel>okólnik</skos:prefLabel>
<skos:definition>memorandum, list lub ogłoszenie rozpowszechniane w ramach organizacji lub wysyłane do obszernej listy adresatów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11493">
<skos:prefLabel>orzeczenie Wspólnoty Europejskiej</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11494">
<skos:prefLabel>wyrok</skos:prefLabel>
<skos:definition>1) decyzja sądu; 2) dokument zawierający decyzję sądu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11497">
<skos:prefLabel>instrukcja administracyjna</skos:prefLabel>
<skos:definition>wskazówki dotyczące teorii i praktyki zarządzania biurem, firmą lub organizacją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11498">
<skos:prefLabel>instrukcja techniczna</skos:prefLabel>
<skos:definition>instrukcja lub edukacja dotycząca zagadnień mechanicznych, przemysłowych lub nauki stosowanej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11499">
<skos:prefLabel>prawo</skos:prefLabel>
<skos:definition>zestaw prawomocnych zasad działania, aprobowanych przez oficjalne organa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11502">
<skos:prefLabel>odstrzał</skos:prefLabel>
<skos:definition>oficjalne pozwolenie  wydawanie osobie lub organizacji i zezwalające na upolowanie w określonym czasie określonej ilości zwierząt określonych gatunków i w określonym wieku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11504">
<skos:prefLabel>pozwolenie na budowę</skos:prefLabel>
<skos:definition>wymagane prawem zezwolenie na budowę lub zmiany konstrukcyjne w obrębie budynku lub budowli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11506">
<skos:prefLabel>karta wędkarska</skos:prefLabel>
<skos:definition>oficjalne pozwolenie  wydawane osobie lub organizacji i zezwalające na dokonanie w określonym czasie odłowienia określonej ilości ryb określonego gatunku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11508">
<skos:prefLabel>projekt przepisu</skos:prefLabel>
<skos:definition>forma proponowanych aktów prawnych w procesie ich uzgadniania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11514">
<skos:prefLabel>przepis</skos:prefLabel>
<skos:definition>zasada ustanowiona przez rząd, zarząd lub organ ustawodawczy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11517">
<skos:prefLabel>wdrożenie dyrektywy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11521">
<skos:prefLabel>koncesja</skos:prefLabel>
<skos:definition>upoważnienie wydawane przez organ administracji do wykonywania określonej działalności i czerpania z niej korzyści</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11522">
<skos:prefLabel>publiczna debata</skos:prefLabel>
<skos:definition>omówienie, komentowanie lub wymiana zdań na tematy publiczne, przeprowadzone w sposób dostępny dla publiczności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11523">
<skos:prefLabel>decyzja o przedsięwzięciu użyteczności publicznej</skos:prefLabel>
<skos:definition>urzędowa decyzja pozwalająca na przejęcie własności prywatnej na cele publiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11524">
<skos:prefLabel>własnośc prywatna</skos:prefLabel>
<skos:definition>tereny będące własnością osób fizycznych lub firm, a nie skarbu państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11525">
<skos:prefLabel>drogi wodne</skos:prefLabel>
<skos:definition>rzeki, kanały i jeziora będące własnością państwową, w przeciwieństwie do własności prywatnej lub korporacyjnej (spółdzielczej)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11526">
<skos:prefLabel>morskie wody terytorialne</skos:prefLabel>
<skos:definition>obszary mórz i oceanów, które są własnością danego państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11527">
<skos:prefLabel>prawo dostępu</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11530">
<skos:prefLabel>dochodzenie w sprawie przedsięwzięcia użyteczności publicznej</skos:prefLabel>
<skos:definition>formalne dochodzenie w sprawie przedsięwzięcia użyteczności publicznej przeprowadzane przez powołany lub wyznaczony do tego celu organ</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11531">
<skos:prefLabel>rozprawa administracyjna</skos:prefLabel>
<skos:definition>formalne dochodzenie w sprawie przedsięwzięcia użyteczności publicznej przeprowadzane przez powołany lub wyznaczony do tego celu organ</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11532">
<skos:prefLabel>delegowanie obowiązków</skos:prefLabel>
<skos:definition>proces przekazywania uprawnień do podejmowania decyzji lub sprawowania funkcji administracyjnych pomiędzy organami administracji publicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11535">
<skos:prefLabel>władze państwowe</skos:prefLabel>
<skos:definition>prawo rządu do nakazywania i ograniczania działań przemysłu, organizacji i obywateli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11536">
<skos:prefLabel>służebność</skos:prefLabel>
<skos:definition>prawo użytkowania cudzej nieruchomości jako miejsca dostępu do własnej nieruchomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11540">
<skos:prefLabel>przestępstwo</skos:prefLabel>
<skos:definition>działanie dokonane z naruszeniem obowiązków obywatela względem społeczeństwa oraz przepisów prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11548">
<skos:prefLabel>uprawnienia policji</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11549">
<skos:prefLabel>prawo karne</skos:prefLabel>
<skos:definition>ogół norm określających postępowanie karne organów państwowych wobec obywateli naruszających  przepisy zawierające zakazy i nakazy, których naruszenie lub niespełnienie zagrożone jest karą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11556">
<skos:prefLabel>wyrok</skos:prefLabel>
<skos:definition>orzeczenie sądu dotyczące winy lub kary; w procesie cywilnym orzeczenie sądu rozstrzygające spór</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11559">
<skos:prefLabel>konflikt</skos:prefLabel>
<skos:definition>spór lub sprzeczność interesów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11563">
<skos:prefLabel>proces</skos:prefLabel>
<skos:definition>postępowanie prawne; rozstrzyganie sporu przez sąd</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11568">
<skos:prefLabel>sąd administracyjny</skos:prefLabel>
<skos:definition>niezależny organ, który podejmuje decyzje w przypadku sporów lub wątpliwości w odniesieniu do nadzorowanych przez siebie organów administracji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11571">
<skos:prefLabel>Europejski Trybunał Sprawiedliwości</skos:prefLabel>
<skos:definition>instytucja ustanowiona Traktatem Rzymskim, dla nadzorowania przestrzegania postanowień traktatu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11583">
<skos:prefLabel>praworządność</skos:prefLabel>
<skos:definition>przestrzeganie, poszanowanie obowiązujących praw, postępowanie zgodne z prawem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11587">
<skos:prefLabel>proces</skos:prefLabel>
<skos:definition>tocząca się przed sądem dla rozstrzygnięcia sporu analiza postępowania stron</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11594">
<skos:prefLabel>sąd administracyjny</skos:prefLabel>
<skos:definition>państwowy organ wymiaru sprawiedliwości roztrzygający spory zgodnie z prawem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11611">
<skos:prefLabel>dzierżawa</skos:prefLabel>
<skos:definition>porozumienie związane z użyczeniem prawa do korzystania z ziemi lub nieruchomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11612">
<skos:prefLabel>certyfikacja</skos:prefLabel>
<skos:definition>formalne pisemne potwierdzenie faktu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11618">
<skos:prefLabel>homologacja</skos:prefLabel>
<skos:definition>formalna aprobata zgodności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11621">
<skos:prefLabel>powiadomienie</skos:prefLabel>
<skos:definition>informacja o zdarzeniu przekazywana najczęściej drogą oficjalną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11623">
<skos:prefLabel>prawo pierwokupu</skos:prefLabel>
<skos:definition>prawo do pierwszeństwa zakupu ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11624">
<skos:prefLabel>zasiedzenie</skos:prefLabel>
<skos:definition>nabycie prawa do nieruchomości spowodowane jej ciągłym i długotrwałym użytkowaniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11627">
<skos:prefLabel>represja</skos:prefLabel>
<skos:definition>formalne działania (np. aresztowanie, zakaz działania) podejmowane w celu ograniczenia działań praktycznych związanych z krzewieniem idei lub informowaniem o faktach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11632">
<skos:prefLabel>delegowanie kompetencji</skos:prefLabel>
<skos:definition>powierzenie lub przypisanie części lub całości działań, obowiązków i władzy innej osobie lub instytucji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11633">
<skos:prefLabel>strumień zanieczyszczeń</skos:prefLabel>
<skos:definition>ciągła dyfuzja lub przemieszczanie się zanieczyszczeń, także tempo tego procesu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11635">
<skos:prefLabel>obszar wzajemnego oddziaływania powierzchni granicznych woda morska-powietrze</skos:prefLabel>
<skos:definition>morze i powietrze atmosferyczne to ciecze pozostające we wzajemnym kontakcie lecz w różnych stanach skupienia: ciekłym i gazowym; powierzchnia ich wzajemnego kontaktu ogranicza lecz nie zapobiega wymianie masy i energii; procesy te zachodzą najbardziej efektywnie, gdy występuje turbulencja; zarówno ciepło jak i para wodna przemieszczają się w kierunku od morza do powietrza; wymiana ciepła następuje podczas trzech zasadniczych procesów: promieniowania, przewodzenia i parowania z przewagą procesu parowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11636">
<skos:prefLabel>rozprzestrzenianie się zanieczyszczeń</skos:prefLabel>
<skos:definition>niekontrolowane przemieszczanie się ciekłych lub gazowych zanieczyszczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11638">
<skos:prefLabel>nadzwyczajne zagrożenie środowiska</skos:prefLabel>
<skos:definition>zanieczyszczenie spowodowane rozlaniem znacznych ilości ropy naftowej, uwolnieniem substancji radioaktywnych lub awaryjnym zrzutem do wód lub powietrza substancji z procesów przemysłowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11639">
<skos:prefLabel>skażenie bakteriologiczne</skos:prefLabel>
<skos:definition>skażenie wody, gleby i/lub powietrza bakteriami chorobotwórczymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11641">
<skos:prefLabel>emisja niezorganizowana</skos:prefLabel>
<skos:definition>zanieczyszczenie, któremu nie sposób przypisać jednego źródła, np. spływ pestycydów z pól, kwaśny deszcz itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11642">
<skos:prefLabel>zanieczyszczenia krajowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11643">
<skos:prefLabel>zanieczyszczenia nieorganiczne</skos:prefLabel>
<skos:definition>zanieczyszczenia pochodzące głównie z działań wydobywania kopalin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11644">
<skos:prefLabel>zanieczyszczenia organiczne</skos:prefLabel>
<skos:definition>zanieczyszczenia powodowane przez substancje pochodzenia zwierzęcego lub roślinnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11645">
<skos:prefLabel>zanieczyszczenia fotochemiczne</skos:prefLabel>
<skos:definition>zanieczyszczenia powietrza, w tym ozon i inne związki chemiczne tworzone poprzez działanie światła na tlenki azotu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11646">
<skos:prefLabel>zanieczyszczenie morza ze źródeł lądowych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11647">
<skos:prefLabel>zanieczyzczenia toksyczne</skos:prefLabel>
<skos:definition>zanieczyszczenia substancjami toksycznymi, o negatywnym oddziaływaniu na organizmy żywe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11648">
<skos:prefLabel>zrzut do rzeki</skos:prefLabel>
<skos:definition>umieszczanie odpadów w postaci stałej ciekłej lub gazowej w wodach rzeki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11649">
<skos:prefLabel>składowanie podziemne</skos:prefLabel>
<skos:definition>zrzut, emisja lub składowanie zanieczyszczeń lub odpadów pod powierzchnią ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11651">
<skos:prefLabel>zanieczyszczenia miejskie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11661">
<skos:prefLabel>zapobieganie</skos:prefLabel>
<skos:definition>działania podejmowane wyprzedzająco dla ograniczenia występowania jakiegoś zjawiska lub jego skutków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11662">
<skos:prefLabel>ochrona</skos:prefLabel>
<skos:definition>każde zapobiegawcze działanie, procedura lub instalacja, która ma na celu ochronę ludzi, mienia lub środowiska przed szkodą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11671">
<skos:prefLabel>silna kwasowość</skos:prefLabel>
<skos:definition>silna jonizacja kwasu w roztworze wodnym; cecha roztworów elektrolitów, w których jest silna przewaga stężenia jonów wodorowych nad stężeniem jonów wodorotlenowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11672">
<skos:prefLabel>biomarker</skos:prefLabel>
<skos:definition>metabolit, którego nadmiar wskazuje na stan choroby lub warunki toksyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11675">
<skos:prefLabel>skażenie biologiczne</skos:prefLabel>
<skos:definition>obecność w środowisku bakterii, grzybów  i/lub wirusów, które powodować mogą negatywne efekty zdrowotne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11676">
<skos:prefLabel>skażenie chemiczne</skos:prefLabel>
<skos:definition>występowanie lub nadmiar substancji chemicznej, która uniemożliwia wykorzystanie materiału zgodnie z przeznaczeniem; także skutki takiego zjawiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11678">
<skos:prefLabel>biologiczne skutki zanieczyszczenia</skos:prefLabel>
<skos:definition>skutki zanieczyszczenia dla organizmów żywych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11686">
<skos:prefLabel>nieodwracalność zjawiska</skos:prefLabel>
<skos:definition>rodzaj procesu, który wyklucza osiągnięcie stanu środowiska sprzed jego rozpoczęcia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11691">
<skos:prefLabel>cel jakościowy</skos:prefLabel>
<skos:definition>każdy cel dotyczący produktu, usługi lub działania, który świadczy o jego dążeniu do doskonałości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11692">
<skos:prefLabel>cząstka materialna</skos:prefLabel>
<skos:definition>maleńkie cząstki materii posiadające określoną objętości i kształt oraz stawiające opór siłom dążącym do zmiany tej objętości lub kształtu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11693">
<skos:prefLabel>zdolność oczyszczania</skos:prefLabel>
<skos:definition>zdolność systemu do regeneracji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11700">
<skos:prefLabel>czujnik</skos:prefLabel>
<skos:definition>urządzenie mierzące wartość lub zmianę wartości parametru i przetwarzające ją w informację o przebiegu procesu lub zjawiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11701">
<skos:prefLabel>oprzyrządowanie</skos:prefLabel>
<skos:definition>projektowanie, produkowanie i stosowanie instrumentów dla wykrywania, obserwowania, mierzenia, kontrolowania i dokumentowania zjawisk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11702">
<skos:prefLabel>metrologia</skos:prefLabel>
<skos:definition>nauka o pomiarach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11706">
<skos:prefLabel>satelita obserwacyjny</skos:prefLabel>
<skos:definition>stworzone przez człowieka urządzenie umieszczone na orbicie, które odbiera, przetwarza i wysyła sygnały</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11713">
<skos:prefLabel>atrazyna</skos:prefLabel>
<skos:definition>herbicyd z grupy tyrazyn, stsowany między innymi przy uprawie kukurydzy; z uwagi na łatwą rozpuszczalnośc stanowi zagrożenie dla ekosystemów wodnych, w szczególnosci dla fitoplanktonu i glonów, a także dla wód podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11714">
<skos:prefLabel>azot organiczny</skos:prefLabel>
<skos:definition>zasadnicza substancja odżywcza dla roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11718">
<skos:prefLabel>chlorowce</skos:prefLabel>
<skos:definition>substancje zawierające halogeny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11734">
<skos:prefLabel>tlenek siarki</skos:prefLabel>
<skos:definition>w temperaturze pokojowej - gaz; otrzymywany przy wyładowywaniach elektrycznych w atmosferze par siarki i dwutlenku siarki w niskich temperaturach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11745">
<skos:prefLabel>piralen</skos:prefLabel>
<skos:definition>pierwiastek chemiczny z grupy polichlorowanych dwufenyli, stosowany przy produkcji urządzeń elektrycznych takich jak transformatory i kondensatory</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11752">
<skos:prefLabel>azbestoza</skos:prefLabel>
<skos:definition>postępująca nieodwracalna choroba płuc spowodowana wdychaniem włókien azbestu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11762">
<skos:prefLabel>genotoksyczność</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11763">
<skos:prefLabel>zatrucie</skos:prefLabel>
<skos:definition>stan spowodowany przez kontakt z trucizną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11771">
<skos:prefLabel>testy biologiczne</skos:prefLabel>
<skos:definition>laboratoryjne określenie oddziaływania substancji na określone organizmy żywe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11779">
<skos:prefLabel>całkowity węgiel organiczny</skos:prefLabel>
<skos:definition>ilość węgla zawartego w związkach organicznych w próbce wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11781">
<skos:prefLabel>rozkład chemiczny</skos:prefLabel>
<skos:definition>działanie lub proces rozpadu lub rozkładu molekuł, zachodzący naturalnie lub wywołany sztucznie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11782">
<skos:prefLabel>badania laboratoryjne</skos:prefLabel>
<skos:definition>testy, badania i oszacowania wykonywane w laboratorium</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11786">
<skos:prefLabel>biologiczny wskaźnik zanieczyszczenia</skos:prefLabel>
<skos:definition>skala jakości środowiska oparta na występujących w tym środowisku organizmach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11787">
<skos:prefLabel>substancja utlenialna</skos:prefLabel>
<skos:definition>substancja, która wchodzi w związki chemiczne z tlenem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11788">
<skos:prefLabel>tlen rozpuszczony</skos:prefLabel>
<skos:definition>ilość rozpuszczonego tlenu zawartego w wodzie i stwarzającego warunki dla życia biologicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11789">
<skos:prefLabel>fotodegradacja</skos:prefLabel>
<skos:definition>zdolność do rozkładu pod wpływem długotrwałego działania światła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11791">
<skos:prefLabel>substancja niezropuszczalna</skos:prefLabel>
<skos:definition>substancja niezdolna do tworzenia roztworu, szczególnie z wodą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11792">
<skos:prefLabel>substancja nielotna</skos:prefLabel>
<skos:definition>substancja niezdolna do przejścia z fazy stałej lub płynnej do gazowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11793">
<skos:prefLabel>substancja słaborozkładalna</skos:prefLabel>
<skos:definition>substancja, którą trudno jest przeprowadzić do prostrzej formy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11794">
<skos:prefLabel>substancja lotna</skos:prefLabel>
<skos:definition>substancja zdolna do przejścia w fazę gazową, o niskim punkcie wrzenia i wysokim ciśnieniu pary nasyconej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11795">
<skos:prefLabel>korozyjność</skos:prefLabel>
<skos:definition>skłonność materiału do rozkładu pod wpływem działania chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11798">
<skos:prefLabel>analiza fizyko-chemiczna</skos:prefLabel>
<skos:definition>analiza fizycznych zmian zachodzących na skutek reakcji chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11807">
<skos:prefLabel>badania doświadczalne</skos:prefLabel>
<skos:definition>badania polegające na przeprowadzaniu doświadczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11809">
<skos:prefLabel>granulometria</skos:prefLabel>
<skos:definition>1) określanie wielkości ziaren; 2) proporcja wagowa poszczególnych frakcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11813">
<skos:prefLabel>substancja chemiczna</skos:prefLabel>
<skos:definition>substancja o określonym składzie cząsteczkowym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11814">
<skos:prefLabel>materiał łatwopalny</skos:prefLabel>
<skos:definition>materiał zdolny do wytworzenia palnych par, tzn. takich, które zapalą się w przypadku wystąpienia zapłonu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11816">
<skos:prefLabel>właściwosci organoleptyczne</skos:prefLabel>
<skos:definition>właściwości, które mogą być określone przy pomocy zmysłów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11835">
<skos:prefLabel>słuch</skos:prefLabel>
<skos:definition>system odbioru dźwięków u kręgowców</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11836">
<skos:prefLabel>układ krążenia</skos:prefLabel>
<skos:definition>układ zapewniający obieg krwi w organizmie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11852">
<skos:prefLabel>enterowirus</skos:prefLabel>
<skos:definition>wirusy enterotropowe, tj. mające powinowactwo do tkanek przewodu pokarmowego; materiał genetyczny stanowi RNA; przykładem enterowirusów jest rodzaj Enterovirus z rodziny Picornaviridae, obejmujący różne typy wirusów, w tym Polio (Enterovirus l, Poliovirus hominis) odpowiedzialny za chorobę Heinego–Medina (poliomyelitis), Enterovirus 72 wywołujący zapalenie wątroby typu A, także z tej rodziny wirusy Coxsackie, Echovirus oraz wiele innych chorobotwórczych dla człowieka lub/i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11868">
<skos:prefLabel>biologia zwierząt</skos:prefLabel>
<skos:definition>nauka o zwierzętach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11869">
<skos:prefLabel>biologia roślin</skos:prefLabel>
<skos:definition>nauka o roślinach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11875">
<skos:prefLabel>zasoby genetyczne</skos:prefLabel>
<skos:definition>łączna ilość genów lub informacji genetycznej u wszystkich osobników w wieku reprodukcyjnym w populacji organizmów rozmnażających się płciowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11879">
<skos:prefLabel>ostrość słuchu</skos:prefLabel>
<skos:definition>skuteczność słyszenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11889">
<skos:prefLabel>decybel</skos:prefLabel>
<skos:definition>jednostka służąca do porównywania mocy dźwięków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11896">
<skos:prefLabel>charakterystyka akustyczna</skos:prefLabel>
<skos:definition>charakterystyka przestrzeni mówiąca o jej zdolności do przenoszenia dźwięków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11909">
<skos:prefLabel>widmo akustyczne</skos:prefLabel>
<skos:definition>zakres częstotliwości dźwięku emitowanych przez źródło</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11912">
<skos:prefLabel>wibracje mechaniczne</skos:prefLabel>
<skos:definition>poruszanie się, zwykle niecelowe, często nieporządane maszyn i konstrukcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11918">
<skos:prefLabel>hałas wystepujacy w powietrzu</skos:prefLabel>
<skos:definition>hałas spowodowany przez przemieszczanie się mas powietrza oraz powietrze pod ciśnieniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11921">
<skos:prefLabel>tło akustyczne</skos:prefLabel>
<skos:definition>hałas pochodzący z innych źródeł niż źródło w danej chwili badane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11922">
<skos:prefLabel>hałas spowodowany toczeniem się kół</skos:prefLabel>
<skos:definition>hałas spowodowany przez tarcie pomiędzy kołami i powierzchnią drogi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11923">
<skos:prefLabel>hałas ze źródeł lokalnych</skos:prefLabel>
<skos:definition>łączny hałas ze źródeł położonych w okolicy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11927">
<skos:prefLabel>komfort akustyczny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11930">
<skos:prefLabel>plan ograniczania hałasu</skos:prefLabel>
<skos:definition>sformułowana lub usystematyzowana metoda ograniczenia narażenia na uciążliwy hałas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11958">
<skos:prefLabel>elektrownia pływowa</skos:prefLabel>
<skos:definition>elektrownia wodna, wytwarzająca prąd w oparciu o przepływ wody wywołany pływami morskimi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11967">
<skos:prefLabel>energia hydroelektryczna</skos:prefLabel>
<skos:definition>odnawialna energia zawarta w wodzie napędzającej turbiny elektrowni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11974">
<skos:prefLabel>lokalna elektorownia</skos:prefLabel>
<skos:definition>małe elektrownie służące zaspokojeniu lokalnych potrzeb energetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11975">
<skos:prefLabel>młyn wodny</skos:prefLabel>
<skos:definition>młyn napędzany wodą, najczęściej rzeki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/11990">
<skos:prefLabel>odłączanie elektrowni</skos:prefLabel>
<skos:definition>proces zakończenia eksploatacji elektrowni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12004">
<skos:prefLabel>pierwiastek promieniotwórczy</skos:prefLabel>
<skos:definition>pierwiastek o naturalnej promieniotwórczości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12006">
<skos:prefLabel>reaktor prędki</skos:prefLabel>
<skos:definition>reaktor jądrowy o wysokiej sprawności, powodujący powstawanie większej ilości materiałów rozszczepionych, powodujący powstawanie plutonu 239 z uranu 238</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12007">
<skos:prefLabel>tereny zdegradowane</skos:prefLabel>
<skos:definition>każde miejsce lub obszar, którego użytkowanie jest ograniczone występującym zanieczyszczeniem lub skażeniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12008">
<skos:prefLabel>aerosol</skos:prefLabel>
<skos:definition>układ rozproszony, w którym ośrodkiem rozpraszającym (dyspersyjnym) jest gaz, a fazą rozproszoną ciecz lub ciało stałe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12011">
<skos:prefLabel>biopaliwo</skos:prefLabel>
<skos:definition>gazowe, płynne lub stałe paliwo, w którym energia pochodzi ze źródła biologicznego, odnawialnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12013">
<skos:prefLabel>uszkodzenia lasu</skos:prefLabel>
<skos:definition>uszkodzenia drzewostanu w lesie spowodowane przez kwaśne deszcze, pożary, szkodniki, wylesianie, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12017">
<skos:prefLabel>etanol</skos:prefLabel>
<skos:definition>bezbarwny płyn, stosowany jako rozpuszczalnik; alkohol etylowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12022">
<skos:prefLabel>zanieczyszczenia komunikacyjne</skos:prefLabel>
<skos:definition>zanieczyszczenia powietrza spowodowane przez pracę silników pojazdów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12026">
<skos:prefLabel>opad zanieczyszczeń z atmosfery</skos:prefLabel>
<skos:definition>opad pyłu i drobnych cząstek z powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12027">
<skos:prefLabel>czyste pojazdy</skos:prefLabel>
<skos:definition>pojazdy, które nie emitują zanieczyszczeń do powietrza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12045">
<skos:prefLabel>paloeklimatologia</skos:prefLabel>
<skos:definition>nauka o klimacie w minionych epokach geologicznych i historycznych, występujących zmianach i przyczynach tych zmian</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12060">
<skos:prefLabel>klimat kontynentalny</skos:prefLabel>
<skos:definition>klimat typowy dla wnętrza kontynentu, o gorących latach i mroźnych zimach oraz niewielkiej ilości opadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12061">
<skos:prefLabel>klimat pustynny</skos:prefLabel>
<skos:definition>klimat bardzo suchy, o niedostatecznej dla większości roślin ilości opadów i wilgotności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12062">
<skos:prefLabel>klimat równikowy</skos:prefLabel>
<skos:definition>klimat charakteryzujący się stałymi temperaturami, obfitymi opadami oraz krótką porą suchą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12063">
<skos:prefLabel>klimat śródziemnomorski</skos:prefLabel>
<skos:definition>klimat charakteryzujący się gorącymi suchymi latami oraz deszczowymi zimami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12064">
<skos:prefLabel>klimat górski</skos:prefLabel>
<skos:definition>klimat charakterystyczny dla miejscowości wysoko położonych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12065">
<skos:prefLabel>klimat oceaniczny</skos:prefLabel>
<skos:definition>klimat charakteryzujący się stosunkowo chłodnym latem i łagodną zimą oraz dużą wilgotnością; klimat morski</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12066">
<skos:prefLabel>klimat umiarkowany</skos:prefLabel>
<skos:definition>klimat pośredni między gorącym a zimnym (tropikalnym i polarnym)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12067">
<skos:prefLabel>klimat tropikalny</skos:prefLabel>
<skos:definition>klimat właściwy dla strefy podzwrotnikowej, gorący i wilgotny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12082">
<skos:prefLabel>inwersja temperatury</skos:prefLabel>
<skos:definition>wzrost temperatury powietrza spowodowany wzrostem wysokości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12084">
<skos:prefLabel>burza</skos:prefLabel>
<skos:definition>zjawisko spowodowane silnymi prądami wznoszącymi powietrza, charakteryzujące się wyładowaniami atmosferycznymi i dużymi opadami deszczu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12091">
<skos:prefLabel>gołoledź</skos:prefLabel>
<skos:definition>szklista pokrywa lodowa powstająca w czasie obniżania się temperatury poniżej 0°C wskutek zamarzania spadających przechłodzonych kropli deszczu, mżawki lub opadania mgły, a także z opadu nie przechłodzonego, jeśli zetknie się on z podłożem bardzo wyziębionym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12096">
<skos:prefLabel>użytkowanie wód podziemnych</skos:prefLabel>
<skos:definition>wydobywanie i korzystanie z wód podziemnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12098">
<skos:prefLabel>inflitracja wody do ziemi</skos:prefLabel>
<skos:definition>przemieszczanie się wody w głąb gleby, ziemi lub podłoża</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12104">
<skos:prefLabel>zwierciadło wody</skos:prefLabel>
<skos:definition>powierzchnia wody; lustro wody, w tym wód podziemnych występujące w porach i szczelinach skał skorupy ziemskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12107">
<skos:prefLabel>ochrona zlewni</skos:prefLabel>
<skos:definition>działania, procedury lub instalacje mające na celu zapobieganie powstaniu zaburzeń w funkcjonowaniu zlewni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12108">
<skos:prefLabel>ochrona wód podziemnych</skos:prefLabel>
<skos:definition>wody podziemne są bardzo wrażliwe na zanieczyszczenia, a ich oczyszczanie jest bardzo kosztowne, stąd ogromne znaczenie ochrony i zapobiegania zanieczyszczeniom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12122">
<skos:prefLabel>spływy powodziowe</skos:prefLabel>
<skos:definition>łączna ilość wody spływająca z danej zlewni w okresie powodzi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12123">
<skos:prefLabel>zamulanie</skos:prefLabel>
<skos:definition>zapełnianie osadem partii dennych zbiornika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12128">
<skos:prefLabel>hydrometria</skos:prefLabel>
<skos:definition>dział hydrologii, nauka o pomiarach wodnych zajmująca się zmianami poziomu wód powierzchniowych oraz zjawisk lodowych, pomiarami głębokości rzek i jezior, ruchu i przepływu wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12131">
<skos:prefLabel>przewidywanie powodzi</skos:prefLabel>
<skos:definition>badania nad procesem hydrologicznym powstawania powodzi oraz wnioski z tych badań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12135">
<skos:prefLabel>sieć hydrograficzna</skos:prefLabel>
<skos:definition>konfiguracja przestrzenna naturalnych cieków wodnych danego rejonu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12139">
<skos:prefLabel>sedymentologia</skos:prefLabel>
<skos:definition>nauka badająca mechanizm powstawania osadów oraz czynniki warunkujące ich powstawanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12145">
<skos:prefLabel>zlewnia</skos:prefLabel>
<skos:definition>obszar, z którego wody spływają do wspólnego zbiornika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12151">
<skos:prefLabel>woda śródlądowa spełniająca wymogi kąpieliska</skos:prefLabel>
<skos:definition>warunki, które musi spełniać woda w zbiorniku słodkowodnym, w którym kąpiel jest dozwolona lub nie jest zabroniona</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12152">
<skos:prefLabel>wody publiczne</skos:prefLabel>
<skos:definition>wody będące własnością lub pozostające w zarządzie administracji państwowej lub agendy rządowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12153">
<skos:prefLabel>wody nie publiczne</skos:prefLabel>
<skos:definition>wody będące własnością lub pozostające w zarządzie osoby prywatnej lub prawnej nie reprezentującej organów państwowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12161">
<skos:prefLabel>bagno</skos:prefLabel>
<skos:definition>potoczna nazwa stosowana w Szkocji i Irlandii dla określenia obszarów podmokłych i grząskich, zbudowanych z rozkładającego się materiału organicznego, w szczególności sitowia, wełnianki i mchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12165">
<skos:prefLabel>odsalanie wody</skos:prefLabel>
<skos:definition>proces usuwania części lub całości soli rozpuszczonych w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12166">
<skos:prefLabel>woda morska spełniająca wymogi kąpieliska</skos:prefLabel>
<skos:definition>woda morska spełniająca warunki (fizyczne, chemiczne i bakteriologiczne) dopuszczające kąpiel lub nie powodujące zakazu kąpieli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12170">
<skos:prefLabel>ujście do  morza</skos:prefLabel>
<skos:definition>miejsce, w którym rzeka, kanał, kolektor ściekowy itp. łączy prowadzone wody z wodami morza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12171">
<skos:prefLabel>eksploatacja mórz</skos:prefLabel>
<skos:definition>wykorzystywanie mórz jako źródła pożywienia, surowców mineralnych, energii i wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12174">
<skos:prefLabel>martwa fala</skos:prefLabel>
<skos:definition>fala na morzu utrzymująca się po ustaniu wiatru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12183">
<skos:prefLabel>zanieczyszczenie w ujściach rzek</skos:prefLabel>
<skos:definition>zanieczyszczenie zlokalizowane w odcinku cieku w pobliżu jego ujścia, w zasięgu pływów zbiornika wodnego, do którego wpadają wody danego cieku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12189">
<skos:prefLabel>zarządzanie wodami płynącymi</skos:prefLabel>
<skos:definition>administrowanie lub postępowanie z wodami płynącymi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12198">
<skos:prefLabel>spłukiwanie</skos:prefLabel>
<skos:definition>usuwanie osadów lub zawiesin za pomocą wody przepływającej z dużą prędkością</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12199">
<skos:prefLabel>śluza</skos:prefLabel>
<skos:definition>urządzenie (szczelna komora) umożliwiające komunikację między dwoma zbiornikami o różnych poziomach wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12202">
<skos:prefLabel>usuwanie wodorostów</skos:prefLabel>
<skos:definition>okresowe ręczne lub maszynowe usuwanie nadmiaru roślinności w kanałach i na ich obwałowaniach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12204">
<skos:prefLabel>umacnianie brzegów</skos:prefLabel>
<skos:definition>prace inżynieryjne mające na celu zapobieganie erozji brzegów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12210">
<skos:prefLabel>zbiornik wodny</skos:prefLabel>
<skos:definition>zbiorniki używane do magazynowania wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12213">
<skos:prefLabel>odwodnienie zapory</skos:prefLabel>
<skos:definition>system śluz i przepustów umożliwiający w razie potrzeby opróżnienie sztucznego zbiornika wodnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12214">
<skos:prefLabel>korozja wodna</skos:prefLabel>
<skos:definition>złożone reakcje utleniania i redukcji pomiędzy wodą a materiałami zanurzonymi w niej lub stosowanymi do jej transportu i magazynowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12215">
<skos:prefLabel>poprawa jakości wody</skos:prefLabel>
<skos:definition>poprawa stanu i jakości wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12216">
<skos:prefLabel>smak wody</skos:prefLabel>
<skos:definition>smak wody powodowany być może substancjami rozpuszczonymi lub zawieszonymi: organicznymi, nieorganiczymi, gazami lub solami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12219">
<skos:prefLabel>zielony pływ</skos:prefLabel>
<skos:definition>zakwit zielonego planktonu morskiego, toksyczny, czasem śmiertelny dla ryb, być może spowodowany dopływem biogenów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12220">
<skos:prefLabel>zawiesina</skos:prefLabel>
<skos:definition>układ dyspersyjny, w którym fazę rozproszoną stanowią cząstki ciała stałego, a fazę rozpraszającą - ciecz</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12226">
<skos:prefLabel>zasolenie</skos:prefLabel>
<skos:definition>sposób mierzenia i wyrażania ilości soli rozpuszczonych w wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12231">
<skos:prefLabel>zlewnia</skos:prefLabel>
<skos:definition>obszar, z którego gromadzona jest woda</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12240">
<skos:prefLabel>fontanna</skos:prefLabel>
<skos:definition>strumień wody wyrzucany do góry lub w bok albo urządzenie, najczęściej dekoracyjne, służące do wymuszenia takiego przepływu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12248">
<skos:prefLabel>zbiornik wodny</skos:prefLabel>
<skos:definition>sztuczny lub naturalny zbiornik stosowany do magazynowania wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12254">
<skos:prefLabel>demineralizacja wody</skos:prefLabel>
<skos:definition>usuwanie z wody substancji chemicznych za pomocą reakcji chemicznych, wymiany jonowej lub destylacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12259">
<skos:prefLabel>lokalna oczyszczalnia ścieków</skos:prefLabel>
<skos:definition>proces oczyszczania ścieków w miejscu ich powstawania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12260">
<skos:prefLabel>zbiorcza oczyszczalnia scieków</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12263">
<skos:prefLabel>zbiornik wód deszczowych</skos:prefLabel>
<skos:definition>zbiornik do magazynowania wód opadowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12273">
<skos:prefLabel>kanalizacja deszczowa</skos:prefLabel>
<skos:definition>system kanalizacyjny służący odprowadzaniu wód opadowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12275">
<skos:prefLabel>kanalizacja rozdzielcza</skos:prefLabel>
<skos:definition>system kanalizacyjny służący do odprowadzania ścieków (wód zużytych)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12276">
<skos:prefLabel>kanalizacja zbiorcza</skos:prefLabel>
<skos:definition>system kanalizacyjny służący zarazem do odprowadzania ścieków i wód opadowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12277">
<skos:prefLabel>napowietrzanie</skos:prefLabel>
<skos:definition>wprowadzanie powietrza do ścieków lub wody w celu zwiększenia ilości rozpuszczonego tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12278">
<skos:prefLabel>przydomowa oczyszczalnia ścieków</skos:prefLabel>
<skos:definition>gromadzenie, oczyszczanie i doprowadzanie ścieków z jednego gospodarstwa domowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12279">
<skos:prefLabel>osadnik</skos:prefLabel>
<skos:definition>zbiornik, w którym następuje wydzielenie substancji zawieszonych w postaci osadu, najczęściej  na skutek zwolnienia prędkości przepływu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12280">
<skos:prefLabel>złoże biologiczne</skos:prefLabel>
<skos:definition>biologiczna metoda oczyszczania ścieków poprzez stałe przesączanie ścieków przez materiał złoża pokrytego błoną biologiczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12283">
<skos:prefLabel>ścieki</skos:prefLabel>
<skos:definition>wody zużyte, odprowadzane po wykorzystaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12284">
<skos:prefLabel>ścieki miejskie</skos:prefLabel>
<skos:definition>płynne odpady powstające w jednostce osadniczej na skutek zużycia wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12285">
<skos:prefLabel>ścieki oczyszczone</skos:prefLabel>
<skos:definition>częściowo lub całkowicie oczyszczone wody zużyte, odprowadzane z oczyszczalni ścieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12287">
<skos:prefLabel>biologiczna oczyszczalnia ścieków</skos:prefLabel>
<skos:definition>oczyszczanie ścieków oparte na procesach biochemicznych lub działaniu bakterii w celu utlenienie i ustabilizowania substancji organicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12289">
<skos:prefLabel>oczyszczanie w stawach osadowych</skos:prefLabel>
<skos:definition>wykorzystanie działania słońca i procesów bakteryjnych w płytkich stawach lub systemach stawów czy poletek zalewowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12293">
<skos:prefLabel>samooczyszczanie</skos:prefLabel>
<skos:definition>naturalny, zachodzący bez udziału człowieka proces degradacji zanieczyszczeń i oczyszczania się środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12301">
<skos:prefLabel>zarządzanie zasobami wodnymi</skos:prefLabel>
<skos:definition>sposoby i działania związane z dostawą wody, jej efektywnym wykorzystaniem, ograniczeniem strat wody oraz kosztów jej uzyskania dla zapewnienia stałego i trwałego zaopatrzenia w wodę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12318">
<skos:prefLabel>punkt sortowania odpadów</skos:prefLabel>
<skos:definition>punkt, do którego odpady dostarczane są w celu przeprowadzenia ich segregacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12321">
<skos:prefLabel>składowisko złomu</skos:prefLabel>
<skos:definition>miejsce wyznaczone dla składowania metali z myślą o ich powtórnym wykorzystaniu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12325">
<skos:prefLabel>przemysł surowców wtórnych</skos:prefLabel>
<skos:definition>przemysł przetwarzania odpadów stałych w produkty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12341">
<skos:prefLabel>stosowanie odpadów jako źródła energii</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12342">
<skos:prefLabel>wykorzystywanie odpadów</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12349">
<skos:prefLabel>oczyszczanie plaży</skos:prefLabel>
<skos:definition>proces usuwania odpadów i/lub zanieczyszczeń z brzegu zbiornika wodnego oraz terenów sąsiadujących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12357">
<skos:prefLabel>odpady wielkogabarytowe</skos:prefLabel>
<skos:definition>odpady o znacznych gabarytach, takie jak sprzęt gospodarstwa domowego, meble, części pojazdów, pnie lub konary drzew itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12366">
<skos:prefLabel>bateria alkaliczna</skos:prefLabel>
<skos:definition>pierwotne źródło prądu z elektrolitem alkalicznym, najczęściej wodorotlenkiem potasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12367">
<skos:prefLabel>bateria elektryczna</skos:prefLabel>
<skos:definition>źródło prądu złożone z jednego lub więcej urządzeń przetwarzających energię chemiczną, termiczną, jądrową lub słoneczną w energię elektryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12377">
<skos:prefLabel>odpady z procesów ograniczania zanieczyszczeń</skos:prefLabel>
<skos:definition>odpady powstające w procesach ograniczania emisji zanieczyszczeń z procesów przemysłowych i procesach oczyszczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12379">
<skos:prefLabel>odpady medyczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12381">
<skos:prefLabel>odpady produkcyjne</skos:prefLabel>
<skos:definition>produkt uboczny w procesie przemysłowym, dla którego nie wskazano sposobu wykorzystania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12382">
<skos:prefLabel>odpady metalowe</skos:prefLabel>
<skos:definition>materiał metalowy, powstający w trakcie produkcji lub obróbki, który nie może być wykorzystany w produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12383">
<skos:prefLabel>odpady mieralne</skos:prefLabel>
<skos:definition>odpady powstające w procesie wydobywania kopalin, najczęściej zwałowane na powierzchni ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12390">
<skos:prefLabel>wrak</skos:prefLabel>
<skos:definition>porzucony lub rozbity statek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12394">
<skos:prefLabel>olej mineralny</skos:prefLabel>
<skos:definition>płynny produkt destylacji ropy naftowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12395">
<skos:prefLabel>serwatka</skos:prefLabel>
<skos:definition>mętny płyn wydzielający się ze skrzepu mleka przy jego przeróbce na ser lub kazeinę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12417">
<skos:prefLabel>zatapianie odpadów</skos:prefLabel>
<skos:definition>pozbywanie się odpadów poprzez zatapianie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12429">
<skos:prefLabel>odpady szlifierskie</skos:prefLabel>
<skos:definition>pył lub inne drobne pozostałości powstające w procesie szlifowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12430">
<skos:prefLabel>odpady powstajace w procesie oczyszczania</skos:prefLabel>
<skos:definition>odpady powstające w procesie obróbki, oczyszczania lub unieszkodliwiania odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12431">
<skos:prefLabel>odpady powstające w procesie spalania</skos:prefLabel>
<skos:definition>stałe lub półpłynne odpady powstające w procesie termicznego unieszkodliwiania odpadów przez spalanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12435">
<skos:prefLabel>osady z oczyszczania</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12436">
<skos:prefLabel>osad odwodniony</skos:prefLabel>
<skos:definition>osad, z którego metodami fizycznymi usunięto nadmiar wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12441">
<skos:prefLabel>osad wtórny</skos:prefLabel>
<skos:definition>osady powstające w procesie oczyszczania ścieków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12444">
<skos:prefLabel>zagęszczanie</skos:prefLabel>
<skos:definition>proces inny niż sedymentacja powodujący zagęszczenie cząstek stałych w cieczy, z/lub bez wykorzystania środków chemicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12446">
<skos:prefLabel>stabilizacja osadu</skos:prefLabel>
<skos:definition>proces, najczęściej beztlenowy, usuwania z osadów związków łatwo podlegających biodegradacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12450">
<skos:prefLabel>działalnośc przemysłowa</skos:prefLabel>
<skos:definition>działania i procesy związane z produkcją na skalę przemysłową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12456">
<skos:prefLabel>tereny poprzemysłowe</skos:prefLabel>
<skos:definition>tereny, które były w przeszłości wykorzystywane przemysłowo, a obecnie nie nadają się do wykorzystania rolniczego lub innego niż przemysłowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12459">
<skos:prefLabel>instalacja o znaczeniu strategicznym</skos:prefLabel>
<skos:definition>instalacje objęte ograniczeniem w udzielaniu publicznych informacji z uwagi na interesy obrony narodowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12471">
<skos:prefLabel>ćwiartowanie</skos:prefLabel>
<skos:definition>rozcinanie lub rąbanie na ćwierci lub mniejsze kawałki tusz zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12476">
<skos:prefLabel>chlewnia</skos:prefLabel>
<skos:definition>miejsce przeznaczone na pobyt świń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12482">
<skos:prefLabel>produkcja ropy naftowej</skos:prefLabel>
<skos:definition>przetwarzanie ropy naftowej to ciąg złożonych czynności, w tym wielu niezależnych lub wzajemnie zależnych operacji chemicznych, transportu i magazynowania oraz dystrybucji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12485">
<skos:prefLabel>zbiornik magazynowy produktów ropopochodnych</skos:prefLabel>
<skos:definition>zbiornik służący do magazynowania produktów ropopochodnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12488">
<skos:prefLabel>wiercenia poszukiwawcze ropy naftowej</skos:prefLabel>
<skos:definition>wiercenie otowrów w związku z poszukiwaniem ropy naftowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12511">
<skos:prefLabel>wydobycie kruszywa</skos:prefLabel>
<skos:definition>wydobycie materiałów mineralnych stosowanych do budowy dróg, budynków lub konstrukcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12514">
<skos:prefLabel>przemysł mechaniczny</skos:prefLabel>
<skos:definition>dział gospodarki, w ramach którego są  projektowane, produkowane i rozprowadzane urządzenia mechaniczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12525">
<skos:prefLabel>produkcja drewna</skos:prefLabel>
<skos:definition>proces pozyskiwania i obróbki drewna aż po produkcję wyrobów drewnianych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12528">
<skos:prefLabel>przemysł opakowań</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12576">
<skos:prefLabel>oczyszczanie ścieków na złożach biologicznych</skos:prefLabel>
<skos:definition>oczyszczanie (ścieków) z wykorzystaniem złóż biologicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12583">
<skos:prefLabel>odchlorowanie</skos:prefLabel>
<skos:definition>usuwanie chloru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12586">
<skos:prefLabel>inżynieria</skos:prefLabel>
<skos:definition>sztuka wykonywania robót inżynierskich, umiejętność projektowania i wznoszenia różnych rodzajów budowli oraz konstruowania maszyn i urządzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12587">
<skos:prefLabel>metanizacja</skos:prefLabel>
<skos:definition>proces pozyskiwania metanu z dowolnego źródła, w tym z odchodów zwierzęcych, wysypisk, kopalni węgla kamiennego, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12599">
<skos:prefLabel>oczyszczanie tlenowe</skos:prefLabel>
<skos:definition>wprowadzanie tlenu do ścieków w celu zapewnienia warunków tlenowych w procesie oczyszczania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12600">
<skos:prefLabel>oczyszczanie w procesie beztelnowym</skos:prefLabel>
<skos:definition>rozkład substancji organicznych bez dostępu tlenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12601">
<skos:prefLabel>przetwarzanie biologiczne</skos:prefLabel>
<skos:definition>wykorzystanie w procesach oczyszczania zdolności mikroorganizmów do rozkładu substancji organicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12605">
<skos:prefLabel>przetwarzanie fizykochemiczne</skos:prefLabel>
<skos:definition>każde przetwarzanie ścieków oraz odpadów ciekłych lub stałych, w tym substancji toksycznych wykorzystujące łącznie metody fizyczne i chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12606">
<skos:prefLabel>fizyczne metody oczyszczania</skos:prefLabel>
<skos:definition>proces rozdzielania składników strumienia odpadów lub zmiany ich stanu skupienia bez zmiany ich struktury chemicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12607">
<skos:prefLabel>oczyszczanie wstępne</skos:prefLabel>
<skos:definition>usuwanie ze ścieków cząsteczek zawieszonych oraz unoszących się na powierzchni</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12608">
<skos:prefLabel>drugi stopień oczyszczania</skos:prefLabel>
<skos:definition>etap procesu oczyszczania ścieków, następujący po procesie usuwania zawiesin, najczęściej wykorzystujący organizmy biologiczne do rozkładu substancji organicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12609">
<skos:prefLabel>trzeci stopień oczyszczania</skos:prefLabel>
<skos:definition>proces usuwania substancji nie usuniętych we wcześniejszych etapach oczyszczania ścieków, najczęściej azotu i fosforu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12611">
<skos:prefLabel>witryfikacja</skos:prefLabel>
<skos:definition>proces powstawania ciała szklistego, bezpostaciowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12656">
<skos:prefLabel>podziemny kamieniołom</skos:prefLabel>
<skos:definition>kamieniołom położony pod powierzchnią ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12665">
<skos:prefLabel>geotechnika</skos:prefLabel>
<skos:definition>stosowanie metod matematycznych i zasad sztuki inżynierskiej do zagadnień inżynierii lądowej poprzez zdobywanie, interpretowanie i wykorzystywanie informacji o materiałach tworzących skorupę ziemską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12666">
<skos:prefLabel>mechanika skał</skos:prefLabel>
<skos:definition>dział mechaniki, nauka teoretyczna i stosowana o fizycznych właściwościach skał</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12676">
<skos:prefLabel>wycinanie buszu</skos:prefLabel>
<skos:definition>trzebienie buszu, mechaniczne - ręczne lub maszynowe - lub chemiczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12677">
<skos:prefLabel>zapobieganie pożarom lasu</skos:prefLabel>
<skos:definition>działania, metody i instalacje mające na celu zapobiegnie powstawaniu i rozprzestrzenianiu się pożarów na obszarach leśnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12687">
<skos:prefLabel>ochrona przeciwpowodziowa</skos:prefLabel>
<skos:definition>działania, metody i urządzenia mające na celu ochronę ludzi, dobytku i nieruchomości przed zalaniem wodą terenów zwykle suchych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12711">
<skos:prefLabel>ochrona gruntów w terenach górskich</skos:prefLabel>
<skos:definition>działania stosowane w celu ograniczenia procesów erozji i innego rodzaju degradacji na terenach górskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12717">
<skos:prefLabel>stabilnośc gruntu</skos:prefLabel>
<skos:definition>stabilnośc guntu zależy od jego właściwości, w tym zdolności do absorbcji wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12721">
<skos:prefLabel>wulkanologia</skos:prefLabel>
<skos:definition>dział geologii, nauka zajmująca się przyczynami powstawania wulkanów, badająca ich budowę, kształt i rozmieszczenie na kuli ziemskiej oraz skład materiałów pochodzenia wulkanicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12746">
<skos:prefLabel>przepisy antysejsmiczne</skos:prefLabel>
<skos:definition>przepisy minimalizujące lub ograniczające ryzyko powstające na skutek wstrząsów sejsmicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12763">
<skos:prefLabel>bezpieczeństwo publiczne</skos:prefLabel>
<skos:definition>działania podejmowane, najczęściej na szczeblu lokalnym, w celu zapewnienia bezpieczeństwa mieszkańcom i społeczności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12764">
<skos:prefLabel>bezpieczeństwo przemysłowe</skos:prefLabel>
<skos:definition>działania i techniki wdrażane w celu ograniczenia ryzyka dla zdrowia, środowiska lub własności wynikającego z działalności przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12778">
<skos:prefLabel>narażenie</skos:prefLabel>
<skos:definition>sytuacja lub zbieg okoliczności powodujący wzrost prawdopodobieństwa wystąpienia szkód</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12783">
<skos:prefLabel>obszar występowania zagrożenia</skos:prefLabel>
<skos:definition>każdy obszar lub rejon, w którym znajduje się substancja stwarzająca fizyczne lub chemiczne zagrożenia dla ludzi, zwierząt, roślin lub środowiska jako całości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12784">
<skos:prefLabel>awaria</skos:prefLabel>
<skos:definition>niespodziewany incydent, mogący skutkować narażeniem ludzi, środowiska lub nieruchomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12790">
<skos:prefLabel>katastrofa ekologiczna</skos:prefLabel>
<skos:definition>nagła klęska o znacznym zasięgu powodująca znaczące szkody w środowisku, która zagraża jakości życia lokalnej społeczności i może powodować zejścia śmiertelne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12793">
<skos:prefLabel>zagrożenie jądrowe</skos:prefLabel>
<skos:definition>ryzyko lub zagrożenie dla zdrowia ludzi i środowiska powstałe na skutek emisji promieniowania jądrowego lub niekontrolowanej reakcji jądrowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12807">
<skos:prefLabel>instalacje zwiększonego ryzyka</skos:prefLabel>
<skos:definition>instalacje, których funkcjonowanie wiąże się z możliwością powstania awarii przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12808">
<skos:prefLabel>instalacje niebezpieczne</skos:prefLabel>
<skos:definition>instalacje, których funkcjonowanie wiąże się z możliwością powstania awarii przemysłowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12814">
<skos:prefLabel>zagrożenie biotechnologii</skos:prefLabel>
<skos:definition>zagrożenie dla ludzi, zwierząt lub środowiska spowodowane stosowaniem na skalę przemysłową nowoczesnych metod biotechnologicznych, np. narażenie na kontakt z grzybami, bakteriami itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12816">
<skos:prefLabel>ryzyko jądrowe</skos:prefLabel>
<skos:definition>ryzyko związane z funkcjonowaniem elektrowni jądrowych, transportem odpadów promieniotwórczych lub uwolnieniem do środowiska niebezpiecznych ilości materiałów radioaktywnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12819">
<skos:prefLabel>transport materiałów niebezpiecznych</skos:prefLabel>
<skos:definition>rodzaj transportu regulowany szczególnymi przepisami bezpieczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12849">
<skos:prefLabel>obszar klęski</skos:prefLabel>
<skos:definition>obszar dotknięty klęską, gdzie należy podjąć działania dla ochrony ludzi i zmniejszenia rozmiarów strat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12861">
<skos:prefLabel>analiza ryzyka</skos:prefLabel>
<skos:definition>proces oceny wielkości i prawdopodobieństwa szkód dla ludzi, środowiska i nieruchomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12866">
<skos:prefLabel>informacja prewencyjna</skos:prefLabel>
<skos:definition>sposób przekazywania lub otrzymywania danych dotyczących zalecanych sposobów unikania ryzyka wystąpienia awarii, klęski lub innych niepożądanych a możliwych do uniknięcia zdarzeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12872">
<skos:prefLabel>plan ratowniczy</skos:prefLabel>
<skos:definition>sposób postępowania lub metody działania, które należy podjąć, jeśli zagrożenie dla ludzi lub  środowiska w danym rejonie przekroczy dopuszczalne granice</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12887">
<skos:prefLabel>ubezpieczenie od szkód</skos:prefLabel>
<skos:definition>produkt rynku ubezpieczeń gwarantujący pokrycie całości lub części szkód w zamian za wnoszenie składek</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12889">
<skos:prefLabel>ubezpieczenie od zanieczyszczeń</skos:prefLabel>
<skos:definition>umowa handlowa, która zapewnia ochronę przed ryzykiem lub określonym rodzajem ryzyka związanym z zanieczyszczeniem, unieszkodliwianiem odpadów niebezpiecznych lub podobnymi zagadnieniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12893">
<skos:prefLabel>szkody spowodowane przez wodę</skos:prefLabel>
<skos:definition>szkody spowodowane przez powódź, ulewne deszcze, fale pływowe, fale wywołane wstrząsami sejsmicznymi itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12898">
<skos:prefLabel>ocena szkód</skos:prefLabel>
<skos:definition>ocena lub określenie strat lub szkód dla ludzi, środowiska i nieruchomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12900">
<skos:prefLabel>ocena</skos:prefLabel>
<skos:definition>formalne lub eksperckie oszacowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12905">
<skos:prefLabel>odszkodowanie</skos:prefLabel>
<skos:definition>najczęściej finansowa równowartość poniesionych szkód lub strat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12912">
<skos:prefLabel>instytucja użyteczności publicznej</skos:prefLabel>
<skos:definition>instytucja służąca celom użyteczności publicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12913">
<skos:prefLabel>administracja publiczna</skos:prefLabel>
<skos:definition>instytucja publiczna wykonująca działania o charakterze administracyjnym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12914">
<skos:prefLabel>publiczna instytucja przemysłowo-handlowa</skos:prefLabel>
<skos:definition>instytucja publiczna zajmująca się zarządzaniem w dziedzinie przemysłu i handlu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12923">
<skos:prefLabel>konsultant</skos:prefLabel>
<skos:definition>stanowisko lub funkcja osoby o odpowiednich kwalifikacjach, opłacanej za świadczone usługi i udzielane rady</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12977">
<skos:prefLabel>patent</skos:prefLabel>
<skos:definition>akt stwierdzający własność wynalazku i wyłączne prawo jego stosowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12982">
<skos:prefLabel>biuletyn informacyjny</skos:prefLabel>
<skos:definition>oficjalne ogłoszenie lub oświadczenie przekazywane przedstawicielom mediów przez rzecznika prasowego agendy rządowej lub innej organizacji, często zamiast lub jako uzupełnienie oświadczenia ustnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12985">
<skos:prefLabel>wystąpienie</skos:prefLabel>
<skos:definition>forma przemówienia, przekazanie przez mówcę swoich poglądów słuchaczom</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/12994">
<skos:prefLabel>analiza ekologiczna</skos:prefLabel>
<skos:definition>dokument przedstawiany wraz z opisem zamierzenia, obejmujący wpływ zamierzenia na środowisko oraz jego warianty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13000">
<skos:prefLabel>film kinowy</skos:prefLabel>
<skos:definition>utwór filmowy opisujący dramat, epizod lub zdarzenie, uznany za dziedzinę sztuki i oglądany dla rozrywki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13001">
<skos:prefLabel>film dokumentalny</skos:prefLabel>
<skos:definition>utwór filmowy opisujący autentyczne zdarzenie, epokę lub miejsce, wierny rzeczywistości lub z możliwie małym udziałem fikcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13002">
<skos:prefLabel>flora (dokumnetacja)</skos:prefLabel>
<skos:definition>praca w sposób systematyczny opisująca florę danego regionu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13017">
<skos:prefLabel>raport ministerialny</skos:prefLabel>
<skos:definition>pisemne sprawozdanie lub oświadczenie zawierające szczegółowy opis lub wyniki analizy zdarzenia lub sytuacji, przedstawiany osobie zajmującej z nominacji lub z wyboru wysokie stanowisko polityczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13021">
<skos:prefLabel>raport parlmentarny</skos:prefLabel>
<skos:definition>pisemne sprawozdanie zawierające szczegółowy opis lub wyniki analizy zdarzenia lub sytuacji, przedstawiane oficjalnego gremium ustawodawczemu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13025">
<skos:prefLabel>statut</skos:prefLabel>
<skos:definition>dokument lub jego część, stanowiący część systemu prawnego, o precyzyjnym słownictwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13026">
<skos:prefLabel>praca naukowa</skos:prefLabel>
<skos:definition>rozprawa na konkretny temat, omawiająca przeprowadzone badania, udowadniająca tezy i wysuwająca wnioski, często przedkładana dla uzyskania tytułu naukowego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13040">
<skos:prefLabel>nośnik CD-ROM</skos:prefLabel>
<skos:definition>płyta, na ktorej można przechowywać znaczne ilości danych w postaci elektronicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13041">
<skos:prefLabel>ośrodek informacji</skos:prefLabel>
<skos:definition>placówka zajmująca się zbieraniem i udzielaniem informacji w określonej dziedzinie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13053">
<skos:prefLabel>informacja techniczna</skos:prefLabel>
<skos:definition>dział nauki zajmujący się opracowywaniem i rozpowszechnianiem danych naukowych dotyczących aktualnego stanu wiedzy wchodzącego w zakres techniki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13060">
<skos:prefLabel>sieć informacyjna</skos:prefLabel>
<skos:definition>system utworzony przez zbiory informacyjne (bazy danych lub systemy informacyjno-wyszukiwawcze), pomiędzy którymi występują określone relacje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13077">
<skos:prefLabel>oznaczanie</skos:prefLabel>
<skos:definition>jakościowe lub ilościowe oznaczenie składników materiału</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13088">
<skos:prefLabel>metoda</skos:prefLabel>
<skos:definition>sposób postępowania, świadomy i powtarzalny wybór działania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13098">
<skos:prefLabel>hutnictwo żelaza i stali</skos:prefLabel>
<skos:definition>przemysł obejmujący procesy wytapiania i odlewania żelaza i stali oraz stopów żelaza</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13100">
<skos:prefLabel>siedlisko miejskie</skos:prefLabel>
<skos:definition>oddziaływania i zależności pomiędzy skupiskami ludzkimi, zabudowaniami i środowiskiem biofizycznym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13101">
<skos:prefLabel>dzikie zwierzęta</skos:prefLabel>
<skos:definition>nieudomowione zwierzęta, niezależne od hodowli prowadzonej przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13102">
<skos:prefLabel>gospodarka przestrzenna</skos:prefLabel>
<skos:definition>działania związane z przygotowywaniem i kontrolowaniem wdrażania planów wykorzystania terenu  w działalności ludzkiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13104">
<skos:prefLabel>siedlisko wiejskie</skos:prefLabel>
<skos:definition>biotop zlokalizowany w terenach rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13106">
<skos:prefLabel>jakość wód podziemnych</skos:prefLabel>
<skos:definition>wody podziemne narażone są na zanieczyszczenie i nadmierną ekploatację, a stanowią zarazem dominujące źródło wody do picia; ochrona jakości wód podziemnych wymaga koordynacji działań z zakresu gospodarki wodnej i gospodarki przestrzennej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13107">
<skos:prefLabel>określanie celów</skos:prefLabel>
<skos:definition>wyznaczanie lub wypracowywanie środowiskowych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13111">
<skos:prefLabel>ontogeneza</skos:prefLabel>
<skos:definition>rozwój organizmu, obejmujący wszystkie zmiany zachodzące w nim od chwili powstania zaczątka nowego osobnika aż do naturalnej śmierci</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13115">
<skos:prefLabel>podatek gruntowy</skos:prefLabel>
<skos:definition>podatek od nieruchomości, zależny od wartości terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13117">
<skos:prefLabel>trawniki</skos:prefLabel>
<skos:definition>obszar starannie utrzymanej trawy, szczególnie w pobliżu domów lub w parku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13118">
<skos:prefLabel>cykl biologiczny</skos:prefLabel>
<skos:definition>zespół przeobrażeń lub zdarzeń biologicznych, następujących po sobie i kolejno się powtarzających np. cykl życiowy organizmów roślinnych i zwierzęcych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13120">
<skos:prefLabel>środowisko zycia człowieka</skos:prefLabel>
<skos:definition>otoczenie, w którym mieszkają ludzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13121">
<skos:prefLabel>urbanizacja</skos:prefLabel>
<skos:definition>powiększanie się obszarów miejskich i udziału ludności miejskiej w całości zaludnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13122">
<skos:prefLabel>ogródek przydomowy</skos:prefLabel>
<skos:definition>niewielki obszar ziemi uprawnej przylegający do domu i najczęściej przeznaczony w całości lub w części do uprawy owoców, warzyw, kwiatów i ziół na potrzeby domowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13123">
<skos:prefLabel>fotografia</skos:prefLabel>
<skos:definition>proces otrzymywania trwałych obrazów na materiale światłoczułym w skutek bezpośredniego lub pośredniego działania światła lub innego rodzaju promieniowania na innego rodzaju błony</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13124">
<skos:prefLabel>kapsel</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13127">
<skos:prefLabel>złom</skos:prefLabel>
<skos:definition>odpady produkcyjne i zużyte przedmioty metalowe nadające się do przetopienia w piecach hutniczych lub innych procesach metalurgicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13135">
<skos:prefLabel>głosowanie</skos:prefLabel>
<skos:definition>sformalizowany sposób wyrażania zdania lub dokonywania wyboru</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13136">
<skos:prefLabel>sprzęt rolniczy</skos:prefLabel>
<skos:definition>budynki, maszyny i urządzenia związane z produkcja rolną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13137">
<skos:prefLabel>polityka budżetowa</skos:prefLabel>
<skos:definition>programowy dokument rządowy dotyczący dochodów i wydatków państwa, wpływający na działalność gospodarczą i ukierunkowany na osiągnięcie określonych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13142">
<skos:prefLabel>polityka współpracy gospodarczej;</skos:prefLabel>
<skos:definition>działania polityczne dążące do ustanowienia międzynarodowych porozumień handlowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13143">
<skos:prefLabel>równowaga międzynarodowa</skos:prefLabel>
<skos:definition>system, w ramach którego kraj lub kilka krajów stara się utrzymać równowagę sił aby zapobiec dominacji pojedynczego kraju lub zapobiec konfliktowi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13145">
<skos:prefLabel>finanse Wpólnoty Europejskiej</skos:prefLabel>
<skos:definition>zasoby finansowe Wspólnoty Europejskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13146">
<skos:prefLabel>system prawny</skos:prefLabel>
<skos:definition>sposób, forma i instytucje umożliwiające tworzenie, wdrażanie i interpretowanie prawa oraz ich wzajemne koordynowanie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13147">
<skos:prefLabel>prawa</skos:prefLabel>
<skos:definition>1) Trwała bądź tymczasowa możliwość korzystania lub użytkowania np. nieruchomości; 2) Interes chroniony aktem ustawodawczym</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13148">
<skos:prefLabel>struktura gospodarki</skos:prefLabel>
<skos:definition>złożony system, obejmujący sieć transportową , komunikacyjną, instalacje przemysłowe oraz edukację i technologie, który  pozwala na produkowanie dóbr, usług i innych zasobów o określonej wartości wymiennej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13149">
<skos:prefLabel>dystrybucja</skos:prefLabel>
<skos:definition>dostarczanie dóbr materialnych konsumentom w sieciach sprzedaży hurtowej i detalicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13150">
<skos:prefLabel>stosunki walutowe</skos:prefLabel>
<skos:definition>rozmaite powiązania finansowe pomiędzy krajami i narodami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13151">
<skos:prefLabel>gospodarka walutowa</skos:prefLabel>
<skos:definition>polityka, instytucje, procedury i sposoby dostarczania na rynek monet i banknotów, depozytów bankowych i innych walorów wymienialnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13152">
<skos:prefLabel>wolny przepływ kapitału</skos:prefLabel>
<skos:definition>nieograniczony przepływ pieniędzy, funduszy i innych walorów pomiędzy krajami stosującymi różne waluty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13154">
<skos:prefLabel>struktura społeczna</skos:prefLabel>
<skos:definition>wewnętrzna struktura, która łączy i wspiera członków i części wspólnoty lub organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13156">
<skos:prefLabel>osłona socjalna</skos:prefLabel>
<skos:definition>środki i programy, wdrażane przez jednostki administracji oraz organizacje pozarządowe, mające na celu zapewnienie minimum dobrobytu członkom społeczeństwa będącym w niekorzystnej sytuacji ze względu na podeszły wiek, bezrobocie, stan zdrowia, utratę żywiciela rodziny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13157">
<skos:prefLabel>zrzeszenie nauczycieli</skos:prefLabel>
<skos:definition>grupa lub organizacja utworzona, aby zając się zagadnieniami związanymi z metodami nauczania i statusem społecznym nauczycieli i instruktorów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13159">
<skos:prefLabel>firma</skos:prefLabel>
<skos:definition>kontekst prawny własności firmy, prowadzenia interesów, planowania w celu osiągnięcia celów funkcjonowania firmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13160">
<skos:prefLabel>klasyfikacja działalnosci gospodarczej</skos:prefLabel>
<skos:definition>podział firm i organizacji prowadzących działalność gospodarczą na kategorie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13161">
<skos:prefLabel>status prawny przedsiebiorstwa</skos:prefLabel>
<skos:definition>rodzaj, struktura i cel istnienia jednostki gospodarczej lub instytucji, zgodnie z klasyfikacją wynikającą z przepisów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13164">
<skos:prefLabel>uprawa ziemi</skos:prefLabel>
<skos:definition>całokształt zabiegów zmierzających do uzyskania plonów rolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13165">
<skos:prefLabel>środki produkcji rolnej</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13167">
<skos:prefLabel>przetworzony produkt rolny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13168">
<skos:prefLabel>przemysł rolno-spożywczy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13170">
<skos:prefLabel>geografia gospodarcza</skos:prefLabel>
<skos:definition>dział geografii zajmujący się rozkładem przestrzennym środków produkcji, konsumpcji oraz usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13177">
<skos:prefLabel>kredyt</skos:prefLabel>
<skos:definition>odstąpienie określonej wartości w pieniądzu lub towarach pod warunkiem zwrotu równowartości (wraz z wynagrodzeniem za udzielenie) w określonym terminie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13178">
<skos:prefLabel>wolność</skos:prefLabel>
<skos:definition>niezależność, niezawisłość w odniesieniu do praw politycznych i obywatelskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13179">
<skos:prefLabel>struktura przemysłu</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13182">
<skos:prefLabel>zaopatrzenie</skos:prefLabel>
<skos:definition>produkty magazynowane, gotowe do sprzedaży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13183">
<skos:prefLabel>sterylizacja</skos:prefLabel>
<skos:definition>pozbawienie płodności (zdolności do reprodukcji)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13184">
<skos:prefLabel>koszty środowiskowe funkcjonowania firm</skos:prefLabel>
<skos:definition>stosowanie narzędzi finansowych dla uwzględnienia kwestii ekologicznych w funkcjonowaniu przedsiębiorstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13186">
<skos:prefLabel>plon</skos:prefLabel>
<skos:definition>zbiór części użytkowych roślin uprawnych; dostępna biomasa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13187">
<skos:prefLabel>rozwiązywanie problemów ochrony środowska</skos:prefLabel>
<skos:definition>znajdywanie rozwiązań dla kłopotliwych zagadnień związanych z ochroną środowiska i zarządzaniem zasobami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13188">
<skos:prefLabel>rośliny pastewne</skos:prefLabel>
<skos:definition>rośliny używane do karmienia zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13189">
<skos:prefLabel>rośliny przemysłowe</skos:prefLabel>
<skos:definition>rośliny wykorzystywane w przemyśle, np.bawełna, len, konopie, orzeszki ziemne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13190">
<skos:prefLabel>rośliny wykorzystywane w przemyśle włókienniczym</skos:prefLabel>
<skos:definition>rośliny, z których mogą być produkowane materiały tekstylne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13191">
<skos:prefLabel>rośliny tropikalne</skos:prefLabel>
<skos:definition>rośliny regionów tropikalnych, charakteryzujących się wysokimi temperaturami i dużymi opadami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13192">
<skos:prefLabel>nieruchomość rolna</skos:prefLabel>
<skos:definition>teren przeznaczony pod uprawę rolną wraz z elementami zabudowy trwałej, np. budynkami mieszkalnymi i gospodarczymi znajdującymi się w tym obiekcie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13193">
<skos:prefLabel>gospodarstwo rolne</skos:prefLabel>
<skos:definition>podstawowa jednostka zajmująca się produkcja rolną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13194">
<skos:prefLabel>rodzaj własności</skos:prefLabel>
<skos:definition>rodzaj praw do terenu i egzekwowania tytułu własności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13195">
<skos:prefLabel>geosfera</skos:prefLabel>
<skos:definition>powierzchnia ziemi, oceany, morza i wody śródlądowe oraz atmosfera ziemska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13196">
<skos:prefLabel>produkty petrochemiczne</skos:prefLabel>
<skos:definition>produkty chemiczne wytwarzane w rafineriach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13197">
<skos:prefLabel>parachemikalia</skos:prefLabel>
<skos:definition>wiele rozmaitych  produktów chemicznych, takich jak kleje, żywice, guma, plastik, wybiórcze herbicydy itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13198">
<skos:prefLabel>produkty białkowe</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13199">
<skos:prefLabel>żywność przetworzona</skos:prefLabel>
<skos:definition>żywność poddana obróbce dla zmiany wyglądu lub w celu przedłużenia trwałości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13200">
<skos:prefLabel>produkt gotowy do spożycia</skos:prefLabel>
<skos:definition>produkt żywnościowy przygotowany w ten sposób, aby łatwo i szybko był gotowy do konsumpcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13202">
<skos:prefLabel>urobek górniczy</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13205">
<skos:prefLabel>rośliny okopowe</skos:prefLabel>
<skos:definition>rośliny, których bulwy lub korzenie są jadalne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13207">
<skos:prefLabel>uprawa roli</skos:prefLabel>
<skos:definition>procesy i metody związane z przygotowaniem gleby do uprawy lub polepszeniem warunków wzrostu roślin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13209">
<skos:prefLabel>zbiory zbóż</skos:prefLabel>
<skos:definition>zespół czynności związanych z pozyskiwaniem części roślin stanowiących cel uprawy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13210">
<skos:prefLabel>sprzęt rybacki</skos:prefLabel>
<skos:definition>dotyczy każdego rodzaju sprzętu (statków przetwórni, kutrów, łodzi rybackich, sieci, itp.) stosowanego w rybołówstwie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13211">
<skos:prefLabel>łowisko</skos:prefLabel>
<skos:definition>miejsce połowu ryb</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13212">
<skos:prefLabel>przemysł węglowy</skos:prefLabel>
<skos:definition>przemysł związany z wydobyciem węgla spod ziemi i przygotowaniem go do sprzedaży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13213">
<skos:prefLabel>przemysł energetyczny</skos:prefLabel>
<skos:definition>przemysł związany z przetwarzaniem różnego rodzaju paliw lub innych form energii w energię wykorzystywaną w gospodarstwach domowych, przemyśle, transporcie, itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13214">
<skos:prefLabel>przemysł komunikacyjny</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13215">
<skos:prefLabel>przemysł informatyczny</skos:prefLabel>
<skos:definition>dział gospodarki zajmujący się projektowaniem, konstruowaniem i dystrybucją maszyn elektronicznych służących do magazynowania i obróbki danych oraz związanych z tym technologii i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13216">
<skos:prefLabel>przemysł próżniowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13217">
<skos:prefLabel>przygotowanie towaru do sprzedaży</skos:prefLabel>
<skos:definition>pakowanie, magazynowanie, transport, ekspozycja i prezentacja towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13218">
<skos:prefLabel>mechanika precyzyjna</skos:prefLabel>
<skos:definition>dział nauki i techniki zajmujący się budową drobnych konstrukcji, mechanizmów i przyrządów precyzyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13219">
<skos:prefLabel>inżynieria materiałowa</skos:prefLabel>
<skos:definition>technologia i urządzenia stosowane do obróbki i przetwarzania materiałów (np. szkła, metalu, plastiku i ceramiki, w tym zmiany niektórych ich właściwości)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13220">
<skos:prefLabel>sprzęt wojskowy</skos:prefLabel>
<skos:definition>sprzęt wykorzystywany do działań wojskowych, nie koniecznie do walk</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13221">
<skos:prefLabel>sprzęt audiowizualny</skos:prefLabel>
<skos:definition>urządzenia wykorzystywane jako pomoce naukowe, oddziaływujące na zmysły wzroku i słuchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13222">
<skos:prefLabel>maszyna</skos:prefLabel>
<skos:definition>zespół urządzeń lub urządzenie zawierające mechanizm lub zespół mechanizmów służące do przetwarzania energii albo do wykonywania określonej pracy mechanicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13223">
<skos:prefLabel>narzędzia i maszyny</skos:prefLabel>
<skos:definition>urządzenia umożliwiające wykonywanie jakiejś czynności lub pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13224">
<skos:prefLabel>instalacja ciśnieniowa</skos:prefLabel>
<skos:definition>zespół urządzeń technicznych (przewodów i sprzętu) działających pod ciśnieniem wyższym niż atmosferyczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13225">
<skos:prefLabel>instalacja cieplna</skos:prefLabel>
<skos:definition>urządzenia związane z produkcją i dystrybucją ciepła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13226">
<skos:prefLabel>produkcja przemysłowa</skos:prefLabel>
<skos:definition>tworzenie lub przetwarzanie produktów na skalę przemysłową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13227">
<skos:prefLabel>wielkość przedsiębiorstwa</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13228">
<skos:prefLabel>działalność gospodarcza</skos:prefLabel>
<skos:definition>zespół czynności i działań podejmowanych w celu wytworzenia zysku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13229">
<skos:prefLabel>branża</skos:prefLabel>
<skos:definition>gałąź produkcji lub handlu obejmująca towary lub usługi jednego rodzaju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13231">
<skos:prefLabel>pracownik administracji</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13232">
<skos:prefLabel>usługi budowlane</skos:prefLabel>
<skos:definition>działalność i usługi związane z budynkami mieszkalnymi i biurowymi: budowa, administrowanie i wynajem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13233">
<skos:prefLabel>środek transportu</skos:prefLabel>
<skos:definition>pojazdy używane do przemieszczania osób lub towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13234">
<skos:prefLabel>miejsce przeznaczenia</skos:prefLabel>
<skos:definition>docelowe miejsce podróży</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13236">
<skos:prefLabel>pogarszanie stanu środowiska</skos:prefLabel>
<skos:definition>stopniowe zanieczyszczanie, nadmierna eksploatacja oraz niszczenie środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13237">
<skos:prefLabel>budżet Wspólnoty Europejskiej</skos:prefLabel>
<skos:definition>zestawienie przychodów i wydatków Wspólnoty Europejskiej w określonej jednostce czasu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13238">
<skos:prefLabel>pomoc gospodarcza</skos:prefLabel>
<skos:definition>wsparcie finansowe lub bodźce ekonomiczne dla osób lub instytucji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13239">
<skos:prefLabel>księgowość</skos:prefLabel>
<skos:definition>system ustanowienia, prowadzenia i sprawdzania ksiąg, w które wpisuje się wpływy, wydatki, transakcje i inne zmiany stanu majątkowego przedsiębiorstwa lub instytucji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13240">
<skos:prefLabel>prognozy gospodarcze</skos:prefLabel>
<skos:definition>przewidywanie przyszłych trendów finansowych i gospodarczych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13241">
<skos:prefLabel>taryfa celna</skos:prefLabel>
<skos:definition>urzędowo ustalone i ogłoszone zestawienie stawek opłat za import lub eksport określonych towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13242">
<skos:prefLabel>transakcja handlowa</skos:prefLabel>
<skos:definition>operacja lub umowa handlowa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13244">
<skos:prefLabel>polityka płacowa</skos:prefLabel>
<skos:definition>polityka dotycząca równowartości pieniężnej przekazywanej za wykonywaną pracę lub świadczone usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13245">
<skos:prefLabel>Europejski System Walutowy</skos:prefLabel>
<skos:definition>organizacja utworzona w 1979 r. dla koordynowania polityki finansowej i kursów wymiany walut w Europie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13246">
<skos:prefLabel>rynek walutowy</skos:prefLabel>
<skos:definition>rynek, na którym  przedmiotem handlu są krótkoterminowe instrumenty finansowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13247">
<skos:prefLabel>polityka walutowa</skos:prefLabel>
<skos:definition>działania lub procedury podejmowane przez rząd, organizację lub osobę w związku z działalnością handlową</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13248">
<skos:prefLabel>polityka kredytowa</skos:prefLabel>
<skos:definition>działania podejmowane w celu uregulowania, ograniczenia lub podwyższenia wysokości odroczonej spłaty za dostarczone towary lub wykonane usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13249">
<skos:prefLabel>dług publiczny</skos:prefLabel>
<skos:definition>całkowita kwota zadłużenia państwa obejmująca zobowiązania zarówno wobec wierzycieli krajowych jak i zagranicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13250">
<skos:prefLabel>podatek konsumpcyjny</skos:prefLabel>
<skos:definition>kwota pobierana przez rząd od przedsiębiorstw, zależna najczęściej od łącznej wysokości sprzedaży,  która jest pokrywana przez nabywców produktów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13251">
<skos:prefLabel>podatek od kapitału</skos:prefLabel>
<skos:definition>pobierana przez skarb państwa opłata, zależna od wielkości posiadanego majątku, przeznaczona na inwestycje publiczne i działania społecznie użyteczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13252">
<skos:prefLabel>podatek dochodowy</skos:prefLabel>
<skos:definition>podatek od dochodów osiąganych w skali roku przez przedsiębiorstwa i osoby prywatne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13253">
<skos:prefLabel>polityka podatkowa</skos:prefLabel>
<skos:definition>dostosowanie wysokości zbieranych podatków i planowanych wydatków państwowych tak, aby osiągnąć założone cele</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13254">
<skos:prefLabel>cena płodów rolnych</skos:prefLabel>
<skos:definition>cena, za którą płody rolne mogą być sprzedane lub nabyte</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13255">
<skos:prefLabel>cena rynkowa</skos:prefLabel>
<skos:definition>ilość pieniędzy, za jaką aktualnie można nabyć jednostkę towaru, dobra lub usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13256">
<skos:prefLabel>koszty transportu</skos:prefLabel>
<skos:definition>wydatki związane z przemieszczaniem dóbr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13257">
<skos:prefLabel>polityka cen</skos:prefLabel>
<skos:definition>polityka, filozofia i działania związane z równowartością pieniężną za towary i usługi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13258">
<skos:prefLabel>nasycenie rynku</skos:prefLabel>
<skos:definition>stopień zaspokojenia popytu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13259">
<skos:prefLabel>struktura firmy</skos:prefLabel>
<skos:definition>układ i wzajemne relacje elementów (działów) firmy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13260">
<skos:prefLabel>zarządzanie fianansami</skos:prefLabel>
<skos:definition>zarządzanie krótko- i długoterminowe pozyskiwaniem i zarządzaniem kapitału</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13261">
<skos:prefLabel>rachunkowść księgowa</skos:prefLabel>
<skos:definition>zbieranie i przetwarzanie informacji finansowych w ramach organizacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13262">
<skos:prefLabel>polityka handlowa</skos:prefLabel>
<skos:definition>procedura, filozofia i działania podejmowane w celach handlowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13263">
<skos:prefLabel>ograniczenie konkurencyjności</skos:prefLabel>
<skos:definition>norma lub przepis ograniczający swobodny przepływ towarów i usług</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13264">
<skos:prefLabel>przepisy dotyczące obcokrajowców</skos:prefLabel>
<skos:definition>przepisy prawa krajowego dotyczące obywateli innych państw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13265">
<skos:prefLabel>prawo międzynarodowe</skos:prefLabel>
<skos:definition>ogół norm określających stosunki między państwami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13266">
<skos:prefLabel>międzynarodwe przepisy gospodarcze</skos:prefLabel>
<skos:definition>przepisy dotyczące relacji handlowych pomiędzy co najmniej dwoma krajami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13268">
<skos:prefLabel>odpowiedzialnośc karna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13269">
<skos:prefLabel>zamówienia publiczne</skos:prefLabel>
<skos:definition>kontrakt w ramach, którego organizacje lub osoby prywatne mogą wykonywać zadania opłacane z budżetu państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13270">
<skos:prefLabel>przepisy celne</skos:prefLabel>
<skos:definition>przepisy dotyczące opłat wnoszonych za import lub eksport towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13271">
<skos:prefLabel>prawo rodzinne</skos:prefLabel>
<skos:definition>zespół norm prawnych regulujących stosunki prawne dotyczące małżeństwa, stosunki między rodzicami i dziećmi, adopcję i opiekę itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13272">
<skos:prefLabel>przepisy transportowe</skos:prefLabel>
<skos:definition>przepisy dotyczące bezpiecznego poruszania się środków transportu na ziemi, w powietrzu i na wodzie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13273">
<skos:prefLabel>przepisy dotyczące produkcji rolnej</skos:prefLabel>
<skos:definition>przepisy dotyczące ziemi, uprawy roślin i hodowli zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13274">
<skos:prefLabel>przepisy dotyczące więziennictwa</skos:prefLabel>
<skos:definition>przepisy dotyczące budowy, użytkowania i administrowania więzień, zakładów karnych itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13275">
<skos:prefLabel>dzierżawa</skos:prefLabel>
<skos:definition>używanie lub użytkowanie nieruchomości oparte na umowie, za określoną opłatą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13276">
<skos:prefLabel>przepisy dotyczące przetwarzania danych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13277">
<skos:prefLabel>przepisy dotyczące konkurencyjności</skos:prefLabel>
<skos:definition>dział przepisów zajmujący się zagadnieniami takimi jak monopole i fuzje oraz inne działania mające wpływ na konkurencyjność przedsiębiorstw</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13278">
<skos:prefLabel>ograniczenia w wolnym handlu</skos:prefLabel>
<skos:definition>działania ograniczające możliwość wolnej wymiany towarów i usług w obrębie danego kraju lub pomiędzy krajami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13279">
<skos:prefLabel>Układ Wspólnotowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13280">
<skos:prefLabel>przepisy transportowe</skos:prefLabel>
<skos:definition>przepisy dotyczące bezpieczeństwa i porządku przy przewozie osób i towarów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13281">
<skos:prefLabel>system prawny Wspólnot  Europejskich</skos:prefLabel>
<skos:definition>przepisy prawa Wspólnoty Europejskiej obowiązujące w krajach członkowskich</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13282">
<skos:prefLabel>przepis Wspólnot Europejskich</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13283">
<skos:prefLabel>wyrok</skos:prefLabel>
<skos:definition>orzeczenie sądowe lub administracyjne  dotyczące interpretacji przepisu, winy lub kary, rozstrzygające spór</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13284">
<skos:prefLabel>procedura prawna</skos:prefLabel>
<skos:definition>procedury ustanowione bądź zalecane w przepisach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13285">
<skos:prefLabel>dostęp do sprawiedliwości</skos:prefLabel>
<skos:definition>prawo obywateli do zwracanie się do organów państwa odpowiedzialnych za wymierzanie sprawiedliwości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13286">
<skos:prefLabel>organizacja</skos:prefLabel>
<skos:definition>korporacja, agenda rządowa, instytucja, przedsiębiorstwo lub grupa ludzi zjednoczonych wspólnym planem, programem działania, wspólnymi poglądami, zadaniami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13287">
<skos:prefLabel>palestra</skos:prefLabel>
<skos:definition>ogół osób zajmujących się zawodowo doradzaniem klientom w sprawach przepisów, reprezentowaniem ich w postępowaniach sądowych i obsługą prawną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13288">
<skos:prefLabel>system prawny</skos:prefLabel>
<skos:definition>organizacja i sposób działania sądów i innych organizacji odpowiedzialnych za wdrażanie i egzekwowanie prawa lub jego interpretację</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13289">
<skos:prefLabel>jednostka administracyjna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13290">
<skos:prefLabel>procedura legislacyjna</skos:prefLabel>
<skos:definition>unormowany przepisami i  zwyczajami sposób działania ciała ustawodawczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13291">
<skos:prefLabel>sposób zarządzania</skos:prefLabel>
<skos:definition>systematyczne działania podejmowane dla osiągnięcia założonych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13292">
<skos:prefLabel>struktura instytucjonalna</skos:prefLabel>
<skos:definition>wchodzące w skład instytucji elementy i ich wzajemne powiązania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13293">
<skos:prefLabel>działalność instytucjonalna</skos:prefLabel>
<skos:definition>zadania, działania lub funkcje wypełniane i wykonywane przez organy rządowe i inne organizacje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13294">
<skos:prefLabel>wspólna polityka gospodarcza</skos:prefLabel>
<skos:definition>zespół zasad ustanowionych przez Wspólnotę Europejską obejmujący politykę importową i eksportową, porozumienia celne z krajami z poza Wspólnoty itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13295">
<skos:prefLabel>wspólna polityka celna</skos:prefLabel>
<skos:definition>działania przyjęte i wdrażane przez kraje członkowskie polegające na nakładaniu ceł na towary importowane spoza Wspólnoty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13296">
<skos:prefLabel>polityka badawcza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13297">
<skos:prefLabel>polityka budowlana</skos:prefLabel>
<skos:definition>działania podejmowane przez rząd lub inną organizację  w celu zorganizowania i przeprowadzenia budowy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13298">
<skos:prefLabel>polityka informacji</skos:prefLabel>
<skos:definition>procedura i praktyka przejęta przez rząd dotycząca sposobu przekazywania informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13299">
<skos:prefLabel>polityka produkcyjna</skos:prefLabel>
<skos:definition>środki i działania podejmowane przez rząd dla sterowania sferą produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13300">
<skos:prefLabel>polityka dotycząca badań przestrzeni kosmicznej</skos:prefLabel>
<skos:definition>działania przyjęte i stosowane przez rząd lub inną organizację wspierające badania przestrzeni kosmicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13301">
<skos:prefLabel>region gospodarczy</skos:prefLabel>
<skos:definition>region lub obszar administracyjny wyznaczony w oparciu o kryterium materialne związane z produkcją lub dystrybucją</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13302">
<skos:prefLabel>polityka dotycząca udzielania pomocy socjalnej i humanitarnej</skos:prefLabel>
<skos:definition>działania przyjęte i stosowane przez rząd lub inną organizację dotyczące świadczenia pomocy, często na terenie innego kraju</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13303">
<skos:prefLabel>pomoc humanitarna</skos:prefLabel>
<skos:definition>wsparcie lub pomoc świadczona aby ratować życie ludzkie lub łagodzić cierpienie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13304">
<skos:prefLabel>konflikt międzynarodowy</skos:prefLabel>
<skos:definition>sprzeczność interesów, brak zgody lub wojna między krajami, często powodująca konieczność mediacji lub interwencji ze strony krajów trzecich i organizacji międzynarodowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13305">
<skos:prefLabel>działania zmierzające do zachowania pokoju</skos:prefLabel>
<skos:definition>działania zapobiegania, przeciwdziałania, ograniczania lub zakończenie wrogich działań pomiędzy grupami lub krajami, podejmowane przez strony trzecie przy użyciu pozostających w dyspozycji organizacji międzynarodowych sił wojskowych i cywilnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13306">
<skos:prefLabel>ograniczanie zanieczyszczeń</skos:prefLabel>
<skos:definition>procedura lub działanie podejmowane dla ograniczenia zmian w środowisku powodowanych bezpośrednio lub pośrednio przez człowieka</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13307">
<skos:prefLabel>poziom wykształcenia</skos:prefLabel>
<skos:definition>pozycja w skali zdobytej wiedzy, najczęściej potwierdzona urzędowym zaświadczeniem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13308">
<skos:prefLabel>wykształacenie ogólne</skos:prefLabel>
<skos:definition>nieformalny lub zinstytucjonalizowany sposób przekazywania wiedzy ogólnej nie ograniczający się do jednej dziedziny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13309">
<skos:prefLabel>lekcje</skos:prefLabel>
<skos:definition>materiał przerabiany w instytucji związanej z kształceniem, w tym praca w szkole i prace domowe</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13310">
<skos:prefLabel>życie szkolne</skos:prefLabel>
<skos:definition>każde i wszystkie doznania związane z uczęszczaniem do ośrodka kształcenia lub spędzony tam czas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13311">
<skos:prefLabel>materiały dydaktyczne</skos:prefLabel>
<skos:definition>materiały wykorzystywane dla ułatwienia nauczania i uczenia się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13312">
<skos:prefLabel>system informacji dokumentacyjnej</skos:prefLabel>
<skos:definition>system informacyjno-wyszukiwawczy, którego zbiór wyszukiwawczy składa się z dokumentów pochodnych, najczęściej w postaci charakterystyk wyszukiwawczych dokumentów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13313">
<skos:prefLabel>środki komunikacji</skos:prefLabel>
<skos:definition>środki, za pomocą których odbywa się przekazywanie informacji we współczesnych społeczeństwach, np.prasa, radio, telewizja, Internet</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13314">
<skos:prefLabel>system komunikowania się</skos:prefLabel>
<skos:definition>usystematyzowany zbiór ludzi, urządzeń i innych zasobów służących do wymiany informacji i danych za pomocą zrozumiałaych symboli</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13316">
<skos:prefLabel>system przetwarzania danych</skos:prefLabel>
<skos:definition>system przetwarzania przez komputer dowolnych informacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13317">
<skos:prefLabel>analiza socjologiczna</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13318">
<skos:prefLabel>behawioryzm</skos:prefLabel>
<skos:definition>badanie zachowania się ludzi i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13319">
<skos:prefLabel>służba zdrowia</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13320">
<skos:prefLabel>sztuczne zapłodnienie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13321">
<skos:prefLabel>zdrowie zwierząt</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13324">
<skos:prefLabel>ofiara wojny</skos:prefLabel>
<skos:definition>osoba, która ucierpiała na skutek niszczących działań podejmowanych w ramach konfliktu zbrojnego w szczególności śmierć, uszczerbek na zdrowiu, utrata własności, konieczność przemieszczania się</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13325">
<skos:prefLabel>migracja wewnętrzna</skos:prefLabel>
<skos:definition>przemieszczanie się ludności w granicach kraju lub regionu, często związane z poszukiwaniem korzystniejszych warunków pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13326">
<skos:prefLabel>gęstość zaludnienia</skos:prefLabel>
<skos:definition>liczba ludności zamieszkującej dany obszar przypadająca na jednostkę powierzchni terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13327">
<skos:prefLabel>struktura populacji</skos:prefLabel>
<skos:definition>grupy mieszkańców danego regionu lub kraju i ich wzajemne proporcje</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13329">
<skos:prefLabel>prawa jednostki</skos:prefLabel>
<skos:definition>przepisy i gwarancje ustanowione prawem oraz wynikające z zasad moralnych przyznane każdemu członkowi danej społeczności</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13330">
<skos:prefLabel>prawa gospodarcze</skos:prefLabel>
<skos:definition>przepisy i gwarancje ustanowione prawem dotyczące dostępu do oraz udziału w środkach produkcji i zysków z niej płynących</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13333">
<skos:prefLabel>trendy opinii publicznej</skos:prefLabel>
<skos:definition>ogólne kierunki i zmiany punktów widzenia społeczeństwa jako całości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13335">
<skos:prefLabel>ideologia polityczna</skos:prefLabel>
<skos:definition>system poglądów, idei, pojęć politycznych grupy ludzi, uwarunkowany czasem, miejscem, stosunkami społecznymi wartościujący wydarzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13336">
<skos:prefLabel>stan cywilny</skos:prefLabel>
<skos:definition>zespół cech określających sytuację osoby fizycznej w rodzinie, zwłaszcza w relacji do związku małżeńskiego (np. mężatka, rozwiedziony, wdowiec), a w szerszym znaczeniu — jej sytuację osobistą</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13337">
<skos:prefLabel>grupa społeczno-kulturowa</skos:prefLabel>
<skos:definition>grupa ludzi o poczuciu wspólnoty etnicznej, rodowej lub regionalnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13338">
<skos:prefLabel>struktura zatrudnienia</skos:prefLabel>
<skos:definition>organizacja i proporcje ilościowe różnego rodzaju zawodów i etatów w obrębie przedsiębiorstwa lub gospodarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13339">
<skos:prefLabel>stanowisko</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13340">
<skos:prefLabel>rozwiązanie stosunku pracy</skos:prefLabel>
<skos:definition>dokument lub działanie skutkujące rozwiązaniem umowy o stosunku pracy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13341">
<skos:prefLabel>ludność czynna zawodowo w rolnictwie</skos:prefLabel>
<skos:definition>liczba osób utrzymujących się z własnej pracy zawodowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13342">
<skos:prefLabel>urlop z przyczyn socjalnych</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13343">
<skos:prefLabel>siła robocza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13550">
<skos:prefLabel>obóz</skos:prefLabel>
<skos:definition>1) Miejsce, w którym ustawione są namioty lub inne tymczasowe budowle; 2) tymczasowe budowle wykorzystywane przez ludzi np. wojskowych, harcerzy, turystów, Romów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13552">
<skos:prefLabel>brzeg</skos:prefLabel>
<skos:definition>granica miedzy wodą a lądem (od strony lądu)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13553">
<skos:prefLabel>zrywka drewna</skos:prefLabel>
<skos:definition>ściąganie ściętych drzew do miejsca magazynowania, załadunku lub przetwarzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13555">
<skos:prefLabel>tantiemy</skos:prefLabel>
<skos:definition>finansowe zadośćuczynienie za użytkowanie cudzej własności, najczęściej w postaci ustalonej części zysków z jej użytkowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13556">
<skos:prefLabel>akt nadania</skos:prefLabel>
<skos:definition>formalny zapis transakcji i działań podejmowanych w ramach danej jednostki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13557">
<skos:prefLabel>odchody zwierzęce</skos:prefLabel>
<skos:definition>produkty szkodliwe lub zbędne wydalane przez zwierzęta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13560">
<skos:prefLabel>zasiłek</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13561">
<skos:prefLabel>konserwacja</skos:prefLabel>
<skos:definition>utrzymywanie w dobrym stanie, dbanie, pielęgnowanie  w celu zabezpieczenia przed szybkim zużyciem się, zniszczeniem lub zepsuciem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13562">
<skos:prefLabel>malarnia</skos:prefLabel>
<skos:definition>pomieszczenie, dział jakiegoś przedsiębiorstwa, fabryki itp., gdzie się barwi, maluje produkowane wyroby; warsztat malarski</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13567">
<skos:prefLabel>złoże soli</skos:prefLabel>
<skos:definition>miejsce, gdzie tworzą się lub istnieją osady soli krystalicznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13568">
<skos:prefLabel>szlak turytyczny</skos:prefLabel>
<skos:definition>wyznaczony szlak, po którym można maszerować dla przyjemności lub dla zażycia ruchu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13570">
<skos:prefLabel>przelew</skos:prefLabel>
<skos:definition>urządzenie lub budowla służąca do odprowadzania nadmiaru wody ze zbiornika lub przewodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13572">
<skos:prefLabel>sadzonki</skos:prefLabel>
<skos:definition>młode rośliny powstałe na skutek ukorzenienia fragmentu istniejącej rośliny, rozmnożone wegetatywnie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13576">
<skos:prefLabel>eko-bilans</skos:prefLabel>
<skos:definition>zużycie energii i zasobów oraz zanieczyszczenia powodowane w cyklu produkcyjnym danego produktu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13577">
<skos:prefLabel>tlenek azotu</skos:prefLabel>
<skos:definition>bezbarwny gaz, który w temperaturze pokojowej tworzy z tlenem tlenki azotu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13613">
<skos:prefLabel>piec przemysłowy</skos:prefLabel>
<skos:definition>urządzenie przemysłowe do przeprowadzania procesów technologicznych, które zachodzą w wysokich temperaturach</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13639">
<skos:prefLabel>wołowy</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13697">
<skos:prefLabel>aprobata</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13722">
<skos:prefLabel>oczyszczanie</skos:prefLabel>
<skos:definition>doprowadzanie do stanu czystości, usuwanie nieporządnych substancji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13748">
<skos:prefLabel>śródmieście</skos:prefLabel>
<skos:definition>1) Obszar śródmiejski, położony w poblizu centrum, gdzie uboga ludnośc zamieszkuje w złych warunkach; 2) zdegradowane obszary śródmiejskie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13781">
<skos:prefLabel>śluza</skos:prefLabel>
<skos:definition>ruchome zamknięcie  stanowiące część komory umożliwiającej przepływanie statków i barek między zbiornikami o różnych poziomach wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13839">
<skos:prefLabel>oddziaływanie akustyczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13921">
<skos:prefLabel>lasy liściaste</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13922">
<skos:prefLabel>lasy mieszane</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13937">
<skos:prefLabel>sankcje karne</skos:prefLabel>
<skos:definition>konsekwencje prawne (np. kara pozbawienia wolności, odszkodowanie), jakie powinny być zastosowane przez organy państwowe wobec osób, których zachowanie nie było zgodne z obowiązującą normą prawną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/13976">
<skos:prefLabel>drzewa iglaste</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14082">
<skos:prefLabel>mieszkania zastępcze</skos:prefLabel>
<skos:definition>miejsca zamieszkania przyznawane ofiarom nagłych i najczęściej niespodziewanych zdarzeń, w których ucierpieli ludzie, środowisko i nieruchomości</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14098">
<skos:prefLabel>segregacja u źródła</skos:prefLabel>
<skos:definition>klasyfikacja i rozdzielanie odpadów stałych na rodzaje w miejscu, w którym są one wytwarzane</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14146">
<skos:prefLabel>ochrona obszarów nadmorskich</skos:prefLabel>
<skos:definition>działania związane z planowaniem projektów, ich zatwierdzaniem lub zakazem przeprowadzania niektórych prac w obszarach nadmorskich, aby ochronić je przed niszczącymi działaniami wynikającymi z niestarannie zaplanowanych działań</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14163">
<skos:prefLabel>budynek ministerstwa</skos:prefLabel>
<skos:definition>budynek użytkowany przez administrację państwową szczebla centralnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14165">
<skos:prefLabel>zdjęcie lotnicze</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14194">
<skos:prefLabel>bagno</skos:prefLabel>
<skos:definition>obszar trwale podmokły, porośnięty roślinnością przystosowaną do warunków środowiska, np. mangrowce</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14208">
<skos:prefLabel>proces legislacyjny</skos:prefLabel>
<skos:definition>proces niezbędny, aby przygotować i wdrożyć przepis prawa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14236">
<skos:prefLabel>pojemniki zwrotne</skos:prefLabel>
<skos:definition>pojemnik, którego zwrot po wykorzystaniu zawartości służy jego powtórnemu wykorzystaniu w celu powtórnego użycia lub przetworzenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14243">
<skos:prefLabel>dyfuzja zanieczyszczeń w pomieszczeniach</skos:prefLabel>
<skos:definition>rozkład i przenoszenie w powietrzu zanieczyszczeń powstałych w procesach spalania lub produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14245">
<skos:prefLabel>półprodukt</skos:prefLabel>
<skos:definition>częściowo przetworzony produkt, wykorzystywany jako surowiec w kolejnym etapie produkcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14254">
<skos:prefLabel>czynniki mutagenne</skos:prefLabel>
<skos:definition>czynnik zdolny do wywołania mutacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14330">
<skos:prefLabel>substancje teratogenne</skos:prefLabel>
<skos:definition>substancje powodujące anomalie w rozwoju płodu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14418">
<skos:prefLabel>zbiorczy system centralnego ogrzewania</skos:prefLabel>
<skos:definition>system ogrzewania ze wspólnego źródła domów mieszkalnych w mieście lub na obszarze miasta</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14499">
<skos:prefLabel>akcyza</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14507">
<skos:prefLabel>muchówki</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14543">
<skos:prefLabel>las iglasty</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14581">
<skos:prefLabel>metro</skos:prefLabel>
<skos:definition>szybka elektryczna kolej miejska, zwykle podziemna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14588">
<skos:prefLabel>zamówienie pubiczne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14602">
<skos:prefLabel>organizmy zmodyfikowane genetycznie</skos:prefLabel>
<skos:definition>organizm, w którym na skutek działania czynników zewnętrznych nastąpiła modyfikacja zestawu genów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14615">
<skos:prefLabel>turystyka wodna</skos:prefLabel>
<skos:definition>wiosłowanie, żeglowanie, pływanie łódką mające cele krajoznawcze lub będące formą czynnego wypoczynku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14619">
<skos:prefLabel>substancja powierzchniowo czynna</skos:prefLabel>
<skos:definition>substancja rozpuszczalna złożona z dwóch lub większej liczby atomów, charakteryzujących się wiązaniem chemicznym, które osłabia napięcie powierzchniowe pomiędzy cieczami lub cieczą i ciałem stałym; często używana jako detergent</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14636">
<skos:prefLabel>pH</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14651">
<skos:prefLabel>rękodzieło</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14652">
<skos:prefLabel>odpady zwierzęce</skos:prefLabel>
<skos:definition>substancje i składniki odpadowe powstałe przy produkcji zwierzęcej, części zwierząt i padłe sztuki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14663">
<skos:prefLabel>mapa morska</skos:prefLabel>
<skos:definition>mapa służąca do nawigacji morskiej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14671">
<skos:prefLabel>polityka ochrony przyrody</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14717">
<skos:prefLabel>higiena środowiskowa</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14835">
<skos:prefLabel>proekologiczne instrumenty fiskalne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14837">
<skos:prefLabel>górnictwo</skos:prefLabel>
<skos:definition>przemysł zajmujący się wydobywaniem z ziemi kopalin użytecznych i ich przeróbką oraz działania polegające na wydobyciu kopalin</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14842">
<skos:prefLabel>Morze Czarne</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14843">
<skos:prefLabel>Morze Kaspijskie</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14844">
<skos:prefLabel>Morze Śródziemne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14845">
<skos:prefLabel>informacja lokalna</skos:prefLabel>
<skos:definition>wiedza typowa dla miejscowej kultury lub społeczeństwa, wykorzystywana do podejmowania decyzji dotyczącej tego społeczeństwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14847">
<skos:prefLabel>stowarzyszenie zawodowe</skos:prefLabel>
<skos:definition>dobrowolny i trwały związek grupy osób pracujących w tym samym zawodzie, zorganizowany dla realizacji wspólnych celów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14848">
<skos:prefLabel>dyscyplina naukowa</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14849">
<skos:prefLabel>trwałe zanieczyszczenia organiczne</skos:prefLabel>
<skos:definition>zanieczyszczenia organiczne, które w ograniczonym stopniu podlegają procesom rozkładu i kumulują się w środowisku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14850">
<skos:prefLabel>resuspensja osadu</skos:prefLabel>
<skos:definition>przemieszczanie się, ruch nierozpuszczalnych cząstek na skutek działania wiatru lub wody</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14851">
<skos:prefLabel>niszczenie siedlisk</skos:prefLabel>
<skos:definition>rosnące zapotrzebowanie terenu i ekspansja działań człowieka na tereny dotychczas nieużytkowane gospodarczo grozi wyginięciem niektórych gatunków roślin i zwierząt i stanowi zagrożenie dla zasobów genetycznych na naszej planecie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14852">
<skos:prefLabel>działalność gospodarcza na lądzie</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14853">
<skos:prefLabel>zmiany fizyczne</skos:prefLabel>
<skos:definition>dowolna zmiana ciała lub substancji nie powodujaca zmiany jej składu chemicznego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14854">
<skos:prefLabel>ocena selektywna</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14856">
<skos:prefLabel>infrastruktura informacyjna</skos:prefLabel>
<skos:definition>zespół podstawowych urządzeń z dziedziny informatyki, telekomunikacji, niezbędnych do należytego funkcjonowania informacji naukowej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14857">
<skos:prefLabel>informacja geograficzna</skos:prefLabel>
<skos:definition>wszystkie formy informacji mające odniesienie geograficzne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14858">
<skos:prefLabel>ośrodek przetwarzania danych</skos:prefLabel>
<skos:definition>instytucja stworzona w celu pozyskiwania, analizowania, przetwarzania, magazynowania i rozpowszechniania jednego lub wielu rodzajów danych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14859">
<skos:prefLabel>dostawca usług internetowych</skos:prefLabel>
<skos:definition>dostawca usług w sieci Internet</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14860">
<skos:prefLabel>sieć komputerowa</skos:prefLabel>
<skos:definition>system relacyjnych komputerów automatycznie przetwarzających informacje (dane) zapisane cyfrowo lub analogowo</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14861">
<skos:prefLabel>sieć komputerowa rozległa (WAN)</skos:prefLabel>
<skos:definition>sieć komputerowa o dużym zasięgu rozciągająca się na dużych obszarach geograficznych; sieci rozlegle łączą miliony komputerów na całej Ziemi; największą i najpopularniejszą siecią rozległą jest sieć Internet</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14862">
<skos:prefLabel>Internet</skos:prefLabel>
<skos:definition>sieć komputerowa rozległa o zasięgu globalnym; ogólnoświatowy zbiór wzajemnie połączonych sieci komputerowych lokalnych i rozległych; sieć sieci; nie jest to miejsce lecz sposób komunikacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14863">
<skos:prefLabel>baza danych WWW</skos:prefLabel>
<skos:definition>zbiór dokumentów hipertekstowych powiązanych ze sobą i używanych w Internet</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14864">
<skos:prefLabel>dokument początkowy</skos:prefLabel>
<skos:definition>w Web dokument rozpoczynający działalność po uruchomieniu przeglądarki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14865">
<skos:prefLabel>hipertekst</skos:prefLabel>
<skos:definition>tekst zawierający odwołania do innych dokumentów tekstowych; podstawowymi elementami hipertekstu są węzły i powiązania; węzeł to wydzielony, ze względu na zawartość informacyjną, fragment tekstu (słowo, fraza, zdanie, akapit, rysunek, schemat, wideo klip itp.) lub dokument w bazie danych; system hipertekstowy może obejmować jedną lub wiele tekstowych albo multimedialnych baz danych (hipermedia)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14866">
<skos:prefLabel>system BBS</skos:prefLabel>
<skos:definition>zestaw komputerów i modemów pracujących z oprogramowaniem biuletynu informacyjnego, który umożliwia użytkownikom łączenie, wysyłanie poczty, uczestnictwo w konferencji i czasami dostęp do Internet</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14867">
<skos:prefLabel>grupa tematyczna</skos:prefLabel>
<skos:definition>publiczne forum wymiany idei, artykułów i informacji, które są przesyłane do wybranych komputerów w sieci Internet</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14868">
<skos:prefLabel>poczta elektroniczna</skos:prefLabel>
<skos:definition>komunikat lub dokument przekazany elektronicznie za pośrednictwem telekomunikacji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14869">
<skos:prefLabel>techniki multimedialne</skos:prefLabel>
<skos:definition>technika łącząca tekst, dźwięk i nieruchome lub ruchome obrazy, często pozwalająca na interaktywny udział odbiorcy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14870">
<skos:prefLabel>prezentacja audiowizualna</skos:prefLabel>
<skos:definition>prezentacja lub wykład wykorzystujące zmysł zarówno wzroku jak i słuchu odbiorcy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14871">
<skos:prefLabel>pakiet informacyjny</skos:prefLabel>
<skos:definition>zestaw materiałów dotyczących jakiegoś zagadnienia zwykle umieszczony w pojemniku</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14872">
<skos:prefLabel>eksponat</skos:prefLabel>
<skos:definition>przedmiot wystawiony na pokaz w muzeum, na wystawie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14873">
<skos:prefLabel>biuletyn informacyjny</skos:prefLabel>
<skos:definition>periodycznie publikowany biuletyn adresowany do członków grupy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14874">
<skos:prefLabel>program radiowy</skos:prefLabel>
<skos:definition>transmisja sygnału dźwiękowego za pomocą fal elektromagnetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14875">
<skos:prefLabel>program telewizyjny</skos:prefLabel>
<skos:definition>transmisja sygnału adiowizualnego za pomocą fal elektromagnetycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14876">
<skos:prefLabel>stosunki między daną organizacją a ludnością</skos:prefLabel>
<skos:definition>metody i działania stosowane przez jednostki, organizacje, korporacje lub rządy do promowania właściwych stosunków ze społeczeństwem</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14878">
<skos:prefLabel>system informacji bibliograficznej</skos:prefLabel>
<skos:definition>usystematyzowany zbiór ludzi, urządzeń i innych zasobów służących do wymiany danych dotyczących historii, opisu, porównania i klasyfikacji książek i innych prac</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14879">
<skos:prefLabel>informacja bibliograficzna</skos:prefLabel>
<skos:definition>zbiór opisów bibliograficznych dokumentów dobranych według określonych kryteriów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14880">
<skos:prefLabel>referencyjny system informacyjny</skos:prefLabel>
<skos:definition>usystematyzowany zbiór ludzi, urządzeń i innych zasobów służących pomocą w odnalezieniu innych źródeł informacji dotyczących tego samego zagadnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14881">
<skos:prefLabel>informacja referencyjna</skos:prefLabel>
<skos:definition>odnośniki do innego źróła informacji dotyczącego danego zagadnienia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14882">
<skos:prefLabel>system informacji statystycznej</skos:prefLabel>
<skos:definition>usystematyzowany zbiór ludzi, urządzeń i innych zasobów służących do wymiany danych  numerycznych uprzednio zebranych, zaklasyfikowanych i zinterpretowanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14883">
<skos:prefLabel>informacja statystyczna</skos:prefLabel>
<skos:definition>informacja wynikająca z badania zjawisk i procesów, w jakich biorą udział dokumenty oraz interpretacji tych danych przy zastosowaniu metod matematycznych i statystycznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14884">
<skos:prefLabel>obsługa biblioteczna</skos:prefLabel>
<skos:definition>obsługa użytkowników bibliotek i ich zasobów wynikająca z obowiązków powołanych do tego celu instytucji publicznych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14885">
<skos:prefLabel>serwis informacyjny</skos:prefLabel>
<skos:definition>obsługa użytkowników w zakresie informacji dotyczącej zasobów bibliotecznych i baz danych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14886">
<skos:prefLabel>indeksowanie dokumentów</skos:prefLabel>
<skos:definition>tworzenie charakterystyki wyszukiwawczej dokumentu i/lub instrukcji wyszukiwawczej; na proces ten składają się następujące po sobie czynności: analizowanie, selekcjonowanie informacji i tłumaczenie; jakość procesu indeksowania ma zasadniczy wpływ na jakość systemu informacyjno-wyszukiwawczego, a więc i na stopień zaspokojenia przez system potrzeb informacyjnych użytkowników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14887">
<skos:prefLabel>wypożyczanie dokumentów</skos:prefLabel>
<skos:definition>wypożyczanie książek i/lub innych dokumentów poza bibliotekę</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14888">
<skos:prefLabel>wypożyczenia międzybiblioteczne</skos:prefLabel>
<skos:definition>wypożyczanie książek i/lub innych dokumentów pomiędzy bibliotekami</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14889">
<skos:prefLabel>selektywna dystrybucja informacji</skos:prefLabel>
<skos:definition>periodyczne dostarczanie użytkownikowi przez system informacyjno-wyszukiwawczy (zazwyczaj zautomatyzowany) bieżącej informacji w odpowiedzi na zapytanie informacyjne sformułowane w postaci profilu użytkownika</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14890">
<skos:prefLabel>obsługa informacyjna z zasobów na nośniku CD-ROM</skos:prefLabel>
<skos:definition>pomoc w wyszukiwaniu danych przechowywanych na optycznych nośnikach danych cyfrowych świadczona przez odpowiednio przeszkolonych pracowników biblioteki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14891">
<skos:prefLabel>internetowy system obsługi</skos:prefLabel>
<skos:definition>pomoc w wyszukiwaniu danych w internecie świadczona przez odpowiednio przeszkolonych pracowników biblioteki lub internetowy system informacyjny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14892">
<skos:prefLabel>system wymiany informacji</skos:prefLabel>
<skos:definition>określony pewnymi zasadami przepływ informacji zapewniający jak najlepszy do niej dostęp</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14893">
<skos:prefLabel>system wspomagania decyzji</skos:prefLabel>
<skos:definition>usystematyzowany zbiór ludzi, urządzeń i innych zasobów służących do analizowania danych i przedstawiania ich decydentowi w sposób najlepiej służący podejmowanej decyzji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14894">
<skos:prefLabel>opracowywanie oprogramowania</skos:prefLabel>
<skos:definition>opracowanie ciągu instrukcji napisanych w języku zrozumiałym dla komputera, pozwalających wykonać jakąś operację, np. przetwarzanie tekstów, obrazów, wykonywanie obliczeń</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14896">
<skos:prefLabel>wymiana informacji</skos:prefLabel>
<skos:definition>wymiana danych pomiędzy dwoma lub większą liczbą uczestników</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14897">
<skos:prefLabel>relacyjna baza danych</skos:prefLabel>
<skos:definition>zbiór informacji liczbowych w postaci tabel, którymi można manipulowac bez zmiany ich treści</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14898">
<skos:prefLabel>skaner multispektralny</skos:prefLabel>
<skos:definition>multispektralny skaner (MSS) wykonujący zdjęcia cyfrowe z rozdzielczością 80 m, umieszczony na pokładzie satelitów amerykańskich Landsat</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14899">
<skos:prefLabel>rozdzielczość</skos:prefLabel>
<skos:definition>1) wymiar najmniejszego elementu, który można przedstawić w strukturze danych; 2) najmniejsze oddalenie dwu obiektów, przy którym są one rozróżnialne na obrazie; termin jest także używany do opisu gęstości siatki rastra (rozdzielczość przestrzenna), liczby kanałów (rozdzielczość spektralna), częstości zobrazowania (rozdzielczość czasowa)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14900">
<skos:prefLabel>piksel</skos:prefLabel>
<skos:definition>skrót od ang. picture element; najmniejszy, niepodzielny element tworzący obraz cyfrowy; w rastrowym modelu danych, dane są przedstawione w formie tablicy; pojedynczej komórce tej tablicy odpowiada piksel, do którego jest przypisana wartość tej komórki (jej atrybut)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14901">
<skos:prefLabel>pasmo spektralne</skos:prefLabel>
<skos:definition>1) zakres długości fal elektromagnetycznych używany do opisu czułości urządzeń teledetekcyjnych; 2) plik zawierający obraz cyfrowy zarejestrowany w danym paśmie urządzenia teledekcyjnego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14902">
<skos:prefLabel>identyfikacja obrazu</skos:prefLabel>
<skos:definition>ciąg danych numerycznych, który pozwala na zidentyfikowanie określonego obrazu w ramach bazy danych GIS</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14903">
<skos:prefLabel>numeryczny system przetwarzania obrazów</skos:prefLabel>
<skos:definition>zestaw urządzeń przeznaczony do zbierania i przetwarzania obrazów w formie danych dyskretnych lub liczbowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14904">
<skos:prefLabel>technika numerycznego przetwarzania obrazów</skos:prefLabel>
<skos:definition>techniki stosowane przy kalibracji obrazu, przy pozyskiwaniu lub przesyłaniu danych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14905">
<skos:prefLabel>rozpoznawanie na podstawie wzorców</skos:prefLabel>
<skos:definition>identyfikacja wzorów pozwalająca na zaklasyfikowanie danych z teledetekcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14906">
<skos:prefLabel>fotomozaika</skos:prefLabel>
<skos:definition>złożone zdjęcie lotnicze, skomponowane z kilku zdjęć dotyczących częściowo pokrywającego się terenu</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14907">
<skos:prefLabel>filtracja obrazu</skos:prefLabel>
<skos:definition>proces generalizacji danych, polegający na redukowaniu różnorodności spektralnej lub przestrzennej elementów obrazu cyfrowego za pomocą filtrów cyfrowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14908">
<skos:prefLabel>obraz wzmocniony</skos:prefLabel>
<skos:definition>filtrowanie danych w procesie obróbki obrazu mającego na celu podkreślanie zagadnień istonych w interpretacji wizualnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14909">
<skos:prefLabel>korekcja zniekształceń geometrycznych</skos:prefLabel>
<skos:definition>poprawienie geometrii obrazu cyfrowego przez transformacje geometryczne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14910">
<skos:prefLabel>nakładanie obrazów</skos:prefLabel>
<skos:definition>proces przypisywania współrzędnych na mapie punktom kontrolnym o znanych współrzędnych w terenie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14911">
<skos:prefLabel>klasyfikacja</skos:prefLabel>
<skos:definition>metoda generalizacji polegająca na grupowaniu w klasy danych mających jakąś wspólną cechę; zbiór powstały w ten sposób jest mniej zróżnicowany i mniejszy objętościowo; klasyfikacja dotyczy najczęściej atrybutów obiektów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14912">
<skos:prefLabel>klasyfikacja nadzorowana</skos:prefLabel>
<skos:definition>komputerowa analiza zdjęcia, podczas której tworzące je piksele zostają przyporządkowane określonym klasom według zastosowanego kryterium prawdopodobieństwa; klasy zostały tu określone za pomocą zidentyfikowanych obszarów reprezentatywnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14913">
<skos:prefLabel>klasyfikacja nienadzorowana</skos:prefLabel>
<skos:definition>klasyfikacja automatyczna, w której z góry zakłada się podział pikseli na określoną liczbę klas</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14914">
<skos:prefLabel>skład koloru</skos:prefLabel>
<skos:definition>określenie stosowane w teledetekcji dla przypisania różnych kolorów różnym pasmom spektrum</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14915">
<skos:prefLabel>korekcja atmosferyczna</skos:prefLabel>
<skos:definition>uwzględnienie w danych z teledetekcji skutków spowodowanych przez rozproszenie i absorbcję  światła</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14916">
<skos:prefLabel>geograficzny system numeryczny</skos:prefLabel>
<skos:definition>zorganizowany system składający się z komputera, oprogramowania, danych geograficznych i obsługi, zaprojektowany w celu efektywnego przechowywania, uaktualniania, przetwarzania, analizowania i wyświetlania wszystkich form informacji mających odniesienie geograficzne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14917">
<skos:prefLabel>geograficzny format numeryczny</skos:prefLabel>
<skos:definition>dane zbierane zdalnie w postaci numerycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14918">
<skos:prefLabel>wektor</skos:prefLabel>
<skos:definition>rodzaj danych przestrzennych, w których podstawową jednostką informacji jest punkt (wektor) i linia (grupa punktów); linie mogą być zorganizowane w łańcuchy i wieloboki</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14919">
<skos:prefLabel>punkt</skos:prefLabel>
<skos:definition>1)pojedynczy obiekt o położeniu (x, y)w układzie współrzędnych; ma zerową długość i powierzchnię; 2)punkt reprezentuje obiekty, które w danej skali są zbyt małe, aby przedstawić je jako powierzchnię</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14920">
<skos:prefLabel>linia</skos:prefLabel>
<skos:definition>reprezentacja geometryczna obiektu zbyt wąskiego, aby można go przedstawić w danej skali jako powierzchnię; w modelach środowiska przyrodniczego linia jest analogiem dróg, rzek, granic itp.; w modelu wektorowym linia rozpoczyna się i kończy w węźle</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14921">
<skos:prefLabel>poligon</skos:prefLabel>
<skos:definition>granica obszaru wyznaczonego przez wielobok, służy do opisywania obiektów powierzchniowych: lasy, zbiorniki wodne, działki, podział administracyjny</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14922">
<skos:prefLabel>raster</skos:prefLabel>
<skos:definition>uporządkowany zbiór punktów (pikseli) umieszczonych w węzłach umownej siatki (grid); raster jest jednym z modeli danych przestrzennyc; odległość między elementami rastra określa jego rozdzielczość</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14923">
<skos:prefLabel>atrybut</skos:prefLabel>
<skos:definition>1) opisowa informacja (cecha) charakteryzująca jednostkę przyrodniczą, obiek; 2) cecha opisująca encję (najmniejsza jednostka przyrodnicza) w relacyjnej bazie danych, odpowiada polu w rekordzie tablicy</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14924">
<skos:prefLabel>technika numeryczna GIS</skos:prefLabel>
<skos:definition>konwersja danych z postaci analogowej na numeryczną</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14925">
<skos:prefLabel>interpolacja</skos:prefLabel>
<skos:definition>szacowanie wartości zmiennej w punkcie interpolacji na podstawie sąsiednich wartości określonych w punktach pomiarowych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14926">
<skos:prefLabel>gridding</skos:prefLabel>
<skos:definition>metoda interpolacji nieznanych wartości na powierzchni, określająca je we wszystkich węzłach regularnej siatki na podstawie wartości w dowolnie rozmieszczonych punktach kontrolnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14927">
<skos:prefLabel>konwersja formatu rastrowego na wektorowy</skos:prefLabel>
<skos:definition>metoda przekształcania danych rastrowych w wektorowe lub odwrotnie, najczęściej w opraciu o procedury i programy stanowiące część oprogramowania GIS</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14928">
<skos:prefLabel>konwersja formatu wektorowego na rastrowy</skos:prefLabel>
<skos:definition>metoda przekształcania danych rastrowych w wektorowe lub odwrotnie, najczęsciej w opraciu o procedury i programy stanowiące część oprogramowania GIS</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14930">
<skos:prefLabel>granica państwa</skos:prefLabel>
<skos:definition>linia oddzielająca terytorium jednego państwa od innych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14931">
<skos:prefLabel>granica wewnątrz państwa</skos:prefLabel>
<skos:definition>rozgraniczenie terenów położonych w obrębie jednego państwa</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14932">
<skos:prefLabel>granica administracyjna</skos:prefLabel>
<skos:definition>granica przestrzenna terenu bedącego we władaniu danej jednostki administracyjnej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14933">
<skos:prefLabel>dane przestrzenne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14934">
<skos:prefLabel>odwzorowanie geograficzne</skos:prefLabel>
<skos:definition>odwzorowanie na płaszczyźnie powierzchni kuli ziemskiej lub jej części, wraz z siatką równoleżników i południków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14935">
<skos:prefLabel>system współrzędnych</skos:prefLabel>
<skos:definition>wielkości określające położenie dowolnego punktu na powierzchni kuli ziemskiej (szerokość i długość geograficzna)</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14936">
<skos:prefLabel>szerokość geograficzna</skos:prefLabel>
<skos:definition>kąt zawarty między kierunkiem pionu w miejscu obserwacji a płaszczyzną równika; miejsce na kuli ziemskiej wyznaczone tym kątem na mapie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14937">
<skos:prefLabel>długość geograficzna</skos:prefLabel>
<skos:definition>kąt zawarty między południkiem danej miejscowości a stałym południkiem, tzw. zerowym (Greenwich); miejsce na kuli ziemskiej wyznaczone tym kątem na mapie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14938">
<skos:prefLabel>centrum teledetekcji</skos:prefLabel>
<skos:definition>ośrodek wykorzystujący technikę zdalnego pozyskiwania danych, które są przestrzennie odniesione do powierzchni Ziemi</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14939">
<skos:prefLabel>pracownia GIS</skos:prefLabel>
<skos:definition>pracownia, w której przechowuje się, uaktualniania, przetwarza, analizuje i wyświetla wszystkie formy informacji mające odniesienie geograficzne</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14940">
<skos:prefLabel>obsługa w trybie on-line</skos:prefLabel>
<skos:definition>obsługa informacyjna prowadzona w komputerowych systemach interakcyjnych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14947">
<skos:prefLabel>wysypisko</skos:prefLabel>
<skos:definition>miejsce, gdzie się  wysypuje, wyrzuca odpady, najczęściej bez wymaganych zabezpieczeń środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14949">
<skos:prefLabel>ekonomika przedsiębiorstwa</skos:prefLabel>
<skos:definition>analiza zakupów i sprzedaży uwzględniająca badania naukowe nad produkcją, dystrybucją i konsumpcją dóbr</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14950">
<skos:prefLabel>odstraszanie</skos:prefLabel>
<skos:definition>kara mająca na celu powstrzymanie przestępcy przed ponowieniem czynu oraz zniechęcenie innych do podejmowania tego typu czynów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14951">
<skos:prefLabel>porzucone tereny zanieczyszczone</skos:prefLabel>
<skos:definition>porzucone lub nieużywane wysypiska lub zwałowiska, stacje beznynowe, koksownie, dawne tereny przemysłowe itp. mogące powodować potencjalne zagrożenie nie tylko dla ziemi i wód podziemnych ale także dla ludzi i środowiska przyrodniczego</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14955">
<skos:prefLabel>teren zewnętrzny</skos:prefLabel>
<skos:definition>teren niezabudowany</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14956">
<skos:prefLabel>wydobycie spod dna morza</skos:prefLabel>
<skos:definition>najcenniejszym zasobem wydobywanym spod dna morza jest ropa naftowa; aktualnie około 15% wydobycia ma miejsce na morzu; w Anglii i Japonii prowadzone jest także wydobycie węgla spod dna morza; działalność wydobywacza na morzu ma istotne, negatywne oddziaływanie na środowisko</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14957">
<skos:prefLabel>półpłynny nawóz naturalny</skos:prefLabel>
<skos:definition>definicja zbędna</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14959">
<skos:prefLabel>przemysł elektrotechniczny</skos:prefLabel>
<skos:definition>przemysł zajmujący się produkcją urządzeń do wytwarzania, przesyłania, rozdzielania i użytkowania energii elektrycznej</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14960">
<skos:prefLabel>przedsiębiorstwo usługowe</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14962">
<skos:prefLabel>oczyszczanie wody</skos:prefLabel>
<skos:definition>proces, w którym z wody usuwane są zanieczyszczenia</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14963">
<skos:prefLabel>lokalny system zaopatrzenia w ciepło</skos:prefLabel>
<skos:definition>zapewnienie opału lub energii cieplnej na potrzeby danej wspólnoty</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14964">
<skos:prefLabel>spedytor</skos:prefLabel>
<skos:definition>osoba lub przedsiębiorstwo zajmujące się przewozem i dostawą towarów oraz załatwianiem formalności z tym związanych</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14965">
<skos:prefLabel>zapobieganie powstawaniu odpadów</skos:prefLabel>
<skos:definition>działania prowadzące do tego, aby ograniczyć (lub wyeliminować) powstawanie odpadów w procesach produkcji i konsumpcji</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14966">
<skos:prefLabel>prawo przyrody do istnienia</skos:prefLabel>
<skos:definition>nienaruszalne moralne prawo organizmów innych niż ludzie oraz ich siedlisk do istnienia w stanie niezagrożonym lub niezmienionym przez działalność ludzką, postulowane przez etyków środowiska</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14967">
<skos:prefLabel>przestępstwo przeciwko środowisku</skos:prefLabel>
<skos:definition>niezgodna z prawem działalność powodująca szkody w środowisku np. nielegalne składowanie odpadów, zanieczyszczanie wody itp.</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14968">
<skos:prefLabel>prawo lokalne</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14970">
<skos:prefLabel>władze lokalne</skos:prefLabel>
<skos:definition>administrowanie i wdrażanie praw oraz polityk rządowych na ograniczonym obszarze</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14971">
<skos:prefLabel>transakcja międzynarodowa</skos:prefLabel>
<skos:definition>porozumienie lub działanie między dwoma lub więcej krajami dotyczące handlu, przedsiębiorczosci lub innych zagadnień</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14972">
<skos:prefLabel>trasowanie dróg</skos:prefLabel>
<skos:definition>ustanawianie przebiegu trasy, drogi, autostrady na etapie planowania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14973">
<skos:prefLabel>monitoring jakości powietrza</skos:prefLabel>
<skos:definition>regularne sprawdzanie i dokumentowanie jakości powietrza w danym regionie</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14974">
<skos:prefLabel>inicjatywa obywatelska</skos:prefLabel>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14975">
<skos:prefLabel>obszar poddany antropopresji</skos:prefLabel>
<skos:definition>obszary narażone na presję z tytułu bardzo dużej liczby turystów oraz niedostatecznego planowania i zarządzania</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14976">
<skos:prefLabel>składowanie</skos:prefLabel>
<skos:definition>przechowywanie materiałów, wyrobów lub odpadów</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14982">
<skos:prefLabel>schronisko górskie</skos:prefLabel>
<skos:definition>schronienie zapewniające względnie bezpieczne warunki, zlokalizowane w terenie górskim</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14983">
<skos:prefLabel>badanie przestrzeni kosmicznej</skos:prefLabel>
<skos:definition>badania dotyczące zjawisk zachodzących poza atmosferą ziemską</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/14985">
<skos:prefLabel>&quot;czerwona lista&quot;</skos:prefLabel>
<skos:definition>powstała z inicjatywy IUCN lista zagrożonych gatunków roślin i zwierząt</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/15020">
<skos:prefLabel>izolacja</skos:prefLabel>
<skos:definition>materiał nie przewodzący ciepła, prądu lub dźwięków</skos:definition>
</rdf:Description>
<rdf:Description rdf:about="http://www.eionet.europa.eu/gemet/concept/15027">
<skos:prefLabel>strącanie</skos:prefLabel>
<skos:definition>oddzielanie cieczy od osadu polegające na ostrożnym usunięciu jej (np. przez zlanie) znad osadu</skos:definition>
</rdf:Description>
</rdf:RDF>
